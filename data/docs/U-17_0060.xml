  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0060_r"/>
      <p facs="#facs_1_region_1513083338119_1">
        <lb facs="#facs_1_region_1513083338119_1l1" n="N001"/>Wir<reg>,</reg> <persName ref="per006964">Agnes<reg>,</reg> von gottes gnaden wi<choice>
                        <abbr>ln̄t</abbr>
                        <expan>lent</expan>
                    </choice> chûnginne ze Un<choice>
                        <abbr>gˀn</abbr>
                        <expan>gern</expan>
                    </choice>
                </persName>
                <reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jechen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> tuͦn kunt allen den<reg>,</reg> die disen ansechent oder hoͤ<choice>
                    <abbr>rēt</abbr>
                    <expan>rent</expan>
                </choice>
        <lb facs="#facs_1_region_1513083338119_1l2" n="N002"/>lesen<reg>,</reg> <reg type="star">✳</reg> daz wir koͧft haben den <orgName ref="org002627">er<choice>
                        <abbr>brˀen</abbr>
                        <expan>beren</expan>
                    </choice>
                    <reg>,</reg> geistlich froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> der eptischin <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent<reg>,</reg> ùnserr stift ze Chûngsvelt<reg>,</reg> Sant
        <lb facs="#facs_1_region_1513083338119_1l3" n="N003"/>Cla<choice>
                        <abbr>rē</abbr>
                        <expan>ren</expan>
                    </choice> ordens<reg>,</reg> gelegen in Constent<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bistuͦm</orgName>
                <reg>,</reg> disù nach geschriben guͦter<reg>,</reg> <reg type="star">✳</reg> die wir koͧften umb <persName ref="per018970">un<choice>
                        <abbr>sˀn</abbr>
                        <expan>sern</expan>
                    </choice> ge<choice>
                        <abbr>tˀ</abbr>
                        <expan>tru</expan>
                    </choice>wen diener<reg>,</reg>
        <lb facs="#facs_1_region_1513083338119_1l4" n="N004"/>Ruͦd<choice>
                        <abbr>gē</abbr>
                        <expan>ger</expan>
                    </choice> dem Schenken</persName>
                <reg>.</reg> von erst: den hof <placeName ref="loc012638">Rùti</placeName>
                <reg>,</reg> der giltet jerlich <reg type="star">✳</reg> <measure>drù malter habern</measure>
                <reg>,</reg> <measure>siben mùt</measure> <measure>zwei viertel rog<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </measure>
                <reg>,</reg>
        <lb facs="#facs_1_region_1513083338119_1l5" n="N005"/>
                <measure>vier huͤnr</measure>
                <reg>,</reg> und ze <placeName ref="loc009457">Hentschikon</placeName> ein matten<reg>,</reg> giltet jerlich <measure>fûnf viertel <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>
                    <choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ein aker<reg>,</reg> giltet jerlich <measure>drû viertel
        <lb facs="#facs_1_region_1513083338119_1l6" n="N006"/>
                    <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>
                    <choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure>
                <reg>,</reg> <measure>ein viertel rog<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
