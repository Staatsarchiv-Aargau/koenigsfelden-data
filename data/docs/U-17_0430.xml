  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0430_r"/>
      <p facs="#facs_1_region_1517319159965_2">
        <lb facs="#facs_1_region_1517319159965_2l1" n="N001"/>Ich<reg>,</reg> <persName ref="per016076">Heinrich Jel<reg>,</reg> geschworner weibel <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> statt ze Waltzhuͦt</persName><reg>,</reg> tuͦn kunt<reg>,</reg> dz ich uff disem hùttigen tag<reg>,</reg> als dirre brieff ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> ist<reg>,</reg> offenlich ze gericht sassz ze <placeName ref="loc007770">Walshuͦt</placeName> in <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>
        <lb facs="#facs_1_region_1517319159965_2l2" n="N002"/>statt<reg>,</reg> an offner<reg>,</reg> fryer straͦss<reg>,</reg> an statt <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org000088">durlùchten<reg>,</reg> hochgebornen fùrsten der herztogen von Oͤs<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName><reg>,</reg> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> gnedi<choice>
                    <abbr>g₎</abbr>
                    <expan>gen</expan>
                </choice> <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren<reg>,</reg> und kam fùr mich in offen<reg>,</reg> verban<choice>
                    <abbr>nē</abbr>
