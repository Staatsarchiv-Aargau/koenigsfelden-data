  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0695_r"/>
      <p facs="#facs_1_region_1514667226959_11">
        <lb facs="#facs_1_region_1514667226959_11l1" n="N001"/>Ich<reg>,</reg> <persName ref="per016813">Clewi Scherntzer von Luppfen</persName><reg>,</reg> be<choice>
                    <abbr>ken̄</abbr>
                    <expan>kenn</expan>
                </choice> offenlich mitt disem brieff<reg>,</reg> das ich dem <orgName ref="org002627">jarzitampt ze Kùngsfeld</orgName> von ei<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> guͦt<reg>,</reg>
        <lb facs="#facs_1_region_1514667226959_11l2" n="N002"/>das ich von dem <reg type="star">✳</reg> buw<reg>,</reg>
                <reg type="star">✳</reg> jerlich zegebent pflichtig bin <reg type="star">✳</reg> <measure>zwey fiertel ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure><reg>,</reg> <measure>sechs fiertel</measure> und <measure>drig fierling roggen</measure><reg>,</reg> <measure>drig mùtt ha<choice>
                        <abbr>bˀ</abbr>
                        <expan>ber</expan>
                    </choice>
                </measure>
        <lb facs="#facs_1_region_1514667226959_11l3" n="N003"/>und <measure>vierzechen schilling haller ewigs zinsz</measure> <reg type="star">✳</reg> und das ich daby nach volkomner rech<choice>
                    <abbr>nuḡ</abbr>
                    <expan>nung</expan>
                </choice><reg>,</reg> die ich da<choice>
                    <abbr>rub̄</abbr>
                    <expan>rumb</expan>
                </choice> mitt der <persName ref="per007247">erwirdigen<reg>,</reg> wolgebor<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
        <lb facs="#facs_1_region_1514667226959_11l4" n="N004"/>frowen<reg>,</reg> frow Evan von Erdbach<reg>,</reg> jetz pflegerin desselben jarzitampts</persName><reg>,</reg> und <persName ref="per011329">Cuͦnrat Widmer</persName><reg>,</reg> irm amptman<reg>,</reg> uff <date when="1457-02-20">
                    <choice>
                        <abbr>sun̄</abbr>
                        <expan>sunn</expan>
                    </choice>tag vor <reg type="star">✳</reg> santt Matis tag
        <lb facs="#facs_1_region_1514667226959_11l5" n="N005"/>vor da<choice>
                        <abbr>tū</abbr>
                        <expan>tum</expan>
                    </choice> disz briefs nechst<choice>
                        <abbr>vˀ</abbr>
                        <expan>ver</expan>
                    </choice>gangen</date> in biwesen her <persName ref="per016816">Caspar Gerwers<reg>,</reg> lùtpriesters ze Windisch</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per016126">Hensli Zymermans von Bir</persName> und <persName ref="per016818">Uͤlrich Swanders
        <lb facs="#facs_1_region_1514667226959_11l6" n="N006"/>von Husen</persName> <reg type="star">✳</reg> getan und da<choice>
                    <abbr>rub̄</abbr>
                    <expan>rumb</expan>
                </choice> disz nachgeschrib<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> teding ervolget und ufgeno<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> hab<reg>,</reg> <reg type="star">✳</reg> recht und redlich schuldig bin und blib vom sechsundfùnf<reg type="lb">¬</reg><lb facs="#facs_1_region_1514667226959_11l7" n="N007" break="no"/>zigisten jar und davor <measure>vier mùtt kernen</measure><reg>,</reg> <measure>fùnfzechen mùtt</measure><reg>,</reg> <measure>zwey fiertel</measure> und <measure>drig fierling roggen</measure><reg>,</reg> <measure>siben malter</measure> und <measure>ein fiertel ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure><reg>,</reg> <measure>nùn pfund</measure>
        <lb facs="#facs_1_region_1514667226959_11l8" n="N008"/>und <measure>zwen schil<choice>
                        <abbr>līg</abbr>
                        <expan>ling</expan>
                    </choice> haller</measure> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>sessener zinsz<reg>,</reg> <reg type="star">✳</reg> da<choice>
                    <abbr>rub̄</abbr>
                    <expan>rumb</expan>
                </choice> <persName ref="per016819">Heini Lalli von Bir bùrg</persName> ist<reg>,</reg> <reg type="star">✳</reg> des ich<reg>,</reg> der selb <persName ref="per016819">Heini Lalli</persName><reg>,</reg> gichtig bin<reg>.</reg> <reg type="star">✳</reg> und ist disz die teding hie<choice>
                    <abbr>rub̄</abbr>
                    <expan>rumb</expan>
                </choice>
        <lb facs="#facs_1_region_1514667226959_11l9" n="N009"/>berett<reg>,</reg> mittnamen<reg>,</reg> <reg type="star">✳</reg> das ich<reg>,</reg> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <persName ref="per016813">Clewi Scherntzer</persName><reg>,</reg> oder min erben sond dem jarzitampt <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> sinen pflegern <reg type="star">✳</reg> fùr krieg und all sachen soͤ<reg type="lb">¬</reg><lb facs="#facs_1_region_1514667226959_11l10" n="N010" break="no"/>lich <choice>
                    <abbr>sum̄</abbr>
                    <expan>summ</expan>
                </choice> der zinsen <reg type="star">✳</reg> bezaln<reg>,</reg> <reg type="star">✳</reg> als hernach stât<reg>.</reg> <reg type="star">✳</reg> namlich sond wir inen hinfùr alle jar je uff <date when="0001-11-11">santt Martins tag</date> bezaln und geben den jerlichen
        <lb facs="#facs_1_region_1514667226959_11l11" n="N011"/>zinsz vom ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> guͦt<reg>,</reg> so je gevalt <reg type="star">✳</reg> und sich als vor stat gebùrt<reg>,</reg> <measure>zwey fiertel kernen</measure><reg>,</reg> <measure>sechs vierteil</measure> und <measure>drig vierding roggen</measure><reg>,</reg> <measure>drig mùtt ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_region_1514667226959_11l12" n="N012"/>
                <measure>vierzechen schilling haller</measure><reg>,</reg> und darzuͦ und mitt an die <choice>
                    <abbr>obˀn</abbr>
                    <expan>obern</expan>
                </choice> usstelli ouch sovil<reg>,</reg> untz die gantz zalt wirt<reg>,</reg> <reg type="star">✳</reg> und damitt das guͦt in buw <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> eren
        <lb facs="#facs_1_region_1514667226959_11l13" n="N013"/>halten nach lands recht<reg>.</reg> und welich zil hinfùr ich oder min erben vor dem letsten <reg type="star">✳</reg> unbezalt ùbersaͤssen<reg>,</reg> <reg type="star">✳</reg> also das wir daruff oder von dannen
        <lb facs="#facs_1_region_1514667226959_11l14" n="N014"/>bisz wienacht nechstkoment die zwen zinsz<reg>,</reg> als jetz stat<reg>,</reg> allenklich nitt bezalten<reg>,</reg> desgli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> ouch uff das letst zil<reg>,</reg> <reg type="star">✳</reg> oder das guͦt nach lands recht
        <lb facs="#facs_1_region_1514667226959_11l15" n="N015"/>in buw und eren nitt hetten<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> oder dannenthin<reg>,</reg> <choice>
                    <abbr>wen̄</abbr>
                    <expan>wenn</expan>
                </choice> si wellent<reg>,</reg> mag die ob<choice>
                    <abbr>gen̄t</abbr>
                    <expan>genant</expan>
                </choice> min gnedig frow<reg>,</reg> die jarzitpflegerin<reg>,</reg> ir nachko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> oder <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1514667226959_11l16" n="N016"/>ir amptlùt<reg>,</reg> <reg type="star">✳</reg> knecht oder botten von ir wegen <reg type="star">✳</reg> mich oder min er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> von dem guͦt<reg>,</reg> dahar der zinsz gaͧt<reg>,</reg> <reg type="star">✳</reg> mitt gericht o<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> aͧne triben<reg>,</reg> <reg type="star">✳</reg> bringen
        <lb facs="#facs_1_region_1514667226959_11l17" n="N017"/>und das zuͦ irn handen nemen<reg>,</reg> ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice><reg>,</reg> besetzen<reg>,</reg> hinlichen und in allweg damitt tuͦn nach irm wil<choice>
                    <abbr>ln̄</abbr>
                    <expan>len</expan>
                </choice><reg>,</reg> aͧn min<reg>,</reg> <reg type="star">✳</reg> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genantem</expan>
                </choice> <persName ref="per016813">Clewi Scherntzers</persName><reg>,</reg>
        <lb facs="#facs_1_region_1514667226959_11l18" n="N018"/>und ouch aͧn min<reg>,</reg> <persName ref="per016819">Heini Lallis</persName><reg>,</reg> <reg type="star">✳</reg> unser erben und mengklichs von unser wegen sumen<reg>,</reg> irren und insprechen<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> wir sond dannethin allweg
        <lb facs="#facs_1_region_1514667226959_11l19" n="N019"/>gantz von dem guͤt sin<reg>,</reg> dehein vord<choice>
                    <abbr>rūg</abbr>
                    <expan>rung</expan>
                </choice><reg>,</reg> recht noch ansprach daran noch zuͦ <reg type="star">✳</reg> nitt mer haben<reg>,</reg> <reg type="star">✳</reg> gewin<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> <reg type="star">✳</reg> noch si <reg type="star">✳</reg> noch <choice>
                    <abbr>in̄</abbr>
                    <expan>inn</expan>
                </choice>ha<choice>
                    <abbr>bˀ</abbr>
                    <expan>ber</expan>
                </choice> des guͦts<reg>,</reg> so
        <lb facs="#facs_1_region_1514667226959_11l20" n="N020"/>dan<choice>
                    <abbr>ēt</abbr>
                    <expan>ent</expan>
                </choice>hin daruff ko<choice>
                    <abbr>mēt</abbr>
                    <expan>ment</expan>
                </choice><reg>,</reg> das buwent<reg>,</reg> ha<choice>
                    <abbr>bēt</abbr>
                    <expan>bent</expan>
                </choice> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> besitzent<reg>,</reg> <reg type="star">✳</reg> daran noch deshal<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> weder sumen<reg>,</reg> irren<reg>,</reg> bekùm<choice>
                    <abbr>bˀn</abbr>
                    <expan>bern</expan>
                </choice> noch dehein unkomlich sach
        <lb facs="#facs_1_region_1514667226959_11l21" n="N021"/>zuͦfuͤgen noch tuͦn<reg>,</reg> weder mitt worten<reg>,</reg> wercken noch in ander wise<reg>,</reg> sunder si daran und deshalb gantz unbekùm<choice>
                    <abbr>bˀt</abbr>
                    <expan>bert</expan>
                </choice><reg>,</reg> unersuͤcht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> unansprech<reg type="lb">¬</reg><lb facs="#facs_1_region_1514667226959_11l22" n="N022" break="no"/>ig ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> und lassen<reg>.</reg> und darzuͦ sol inen denocht nùtzit des minder ir recht zuͦ uns umb die unbezalten zinsz<reg>,</reg> so vil der wer<reg>,</reg> <reg type="star">✳</reg> behalten und er<reg type="lb">¬</reg><lb facs="#facs_1_region_1514667226959_11l23" n="N023" break="no"/>loupt sin<reg>,</reg> untz die gantz zalt werden<reg>,</reg> also das uns davor noch wider gantz nùtzit schirmen<reg>,</reg> behelfen<reg>,</reg> wir fùrzi<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> noch geniessen<reg>,</reg> sun<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> bi
        <lb facs="#facs_1_region_1514667226959_11l24" n="N024"/>dem<reg>,</reg> so vor stât<reg>,</reg> ân alle fùrwort bestan sol<reg>,</reg> <reg type="star">✳</reg> als ouch wir<reg>,</reg> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genante</expan>
                </choice> <persName ref="per016813">Clewi Scherntzer</persName> und <persName ref="per016819">Heini Lalli</persName><reg>,</reg> das fùr uns und unser erben in wise<reg>,</reg> als
        <lb facs="#facs_1_region_1514667226959_11l25" n="N025"/>vor stât<reg>,</reg> an <persName ref="per011329">Cuͦnrat Widmers</persName> hand an statt als vor <reg type="star">✳</reg> bi unser truw <reg type="star">✳</reg> gelopt und versprochen habent<reg>.</reg> <reg type="star">✳</reg> doch ist he<choice>
                    <abbr>rin̄</abbr>
                    <expan>rinn</expan>
                </choice> fùro be<reg type="lb">¬</reg><lb facs="#facs_1_region_1514667226959_11l26" n="N026" break="no"/>rett<reg>,</reg> das ich<reg>,</reg> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <persName ref="per016813">Scherntzer</persName><reg>,</reg> oder min er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> mogent das guͦt<reg>,</reg> ob<choice>
                    <abbr>gen̄t</abbr>
                    <expan>genant</expan>
                </choice><reg>,</reg> sampt oder sunder hinlichen<reg>,</reg> doch also<reg>,</reg> das es in buw
        <lb facs="#facs_1_region_1514667226959_11l27" n="N027"/>und eren gehalten und die zwen zinsz jerlich davon bezalt werdent immasz<reg>,</reg> als vor stat<reg>.</reg> <reg type="star">✳</reg> woͤl<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <choice>
                    <abbr>abˀ</abbr>
                    <expan>aber</expan>
                </choice> ich oder min er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> unser
