  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0643_r"/>
      <p facs="#facs_1_region_1521233687209_19">
        <lb facs="#facs_1_region_1521233687209_19l1" n="N001"/>Wir nachbenempten<reg>,</reg> <persName ref="per011200">Hans Frenckli<reg>,</reg> jetz vogt ze Lentzburg<reg>,</reg> <reg type="star">✳</reg> obman</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per011277">Ludwig Zechender<reg>,</reg> <reg type="star">✳</reg> schult<choice>
                        <abbr>h̄s</abbr>
                        <expan>heiss</expan>
                    </choice> ze Arow</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per015960">Ruͤdolff Kym<reg>,</reg> altschult<choice>
                        <abbr>h̄s</abbr>
                        <expan>heiss</expan>
                    </choice> Zofingen</persName><reg>,</reg> <persName ref="per011201">Niclaus
        <lb facs="#facs_1_region_1521233687209_19l2" n="N002"/>Fricker<reg>,</reg> schult<choice>
                        <abbr>h̄s</abbr>
                        <expan>heiss</expan>
                    </choice> ze Brugg</persName><reg>,</reg> und <persName ref="per015961">Bertschi Kieser<reg>,</reg> schult<choice>
                        <abbr>h̄s</abbr>
                        <expan>heiss</expan>
                    </choice> ze Lentzburg</persName><reg>,</reg> zuͦgesetzten in diser hienachgemelten sach<reg>,</reg> be<choice>
                    <abbr>kē</abbr>
                    <expan>ken</expan>
                </choice>nent offenlich mitt di<choice>
                    <abbr>sē</abbr>
                    <expan>sem</expan>
                </choice> brieff<reg>,</reg>
        <lb facs="#facs_1_region_1521233687209_19l3" n="N003"/>als <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> die <orgName ref="org002627">erwirdigen<reg>,</reg> unser gneͣdigen frowen<reg>,</reg> eptissin und convent ze Kùngsfeld</orgName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> zuͦ irm jarzit ampt ein guͤtli<reg>,</reg> gelegen ze <placeName ref="loc009760">Rubiswil</placeName><reg>,</reg> ist si
        <lb facs="#facs_1_region_1521233687209_19l4" n="N004"/>nach <choice>
                    <abbr>in̄</abbr>
                    <expan>inn</expan>
                </choice>halt ir brieff von einer <persName ref="per011917">frowen von Rinach<reg>,</reg> geborn von Swartzenhorn</persName><reg>,</reg> ankomen<reg>,</reg> <reg type="star">✳</reg> das ettwan die <orgName ref="org003641">Zobristen von Rubiswil</orgName> gebuwen<reg>,</reg> <reg type="star">✳</reg> gehept<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1521233687209_19l5" n="N005"/>und das vor ettwas jaren <persName ref="per015962">Hansen Richinen von Rubiswil</persName> <reg type="star">✳</reg> mitt irm offnen kouffbrieff fùr eigen <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koufft habent<reg>,</reg> <reg type="star">✳</reg> insoͤlich guͤt aber bishar <persName ref="per015963">Wernli
        <lb facs="#facs_1_region_1521233687209_19l6" n="N006"/>Schult<choice>
                        <abbr>h̄s</abbr>
                        <expan>heiss</expan>
                    </choice> von Lentzburg</persName> <reg type="star">✳</reg> anspràch gehept und gemeint hat<reg>,</reg> <reg type="star">✳</reg> er soͤlt daruff und von<reg>,</reg> als von <persName ref="per015966">Zùllis</persName> guͦt<reg>,</reg> haben <reg type="star">✳</reg> <measure>siben fiertel und ein vierling haber</measure><reg>,</reg>
        <lb facs="#facs_1_region_1521233687209_19l7" n="N007"/>
                <measure>vier zechendhal<choice>
                        <abbr>bē</abbr>
                        <expan>ben</expan>
                    </choice> <choice>
                        <abbr>pfē</abbr>
                        <expan>pfen</expan>
                    </choice>ning</measure> und <measure>ein huͦn gùlt</measure> ze vogtig<reg>,</reg> gehorte in die <measure>vierzechen tschuͦpossen</measure> ze <placeName ref="loc009760">Rubiswil</placeName><reg>,</reg> die inden hoff gon <placeName ref="loc009701">Halderwanck</placeName> gehorten<reg>,</reg>
        <lb facs="#facs_1_region_1521233687209_19l8" n="N008"/>und der ob<choice>
                    <abbr>gen̄t</abbr>
                    <expan>genant</expan>
                </choice> <persName ref="per015962">Richiner</persName> und wer das guͤtli <choice>
                    <abbr>in̄</abbr>
                    <expan>inn</expan>
                </choice>hett <reg type="star">✳</reg> soͤlt deshalben hoffhoͤrig sin <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> soͤlichs a<choice>
                    <abbr>bˀ</abbr>
                    <expan>ber</expan>
                </choice> unser <orgName ref="org002627">frowen von Kùngsfeld</orgName> nitt meinten <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg> sind
        <lb facs="#facs_1_region_1521233687209_19l9" n="N009"/>also deshalben in zwey<choice>
                    <abbr>ūg</abbr>
                    <expan>ung</expan>
                </choice> gewesen und der nach vil ergangnen sachen uff uns<reg>,</reg> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> obman und zuͦsatz<reg>,</reg> dero wir<reg>,</reg> <persName ref="per015960">Ruͦdolff Kym</persName> und <persName ref="per011201">Niclaus
        <lb facs="#facs_1_region_1521233687209_19l10" n="N010"/>Fricker</persName><reg>,</reg> uff unser <orgName ref="org002627">frowen von Kùngsfeld</orgName> sit<reg>,</reg> und wir<reg>,</reg> <persName ref="per011277">Ludwig Zechen<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice>
                </persName> und <persName ref="per015961">Bertschi Kieser</persName><reg>,</reg> uff <persName ref="per015963">Wernli Schult<choice>
                        <abbr>hn̄</abbr>
                        <expan>heissen</expan>
                    </choice>
                </persName> sit <reg type="star">✳</reg> gewesen sind<reg>,</reg> <reg type="star">✳</reg> und uff un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> spruch
        <lb facs="#facs_1_region_1521233687209_19l11" n="N011"/>komen<reg>,</reg> <reg type="star">✳</reg> hand uns ouch daruff die sach und zwey<choice>
                    <abbr>ūg</abbr>
                    <expan>ung</expan>
                </choice> uffgeben und un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> spruch zehalten versprochen <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>,</reg> als <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> umb soͤlichs notdurftig und billich
        <lb facs="#facs_1_region_1521233687209_19l12" n="N012"/>ist<reg>.</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> uff das hand wir zuͦtagen da<choice>
                    <abbr>rub̄</abbr>
                    <expan>rumb</expan>
                </choice> gehept<reg>,</reg> <reg type="star">✳</reg> der <reg type="star">✳</reg> der letst uff hùtt<reg>,</reg> da<choice>
                    <abbr>tū</abbr>
                    <expan>tum</expan>
                </choice> disz briefs<reg>,</reg> ze <placeName ref="loc000121">Arow</placeName> gewesen ist<reg>,</reg> <reg type="star">✳</reg> si beidteil <choice>
                    <abbr>um̄</abbr>
                    <expan>umb</expan>
                </choice> soͤlich ir zwey<choice>
                    <abbr>ūg</abbr>
                    <expan>ung</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir <choice>
                    <abbr>kun̄t</abbr>
                    <expan>kunnt</expan>
                </choice>
                <reg type="lb">¬</reg><lb facs="#facs_1_region_1521233687209_19l13" n="N013" break="no"/>schaft da<choice>
                    <abbr>rub̄</abbr>
                    <expan>rumb</expan>
                </choice><reg>,</reg> die si deshalben zougten<reg>,</reg> eigenlich <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>hoͤrt <reg type="star">✳</reg> und daruff wir<reg>,</reg> und mitt uns der <persName ref="per015968"><choice>
                        <abbr>from̄</abbr>
                        <expan>fromm</expan>
                    </choice><reg>,</reg> vest junckher Abrecht von Mùlinen</persName><reg>,</reg> <persName ref="per015963">Wernlin Schult<choice>
                        <abbr>hn̄</abbr>
                        <expan>heissen</expan>
                    </choice>
                </persName>
        <lb facs="#facs_1_region_1521233687209_19l14" n="N014"/>mitt guͤtlicher<reg>,</reg> frùntli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> bett ange<choice>
                    <abbr>kerˀt</abbr>
                    <expan>kerrt</expan>
                </choice> und <choice>
                    <abbr>in̄</abbr>
                    <expan>inn</expan>
                </choice> gebetten<reg>,</reg> soͤlich sin ansprach durch der ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> unser frowen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ouch unser willen abzelassen<reg>.</reg> sunder
        <lb facs="#facs_1_region_1521233687209_19l15" n="N015"/>han ich<reg>,</reg> <persName ref="per015960">Ruͤdolff Kym</persName><reg>,</reg> <choice>
                    <abbr>in̄</abbr>
                    <expan>inn</expan>
                </choice> gebetten<reg>,</reg> <reg type="star">✳</reg> mir das darin<reg>,</reg> als mich <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> dieselben min <orgName ref="org002627">gnedigen frowen von Kùngsfeld</orgName> jetz zuͦ irm hoffmeis<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> uffgeno<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> habent<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1521233687209_19l16" n="N016"/>zeschencken und ergeben <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg> uff das hât er<reg>,</reg> der vil<choice>
                    <abbr>gen̄t</abbr>
                    <expan>genant</expan>
                </choice> <persName ref="per015963">Wernli Schult<choice>
                        <abbr>h̄s</abbr>
                        <expan>heiss</expan>
                    </choice></persName><reg>,</reg> nach bedencken geanttwurt<reg>,</reg> er well uns soͤlicher bett nitt <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>zi<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> <reg type="star">✳</reg> und <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1521233687209_19l17" n="N017"/>hât daruff vor uns <reg type="star">✳</reg> soͤlich obgemeldet <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> namlich all sin vord<choice>
                    <abbr>rūg</abbr>
                    <expan>rung</expan>
                </choice><reg>,</reg> recht und ansprach<reg>,</reg> so er der obge<choice>
                    <abbr>nēp</abbr>
                    <expan>nemp</expan>
                </choice>ten gùlt und vogtig halb zuͦ dem vorgeschrib<reg type="lb">¬</reg><lb facs="#facs_1_region_1521233687209_19l18" n="N018" break="no"/><choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> guͤtlin <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> zum <persName ref="per015962">Richi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </persName> von des guͤtlis und <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> gùlt und vogtig wegen bishar je gehept hât <reg type="star">✳</reg> oder hinfùr er <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> sin erben je<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> geha<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> moͤch<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> oder
        <lb facs="#facs_1_region_1521233687209_19l19" n="N019"/>gewinnen<reg>,</reg> un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> <orgName ref="org002627">frowen von Kùngsfeld</orgName> und zuͦ irn handen<reg>,</reg> vorab umb gottes und darnach umb soͤlicher unser bett willen<reg>,</reg> lidklich uffgeben <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> zuͦ<reg type="lb">¬</reg><lb facs="#facs_1_region_1521233687209_19l20" n="N020" break="no"/>gefuͤgt<reg>,</reg> <reg type="star">✳</reg> doch im vorbehept sin recht zuͦ an<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice> guͤ<choice>
                    <abbr>tˀn</abbr>
                    <expan>tern</expan>
                </choice> und stucken ze <placeName ref="loc009760">Rubiswil</placeName><reg>,</reg> die <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> in und zuͦ dem ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> guͤtlin nitt gehoͤ<choice>
                    <abbr>rēt</abbr>
                    <expan>rent</expan>
                </choice><reg>.</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> bi soͤli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> gab
        <lb facs="#facs_1_region_1521233687209_19l21" n="N021"/>es ouch hinfùr ewklich bliben und daruff <choice>
                    <abbr>kun̄t</abbr>
                    <expan>kunnt</expan>
                </choice>schaft und an<choice>
                    <abbr>dˀs</abbr>
                    <expan>ders</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> so dawider sin moͤcht<reg>,</reg> <reg type="star">✳</reg> hin und ab <reg type="star">✳</reg> und geneinan<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> hie<choice>
                    <abbr>rub̄</abbr>
                    <expan>rumb</expan>
                </choice> guͤtlich <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>richt <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>slicht
        <lb facs="#facs_1_region_1521233687209_19l22" n="N022"/>sin sond<reg>,</reg> ouch <persName ref="per015962">Hans Richi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice></persName><reg>,</reg> ob<choice>
                    <abbr>gen̄t</abbr>
                    <expan>genant</expan>
                </choice><reg>,</reg> sin er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> und nachkomen bi dem guͤtlin nach <choice>
                    <abbr>in̄</abbr>
                    <expan>inn</expan>
                </choice>halt sins kouffbriefs beliben <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dabi bestân ân fùro insprechen
        <lb facs="#facs_1_region_1521233687209_19l23" n="N023"/>und glichlich<reg>,</reg> als ob diser obgemelten sach und ansprach nie gedacht noch gewesen wer<reg>,</reg> alles ungevarlich<reg>.</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> und des alles ze wa<choice>
                    <abbr>rē</abbr>
                    <expan>rem</expan>
                </choice> ves<choice>
                    <abbr>tē</abbr>
                    <expan>tem</expan>
                </choice> und
        <lb facs="#facs_1_region_1521233687209_19l24" n="N024"/>ewigem urkùnd han ich<reg>,</reg> ob<choice>
                    <abbr>gên</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <persName ref="per011200">Hans Frenckli<reg>,</reg> obman</persName><reg>,</reg> von min <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> des zuͦsatzs wegen<reg>,</reg> doch uns und un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> àn schaden<reg>,</reg> min insigel und
        <lb facs="#facs_1_region_1521233687209_19l25" n="N025"/>darzuͦ und mitt han ich<reg>,</reg> <persName ref="per015963">Wernli Schult<choice>
                        <abbr>h̄s</abbr>
                        <expan>heiss</expan>
                    </choice></persName><reg>,</reg> fùr mich und min er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> min insigel<reg>,</reg> des alles zeme<choice>
                    <abbr>rˀ</abbr>
                    <expan>rer</expan>
                </choice> sicherheit und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gicht<reg>,</reg> <reg type="star">✳</reg> offenlich gehenckt an
        <lb facs="#facs_1_region_1521233687209_19l26" n="N026"/>disen brieff<reg>,</reg> <reg type="star">✳</reg> der geben ist uff <date when="1449-02-17">fritag nechst nach <choice>
                        <abbr>santt̄</abbr>
                        <expan>sanntt</expan>
                    </choice> Gallen tag<reg>,</reg> nach Cristi gebùrt <reg type="star">✳</reg> do man zalt vierzechenhundertvierzig und nùn jar</date><reg>.</reg>
