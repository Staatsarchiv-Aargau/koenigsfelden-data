  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0028_r"/>
      <p facs="#facs_1_region_1492770417596_142">
        <lb facs="#facs_1_line_1492770439307_144" n="N001"/>In gottes na<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> amen<reg>.</reg> <reg type="star">✳</reg> allen dien<reg>,</reg> <reg type="star">✳</reg> die disen brief ansehent oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> kûnd ich<reg>,</reg> <persName ref="per007121">
                    <choice>
                        <abbr>Uͦlr̄</abbr>
                        <expan>Uͦlrich</expan>
                    </choice> von Tenwile der junger<reg>,</reg> <choice>
                        <abbr>b̃ger</abbr>
                        <expan>burger</expan>
                    </choice>
        <lb facs="#facs_1_line_1492770442427_145" n="N002"/>ze Arowe</persName>
                <reg>,</reg> <reg type="star">✳</reg> daz ich han <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧfet <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gegeben in rechtes koͧfes namen <reg type="star">✳</reg> <persName ref="per015724">hern <reg type="star">✳</reg> <choice>
                        <abbr>Walth̄</abbr>
                        <expan>Walther</expan>
                    </choice> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Windesch<reg>,</reg> dem techan</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per013068">Ruͦ<choice>
                        <abbr>dol̄</abbr>
                        <expan>dolf</expan>
                    </choice>
        <lb facs="#facs_1_line_1492770445564_146" n="N003"/>
                    <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Savenwile<reg>,</reg> <reg type="star">✳</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> ze <reg type="star">✳</reg> Brugge</persName>
                <reg>,</reg> <reg type="star">✳</reg> zuͦ der erwirdigen fro<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> handen<reg>,</reg> <reg type="star">✳</reg> <persName ref="per007124">Hedwi<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                    <reg>,</reg> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> gottes gnaden ebtisschinon <reg type="star">✳</reg> des
        <lb facs="#facs_1_line_1492770448807_147" n="N004"/>klosters ze Kûngsvelt</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dem <orgName ref="org002627">convente <reg type="star">✳</reg> des selben gotzhus<reg>,</reg> Sant Claren ordens</orgName>
                <reg>,</reg> <reg type="star">✳</reg> min guͦt<reg>,</reg> gelegen ze <placeName ref="loc011388">Egliswile</placeName> in <choice>
                    <abbr>dē</abbr>
                    <expan>dem</expan>
                </choice>
        <lb facs="#facs_1_line_1492770451858_148" n="N005"/>banne<reg>,</reg> <reg type="star">✳</reg> <measure>siben stucke kernen geltz</measure> <reg type="star">✳</reg> mis lidi<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> friien eigens<reg>,</reg> <reg type="star">✳</reg> daz <reg type="star">✳</reg> <persName ref="per007125">
                    <choice>
                        <abbr>Uͦlr̄</abbr>
                        <expan>Uͦlrich</expan>
                    </choice> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Altwis</persName> buwet<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>drû stucke
        <lb facs="#facs_1_line_1492770456530_149" n="N006"/>ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> geltz</measure> mis eigens<reg>,</reg> <reg type="star">✳</reg> daz <reg type="star">✳</reg> <persName ref="per007126">Ruͦ<choice>
                        <abbr>dol̄</abbr>
                        <expan>dolf</expan>
                    </choice> am Burchwege</persName> buwet<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <orgName ref="org000088">minen her<choice>
                        <abbr>rē</abbr>
                        <expan>ren</expan>
                    </choice> dien <reg type="star">✳</reg> herzogen</orgName> <reg type="star">✳</reg> zinshaft ist <choice>
                    <abbr>umb̄</abbr>
                    <expan>umbe</expan>
                </choice> <reg type="star">✳</reg> <measure>vûnf
        <lb facs="#facs_1_line_1492770461261_150" n="N007"/>schi<choice>
                        <abbr>linḡ</abbr>
                        <expan>linge</expan>
                    </choice>
                </measure> jerglichs <reg type="star">✳</reg> ze vogtrechte <reg type="star">✳</reg> uber <reg type="star">✳</reg> dû vorge<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>den druͦ stucke<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>umb̄</abbr>
                    <expan>umbe</expan>
                </choice> <measure>sechs <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> zweinzig <reg type="star">✳</reg> marck silbers</measure>
                <reg>,</reg> <reg type="star">✳</reg> des
        <lb facs="#facs_1_line_1492770465189_151" n="N008"/>ich ze minen noͤten <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dûrften <reg type="star">✳</reg> bin gar <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ganzlich gewert<reg>,</reg> <reg type="star">✳</reg> die selben <measure>zehen <reg type="star">✳</reg> stucke ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> gelz</measure> <reg type="star">✳</reg> ze besizzenne <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770468162_152" n="N009"/>frilich <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze niessenne <reg type="star">✳</reg> ruͦwlich <reg type="star">✳</reg> in allem dem rechte<reg>,</reg> <reg type="star">✳</reg> als es mich <reg type="star">✳</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> minen vordren angerbet ist<reg>,</reg> <reg type="star">✳</reg> an min <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> allr
        <lb facs="#facs_1_line_1492770472326_153" n="N010"/>minr erben widersprache<reg>.</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> bin <reg type="star">✳</reg> oͧch des vorgenanden guͦtes des gnanden convenz rechter were <reg type="star">✳</reg> vûr eigen <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1492770475734_154" n="N011"/>vogteige<reg>,</reg> als da vorgeschriben stat<reg>,</reg> <reg type="star">✳</reg> an allen dien stetten<reg>,</reg> <reg type="star">✳</reg> da si werscheft bedurfen <reg type="star">✳</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> mir <reg type="star">✳</reg> oder minen erben<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1492770479560_155" n="N012"/>enziehe mich willclich <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gerne <reg type="star">✳</reg> an disem gegenwuͦrtigen briefe <reg type="star">✳</reg> vûr mich <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> min erben <reg type="star">✳</reg> allr der vorder<reg>,</reg> <reg type="star">✳</reg> ansprahe <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1492770483493_156" n="N013"/>rechtunge<reg>,</reg> <reg type="star">✳</reg> so ich <reg type="star">✳</reg> oder dekeim min erbe <reg type="star">✳</reg> jemer koͤnden oder moͤchten <reg type="star">✳</reg> nach sitte ald gewonheit des landes <reg type="star">✳</reg> an <reg type="star">✳</reg> daz vor<reg type="lb">¬</reg><lb facs="#facs_1_line_1492770486932_157" n="N014" break="no"/>genand <reg type="star">✳</reg> guͦt gewinnen <reg type="star">✳</reg> vor geislichem <reg type="star">✳</reg> oder weltlichem gerichte<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> daz <reg type="star">✳</reg> dir koͧf war <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> stete belibe<reg>,</reg> <reg type="star">✳</reg> so han ich gebet<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_line_1492770494670_158" n="N015"/>
                <persName ref="per007127">
                    <choice>
                        <abbr>Chuͦnr̄</abbr>
                        <expan>Chuͦnrad</expan>
                    </choice>
                    <reg>,</reg> den <choice>
                        <abbr>schult̄</abbr>
                        <expan>schultheiss</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> den <reg type="star">✳</reg> <orgName ref="org004582">rat <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
