  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0340_r"/>
      <p facs="#facs_1_region_1501249966871_394">
        <lb facs="#facs_1_line_1501249997373_396" n="N001"/>Allen die<reg>,</reg> die disen <reg type="star">✳</reg> <choice>
                    <sic>bref</sic>
                    <corr>brief</corr>
                </choice> an sehent oder hoͤrent lesen kùnde ich<reg>,</reg> <reg type="star">✳</reg> <persName ref="per020350">Chuͤni Scherling</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich offenlich mit disem
        <lb facs="#facs_1_line_1501250001277_397" n="N002"/>brief <reg type="star">✳</reg> fùr mich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alle min <reg type="star">✳</reg> erben<reg>,</reg> <reg type="star">✳</reg> dz ich ein <reg type="star">✳</reg> mùli enphfangen han<reg>,</reg> <placeName ref="loc000857">Zovingen</placeName> <reg type="star">✳</reg> gelegen ist <reg type="star">✳</reg> an der <placeName ref="loc011455">Wigerren</placeName>
                <reg>,</reg> ist genemmet
        <lb facs="#facs_1_line_1501250004389_398" n="N003"/>die <placeName ref="loc012924">Ober Mùli</placeName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> dem <reg type="star">✳</reg> <persName ref="per013222">frommen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> vesten rit<choice>
                        <abbr>t₎</abbr>
                        <expan>ter</expan>
                    </choice>
                    <reg>,</reg> hern Egbrecht <reg type="star">✳</reg> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Mùlinon<reg>,</reg> <reg type="star">✳</reg> der do ze mal <reg type="star">✳</reg> phfle<choice>
                        <abbr>g₎</abbr>
                        <expan>ger</expan>
                    </choice> des klosters</persName> <reg type="star">✳</reg> wz <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501250008423_399" n="N004"/>ze <orgName ref="org002627">Kùngsvelt</orgName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> giben dem<reg>,</reg> dem forgenanten kloster<reg>,</reg> alle jar <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> der <measure>mùli zins sibinzig mùt <reg type="star">✳</reg> korns<reg>,</reg> <reg type="star">✳</reg> des ist der zwen <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501250012023_400" n="N005"/>teil kerns <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> der tritteil mùli korns</measure>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>ein phfunt stebler fùr ein swin</measure>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gat der zins an <reg type="star">✳</reg> uf <date when="1367-09-01">sant Frenen tag</date>
                <reg>,</reg>
        <lb facs="#facs_1_line_1501250015606_401" n="N006"/>der nù kunt<reg>,</reg> als dirr brief <reg type="star">✳</reg> geben ist<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sol och den <reg type="star">✳</reg> zins geben ze vier ziten <reg type="star">✳</reg> in dem jar<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> och ich<reg>,</reg> der forgenant
        <lb facs="#facs_1_line_1501250019670_402" n="N007"/>
                <persName ref="per020350">Chuͤntzi Scherling</persName>
                <reg>,</reg> <reg type="star">✳</reg> zuͦ rechten <reg type="star">✳</reg> bùrgen geben <reg type="star">✳</reg> dem obgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <persName ref="per013222">hern <reg type="star">✳</reg> Egbrecht</persName>
                <reg>,</reg> <reg type="star">✳</reg> an des <reg type="star">✳</reg> klosters stat<reg>,</reg> <persName ref="per020325">Pe<choice>
                        <abbr>t₎</abbr>
                        <expan>ter</expan>
                    </choice> Prenger <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice>
        <lb facs="#facs_1_line_1501250024034_403" n="N008"/>Muhein</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per020326">Uͦlrichen <choice>
                        <abbr>Schulth̄</abbr>
                        <expan>Schultheiss</expan>
                    </choice> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wile</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per020327">Chuͦni Huͤber <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wile</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per020451">Chuͦnrat Scherling <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wile</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per020328">Ruͦdi Leinbach <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wile</persName>
                <reg>,</reg> <reg type="star">✳</reg> fùr zins
        <lb facs="#facs_1_line_1515593411897_8" n="N009"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> fùr unbuwͤ <reg type="star">✳</reg> der obgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> mùli<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sol och
        <lb facs="#facs_1_line_1515593417683_9" n="N010"/>
                <add place="above">ich<reg>,</reg>
                </add>
        <lb facs="#facs_1_line_1515593411897_7" n="N011"/>der forgenant <persName ref="per020350">Chuͤntzi Scherling</persName>
                <reg>,</reg> <measure>ein ros</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>ein kar<choice>
                        <abbr>rē</abbr>
                        <expan>ren</expan>
                    </choice>
                </measure> in der selben
        <lb facs="#facs_1_line_1501250032183_405" n="N012"/>mùli <reg type="star">✳</reg> han<reg>,</reg> <reg type="star">✳</reg> dz geschetzet ist fùr <measure>nùn phfunt stebler</measure>
                <reg>.</reg> <reg type="star">✳</reg> wa dz were<reg>,</reg> dz ich <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> der mùli wurt gan<reg>,</reg> <reg type="star">✳</reg> dz man denn ze
        <lb facs="#facs_1_line_1501250037761_406" n="N013"/>mal diz ros <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> den karren <reg type="star">✳</reg> in der mùli finden sol<reg>,</reg> <reg type="star">✳</reg> als bi <measure>nùn phfunden</measure> geschetzet ist<reg>.</reg> <reg type="star">✳</reg> wer aber<reg>,</reg> dz <reg type="star">✳</reg> dz ros <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>
        <lb facs="#facs_1_line_1501250042742_407" n="N014"/>karr <reg type="star">✳</reg> ze krank were <reg type="star">✳</reg> fùr <measure>nùn phfunt</measure>
                <reg>,</reg> <reg type="star">✳</reg> so sol ich nach ziehen<reg>,</reg> <reg type="star">✳</reg> ist aber ez pesser<reg>,</reg> <reg type="star">✳</reg> so sol man mir nach ziehen<reg>,</reg> <reg type="star">✳</reg>
