      <p facs="#facs_2_region_1593461661433_2907" rend="dorsual_37_sammelschicht_nach">
        <lb facs="#facs_2_line_1593461665587_2910" n="N001"/>ein lächenbrieff
        <lb facs="#facs_2_line_1593461667526_2915" n="N002"/>Jacobe witwen von</p>
      <p facs="#facs_2_region_1593461674592_2920" rend="dorsual_37_sammelschicht_nach">
        <lb facs="#facs_2_line_1593461681520_2924" n="N001"/>von Offenbùrg geborne
        <lb facs="#facs_2_line_1593461683663_2929" n="N002"/>von Mülinenn <reg type="star">✳</reg>
            </p>
      <p facs="#facs_2_region_1593456058299_2418" rend="dorsual_37_sammelschicht_nach">
        <lb facs="#facs_2_line_1593456061655_2421" n="N001"/>Köngsfelden
        <lb facs="#facs_2_line_1593456063577_2426" n="N002"/>ùmb Geyszliszmat
        <lb facs="#facs_2_line_1593461699243_2935" n="N003"/>da<choice>
                    <abbr>t₎</abbr>
                    <expan>tum</expan>
                </choice> <choice>
                    <abbr>st<reg type="star">✳</reg>
                    </abbr>
                    <expan>sankt</expan>
                </choice> Johanstag 1612 <reg type="star">✳</reg>
            </p>
