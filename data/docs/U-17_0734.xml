      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0734_r"/>
      <p facs="#facs_1_region_1523222045533_9">
        <lb facs="#facs_1_region_1523222045533_9l1" n="N001"/>Ich<reg>,</reg> <persName ref="per011344">Ruͤdi Widmer von Birmenstorff<reg>,</reg> dero erwirdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> geistli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> gnàdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> fröwen zuͦ Kùngsfel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice> <reg type="star">✳</reg> vogt daselbs</persName><reg>,</reg> <reg type="star">✳</reg> tuͦn kuntt mengklichem mitt disem briefe<reg>,</reg> <reg type="star">✳</reg> das
        <lb facs="#facs_1_region_1523222045533_9l2" n="N002"/>ich uff hùte<reg>,</reg> als sin datum wist<reg>,</reg> zuͦ <placeName ref="loc009231">Birmenstorff</placeName> im dorff in des <persName ref="per011346">jungen Swabs</persName> hus<reg>,</reg> so zum dinghoff und dem herbst gericht gehört<reg>,</reg> <reg type="star">✳</reg> inna<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> <orgName ref="org002627">dero sel<choice>
                        <abbr>bn̄</abbr>
                        <expan>ben</expan>
                    </choice> mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice>
        <lb facs="#facs_1_region_1523222045533_9l3" n="N003"/>gnàdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> fröwen</orgName> und von sunder empfelhens we<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> des ersamen<reg>,</reg> wisen <persName ref="per015324">Uͦlrichs am Buͤl<reg>,</reg> ir hofmeisters zuͦ Kùngsfel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </persName><reg>,</reg> mins lie<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> her<choice>
                    <abbr>nˀ</abbr>
                    <expan>ren</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> mitt of<choice>
                    <abbr>fē</abbr>
                    <expan>fen</expan>
                </choice><reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>ban<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> herpst<reg type="lb">¬</reg><lb facs="#facs_1_region_1523222045533_9l4" n="N004" break="no"/>gericht nach dinghofs recht <reg type="star">✳</reg> offenlich zuͦ gericht gesessen bin<reg>.</reg> <reg type="star">✳</reg> in dasselb gericht fùr mich <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>fùrsprechet <choice>
                    <abbr>kom̄en</abbr>
                    <expan>kommen</expan>
                </choice> ist der ersam<reg>,</reg> wysz <persName ref="per015324">Uͦlrich am Buͤl<reg>,</reg> hofmeis<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> zù</persName>
        <lb facs="#facs_1_region_1523222045533_9l5" n="N005"/>
                <persName ref="per015324">Kùngsfel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </persName><reg>,</reg> erstgenempt<reg>,</reg> und offnet durch sin fùrspre<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> als <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> die <choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <orgName ref="org002627">min gnaͤdig fröwen zuͦ Kùngsfel<choice>
                        <abbr>dn</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName>̄ als kilchher<choice>
                    <abbr>ˀn</abbr>
                    <expan>ren</expan>
                </choice> <choice>
                    <abbr>unsrˀ</abbr>
                    <expan>unser</expan>
                </choice> lùtkil<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> zuͦ <placeName ref="loc009231">Birmenstorff</placeName> <reg type="star">✳</reg> ein
        <lb facs="#facs_1_region_1523222045533_9l6" n="N006"/>sigrist ze setzen und da habent und <choice>
                    <abbr>abˀ</abbr>
                    <expan>aber</expan>
                </choice> zuͦ der selben sigristy ein hus <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ettwas guͤter gehörent und geordent sind<reg>,</reg> <reg type="star">✳</reg> die ein jegkli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> sigrist innha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice><reg>,</reg> buwen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_region_1523222045533_9l7" n="N007"/>niessen sol<reg>.</reg> <reg type="star">✳</reg> solich guͤter nuͦn die sigristen je ei<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> dem an<choice>
                    <abbr>dnˀ</abbr>
                    <expan>dern</expan>
                </choice> <reg type="star">✳</reg> dahar sin gerechtikeit daran zekouffen ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koufft haben<reg>.</reg> <reg type="star">✳</reg> das <choice>
                    <abbr>abˀ</abbr>
                    <expan>aber</expan>
                </choice> in den <choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> hoffmeis<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>
        <lb facs="#facs_1_region_1523222045533_9l8" n="N008"/>unbillich bedunk ze sind<reg>,</reg> sun<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> das ein jegkli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> sigrist<reg>,</reg> <choice>
                    <abbr>wen̄</abbr>
                    <expan>wenn</expan>
                </choice> er der kil<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> nit mer dienen<reg>,</reg> noch sigrist sin wil<reg>,</reg> <reg type="star">✳</reg> das ampt mitt sampt den guͤ<choice>
                    <abbr>tnˀ</abbr>
                    <expan>tern</expan>
                </choice> zuͦ der sigristy
        <lb facs="#facs_1_region_1523222045533_9l9" n="N009"/>gehörend densel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> gnädi<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> fröwen <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> irm hoffmeis<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> uffge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> und die ein kùnffti<choice>
                    <abbr>gˀ</abbr>
                    <expan>ger</expan>
                </choice> sigrist von inen empfachen soͤll<reg>.</reg> und batt <reg type="star">✳</reg> an urteil ze erva<choice>
                    <abbr>rn̄</abbr>
                    <expan>ren</expan>
                </choice>
        <lb facs="#facs_1_region_1523222045533_9l10" n="N010"/>und im ze erscheinen<reg>,</reg> wie das also nuͦn hinfùr gebrucht und gehalten werden sölte<reg>.</reg> und darumb ward nach <del rend="strikethrough">nach</del> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice><reg>,</reg> des ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> richters<reg>,</reg> urfrag mitt
        <lb facs="#facs_1_region_1523222045533_9l11" n="N011"/>einheͤlli<choice>
                    <abbr>gˀ</abbr>
                    <expan>ger</expan>
                </choice><reg>,</reg> gesamno<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urteil uff den eid erteilt und damit erscheint und ze recht gespro<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice><reg>,</reg> das ein jegkli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> sigrist der kil<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> zuͦ <placeName ref="loc009231">Birmenstorff</placeName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>wen̄</abbr>
                    <expan>wenn</expan>
                </choice> er
        <lb facs="#facs_1_region_1523222045533_9l12" n="N012"/>zuͦ sigristen uffgeno<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> und empfan<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> wirt<reg>,</reg> <reg type="star">✳</reg> die ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> guͤter zuͦ dersel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> sigristy gehörend <reg type="star">✳</reg> von den genemp<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> <orgName ref="org002627">mi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> gnädigen <choice>
                        <abbr>odˀ</abbr>
                        <expan>oder</expan>
                    </choice> irm hofmeis<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> zuͦ Kùngsfel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName>
        <lb facs="#facs_1_region_1523222045533_9l13" n="N013"/>empfachen und die in guͦten gewonlichen bùw und eren haben<reg>,</reg> nùtzen<reg>,</reg> niessen und behalten<reg>.</reg> <reg type="star">✳</reg> er sol ouch diesel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> güter<reg>,</reg> <choice>
                    <abbr>wen̄</abbr>
                    <expan>wenn</expan>
                </choice> er nit mer sigrist sin wil <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> ab<reg type="hyphen">-</reg><lb facs="#facs_1_region_1523222045533_9l14" n="N014" break="no"/>gesetzt wirt<reg>,</reg> in guͤten gewonli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> bùw und eren den <orgName ref="org002627">
                    <choice>
                        <abbr>gen̄</abbr>
                        <expan>genanten</expan>
                    </choice> mi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> gnädi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> frowen <choice>
                        <abbr>odˀ</abbr>
                        <expan>oder</expan>
                    </choice> irm hofmeis<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> zuͦ Kùngsfel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName> wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>umb uffge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice>
                <reg>.</reg> es sol ouch ein jegklicher si<reg type="lb">¬</reg><lb facs="#facs_1_region_1523222045533_9l15" n="N015" break="no"/>grist den genemp<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> gnädi<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> fröwen<reg>,</reg> irm hofmeis<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <reg type="star">✳</reg> ein lùtpriester <reg type="star">✳</reg> der kilchen und den <orgName ref="org004293">kilchgnossen zü Birmenstorff</orgName> dienstig<reg>,</reg> gehoͤrig und gewàrtig
        <lb facs="#facs_1_region_1523222045533_9l16" n="N016"/>sin und die vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> guͤ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice><reg>,</reg> zuͦ der sigristye gehörend<reg>,</reg> <reg type="star">✳</reg> än mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> gnädi<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> frowen <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> irs hoffmeis<choice>
                    <abbr>tsˀ</abbr>
                    <expan>ters</expan>
                </choice> gunst<reg>,</reg> wissen und willen <reg type="star">✳</reg> eins <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> mer in dheinen weg <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>än<choice>
                    <abbr>dnˀ</abbr>
                    <expan>dern</expan>
                </choice><reg>,</reg>
        <lb facs="#facs_1_region_1523222045533_9l17" n="N017"/>
                <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>lyhen noch <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>kouffen<reg>.</reg> und ob wer<reg>,</reg> das sich ein sigrist nit inmasz hielte mit diens<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice><reg>,</reg> nit gehoͤrig noch gewärtig weͣr <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> sich susz in an<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> wèg <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>handelte
        <lb facs="#facs_1_region_1523222045533_9l18" n="N018"/>und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>schuldte also<reg>,</reg> das er den <choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> minen gnädi<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> fröwen<reg>,</reg> <reg type="star">✳</reg> der kilchen<reg>,</reg> <reg type="star">✳</reg> ein lùtpries<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> den kilchgnos<choice>
                    <abbr>sn̄</abbr>
                    <expan>sen</expan>
                </choice> nit eben <reg type="star">✳</reg> fuͦgklich noch <choice>
                    <abbr>kom̄</abbr>
                    <expan>komm</expan>
                </choice>lich wer<reg>,</reg> all<choice>
                    <abbr>dan̄</abbr>
                    <expan>dann</expan>
                </choice> mö<choice>
                    <abbr>gēt</abbr>
                    <expan>gent</expan>
                </choice>
        <lb facs="#facs_1_region_1523222045533_9l19" n="N019"/>si in <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>wand<choice>
                    <abbr>ln̄</abbr>
                    <expan>len</expan>
                </choice><reg>,</reg> abset<choice>
                    <abbr>zn̄</abbr>
                    <expan>zen</expan>
                </choice> und ein an<choice>
                    <abbr>dnˀ</abbr>
                    <expan>dern</expan>
                </choice> uffnemen und empfachen <reg type="star">✳</reg> mitt sampt den gedach<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> guͤ<choice>
                    <abbr>tnˀ</abbr>
                    <expan>tern</expan>
                </choice> zuͦ der sigristy gehörent<reg>,</reg> <reg type="star">✳</reg> die all<choice>
                    <abbr>dan̄</abbr>
                    <expan>dann</expan>
                </choice> mitt <reg type="star">✳</reg> dem nutz<reg>,</reg> höw <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_region_1523222045533_9l20" n="N020"/>stròw dem kùnfftigen sigris<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> wer<choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> und gevolgen sollent<reg>.</reg> und sind hieby in gericht und des urteilspre<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> gewesen <reg type="star">✳</reg> die er<choice>
                    <abbr>bnˀ</abbr>
                    <expan>bern</expan>
                </choice> <persName ref="per011345">Ruͤdi Swab</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per011346">Hans Swab<reg>,</reg> der
        <lb facs="#facs_1_line_1535107144132_1" n="N021"/>jung</persName><reg>,</reg> <persName ref="per011347">Hans Weͣber</persName><reg>,</reg> <persName ref="per011348">Heini Widmer</persName><reg>,</reg> <persName ref="per011349">Uͦlli Weͣber</persName><reg>,</reg> <persName ref="per011455">Henszli Mei<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName><reg>,</reg> <persName ref="per011456">Werna Schnider</persName><reg>,</reg> <persName ref="per011457">Ruͤdi Eglolff</persName><reg>,</reg> <persName ref="per011458">Ruͤdi Täͤttwil</persName><reg>,</reg> <persName ref="per011460">Cuͤni Brunegger</persName><reg>,</reg> <persName ref="per011461">Uͦlli Tättwil</persName><reg>,</reg> <persName ref="per011350">Ruͤdi Man</persName><reg>,</reg> <persName ref="per011351">Heini Andres</persName><reg>,</reg>
        <lb facs="#facs_1_region_1523222045533_9l21" n="N022"/>
                <persName ref="per011352">Henszli Widmer</persName> <reg type="star">✳</reg> und ander er<choice>
                    <abbr>bˀ</abbr>
                    <expan>ber</expan>
                </choice> lùte gnuͦg des dinghofs und herbstgerichtz zuͦ <placeName ref="loc009231">Birmenstorff</placeName>
                <reg>.</reg> und wand disz also ingericht volgangen und beschechen ist
        <lb facs="#facs_1_region_1523222045533_9l22" n="N023"/>und <choice>
                    <abbr>abˀ</abbr>
                    <expan>aber</expan>
                </choice> ich<reg>,</reg> ob<choice>
                    <abbr>gēn</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> richter<reg>,</reg> eigens insigels nit enhab<reg>,</reg> darumb so han ich den ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per015324">Uͦlri<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> am Buͤl</persName><reg>,</reg> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
