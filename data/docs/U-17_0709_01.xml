                        <expan>ter</expan>
                    </choice></persName><reg>,</reg>
        <lb facs="#facs_1_region_1523269155612_16l3" n="N003"/>hand<reg>,</reg> <persName ref="per011487">Uͦlrich Lener</persName><reg>,</reg> <persName ref="per011480">Werna Stilli</persName> und <persName ref="per016069">Fridli Mùsz<choice>
