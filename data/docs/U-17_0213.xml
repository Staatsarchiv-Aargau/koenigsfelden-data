  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0213_r"/>
      <p facs="#facs_1_region_1501236571499_942">
        <lb facs="#facs_1_line_1501236575573_943" n="N001"/>Allen den<reg>,</reg> die disen brief ansehent oder hoͤrent lesen<reg>,</reg> kùnde <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich ich<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per019680">Johans Batzli</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per019681">Johansen
        <lb facs="#facs_1_line_1501236577974_944" n="N002"/>Batzlins seligen sun von Arowe</persName>
                <reg>,</reg> <reg type="star">✳</reg> umb die schuͦppossen gelegen ze <placeName ref="loc009761">Sure</placeName> in dem banne<reg>,</reg> <reg type="star">✳</reg> die da buwet
        <lb facs="#facs_1_line_1501236580375_945" n="N003"/>
                <persName ref="per019684">Ruͦdi <reg type="star">✳</reg> Attelwile</persName> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> giltet <measure>fûnf stucke an korne</measure>
                <reg>,</reg> <reg type="star">✳</reg> <measure>drie mùt roggen</measure>
                <reg>,</reg> <reg type="star">✳</reg> <measure>zwein mùt kernen</measure>
        <lb facs="#facs_1_line_1501236582679_946" n="N004"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ein <reg type="star">✳</reg> <measure>mal<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> umb die vogti<reg>,</reg> <reg type="star">✳</reg> gelegen ze <placeName ref="loc009757">Talhein</placeName> in dem banne<reg>,</reg> <reg type="star">✳</reg> die da giltet <measure>fùnf
        <lb facs="#facs_1_line_1501236585527_947" n="N005"/>schilling <choice>
                        <abbr>pfē</abbr>
                        <expan>pfen</expan>
                    </choice>
                    <choice>
                        <abbr>niḡ</abbr>
                        <expan>ning</expan>
                    </choice>
                </measure> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>ein viertel ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure>
                <reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per019686">
                    <choice>
                        <abbr>Chuͦnr̄</abbr>
                        <expan>Chuͦnrad</expan>
                    </choice> Ùwer</persName> git <measure>vier schillinge <choice>
                        <abbr>pfē</abbr>
                        <expan>pfen</expan>
                    </choice>
                    <choice>
                        <abbr>niḡ</abbr>
                        <expan>ning</expan>
                    </choice>
                </measure>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per019688">Ruͦdi <reg type="star">✳</reg> von
        <lb facs="#facs_1_line_1501236588287_948" n="N006"/>Brugge</persName> <reg type="star">✳</reg> git ein <measure>schil<choice>
                        <abbr>linḡ</abbr>
                        <expan>linge</expan>
                    </choice> <choice>
                        <abbr>pfē</abbr>
                        <expan>pfen</expan>
                    </choice>
                    <choice>
                        <abbr>niḡ</abbr>
                        <expan>ning</expan>
                    </choice>
                </measure> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>ein viertel ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure>
                <reg>,</reg> <reg type="star">✳</reg> so min muͦter<reg>,</reg> <persName ref="per019682">Gerdrut Batzlina</persName>
                <reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧft
        <lb facs="#facs_1_line_1501236591455_949" n="N007"/>hat <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in koͧfes wise hingegeben het <reg type="star">✳</reg> recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redelich <reg type="star">✳</reg> den <orgName ref="org002627">er<choice>
                        <abbr>bˀen</abbr>
                        <expan>beren</expan>
                    </choice>
                    <reg>,</reg> geistli<choice>
                        <abbr>chē</abbr>
                        <expan>chen</expan>
                    </choice> frowen <reg type="star">✳</reg> der abtissen
        <lb facs="#facs_1_line_1501236594599_950" n="N008"/>
                    <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem <choice>
                        <abbr>cō</abbr>
                        <expan>con</expan>
                    </choice>vente <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> ir nachko<choice>
                        <abbr>mē</abbr>
                        <expan>men</expan>
                    </choice> <reg type="star">✳</reg> dez gotzhuses ze Kùngesvelt</orgName>
                <reg>,</reg> <reg type="star">✳</reg> daz der selbe koͧf <reg type="star">✳</reg> mit mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> guͦten
        <lb facs="#facs_1_line_1501236603144_951" n="N009"/>willen <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gunst beschechen ist<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> loben oͧch <reg type="star">✳</reg> an disem brief <reg type="star">✳</reg> vùr mich <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> min er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> da wider
        <lb facs="#facs_1_line_1501236606720_952" n="N010"/>niemer ze tuͤne<reg>,</reg> <reg type="star">✳</reg> weder mit wor<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <reg type="star">✳</reg> noch mit werken <reg type="star">✳</reg> an geistlichem <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> an weltlichem gerichte<reg>,</reg>
                <reg type="star">✳</reg> noch
        <lb facs="#facs_1_line_1501236610465_953" n="N011"/>usserunde gerichtes<reg>.</reg> <reg type="star">✳</reg> ich entzich mich oͧch an disem brief vûr mich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> min er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> aller vorder <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ansprache<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501236613648_954" n="N012"/>so ich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> oder jeman ander von un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> we<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <reg type="star">✳</reg> an die vorgeschri<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> schuͦppossen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> vogti <reg type="star">✳</reg> han <reg type="star">✳</reg> moͤchte <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501236617296_955" n="N013"/>suͦ<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> gewin<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> <reg type="star">✳</reg> wider die vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genante</expan>
                </choice> <orgName ref="org002627">abtissen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> den <choice>
                        <abbr>cō</abbr>
                        <expan>con</expan>
                    </choice>vente <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> ir nachkomen <reg type="star">✳</reg> dez gotzhuses ze Kùnges<reg type="hyphen">-</reg><lb facs="#facs_1_line_1501236620737_956" n="N014" break="no"/>velt</orgName> mit dekeinem rechte <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> mit dekeinen vùnden geschribenen <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> ungeschribnen<reg>,</reg> <reg type="star">✳</reg> vor geistlichem oder <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501236624289_957" n="N015"/>weltlichem gerichte<reg>,</reg> <reg type="star">✳</reg> ane alle ge<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>de <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> har umbe<reg>,</reg> <reg type="star">✳</reg> wan ich eigens ingesigels nût enhan<reg>,</reg> <reg type="star">✳</reg> so han ich gebeten
        <lb facs="#facs_1_line_1501236627713_958" n="N016"/>den <persName ref="per019689">er<choice>
                        <abbr>bˀen</abbr>
                        <expan>beren</expan>
                    </choice>
                    <reg>,</reg> ves<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> rit<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> Johansen von Buͦbenberg<reg>,</reg> <choice>
                        <abbr>schulth̄</abbr>
                        <expan>schultheiss</expan>
                    </choice> ze <choice>
                        <abbr>Bˀ</abbr>
                        <expan>Ber</expan>
                    </choice>ne</persName>
                <reg>,</reg> <reg type="star">✳</reg> daz er sin ingesigel gehencket het an disen
        <lb facs="#facs_1_line_1501236631113_959" n="N017"/>brief vûr mich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> min er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> ze einem of<choice>
                    <abbr>fē</abbr>
                    <expan>fen</expan>
                </choice>ne urkùnde <reg type="star">✳</reg> aller der din<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> so da vor von mir geschriben
        <lb facs="#facs_1_line_1501236634417_960" n="N018"/>stant<reg>.</reg> <reg type="star">✳</reg> daz ich<reg>,</reg> der selbe <persName ref="per019689">
                    <choice>
