  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0570_r"/>
      <p facs="#facs_1_region_1608778134285_982">
        <lb facs="#facs_1_region_1608778134285_982l1" n="N001"/>Ich<reg>,</reg> <persName ref="per029124">Egli Kel<choice>
                        <abbr>lˀ</abbr>
                        <expan>ler</expan>
                    </choice> von Elfin<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </persName><reg>,</reg> tuͦn kunt mitt disem brieff<reg>,</reg> dz ich uff <date when="1430-11-29">sant Andreas abend <reg type="star">✳</reg> des ja<choice>
                        <abbr>r₎</abbr>
                        <expan>res</expan>
                    </choice><reg>,</reg> do<choice>
                        <abbr>mā</abbr>
                        <expan>man</expan>
                    </choice> zalt <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> gotz gebùrt
        <lb facs="#facs_1_region_1608778134285_982l2" n="N002"/>thusing vierhun<choice>
                        <abbr>dˀt</abbr>
                        <expan>dert</expan>
                    </choice> und <choice>
                        <abbr>im̄</abbr>
                        <expan>imm</expan>
                    </choice> drissigosten jar</date> ze <placeName ref="loc009772">Elfin<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </placeName> in dem dorff uff dem gedinkhof an fryer <choice>
                    <abbr>st̃ss</abbr>
                    <expan>strass</expan>
                </choice> offenlich ze gerichte
        <lb facs="#facs_1_region_1608778134285_982l3" n="N003"/>saz in na<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org002627">erwirdigen<reg>,</reg> geistlichen froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice><reg>,</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> aͤptissin und gmei<choice>
                        <abbr>nē</abbr>
                        <expan>nem</expan>
                    </choice> con<choice>
                        <abbr>vē</abbr>
                        <expan>ven</expan>
                    </choice>tes des klosters Kùngsvel<choice>
                        <abbr>d₎</abbr>
                        <expan>den</expan>
                    </choice><reg>,</reg> sant Cla<choice>
                        <abbr>rē</abbr>
                        <expan>ren</expan>
                    </choice> ordens<reg>,</reg>
        <lb facs="#facs_1_region_1608778134285_982l4" n="N004"/>Costant<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bystuͦms</orgName><reg>,</reg> min al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice>gnedigosten froͧwen<reg>,</reg> <reg type="star">✳</reg> von entpfelens wegen des <persName ref="per026023">fùrsichtigen Uͦlrichs Mùntzen<reg>,</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> jetz<choice>
                        <abbr>gn̄̄</abbr>
                        <expan>genanten</expan>
                    </choice> froͧwen
        <lb facs="#facs_1_region_1608778134285_982l5" n="N005"/>hofmeis<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>
                </persName><reg>,</reg> mines lieben <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren<reg>,</reg> <reg type="star">✳</reg> und kament da fùr mich in <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>
                <choice>
                    <abbr>bā</abbr>
                    <expan>ban</expan>
                </choice>nen ge<choice>
                    <abbr>r₎</abbr>
                    <expan>gericht</expan>
                </choice> <persName ref="per029115">Hans Ùtker<reg>,</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> keller <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Mettoͧw</persName><reg>,</reg> an einem teil<reg>,</reg> <reg type="star">✳</reg> und
        <lb facs="#facs_1_region_1608778134285_982l6" n="N006"/>
                <persName ref="per029116">froͧw Ita Stollin</persName> mitt <persName ref="per029117">Bùrgin Stollen</persName><reg>,</reg> irem elichen man <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> wis<choice>
                    <abbr>senth̄</abbr>
                    <expan>senthaften</expan>
                </choice> vogt<reg>,</reg> an<choice>
                    <abbr>dˀs</abbr>
                    <expan>ders</expan>
                </choice> teils<reg>,</reg> <reg type="star">✳</reg> mitt fùr<choice>
                    <abbr>sꝓ</abbr>
                    <expan>spre</expan>
                </choice>chen<reg>.</reg> <reg type="star">✳</reg> offnot <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jach da <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>
        <lb facs="#facs_1_region_1608778134285_982l7" n="N007"/>
                <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per029115">Hans Ùtker</persName><reg>,</reg> wie dz er eines ewigen<reg>,</reg> unwi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>ruͤffli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> koͧffes fùr sich und sin er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧft und ze koͧffen ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> hett der
        <lb facs="#facs_1_region_1608778134285_982l8" n="N008"/>ob<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per029116">froͧ Itan</persName> und <persName ref="per029117">Bùrgin Stolen</persName><reg>,</reg> irem eli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> man und vogt<reg>,</reg> <reg type="star">✳</reg> sine zwo matten<reg>,</reg> so er hette in <placeName ref="loc011207">Efin<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </placeName> bann<reg>,</reg> <choice>
                    <abbr>wˀend</abbr>
                    <expan>werend</expan>
                </choice> frye
        <lb facs="#facs_1_region_1608778134285_982l9" n="N009"/>eigen<reg>,</reg> und dar zuͦ zwo kisten<reg>,</reg> <reg type="star">✳</reg> umb alle ir an<choice>
                    <abbr>sp̃ch</abbr>
                    <expan>sprach</expan>
                </choice><reg>,</reg> so si vormals an <persName ref="per029118">Hein<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> <choice>
                        <abbr>im̄</abbr>
                        <expan>imm</expan>
                    </choice> Steinhof</persName><reg>,</reg> iren bruͦ<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice><reg>,</reg> und oͧch an inne je
        <lb facs="#facs_1_region_1608778134285_982l10" n="N010"/>gehept hette von ires vat<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice><reg>,</reg> seli<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice><reg>,</reg> erbes we<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice><reg>,</reg> als dz sin koͧff und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>ti<choice>
                    <abbr>gūg</abbr>
                    <expan>gung</expan>
                </choice> brieff<reg>,</reg> so <choice>
                    <abbr>im̄</abbr>
                    <expan>imm</expan>
                </choice> hùtt zetag vor mir ingerichte
        <lb facs="#facs_1_region_1608778134285_982l11" n="N011"/>ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice><reg>,</reg> inn hette und lùtrote<reg>,</reg> <reg type="star">✳</reg> und satzte uff recht<reg>,</reg> wie er die <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenante</expan>
                </choice> matten und kysten bringen soͤlt zuͦ <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per029116">froͧ Itan</persName><reg>,</reg>
        <lb facs="#facs_1_region_1608778134285_982l12" n="N012"/>ires vogt und ir er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> hand<reg>,</reg> dz si daran habent <choice>
                    <abbr>wˀend</abbr>
                    <expan>werend</expan>
                </choice><reg>.</reg> <reg type="star">✳</reg> und ward erteilt nach mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> urfraͧg einhelklich uff den eid<reg>,</reg> dz er sich
        <lb facs="#facs_1_region_1608778134285_982l13" n="N013"/>
                <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> mat<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> be<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> und oͧch <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> kysten entzi<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> uff ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> soͤlt in min hand <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> des gerichtz we<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <reg type="star">✳</reg> und ichs <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tigen und
        <lb facs="#facs_1_region_1608778134285_982l14" n="N014"/>ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> soͤlt in hande <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ob<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per029116">froͧ Itan</persName> und ires vogtz hand<reg>.</reg> <reg type="star">✳</reg> dis alles geschach durch min hand in<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vor<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per029116">froͧ Itan</persName> und
        <lb facs="#facs_1_region_1608778134285_982l15" n="N015"/>
