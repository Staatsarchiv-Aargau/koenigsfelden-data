                </choice> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> gnedi<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <choice>
                    <abbr>hˀr</abbr>
                    <expan>her</expan>
                </choice>ren stat<reg>,</reg>
        <lb facs="#facs_1_line_1501248511508_185" n="N003"/>
                <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org000088">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzogen <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Oͤsterrich</orgName>
                <reg>,</reg> <persName ref="per020272">bruͦ<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Walther von Mesicon<reg>,</reg> schaf<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> des gotzhus ze Toͤsse in Costent<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bystuͦm</persName>
                <reg>,</reg> einhalb<reg>,</reg>
        <lb facs="#facs_1_line_1501248514208_186" n="N004"/>mit dem <persName ref="per020274">edeln<reg>,</reg> fryen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren<reg>,</reg> <choice>
                        <abbr>hˀn</abbr>
                        <expan>hern</expan>
                    </choice> Hugen <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Guͦttenburg</persName>
                <reg>,</reg> sinem vogt<reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> im in di<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> sache mit urteild ze vogte
        <lb facs="#facs_1_line_1501248518270_187" n="N005"/>geben wart <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> die oͧch einan<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vogtey vorgericht <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jachen<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> an<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>halb <persName ref="per014847">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Hofmeis<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>
                </persName> des <orgName ref="org002627">gotzhus ze Kùngs<reg type="hyphen">-</reg>
        <lb facs="#facs_1_line_1501248520750_188" n="N006" break="no"/>veld<reg>,</reg> Sant Claren ordens</orgName>
                <reg>,</reg> und stuͦnd dar fùr gericht <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per020272">bruͦ<choice>
                        <abbr>dˀ</abbr>
