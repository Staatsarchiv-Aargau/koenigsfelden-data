  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0542_r"/>
      <p facs="#facs_1_region_1507842775284_656">
        <lb facs="#facs_1_line_1507842780317_657" n="N001"/>Ich<reg>,</reg> <persName ref="per026786">Cuͦnrat Walt<choice>
                        <abbr>mā</abbr>
                        <expan>man</expan>
                    </choice> von Bira<reg>,</reg> un<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice>vogt <choice>
                        <abbr>im̄</abbr>
                        <expan>imm</expan>
                    </choice> Eigen</persName><reg>,</reg> tuͦn kunt al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice>
                <choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>li<choice>
                    <abbr>chm̄</abbr>
                    <expan>chem</expan>
                </choice> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>giche offenlich mitt disem brief<reg>,</reg> <reg type="star">✳</reg> dz ich von entpfelens we<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <reg type="star">✳</reg> des <persName ref="per015475">fùrsichtigen<reg>,</reg>
        <lb facs="#facs_1_line_1507842785116_658" n="N002"/>wisen Johansen Negellis von Clingnoͧw<reg>,</reg> hofmeisters ze Kùngsveld</persName><reg>,</reg> <reg type="star">✳</reg> of<choice>
                    <abbr>fn̄</abbr>
                    <expan>fen</expan>
                </choice>lich ze ge<choice>
                    <abbr>r₎</abbr>
                    <expan>richt</expan>
                </choice> saz ze <placeName ref="loc000195">Brugg</placeName><reg>,</reg> vor dem <placeName ref="loc009977">Obren Thor</placeName> uss<reg>,</reg> an rech<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice><reg>,</reg> gewonli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> richtstatt in na<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>
        <lb facs="#facs_1_line_1507842789393_659" n="N003"/>und an statt <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org002627">erwidi<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice><reg>,</reg> geistli<choice>
                        <abbr>chē</abbr>
                        <expan>chen</expan>
                    </choice><reg>,</reg> mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> gnedi<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice> von Kùngsveld</orgName> <reg type="star">✳</reg> uff den <date when="1423-10-14">nechs<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> donstag vor <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> einlif thusing meg<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> tag<reg>,</reg> <reg type="star">✳</reg> des ja<choice>
                        <abbr>r₎</abbr>
                        <expan>res</expan>
                    </choice><reg>,</reg> do<choice>
                        <abbr>mā</abbr>
                        <expan>man</expan>
                    </choice> zalt <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> gottes
        <lb facs="#facs_1_line_1518082159839_2" n="N004"/>gebùrt viertzechenhun<choice>
                        <abbr>dtˀ</abbr>
                        <expan>dert</expan>
                    </choice> zwentzig und in dem dritten jar</date><reg>,</reg> <reg type="star">✳</reg> da fùr mich in <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>
                <choice>
                    <abbr>bā</abbr>
                    <expan>ban</expan>
                </choice>nen ge<choice>
                    <abbr>r₎</abbr>
                    <expan>richt</expan>
                </choice>
        <lb facs="#facs_1_line_1518082165153_3" n="N005"/>kamen
        <lb facs="#facs_1_line_1518082159839_1" n="N006"/>
                <reg type="star">✳</reg> die <persName ref="per026034">beschei<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice> Hein<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> Knonower<reg>,</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> ze Brugg</persName><reg>,</reg> an einem teil <reg type="star">✳</reg> und <persName ref="per026777">Hans
        <lb facs="#facs_1_line_1507842800434_661" n="N007"/>
                    <choice>
                        <abbr>im̄</abbr>
                        <expan>imm</expan>
                    </choice> Hof von Husen</persName> am an<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice> teil<reg>,</reg> <reg type="star">✳</reg> mitt fùr<choice>
                    <abbr>sp̄</abbr>
                    <expan>spre</expan>
                </choice>
                <choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice><reg>.</reg> <reg type="star">✳</reg> offnot und klegt da <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vor<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>gen</expan>
                </choice> <persName ref="per026034">Knonower</persName> hin zuͦ dem <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per026777">Hansen <choice>
                        <abbr>im̄</abbr>
                        <expan>imm</expan>
                    </choice> Hof</persName><reg>,</reg> <reg type="star">✳</reg> wie dz sich vor zi<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> gefuͤgt habe<reg>,</reg> <reg type="star">✳</reg> dz <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>
        <lb facs="#facs_1_line_1507842805539_662" n="N008"/>selb <persName ref="per026777">Hans <choice>
                        <abbr>im̄</abbr>
                        <expan>imm</expan>
                    </choice> Hof</persName> ein hofstatt<reg>,</reg> gele<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> ze <placeName ref="loc008677">Husen</placeName> <choice>
                    <abbr>im̄</abbr>
                    <expan>imm</expan>
                </choice> dorf<reg>,</reg> mitt hus<reg>,</reg> hofstatt und rechtung an<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> zuͦ gehoͤr<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice> an sich gezo<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> und gekoͧft hette <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> dem <persName ref="per026782">al<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> <choice>
                        <abbr>Stˀ</abbr>
                        <expan>Ster</expan>
                    </choice>ren<reg>,</reg> seli<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </persName><reg>,</reg>
        <lb facs="#facs_1_line_1507842810387_663" n="N009"/>ab dem sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> hus und hofstatt <persName ref="per026781">Clausen Tùgin<reg>,</reg> seli<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </persName><reg>,</reg> sinem swecher<reg>,</reg> jerlichs zins gieng <measure>ein mùtt kern</measure><reg>,</reg> <reg type="star">✳</reg> dz selb guͦt und <measure>mùtt <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>
                    <choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> geltz</measure> nu <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> sines wibes we<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> an inn
        <lb facs="#facs_1_line_1507842815035_664" n="N010"/>in erbes wise geval<choice>
                    <abbr>ln̄</abbr>
                    <expan>len</expan>
                </choice> <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> und dz ob<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genante</expan>
                </choice> guͦt hette er nùtt in eren<reg>,</reg> als dz billich und lanzrecht <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice> <reg type="star">✳</reg> und hette dz ob<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genante</expan>
                </choice> hus lassen fulen und <choice>
                    <abbr>zˀ</abbr>
                    <expan>zer</expan>
                </choice>gaͧn<reg>.</reg> <reg type="star">✳</reg> antwùrte <persName ref="per026777">Hans <choice>
                        <abbr>im̄</abbr>
                        <expan>imm</expan>
                    </choice>
        <lb facs="#facs_1_line_1507842820904_665" n="N011"/>Hof</persName> mitt sinem fùr<choice>
                    <abbr>s̄p</abbr>
                    <expan>spre</expan>
                </choice>
                <choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> er <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice> nùtt an gedenk<reg>,</reg> <reg type="star">✳</reg> dz es an inn ko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice> behuset<reg>,</reg> <reg type="star">✳</reg> da stuͤnd wol ein boͤs hus uff als dz ni<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> viele<reg>,</reg> do fuͤre <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <persName ref="per026783">Beck<reg>,</reg> selig</persName><reg>,</reg> dz holtz ab <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> hoffstatt
