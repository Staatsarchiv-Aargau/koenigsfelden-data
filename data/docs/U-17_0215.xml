  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0215_r"/>
      <p facs="#facs_1_region_1501237140210_985">
        <lb facs="#facs_1_line_1501237146639_986" n="N001"/>Allen den<reg>,</reg> die disen brief an sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> tuͦn kunt ich<reg>,</reg> <persName ref="per012505">
                    <choice>
                        <abbr>Joh̄s</abbr>
                        <expan>Johans</expan>
                    </choice> Bluͦmo<reg>,</reg> <reg type="star">✳</reg> schultheis ze Brugg</persName><reg>,</reg> <reg type="star">✳</reg> das fùr mich kam an
        <lb facs="#facs_1_line_1501237149480_987" n="N002"/>offen gericht <reg type="star">✳</reg> <persName ref="per019933">
                    <choice>
                        <abbr>Heinr̄</abbr>
                        <expan>Heinrich</expan>
                    </choice> Gamlikon</persName> <reg type="star">✳</reg> und <persName ref="per019934">
                    <choice>
                        <abbr>Joh̄s</abbr>
                        <expan>Johans</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per019935">Anna</persName> und <persName ref="per019937">Elly</persName><reg>,</reg> <reg type="star">✳</reg> sinù elichù kint<reg>,</reg> <reg type="star">✳</reg> und da offenlich<reg>,</reg> recht und redelich<reg>,</reg> gesunt libes und
        <lb facs="#facs_1_line_1501237151992_988" n="N003"/>sinnes <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jahen<reg>,</reg> <reg type="star">✳</reg> daz si ze koͧffen hant gegeben <reg type="star">✳</reg> den <orgName ref="org002627">erwirdigen<reg>,</reg> geistlichen froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> der epthissi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> <reg type="star">✳</reg> <reg type="star">✳</reg> dem <choice>
                        <abbr>cō</abbr>
                        <expan>con</expan>
                    </choice>vente gemeinlich<reg>,</reg>
        <lb facs="#facs_1_line_1501237154288_989" n="N004"/>Sant Claren <reg type="star">✳</reg> ordens<reg>,</reg> ze Chùngesfelt<reg>,</reg> <reg type="star">✳</reg> gele<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> in Costentzer bystuͦm</orgName><reg>,</reg> <reg type="star">✳</reg> ein matten<reg>,</reg> ist gelegen an <placeName ref="loc011848">Alariet</placeName> <reg type="star">✳</reg> zwùschent <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <persName ref="per019939">Vinse<reg type="lb">¬</reg><lb facs="#facs_1_line_1501237157088_990" n="N005" break="no"/>leri<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <persName ref="per019944">Loͤwi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </persName> mattuͤn<reg>,</reg> <reg type="star">✳</reg> umb <measure>nùn <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> zweinzig phunt nu<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice> phenin<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </measure><reg>,</reg> <reg type="star">✳</reg> die si oͧch gar <choice>
                    <abbr>un̄</abbr>
                    <expan>von</expan>
                </choice> gantzlich <reg type="star">✳</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> in enphan<reg type="lb">¬</reg><lb facs="#facs_1_line_1501237159842_991" n="N006" break="no"/>gen hant <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in ir nutz <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> fro<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> ko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> sint<reg>,</reg> und gaben die vor<choice>
                    <abbr>genͣ</abbr>
                    <expan>genanten</expan>
                </choice> <reg type="star">✳</reg> <persName ref="per019933">
                    <choice>
                        <abbr>Heinr̄</abbr>
                        <expan>Heinrich</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per019934">
                    <choice>
                        <abbr>Joh̄s</abbr>
                        <expan>Johans</expan>
                    </choice> Gamlickon</persName> <reg type="star">✳</reg> die vorge<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>ten mat<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_line_1501237162345_992" n="N007"/>uf<reg>,</reg> <reg type="star">✳</reg> oͧch gab die vor<choice>
                    <abbr>genͣ</abbr>
                    <expan>genanten</expan>
                </choice> <reg type="star">✳</reg> <persName ref="per019935">Anna</persName> <reg type="star">✳</reg> und <persName ref="per019937">Elly</persName> <reg type="star">✳</reg> die vor<choice>
                    <abbr>genͣ</abbr>
                    <expan>genant</expan>
                </choice> mat<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> uf <reg type="star">✳</reg> mit <choice>
                    <abbr>irˀ</abbr>
                    <expan>irer</expan>
                </choice> beider vogtes hant<reg>,</reg> <reg type="star">✳</reg> <persName ref="per019934">
                    <choice>
                        <abbr>Joh̄s</abbr>
                        <expan>Johans</expan>
                    </choice> Gamli<choice>
                        <abbr>kō</abbr>
                        <expan>kon</expan>
                    </choice>
                </persName><reg>,</reg> ir
        <lb facs="#facs_1_line_1501237165178_993" n="N008"/>bruͦ<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> der <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> bei<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> ze ei<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> rechten vogt gegeben wart<reg>,</reg> <reg type="star">✳</reg> an min hant <reg type="star">✳</reg> mit allen rech<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ansprachen<reg>,</reg> <reg type="star">✳</reg> so dar zuͦ <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501237168042_994" n="N009"/>gehoͤren mag<reg>,</reg> <reg type="star">✳</reg> und gab ich <reg type="star">✳</reg> die vor<choice>
                    <abbr>genͣ</abbr>
                    <expan>genant</expan>
                </choice> matten <reg type="star">✳</reg> in <persName ref="per019945">Walthers hant von <reg type="star">✳</reg> Ubenfar</persName> <reg type="star">✳</reg> ze den handen <reg type="star">✳</reg> der obgnan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_line_1501237170836_995" n="N010"/>epthissi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dez <choice>
                    <abbr>cō</abbr>
                    <expan>con</expan>
                </choice>ventes <reg type="star">✳</reg> mit allen nùt<choice>
                    <abbr>zē</abbr>
                    <expan>zen</expan>
                </choice> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> rech<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> so dar zuͦ gehoͤrt<reg>,</reg> <reg type="star">✳</reg> und geviel da mit gesamno<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urteil
        <lb facs="#facs_1_line_1501237174257_996" n="N011"/>bi dem eide<reg>,</reg> <reg type="star">✳</reg> daz <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vor<choice>
                    <abbr>genͣ</abbr>
                    <expan>genant</expan>
                </choice> koͧf alsso uf gege<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ge<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tigot weri<reg>,</reg> <reg type="star">✳</reg> daz er wol kraft sol <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mag han <reg type="star">✳</reg> nu und
        <lb facs="#facs_1_line_1501237177834_997" n="N012"/>har nach<reg>.</reg> <reg type="star">✳</reg> oͧch <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jahen die vorgeschri<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> lùte<reg>,</reg> <persName ref="per019933">
                    <choice>
                        <abbr>Heinr̄</abbr>
                        <expan>Heinrich</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per019934">
                    <choice>
                        <abbr>Joh̄s</abbr>
                        <expan>Johans</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per019935">Anna</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per019937">Elly</persName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vor<choice>
                    <abbr>genͣ</abbr>
                    <expan>genanten</expan>
                </choice> mat<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <reg type="star">✳</reg> wer ze sine fùr
        <lb facs="#facs_1_line_1501237181335_998" n="N013"/>lidig ei<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <reg type="star">✳</reg> beidù nu <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> har nach <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> da wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> nie<choice>
