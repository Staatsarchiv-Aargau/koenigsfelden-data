      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0207_r"/>
      <p facs="#facs_1_region_1516393129268_2">
        <lb facs="#facs_1_region_1516393129268_2l1" n="N001"/>Allen den<reg>,</reg> die disen brief an sehent <reg type="star">✳</reg> oder hoͤrend lesen<reg>,</reg> tuͦn kunt ich<reg>,</reg> <persName ref="per018688">Johans Vasolt<reg>,</reg> <choice>
                        <abbr>schulth̄</abbr>
                        <expan>schultheiss</expan>
                    </choice> ze Sekin<choice>
                        <abbr>g₎</abbr>
                        <expan>gen</expan>
                    </choice></persName><reg>,</reg> <reg type="star">✳</reg> daz fùr mich kament offen<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l2" n="N002" break="no"/>lich an gerichte ze <placeName ref="loc000939">Sekingen</placeName> inder stat <reg type="star">✳</reg> die erbern lùte <persName ref="per019357">junchfrowe Clare Spichwertlin</persName> mit irem wissenthaften voͧgte <persName ref="per018708"><choice>
                        <abbr>Uͦlr₎</abbr>
                        <expan>Uͦlrich</expan>
                    </choice>
        <lb facs="#facs_1_region_1516393129268_2l3" n="N003"/>Ratzen<reg>,</reg> <reg type="star">✳</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> ze Sekingen</persName><reg>,</reg> <reg type="star">✳</reg> ze ei<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> teyle <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per018804">Walther von Rùbeswile<reg>,</reg> wissenthaften schaffener <reg type="star">✳</reg> der ersamen frowen <reg type="star">✳</reg> der ep<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l4" n="N004" break="no"/>tischen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dez conventes gemeinlich ze Kûngesfelde<reg>,</reg> <reg type="star">✳</reg> Sant Claren orden<reg>,</reg> <reg type="star">✳</reg> in Costenzer bystuͦm</persName><reg>,</reg> <reg type="star">✳</reg> ze dem andern teyl<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> verjach
        <lb facs="#facs_1_region_1516393129268_2l5" n="N005"/>da offenlich <reg type="star">✳</reg> die selbe <persName ref="per019357">junchfrow Clara</persName> mit dem vor<choice>
                    <abbr>gen̄t</abbr>
                    <expan>genantem</expan>
                </choice> irem voͧgte<reg>,</reg> <reg type="star">✳</reg> daz si luterlichen durch got <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> durch ir sele heyle wil<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l6" n="N006" break="no"/>len <reg type="star">✳</reg> hette gegeben <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> oͧch offenlich vorgericht<reg>,</reg> <reg type="star">✳</reg> vor mir<reg>,</reg> mit rechter urteyle uf gab<reg>,</reg> <reg type="star">✳</reg> recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redelich<reg>,</reg> <reg type="star">✳</reg> <measure>nùne <reg type="star">✳</reg> vierteyle rog<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l7" n="N007" break="no"/>gen geltes</measure> <reg type="star">✳</reg> ab <measure>einer schuͤppossen</measure><reg>,</reg> <reg type="star">✳</reg> ist gelegen ze <placeName ref="loc011454">Eydckon</placeName> in dem nydern holtze <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> buwet in <persName ref="per019360">Urs Ibach</persName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>ein vierdenzal dinkels
        <lb facs="#facs_1_region_1516393129268_2l8" n="N008"/>geltes</measure> <reg type="star">✳</reg> git <persName ref="per019365">Hans Sachser <reg type="star">✳</reg> von Obren Munphen</persName> <reg type="star">✳</reg> von sinem guͦt <reg type="star">✳</reg> zuͦ des vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per018804">Walthers</persName> handen<reg>,</reg> <reg type="star">✳</reg> der oͧch offenlich die selben gùl<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l9" n="N009" break="no"/>te von der selber <persName ref="per019357">junchfrowen Claren</persName> uf nam <reg type="star">✳</reg> an der <orgName ref="org002627">egenan<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> eptischen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dez conventes stat <reg type="star">✳</reg> ze Kùngesfelde</orgName> <reg type="star">✳</reg> zenies<choice>
                    <abbr>sē</abbr>
                    <expan>sen</expan>
                </choice>
                <reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l10" n="N010" break="no"/>de<reg>,</reg> <reg type="star">✳</reg> zebesitzende <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze hande <reg type="star">✳</reg> je<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> eweklich <reg type="star">✳</reg> in allem dem rechte<reg>,</reg> <reg type="star">✳</reg> als oͧch die selbe <persName ref="per019357">junchfrowe Clare</persName> <reg type="star">✳</reg> die selben gùlte <reg type="star">✳</reg> ùntzint
        <lb facs="#facs_1_region_1516393129268_2l11" n="N011"/>har <reg type="star">✳</reg> genossen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gehebet hat <reg type="star">✳</reg> an alle geverde<reg>.</reg> <reg type="star">✳</reg> oͧch hat die selbe <persName ref="per019357">jungfrow Clare</persName> <reg type="star">✳</reg> mit dez <choice>
                    <abbr>vorgˀ</abbr>
                    <expan>vorgenanten</expan>
                </choice> ires voͧgtes handen <reg type="star">✳</reg> die
        <lb facs="#facs_1_region_1516393129268_2l12" n="N012"/>vorgeschrib<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> gùlte ge<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tigot <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> uff geben <reg type="star">✳</reg> mit handen<reg>,</reg> <reg type="star">✳</reg> mit munden <reg type="star">✳</reg> dem <choice>
                    <abbr>egen̄t</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per018804">Walthern</persName> <reg type="star">✳</reg> an der egenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> eptischen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dez con<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l13" n="N013" break="no"/>ventes stat <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mit aller der gewarsami <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sicherheit<reg>,</reg> <reg type="star">✳</reg> worthen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> werchen<reg>,</reg> <reg type="star">✳</reg> so dar zuͦ hortend <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> noturftig warend<reg>,</reg> <reg type="star">✳</reg> als oͧch da<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l14" n="N014" break="no"/>rumb vor dem gerichte ze <placeName ref="loc000939">Sekingen</placeName> <reg type="star">✳</reg> mit gesamenoter urteyl <reg type="star">✳</reg> uf den eyde <reg type="star">✳</reg> erteilet wart<reg>,</reg> <reg type="star">✳</reg> also daz diz hantgift <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> diz uffe geben
        <lb facs="#facs_1_region_1516393129268_2l15" n="N015"/>nu und hie nach <reg type="star">✳</reg> kraft <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> hantvestin haben sol und mag <reg type="star">✳</reg> gegen dem egenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <persName ref="per018804">Walthern</persName> an der vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> eptischen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dez conven<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l16" n="N016" break="no"/>tes stat <reg type="star">✳</reg> und gegen iren nachkomen je<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> eweklich an alle geverde<reg>.</reg> <reg type="star">✳</reg> oͧch enzeche sich die egenant <persName ref="per019357">junchfrow Clara</persName> <reg type="star">✳</reg> fùr sich <reg type="star">✳</reg> und
        <lb facs="#facs_1_region_1516393129268_2l17" n="N017"/>fùr alle ir erben <reg type="star">✳</reg> mit dem vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> irem voͧgte <reg type="star">✳</reg> aller der ansprache<reg>,</reg> <reg type="star">✳</reg> vorderunge <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> rechtes<reg>,</reg> <reg type="star">✳</reg> so si oder keyne iro erben je<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> an die <choice>
                    <abbr>vorg₎</abbr>
                    <expan>vorgenante</expan>
                </choice>
        <lb facs="#facs_1_region_1516393129268_2l18" n="N018"/>gùlte ge<choice>
                    <abbr>wī</abbr>
                    <expan>win</expan>
                </choice>nen moͤchtind<reg>,</reg> <reg type="star">✳</reg> in welchen wege daz gesin moͤchte<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gelobet oͧch der selber hantgift <reg type="star">✳</reg> werende ze sin <reg type="star">✳</reg> nach rechte <reg type="star">✳</reg> vor
        <lb facs="#facs_1_region_1516393129268_2l19" n="N019"/>geistlichen <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> weltlichen gerichten <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> an allen stetten <reg type="star">✳</reg> und niemer da wider zetuͤnde<reg>,</reg> <reg type="star">✳</reg> inkeynen sachen <reg type="star">✳</reg> oder mit keinen din<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l20" n="N020" break="no"/>gen<reg>,</reg> ane alle geverde <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> zuͦ einem waren urkùnde dirre dingen<reg>,</reg> <reg type="star">✳</reg> so han ich<reg>,</reg> vorgenanti <persName ref="per019357">Clara Spichwertlin</persName><reg>,</reg> min eygen
        <lb facs="#facs_1_region_1516393129268_2l21" n="N021"/>ingesigel gehenket an disen brief<reg>,</reg> <reg type="star">✳</reg> fùr mich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> min erben<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> zuͦ einer merren sicherheit der vorgeschribe<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> dingen<reg>,</reg>
        <lb facs="#facs_1_region_1516393129268_2l22" n="N022"/>so han wir<reg>,</reg> der vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genante</expan>
                </choice> <orgName ref="org007240">
                    <choice>
                        <abbr>schulth̄</abbr>
                        <expan>schultheiss</expan>
                    </choice> und der rate gemeinlich ze Sekingen</orgName><reg>,</reg> <reg type="star">✳</reg> ùnser stette ingesigel gehenket an disen brief<reg>,</reg> <reg type="star">✳</reg> durch
        <lb facs="#facs_1_region_1516393129268_2l23" n="N023"/>der egenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> beyder teylon betten willen<reg>,</reg> <reg type="star">✳</reg> der geben wart an der nechsten <date when="1345-06-22">mittewochen vor sant Johans tag <reg type="star">✳</reg> ze sune<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l24" n="N024" break="no"/>gichten<reg>,</reg> <reg type="star">✳</reg> des jares<reg>,</reg> do man zalte von gottes gebùrte <reg type="star">✳</reg> drùzehen hundert jare<reg>,</reg> dar nach in dem fùnf und vierzigosten
        <lb facs="#facs_1_region_1516393129268_2l25" n="N025"/>jare</date><reg>.</reg> <reg type="star">✳</reg> hie by warend die ersa<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> lùte <persName ref="per018932">Hartman Ratze</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per019180">Walther Wilde</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per018706"><choice>
                        <abbr>Joh̄</abbr>
                        <expan>Johan</expan>
                    </choice> Wibel</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per018710"><choice>
                        <abbr>C</abbr>
                        <expan>Chuͦnrad</expan>
                    </choice>
                    <reg type="star">✳</reg> Rùsseler</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per019893"><choice>
                        <abbr>Joh̄</abbr>
                        <expan>Johan</expan>
                    </choice> Mùller</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per019894"><choice>
                        <abbr>Joh̄</abbr>
                        <expan>Johan</expan>
                    </choice> Wilde</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per019895">Wal<reg type="hyphen">-</reg><lb facs="#facs_1_region_1516393129268_2l26" n="N026" break="no"/>ther <reg type="star">✳</reg> Schroͤter</persName> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per018712">Heini Spise</persName> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ander erber lùten vil<reg>,</reg> <orgName ref="org006779">burger ze Sekingen</orgName><reg>.</reg> <reg type="star">✳</reg>
            </p>
