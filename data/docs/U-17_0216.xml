      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0216_r"/>
      <p facs="#facs_1_region_1501237219015_1005">
        <lb facs="#facs_1_line_1501237227323_1006" n="N001"/>Allen den<reg>,</reg> die disen brief ansehent oder hoͤrent lesen nu oder har nach<reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich ich<reg>,</reg> <comment comment="Wernher Claus, vgl. Siegelumschrift sowie Urkunden Nr. 0256, 0258 und 0276.">
                    <persName ref="per019847">Wernher<reg>,</reg> der vogt</persName>
                </comment>
                <reg>,</reg> daz ich ze gerichte sasz ze <placeName ref="loc000195">Brugg</placeName> vor der stat<reg>,</reg>
        <lb facs="#facs_1_line_1501237231253_1007" n="N002"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <sic>kan</sic>
                    <corr>kam</corr>
                </choice> fùr mich in gerichtes wise <reg type="star">✳</reg> <persName ref="per019970">Ita von Ecquile</persName> <reg type="star">✳</reg> mit <persName ref="per019971">Johansen <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Ecquile</persName>
                <reg>,</reg> ir bruͦder<reg>,</reg> der ir mit rech<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urteil ze vogte geben wart vor
        <lb facs="#facs_1_line_1501237235861_1008" n="N003"/>gerichte<reg>,</reg> und <persName ref="per019973">Adelheit</persName>
                <reg>,</reg> ir tochter<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per019969">Uͦlrich zuͦ <choice>
                        <abbr>dē</abbr>
                        <expan>dem</expan>
                    </choice> Kënel</persName>
                <reg>,</reg> <reg type="star">✳</reg> der ir mit urteil ze vogte geben wart<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gahen offenlich gesunt libes und
        <lb facs="#facs_1_line_1501237240110_1009" n="N004"/>sinnes<reg>,</reg> daz si ze koͧffen hant gùgeben <reg type="star">✳</reg> <reg type="star">✳</reg> den <orgName ref="org002627">erwirdi<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                    <reg>,</reg> geistli<choice>
                        <abbr>chē</abbr>
                        <expan>chen</expan>
                    </choice> froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice>
                    <reg>,</reg> der eppthissin und dem convent gemeinlich ze Chùnigsfelt<reg>,</reg> Sant
        <lb facs="#facs_1_line_1501237244836_1010" n="N005"/>Claren ordens<reg>,</reg> gùle<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> in Costen<choice>
                        <abbr>tzˀ</abbr>
                        <expan>tzer</expan>
                    </choice> bistuͦm</orgName>
                <reg>,</reg> holtz und velt dez <placeName ref="loc011617">Eitenberg</placeName>
                <reg>,</reg> dz si hatten ein hal<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> an <placeName ref="loc011617">Eiten<choice>
                        <abbr>bgˀ</abbr>
                        <expan>berg</expan>
                    </choice>
                </placeName>
                <reg>,</reg> dz gùle<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> ist zwischent <choice>
                    <abbr>dē</abbr>
                    <expan>dem</expan>
                </choice> <persName ref="per019986">Sthapher</persName>
        <lb facs="#facs_1_line_1501237249508_1011" n="N006"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per019983">Uͦlrich Stellin</persName>
                <reg>,</reg> mit al<choice>
                    <abbr>lē</abbr>
                    <expan>len</expan>
                </choice> <choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> rechte<reg>,</reg> als si untz har gùbracht hant<reg>,</reg> <reg type="star">✳</reg> also daz <choice>
                    <abbr>mā</abbr>
                    <expan>man</expan>
                </choice> ei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> wagen weg sol ha<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> in dz holtz<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> an<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice> weg sol
        <lb facs="#facs_1_line_1501237255781_1012" n="N007"/>
                <choice>
                    <abbr>mā</abbr>
                    <expan>man</expan>
                </choice> han<reg>,</reg> wenne es nùt in nutz lit<reg>,</reg> <reg type="star">✳</reg> umb <measure>fûnf und drissig phunt nû<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice> phenni<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> Zovi<choice>
                        <abbr>ngˀ</abbr>
                        <expan>nger</expan>
                    </choice> mùntz</measure>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> verjahen oͧch dez<reg>,</reg> <reg type="star">✳</reg> daz <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> phen<choice>
                    <abbr>nī</abbr>
                    <expan>nin</expan>
                </choice>
                <choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gentzlich
        <lb facs="#facs_1_line_1501237261421_1013" n="N008"/>gùwert we<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in ir nutz <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>kert hetten<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ga<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> die <choice>
                    <abbr>vorḡ</abbr>
                    <expan>vorgenanten</expan>
                </choice> <persName ref="per019970">Ita</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per019971">Hans</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per019973">Adelheit</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per019969">Uͦlrich zuͦ <choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice> Kënel</persName> die <choice>
                    <abbr>vorḡ</abbr>
                    <expan>vorgenanten</expan>
                </choice> guͤ<choice>
                    <abbr>tˀ </abbr>
                    <expan>ter</expan>
                </choice>uf als urteil gab
        <lb facs="#facs_1_line_1501237266396_1014" n="N009"/>
                <persName ref="per018804">Walther <choice>
                        <abbr>dē</abbr>
                        <expan>dem</expan>
                    </choice> Meiier <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Rùbiswile</persName> an der vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> epthissin <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>cō</abbr>
                    <expan>con</expan>
                </choice>ventes stat<reg>,</reg> der iro gewisser ampt<choice>
                    <abbr>mā</abbr>
                    <expan>man</expan>
                </choice> zù <choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> zit<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> waz<reg>,</reg> <reg type="star">✳</reg> mit al<choice>
                    <abbr>lē</abbr>
                    <expan>len</expan>
                </choice> nût<choice>
                    <abbr>zē</abbr>
                    <expan>zen</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> rechte<reg>,</reg> so dar
        <lb facs="#facs_1_line_1501237271901_1015" n="N010"/>zuͦ hoͤrt<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> geviel da mit gesamnotter urteil bi <choice>
                    <abbr>dē</abbr>
                    <expan>dem</expan>
                </choice> eide<reg>,</reg> <reg type="star">✳</reg> daz <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>vorḡ</abbr>
                    <expan>vorgenante</expan>
                </choice> koͧfft also uf geben <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gevertigot <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dz er wol kraft sol <choice>
                    <abbr>un̄</abbr>
