        <lb facs="#facs_1_line_1507841687092_506" n="N001"/>Ich<reg>,</reg> <persName ref="per014815">Jenni von Lo<reg>,</reg> ze disen ziten <choice>
                        <abbr>schulth̄s</abbr>
                        <expan>schultheiss</expan>
                    </choice> ze Lentzburg</persName> <reg type="star">✳</reg> in der stat<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich und tuͦn <choice>
                    <abbr>kūt</abbr>
                    <expan>kunt</expan>
                </choice> menli<choice>
                    <abbr>chm̄</abbr>
                    <expan>chem</expan>
