  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0889_r"/>
      <p facs="#facs_1_region_1521840271289_56">
        <lb facs="#facs_1_region_1521840271289_56l1" n="N001"/>Ich<reg>,</reg> <persName ref="per024358">Hanns Jacob Pur<reg>,</reg> seshafft zuͤ Alttennburg</persName><reg>,</reg> <reg type="star">✳</reg> bekenn <reg type="star">✳</reg> unnd thuͤn kund offennlich mit dysem brieff<reg>,</reg> <reg type="star">✳</reg> das mir als zuͦ hann<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice>
        <lb facs="#facs_1_region_1521840271289_56l2" n="N002"/>min<reg>,</reg> <reg type="star">✳</reg> miner erbenn <reg type="star">✳</reg> unnd nàchkom<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> <reg type="star">✳</reg> durch die <persName ref="per007303">wirdigenn<reg>,</reg> <reg type="star">✳</reg> edlenn <reg type="star">✳</reg> unnd geistli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> fròwen <reg type="star">✳</reg> Anna Saͤgennsserin<reg>,</reg> <reg type="star">✳</reg> conn<choice>
                        <abbr>vēt</abbr>
                        <expan>vent</expan>
                    </choice> fròw <reg type="star">✳</reg> unnd
        <lb facs="#facs_1_region_1521840271289_56l3" n="N003"/>jarzitt meysterinn zuͤ Kùngsfel<choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> als von wegenn desz selbenn jàrzitt ampts <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>c̄</abbr>
                    <expan> cetera</expan>
                </choice> ganntz wolbedauchtlich <choice>
                    <abbr>umb̄</abbr>
                    <expan>umbe</expan>
                </choice> me<choice>
                    <abbr>reˀen</abbr>
                    <expan>reren</expan>
                </choice> nutz <reg type="star">✳</reg> und in
        <lb facs="#facs_1_region_1521840271289_56l4" n="N004"/>eins fryg<choice>
                    <abbr>g₎</abbr>
                    <expan>gen</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> uffrechtenn tuschs wisz <reg type="star">✳</reg> zuͤ geeygnet <reg type="star">✳</reg> unnd ùber gaͤbenn ist <reg type="star">✳</reg> alles für frygg<reg>,</reg> <reg type="star">✳</reg> lidig <reg type="star">✳</reg> eygenn <reg type="star">✳</reg> by <measure>einer juchart<choice>
                        <abbr>t₎</abbr>
                        <expan>ten</expan>
                    </choice> ackers</measure><reg>,</reg> hin<reg type="hyphen">-</reg><lb facs="#facs_1_region_1521840271289_56l5" n="N005" break="no"/>für in min guͦtt <reg type="star">✳</reg> in <placeName ref="loc013295">Haͤmmiken</placeName> dienend<reg>,</reg> <reg type="star">✳</reg> stost in winckel<reg>,</reg> <reg type="star">✳</reg> dàmit die lannnd stràs gestrax irnn ganng <reg type="star">✳</reg> hàtt<reg>,</reg> <reg type="star">✳</reg> vormàls in <persName ref="per017014">Hann<choice>
                        <abbr>sn̄</abbr>
                        <expan>sen</expan>
                    </choice> Fri<reg type="lb">¬</reg><lb facs="#facs_1_region_1521840271289_56l6" n="N006" break="no"/>ckers</persName> guͤtt<reg>,</reg> <reg type="star">✳</reg> so ouch gemelttem jàrzit ampt zinnst<reg>,</reg> <reg type="star">✳</reg> gehoͤrtt <reg type="star">✳</reg> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>c̄</abbr>
                    <expan> cetera</expan>
                </choice><reg>,</reg> dasz also hinfùr als min eygenn guͤtt <reg type="star">✳</reg> nutzenn<reg>,</reg> <reg type="star">✳</reg> niessenn<reg>,</reg>
        <lb facs="#facs_1_region_1521840271289_56l7" n="N007"/>beset<choice>
                    <abbr>zn̄n</abbr>
                    <expan>zenn</expan>
                </choice> <reg type="star">✳</reg> unnd zeentsetzenn <reg type="star">✳</reg> nàch min <reg type="star">✳</reg> unnd mi<choice>
                    <abbr>neˀ</abbr>
                    <expan>ner</expan>
                </choice> erbenn <reg type="star">✳</reg> nutz<reg>,</reg> <reg type="star">✳</reg> wil<choice>
                    <abbr>lnn̄</abbr>
                    <expan>lenn</expan>
                </choice> <reg type="star">✳</reg> unnd geval<choice>
                    <abbr>ln̄</abbr>
                    <expan>len</expan>
                </choice>
                <reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>umb̄</abbr>
                    <expan>umbe</expan>
                </choice> <reg type="star">✳</reg> unnd für soͤliche obbemeltte
        <lb facs="#facs_1_region_1521840271289_56l8" n="N008"/>
                <measure>halbe juchartt ackers</measure> <reg type="star">✳</reg> hab ich dàrgaͤgenn <reg type="star">✳</reg> für mich<reg>,</reg> <reg type="star">✳</reg> all min erbenn <reg type="star">✳</reg> unnd nàchkomn<choice>
