                <reg>,</reg>
        <lb facs="#facs_1_line_1495028636456_35" n="N020"/>
                <persName ref="per007128">schultheitze</persName>
                <reg>,</reg> <reg type="star">✳</reg> und wir<reg>,</reg> <persName ref="per012948">Eppel</persName> und <persName ref="per020813">
