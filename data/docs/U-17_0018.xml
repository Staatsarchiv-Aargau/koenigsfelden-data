                <reg>,</reg> <reg type="star">✳</reg> ich<reg>,</reg> <persName ref="per012929">Ulrich Schoͤni</persName>
                <reg>,</reg> <measure>fumf
        <lb facs="#facs_1_r1009" n="N004"/>phunt</measure>
                <reg>,</reg> <reg type="star">✳</reg> ich<reg>,</reg> <persName ref="per012930">Heinrich Halbstein</persName>
                <reg>,</reg> <measure>zwei phunt</measure>
                <reg>,</reg> <reg type="star">✳</reg> ich<reg>,</reg> <persName ref="per012940">Elsi von Bruke</persName>
                <reg>,</reg> <measure>drizzich schil<reg type="lb">¬</reg>
        <lb facs="#facs_1_r1011" n="N005" break="no"/>linge <choice>
                        <abbr>phē</abbr>
                        <expan>phen</expan>
                    </choice>ninge</measure> <reg type="star">✳</reg> und ich<reg>,</reg> <persName ref="per012931">Walther von Chulmberch</persName>
                <reg>,</reg> <measure>einlef phunt</measure> <reg type="star">✳</reg> enphangen
        <lb facs="#facs_1_r1013" n="N006"/>haben <reg type="star">✳</reg> von der <orgName ref="org000088">herschaft</orgName> <reg type="star">✳</reg> fur die ekker<reg>,</reg> <reg type="star">✳</reg> di wir hatten bi der <placeName ref="loc013396">chapelle
        <lb facs="#facs_1_r1015" n="N007"/>in der chirichoͤre von Windisch</placeName>
                <reg>,</reg> <reg type="star">✳</reg> mit den <reg type="star">✳</reg> ander akker sint wider leit <reg type="star">✳</reg> ze
        <lb facs="#facs_1_r1017" n="N008"/>nutz <reg type="star">✳</reg> dez <orgName ref="org002627">nùwen chlosters<reg>,</reg> <reg type="star">✳</reg> daz die herschaft hat dar geboͧwet</orgName>
                <reg>,</reg> <reg type="star">✳</reg> und ver<reg type="lb">¬</reg>
        <lb facs="#facs_1_r1020" n="N009" break="no"/>zichen uns <reg type="star">✳</reg> fur uns <reg type="star">✳</reg> und fur alle unser erben <reg type="star">✳</reg> allez dez rehtes<reg>,</reg> <reg type="star">✳</reg> daz wir an
        <lb facs="#facs_1_r1022" n="N010"/>den vorgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> ekkern hatten <reg type="star">✳</reg> oder haben mohten<reg>.</reg> <reg type="star">✳</reg> da ditz beschach<reg>,</reg> <reg type="star">✳</reg> da
        <lb facs="#facs_1_r1025" n="N011"/>waren bi <reg type="star">✳</reg> <persName ref="per015724">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Walther<reg>,</reg> der techan von Windisch</persName>
