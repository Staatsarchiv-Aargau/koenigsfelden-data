      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0214_r"/>
      <p facs="#facs_1_region_1501236658456_963">
        <lb facs="#facs_1_line_1501236662473_964" n="N001"/>Allen den<reg>,</reg> die disen brief ansehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen <reg type="star">✳</reg> kùnde und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich ich<reg>,</reg> <reg type="star">✳</reg> <persName ref="per013543">Johans von Seon<reg>,</reg> <choice>
                        <abbr>schulth̄</abbr>
                        <expan>schultheiss</expan>
                    </choice> ze
        <lb facs="#facs_1_line_1501236664810_965" n="N002"/>Aroͧwe</persName>
                <reg>,</reg> <reg type="star">✳</reg> daz fùr mich kam <reg type="star">✳</reg> <persName ref="per019682">frowe Gerdrut Batzlina<reg>,</reg> bur<choice>
                        <abbr>gˀin</abbr>
                        <expan>gerin</expan>
                    </choice> ze Aroͧwe</persName>
                <reg>,</reg> <reg type="star">✳</reg> mit <persName ref="per018950">Nyclausen von Hol<choice>
                        <abbr>dˀn</abbr>
                        <expan>dern</expan>
                    </choice>
                    <reg>,</reg> bur<choice>
                        <abbr>g₎</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName>
        <lb facs="#facs_1_line_1501236667746_966" n="N003"/>ze der selben stat<reg>,</reg> <reg type="star">✳</reg> der ir wissenthafter vogt ist<reg>,</reg> <reg type="star">✳</reg> und offenot vor mir <reg type="star">✳</reg> mit ir vogt<reg>,</reg> <reg type="star">✳</reg> daz si <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧft het
        <lb facs="#facs_1_line_1501236670418_967" n="N004"/>recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redelich <reg type="star">✳</reg> <measure>ein schuͦppuͦsse</measure>
                <reg>,</reg> gele<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> ze <placeName ref="loc009761">Sure</placeName> in dem banne<reg>,</reg> <reg type="star">✳</reg> die da buwet <persName ref="per019684">Ruͧdi Attenwile</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gil<reg type="lb">¬</reg><lb facs="#facs_1_line_1501236672810_968" n="N005" break="no"/>tet <measure>fùnf stuk an korne</measure> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ein vogtei gele<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> ze <placeName ref="loc009757">Talhein</placeName> in d<add place="above">e</add>m banne<reg>,</reg> <reg type="star">✳</reg> die da giltet <measure>fùnf schilling
        <lb facs="#facs_1_line_1501236675858_969" n="N006"/>phennig</measure> <reg type="star">✳</reg> und <measure>ein viertel <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>nen</measure>
                <reg>,</reg> <reg type="star">✳</reg> den <orgName ref="org002627">erberen<reg>,</reg> geistlichen froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice>
                    <reg>,</reg> der epthissi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> <reg type="star">✳</reg> <reg type="star">✳</reg> dem convente <reg type="star">✳</reg> des <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501236679018_970" n="N007"/>gotzhus ze Chùngesfelt<reg>,</reg> Sant Claren ordens<reg>,</reg> in Costentzer bystum gelegen</orgName>
                <reg>,</reg> <reg type="star">✳</reg> <measure>umb vier und fùnftzig
        <lb facs="#facs_1_line_1501236681908_971" n="N008"/>phunt nu<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice> phennig</measure>
                <reg>,</reg> <reg type="star">✳</reg> den si oͧch gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gantzlich <reg type="star">✳</reg> von in <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>richt <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ge<choice>
                    <abbr>wtˀ</abbr>
                    <expan>wert</expan>
                </choice> ist <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in ir nutz und
        <lb facs="#facs_1_line_1501236685570_972" n="N009"/>fro<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>keret sint<reg>.</reg> <reg type="star">✳</reg> dar nach entzech sich dù <choice>
                    <abbr>vorg̃</abbr>
                    <expan>vorgenante</expan>
                </choice> <persName ref="per019682">Gerdrut Batzlina</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vorgescri<choice>
                    <abbr>bnˀ</abbr>
                    <expan>bner</expan>
                </choice> schuͧppuͧsse <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> vogti
        <lb facs="#facs_1_line_1501237055162_973" n="N010"/>mit ir vogtes hant <reg type="star">✳</reg> in <persName ref="per018804">Walthers dez schafners</persName> hant ze <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ege<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>ten <orgName ref="org002627">epthissi<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dez con<choice>
                        <abbr>vē</abbr>
                        <expan>ven</expan>
                    </choice>tes han<choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice>
        <lb facs="#facs_1_line_1501237058418_974" n="N011"/>ze Chùngesfelt <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> ir nachko<choice>
                        <abbr>mē</abbr>
                        <expan>men</expan>
                    </choice>
                </orgName> <reg type="star">✳</reg> als ze <placeName ref="loc000121">Aroͧwe</placeName> recht ist <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> loͧbt oͧch fùr si <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir erben <reg type="star">✳</reg> wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> den
        <lb facs="#facs_1_line_1501237061483_975" n="N012"/>selben koͧffe nie<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> ze tuͤnne <reg type="star">✳</reg> mit wor<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> noch mit werchen <reg type="star">✳</reg> an geistli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> noch an weltli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> gerichte <reg type="star">✳</reg> noch
        <lb facs="#facs_1_line_1501237065059_976" n="N013"/>usse<choice>
                    <abbr>rūt</abbr>
                    <expan>runt</expan>
                </choice> gerichtes <reg type="star">✳</reg> an alle ge<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>de<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> har umbe<reg>,</reg> wand diz alles vor mir<reg>,</reg> dem e<choice>
                    <abbr>geñ</abbr>
                    <expan>genanten</expan>
                </choice> <choice>
                    <abbr>schulth̄</abbr>
                    <expan>schultheiss</expan>
                </choice>
                <reg>,</reg> besche<choice>
                    <abbr>hē</abbr>
                    <expan>hen</expan>
                </choice> ist<reg>,</reg> als
        <lb facs="#facs_1_line_1501237068931_977" n="N014"/>ze <placeName ref="loc000121">Aroͧwe</placeName> in <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> stat recht ist<reg>,</reg> <reg type="star">✳</reg> so han ich dur ir aller bette willen ze ei<choice>
                    <abbr>nē</abbr>
                    <expan>ner</expan>
                </choice> offenne urkùnt und
        <lb facs="#facs_1_line_1501237073435_978" n="N015"/>wa<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice> gezùgsami min ingesigel gehencket an disen brief<reg>.</reg> <reg type="star">✳</reg> ich<reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>vorg̃</abbr>
                    <expan>vorgenante</expan>
                </choice> <persName ref="per018950">Nyclaus <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Hol<choice>
                        <abbr>dˀn</abbr>
                        <expan>dern</expan>
                    </choice>
                </persName>
                <reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich oͧch an
        <lb facs="#facs_1_line_1501237077756_979" n="N016"/>disem brief<reg>,</reg> <reg type="star">✳</reg> daz dù vorgescri<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>vˀt</abbr>
                    <expan>vert</expan>
                </choice>
                <choice>
                    <abbr>gū</abbr>
                    <expan>gun</expan>
                </choice>ge ist besche<choice>
                    <abbr>hē</abbr>
                    <expan>hen</expan>
                </choice> <reg type="star">✳</reg> mit mi<choice>
                    <abbr>nē</abbr>
                    <expan>ner</expan>
                </choice> gunst <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> han<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> <choice>
