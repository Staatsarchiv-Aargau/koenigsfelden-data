  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0338_r"/>
      <p facs="#facs_1_region_1501249773282_371">
        <lb facs="#facs_1_line_1501249777668_372" n="N001"/>Ich<reg>,</reg> <persName ref="per013224">Hanman von Ostran</persName><reg>,</reg> <reg type="star">✳</reg> tuͦn kunt <reg type="star">✳</reg> allen den<reg>,</reg> die disen brieff sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> daz fùr mich kam<reg>,</reg>
        <lb facs="#facs_1_line_1501249780898_373" n="N002"/>do ich offenlich ze gericht sass ze <placeName ref="loc009814">Villna<choice>
                        <abbr>chˀn</abbr>
                        <expan>chern</expan>
                    </choice>
                </placeName> in dem dorff<reg>,</reg> <reg type="star">✳</reg> <persName ref="per020317">Heini Dùfelbeis</persName><reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> min ei<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> ist von dem lip<reg>,</reg> <reg type="star">✳</reg> von
        <lb facs="#facs_1_line_1501249784208_374" n="N003"/>
                <placeName ref="loc009757">Talhein</placeName><reg>,</reg> ze eim teil<reg>,</reg> <reg type="star">✳</reg> und <persName ref="per013227">Fridrich<reg>,</reg> schaff<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice>
                </persName> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org002627">erwirdigen<reg>,</reg> geistli<choice>
                        <abbr>chē</abbr>
                        <expan>chen</expan>
                    </choice> froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice><reg>,</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> aͤptischinen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dez <choice>
                        <abbr>cō</abbr>
                        <expan>con</expan>
                    </choice>ventz <reg type="star">✳</reg> dez <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501249788016_375" n="N004"/>gotzhus ze Kùngsvelt</orgName><reg>,</reg> <reg type="star">✳</reg> ze dem an<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice> teil<reg>,</reg> <reg type="star">✳</reg> und offnz <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genante</expan>
                </choice> <persName ref="per020317">Heini Dù<choice>
                        <abbr>felb̄</abbr>
                        <expan>felbeis</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> daz er ze koͧffenn ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> hett <reg type="star">✳</reg> recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1501249792445_376" n="N005"/>redelich fùr recht eigen <reg type="star">✳</reg> dem jetz<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per013227">Fridrich<reg>,</reg> schaff<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice>
                </persName><reg>,</reg> ze den vorgeschrib<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> froͧ<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> han<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> ein mùli<reg>,</reg> ist gelegen
        <lb facs="#facs_1_line_1501249796002_377" n="N006"/>ni<choice>
                    <abbr>dˀt</abbr>
                    <expan>dert</expan>
                </choice>alp dem dorff ze <placeName ref="loc009757">Talheim</placeName> an dem bach<reg>,</reg> <reg type="star">✳</reg> giltet <measure>jerlichs zwein mùtt kernen<reg>,</reg> <reg type="star">✳</reg> umb dri<choice>
                        <abbr>zē</abbr>
                        <expan>zen</expan>
                    </choice>hen pfunt steb<choice>
                        <abbr>lˀ</abbr>
                        <expan>ler</expan>
                    </choice>
                </measure><reg>,</reg> dero
        <lb facs="#facs_1_line_1501249799330_378" n="N007"/>er gar und gentzlich ge<choice>
                    <abbr>wˀt</abbr>
                    <expan>wert</expan>
                </choice> <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice><reg>,</reg> dez er oͧch <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gach vor mir <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> si in sinen redli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> nutz bewendz hett<reg>.</reg> <reg type="star">✳</reg> und gab <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>
        <lb facs="#facs_1_line_1501249803690_379" n="N008"/>
                <choice>
                    <abbr>egen̄̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per020317">Heini Dù<choice>
                        <abbr>felb̄</abbr>
                        <expan>felbeis</expan>
                    </choice>
                </persName> die vor<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genante</expan>
                </choice> mùli uff vor gericht <reg type="star">✳</reg> und enzeche sich mit minem willen und gunst fùr sich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1501249808124_380" n="N009"/>sin er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice> rech<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> und eigenschaft<reg>,</reg> so er hatt gehept an die ob<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genante</expan>
                </choice> mùli<reg>,</reg> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tigot si oͧch mit mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> hand von
        <lb facs="#facs_1_line_1501249812468_381" n="N010"/>gerichtes wegen <reg type="star">✳</reg> in des ob<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per013227">Fridrich</persName> hand<reg>,</reg> ze den obgeschrib<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> froͧwen handen<reg>,</reg> <reg type="star">✳</reg> mit al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice> gewor<choice>
                    <abbr>sā</abbr>
                    <expan>sam</expan>
                </choice>mi<reg>,</reg> wort<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_line_1501249815738_382" n="N011"/>und <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>
                <choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice><reg>,</reg> so nach gewonheit <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> nach recht dar zuͦ hoͤren solt <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> mocht<reg>,</reg> also daz alle<reg>,</reg> die in geriht
        <lb facs="#facs_1_line_1501249820108_383" n="N012"/>wa<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice> und gefraͤgt wurden von mir<reg>,</reg> <reg type="star">✳</reg> erteilten uff den eid<reg>,</reg> <reg type="star">✳</reg> daz es besche<choice>
                    <abbr>hē</abbr>
                    <expan>hen</expan>
                </choice> <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> dz es wol kraft und
        <lb facs="#facs_1_line_1501249824827_384" n="N013"/>bestetung ha<choice>
                    <abbr>b̄n</abbr>
                    <expan>ben</expan>
                </choice> soͤlt <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> moͤcht nu und hie nach<reg>.</reg> <reg type="star">✳</reg> und loͧpt oͧch <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selb <persName ref="per020317">Heini Dùfelbeis</persName><reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ob<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> mùli recht
        <lb facs="#facs_1_line_1501249828764_385" n="N014"/>
                <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice> sin fùr eigen<reg>,</reg> wa sin die vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> froͧ<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> notdùrftig sint<reg>,</reg> an geistli<choice>
                    <abbr>chē</abbr>
                    <expan>chem</expan>
                </choice> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> weltlichem gericht<reg>.</reg> er
        <lb facs="#facs_1_line_1501249834819_386" n="N015"/>loͧpt oͧch<reg>,</reg> wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> disen koͧff nie<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> ze tuͦnn noch ze spre<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice><reg>,</reg> we<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> mit gericht<reg>,</reg> geistli<choice>
                    <abbr>chē</abbr>
                    <expan>chem</expan>
                </choice> noch weltlichem<reg>,</reg> <reg type="star">✳</reg>
