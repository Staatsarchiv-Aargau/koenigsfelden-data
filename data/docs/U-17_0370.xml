                <reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> die <persName ref="per025807">Uͦlrich <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wile</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per025809">Welti Foͤni</persName> <reg type="star">✳</reg> <reg type="star">✳</reg> und
        <lb facs="#facs_1_region_1518600114525_24l3" n="N003"/>
                <persName ref="per025811">Hans Walt<choice>
                        <abbr>hˀz</abbr>
                        <expan>herz</expan>
                    </choice>
                </persName> <reg type="star">✳</reg> und ir erben <reg type="star">✳</reg> ze dem vorgenanten ak<choice>
                    <abbr>kˀ</abbr>
                    <expan>ker</expan>
                </choice> hatten<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> daz die stoͤse <reg type="star">✳</reg> wrdent gesetzet <reg type="star">✳</reg> an mich<reg>,</reg>
