  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0448_r"/>
      <p facs="#facs_1_region_1507833488976_148">
        <lb facs="#facs_1_line_1507833495077_149" n="N001"/>Diz ist ûber die vischuntzun ze
        <lb facs="#facs_1_line_1507833497437_150" n="N002"/>
                <placeName ref="loc011313">Hermanswile</placeName><reg>,</reg> in dem banne ze
        <lb facs="#facs_1_line_1507833499668_151" n="N003"/>
                <placeName ref="loc010735">Lunghu<choice>
                        <abbr>s₎</abbr>
                        <expan>sen</expan>
                    </choice>
                </placeName><reg>.</reg>
        <lb facs="#facs_1_line_1507833502376_152" n="N004"/>¶ der erst brief: <reg type="star">✳</reg> wir<reg>,</reg> <persName ref="per000347">Albrecht<reg>,</reg>
        <lb facs="#facs_1_line_1507833505041_153" n="N005"/>von gotes gnaden <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzog ze Oͤsterich<reg>,</reg>
        <lb facs="#facs_1_line_1507833508929_154" n="N006"/>ze Stir<reg>,</reg> ze <choice>
                        <sic>Kernen</sic>
                        <corr>Kernden</corr>
                    </choice></persName><reg>,</reg> <reg type="star">✳</reg> tuͦn kunt
        <lb facs="#facs_1_line_1507833512693_155" n="N007"/>
                <choice>
                    <sic>und</sic>
                    <corr>umb</corr>
                </choice> die vischunzun<reg>,</reg> <reg type="star">✳</reg> die wir ha<choice>
                    <abbr>bn̄t</abbr>
                    <expan>bent</expan>
                </choice>
        <lb facs="#facs_1_line_1507833515482_156" n="N008"/>in un<choice>
                    <abbr>s₎m</abbr>
                    <expan>serm</expan>
                </choice> land ze <placeName ref="loc000122">Ergoͧwe</placeName><reg>,</reg> uf der
        <lb facs="#facs_1_line_1507833517710_157" n="N009"/>
                <placeName ref="loc001093">Rûse</placeName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>c̃</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg>
        <lb facs="#facs_1_line_1507833525518_158" n="N010"/>¶ der an<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>: <reg type="star">✳</reg> ¶ wir<reg>,</reg> <persName ref="per006964">Agnesz<reg>,</reg> von gotes
        <lb facs="#facs_1_line_1507833528391_159" n="N011"/>gnaden wilent kûnigin ze Ungern</persName><reg>,</reg>
        <lb facs="#facs_1_line_1507833530669_160" n="N012"/>tuͦn kunt mit urkunt dis briefs<reg>,</reg>
        <lb facs="#facs_1_line_1507833533108_161" n="N013"/>dz wir mit gunst und willen
        <lb facs="#facs_1_line_1507833535909_162" n="N014"/>un<choice>
                    <abbr>s₎s</abbr>
                    <expan>sers</expan>
                </choice> lieben bruͦ<choice>
                    <abbr>dˀs</abbr>
                    <expan>ders</expan>
                </choice> <persName ref="per000347">Albrechtz<reg>,</reg> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzo<choice>
                        <abbr>g₎</abbr>
                        <expan>gen</expan>
                    </choice>
        <lb facs="#facs_1_line_1507833538482_163" n="N015"/>ze Oͤst<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice><reg>,</reg> ze Stir und ze Kernden</persName><reg>,</reg>
        <lb facs="#facs_1_line_1507833541227_164" n="N016"/>geloͤzzet haben die vischentzun<reg>,</reg>
        <lb facs="#facs_1_line_1507833543832_165" n="N017"/>die da hoͤrent in den hof ze <placeName ref="loc010735">Lung<reg type="lb">¬</reg><lb facs="#facs_1_line_1507833546160_166" n="N018" break="no"/>hoff</placeName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>c̃</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg>
        <lb facs="#facs_1_line_1507833548849_167" n="N019"/>¶ der drit<reg>,</reg> dz ist <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ussprùch: <reg type="star">✳</reg> ¶ wir<reg>,</reg>
        <lb facs="#facs_1_line_1507833554260_168" n="N020"/>
                <persName ref="per013239">graff Ruͦdolf von Nydoͧwe<reg>,</reg> der
        <lb facs="#facs_1_line_1507833561717_169" n="N021"/>hochgeborner fûrs<choice>
                        <abbr>t₎</abbr>
                        <expan>ten</expan>
                    </choice><reg>,</reg> un<choice>
                        <abbr>s₎</abbr>
                        <expan>ser</expan>
                    </choice> gnedi<choice>
                        <abbr>g₎</abbr>
                        <expan>gen</expan>
                    </choice>
        <lb facs="#facs_1_line_1507833564522_170" n="N022"/>
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren<reg>,</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzo<choice>
                        <abbr>g₎</abbr>
                        <expan>gen</expan>
                    </choice> von Oͤst<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> lantvogt</persName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>c̃</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1595972567830_629" n="N023"/>¶ dez von <placeName ref="loc009263">Wolun</placeName> guͦt
        <lb facs="#facs_1_line_1595972585223_633" n="N024"/>
                <add place="above">
                    <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> lit oͧch da<reg>.</reg>
                </add>
        <lb facs="#facs_1_line_1595972567829_628" n="N025"/>¶ der erst <reg type="star">✳</reg> <choice>
                    <abbr>b</abbr>
                    <expan>brief</expan>
                </choice> <reg type="star">✳</reg>:
        <lb facs="#facs_1_line_1507833573845_172" n="N026"/>¶ allen den<reg>,</reg> die di<choice>
                    <abbr>sz₎</abbr>
                    <expan>szen</expan>
                </choice> brief an sechent
        <lb facs="#facs_1_line_1507833576526_173" n="N027"/>
                <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent le<choice>
                    <abbr>sz₎</abbr>
                    <expan>szen</expan>
                </choice><reg>,</reg> kûnd ich<reg>,</reg> <persName ref="per014832">Johans
        <lb facs="#facs_1_line_1507833579010_174" n="N028"/>Zwi<choice>
                        <abbr>ckˀ</abbr>
                        <expan>cker</expan>
                    </choice><reg>,</reg> schulthessz ze Baden</persName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>c̃</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg>
        <lb facs="#facs_1_line_1507833582125_175" n="N029"/>¶ der an<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> brief<reg>,</reg> dz guͦt lit oûch ze
        <lb facs="#facs_1_line_1507833585102_176" n="N030"/>
                <placeName ref="loc009263">Wolun</placeName> und ze <placeName ref="loc000401">Hilfikon</placeName>: ¶ ich<reg>,</reg> <persName ref="per022334">Hans
        <lb facs="#facs_1_line_1507833587732_177" n="N031"/>von Sechen<reg>,</reg> rit<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice><reg>,</reg> vogt ze Baden</persName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>c̃</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg>
