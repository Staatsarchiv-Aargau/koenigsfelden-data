      <p facs="#facs_1_region_1512224078545_65">
        <lb facs="#facs_1_region_1512224078545_65l1" n="N001"/>Wir<reg>,</reg> <orgName ref="org002627">aͤpptissin <reg type="star">✳</reg> unnd con<choice>
