      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0040_r"/>
      <p facs="#facs_1_region_1495029335903_82">
        <lb facs="#facs_1_line_1495029340371_83" n="N001"/>Wir<reg>,</reg> <persName ref="per000342">Lûpolt<reg>,</reg> von gotz gnaden <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzog ze Osterrich und ze Stir</persName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jehen und tuͦn kunt allen den<reg>,</reg>
        <lb facs="#facs_1_line_1495029342923_84" n="N002"/>die disen brief ansehent und horent lesen<reg>,</reg> <reg type="star">✳</reg> daz wir <reg type="star">✳</reg> <persName ref="per012971">Egebrehte <reg type="star">✳</reg> dem Roten<reg>,</reg> <reg type="star">✳</reg> <choice>
                        <abbr>kirchˀren</abbr>
                        <expan>kircherren</expan>
                    </choice> ze Rein</persName><reg>,</reg>
        <lb facs="#facs_1_line_1495029345726_85" n="N003"/>umb sinen dienst<reg>,</reg> <reg type="star">✳</reg> den er uns tuͦn sol<reg>,</reg> <reg type="star">✳</reg> gelopt haben <measure>zehen mark sil<choice>
                        <abbr>bˀs</abbr>
                        <expan>bers</expan>
                    </choice>
                </measure><reg>,</reg> <reg type="star">✳</reg> und haben <reg type="star">✳</reg> dar umb <reg type="star">✳</reg> dem <persName ref="per012972">er<choice>
                        <abbr>bnˀ</abbr>
                        <expan>bern</expan>
                    </choice>
        <lb facs="#facs_1_line_1495029348212_86" n="N004"/>rit<choice>
                        <abbr>tˀe</abbr>
                        <expan>tere</expan>
                    </choice> <reg type="star">✳</reg> <choice>
                        <abbr>Frid̄</abbr>
                        <expan>Fridrich</expan>
                    </choice><reg>,</reg> <reg type="star">✳</reg> dem schultheiszen <reg type="star">✳</reg> von Schaffehusen</persName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>setzet an siner stat <reg type="star">✳</reg> <measure>siben stucke geltes an korn</measure> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1495029351021_87" n="N005"/>und <measure>fûnftzehen schilling pfenning</measure> <reg type="star">✳</reg> uf dem zehenden ze <placeName ref="loc013697">Swenden</placeName><reg>,</reg> <reg type="star">✳</reg> ze habend<reg>,</reg> <reg type="star">✳</reg> ze nùtzen <reg type="star">✳</reg> und ze
        <lb facs="#facs_1_line_1495029353374_88" n="N006"/>nieszend als lange<reg>,</reg> untz wir <reg type="star">✳</reg> und un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> bruͦ<choice>
                    <abbr>dˀe</abbr>
                    <expan>dere</expan>
                </choice> <reg type="star">✳</reg> und erben <reg type="star">✳</reg> daz von dem selben schultheiszen <reg type="star">✳</reg> oder
        <lb facs="#facs_1_line_1495029356073_89" n="N007"/>von den erben erlosen mit <measure>zehen marken sil<choice>
                        <abbr>bˀs</abbr>
                        <expan>bers</expan>
                    </choice>
                </measure><reg>.</reg> <reg type="star">✳</reg> und ze eim urkùnde dirre dinge ist di<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> brief besi<reg type="lb">¬</reg><lb facs="#facs_1_line_1495029359026_90" n="N008" break="no"/>gelt mit un<choice>
                    <abbr>sˀm</abbr>
                    <expan>serm</expan>
                </choice> insigel<reg>,</reg> <reg type="star">✳</reg> der ist geben ze <placeName ref="loc000020">Baden</placeName><reg>,</reg> <reg type="star">✳</reg> <date when="1315-01-09">an dem dunrstag nach dem zwelften tage<reg>,</reg> <reg type="star">✳</reg> do man
        <lb facs="#facs_1_line_1495029362139_91" n="N009"/>zalte von gotz geburte <reg type="star">✳</reg> dritzehenhun<choice>
                        <abbr>dˀt</abbr>
                        <expan>dert</expan>
                    </choice> jar <reg type="star">✳</reg> und da nach in dem fûnftzehenden jar</date><reg>.</reg> <reg type="star">✳</reg>
            </p>
