  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0361_r"/>
      <p facs="#facs_1_region_1508516759429_76">
        <lb facs="#facs_1_line_1508516800455_78" n="N001"/>Wir<reg>,</reg> <reg type="star">✳</reg> <persName ref="per007252">Fridrich<reg>,</reg> von gots gnaden Roͤmischer kùnig<reg>,</reg> alle zitt ein merer des riches</persName><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> und wir<reg>,</reg>
        <lb facs="#facs_1_line_1508516803252_79" n="N002"/>
                <persName ref="per000342">Lùpolt</persName><reg>,</reg>
                <reg type="star">✳</reg> <persName ref="per000347">Al<choice>
                        <abbr>b̄r₎</abbr>
                        <expan>brecht</expan>
                    </choice></persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per007258">Heinrich</persName> und <persName ref="per007267">Otte<reg>,</reg> gebruͦder<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> von den selben gnaden <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzogen ze Oͤsterich und Styre<reg>,</reg> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren
        <lb facs="#facs_1_line_1508516806183_80" n="N003"/>ze Ch<add place="above">r</add>ain<reg>,</reg> uf der Marich und ze Portenowe<reg>,</reg> gra<choice>
                        <abbr>vē</abbr>
                        <expan>ven</expan>
                    </choice> ze Kiburg und ze Habspurg und lantgraven in
        <lb facs="#facs_1_line_1508516809344_81" n="N004"/>Elsazzen</persName><reg>,</reg> <reg type="star">✳</reg> verjehen <reg type="star">✳</reg> und tuͦn chunt mit disem prief <reg type="star">✳</reg> allen den<reg>,</reg> die in ansehent<reg>,</reg> lesent oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> dz
        <lb facs="#facs_1_line_1508516812553_82" n="N005"/>wir got und un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> vrowen und allen heiligen ze lobe und eren<reg>,</reg> <reg type="star">✳</reg> und ouch durch un<choice>
                    <abbr>sˀs</abbr>
                    <expan>sers</expan>
                </choice> lieben <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> va<choice>
                    <abbr>tˀs</abbr>
                    <expan>ters</expan>
                </choice><reg>,</reg>
        <lb facs="#facs_1_line_1508516826400_84" n="N006"/>
                <persName ref="per004951">kùnig Albrechtes</persName><reg>,</reg> und un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> lieben froͧwen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> muͦter<reg>,</reg>
        <lb facs="#facs_1_line_1508516827793_85" n="N007"/>
                <persName ref="per007254">
                    <add place="above">chuͤngin</add>
        <lb facs="#facs_1_line_1508516830393_86" n="N008"/>Elizabeten</persName><reg>,</reg> <reg type="star">✳</reg> sele<reg>,</reg> und un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> und aller un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> vordern selen
        <lb facs="#facs_1_line_1508516834361_87" n="N009"/>ze haile <reg type="star">✳</reg> und ze trost<reg>,</reg> <reg type="star">✳</reg> dz <orgName ref="org002627">clos<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> datz Kùnigesveld<reg>,</reg> <reg type="star">✳</reg> Sant Claren ordens<reg>,</reg> <reg type="star">✳</reg> <del rend="rubbing">gege</del> gelegen in Costent<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bistum</orgName><reg>,</reg> un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice>
        <lb facs="#facs_1_line_1508516837401_88" n="N010"/>stiftunge <reg type="star">✳</reg> und un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> lieben froͧwen und muͦter<reg>,</reg> der vorgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice><reg>,</reg> stiftung ist<reg>,</reg> <reg type="star">✳</reg> di auch da erwelt hat zeligen<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1508516840042_89" n="N011"/>gentzlichen in un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> schirme <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
