  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0387_r"/>
      <p facs="#facs_1_region_1593464222475_179">
        <lb facs="#facs_1_region_1593464222475_179l1" n="N001"/>Allen den<reg>,</reg> die disen brief <reg type="star">✳</reg> ansehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> kùnd ich<reg>,</reg> <persName ref="per028440">Hart<choice>
                        <abbr>mā</abbr>
                        <expan>man</expan>
                    </choice> Andres<reg>,</reg> <choice>
                        <abbr>schult̄hs</abbr>
                        <expan>schultheiss</expan>
                    </choice> ze Mellingen</persName>
                <reg>,</reg> <reg type="star">✳</reg> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich offenlich mit
        <lb facs="#facs_1_region_1593464222475_179l2" n="N002"/>disem brief<reg>,</reg> <reg type="star">✳</reg> daz ich an der <orgName ref="org000088">hocherbornen fùrsten<reg>,</reg> <reg type="star">✳</reg> der herzogen ze Oͤs<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName>
                <reg>,</reg> miner gnêdigen <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren<reg>,</reg> stat <reg type="star">✳</reg> in der stat ze <placeName ref="loc000504">Mellin<choice>
                        <abbr>g₎</abbr>
                        <expan>gen</expan>
                    </choice>
                </placeName>
        <lb facs="#facs_1_region_1593464222475_179l3" n="N003"/>an offener strass ze gerichte sazz<reg>,</reg> <reg type="star">✳</reg> da in gericht <reg type="star">✳</reg> fùr mich kament <persName ref="per024108">Niclaus Sche<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice>
                    <reg>,</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> ze Mellingen</persName>
                <reg>,</reg> <reg type="star">✳</reg> mit fùrsprechen ze
        <lb facs="#facs_1_region_1593464222475_179l4" n="N004"/>ein teil<reg>,</reg> <reg type="star">✳</reg> und <persName ref="per014602">Uͦlman Gernass<reg>,</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> ze Mellingen</persName>
                <reg>,</reg> oͧch mit fùrsprechen ze dem an<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice> teil<reg>.</reg> <reg type="star">✳</reg> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jach <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selb <persName ref="per024108">Niclaus Sche<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice>
                </persName>
        <lb facs="#facs_1_region_1593464222475_179l5" n="N005"/>da in gericht<reg>,</reg> <reg type="star">✳</reg> daz er ein ligent guͦt<reg>,</reg> <reg type="star">✳</reg> ze <placeName ref="loc012341">Starchetswile</placeName> gelegen<reg>,</reg> bûwet <reg type="star">✳</reg> <persName ref="per028441">Hûg Witschh</persName>
                <reg>,</reg> gilt <measure>jerlich zwein mùt <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>nen</measure>
                <reg>,</reg>
        <lb facs="#facs_1_region_1593464222475_179l6" n="N006"/>
                <measure>ein herbsthuͦn</measure>
                <reg>,</reg> <reg type="star">✳</reg> <measure>ein vasnacht</measure> und <measure>zwenzig ey<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </measure>
                <reg>,</reg> dem vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <reg type="star">✳</reg> <persName ref="per014602">Uͦlman Gernass</persName> <reg type="star">✳</reg> mit aller ehafti und zuͦgehoͤrung<reg>,</reg> <reg type="star">✳</reg> so darzuͦ
        <lb facs="#facs_1_region_1593464222475_179l7" n="N007"/>gehoͤret<reg>,</reg> <reg type="star">✳</reg> recht <reg type="star">✳</reg> und redlich zekoͧffenn geben <reg type="star">✳</reg> hab <reg type="star">✳</reg> umb <measure>vier pfunt</measure> und umb <measure>zweinzig pfund pfen<choice>
                        <abbr>nīg</abbr>
                        <expan>ning</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> gen<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> und genê<choice>
                        <abbr>mˀ</abbr>
                        <expan>mer</expan>
                    </choice>
        <lb facs="#facs_1_region_1593464222475_179l8" n="N008"/>stebler</measure>
                <reg>,</reg> <reg type="star">✳</reg> dero <reg type="star">✳</reg> der vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genante</expan>
                </choice> <reg type="star">✳</reg> <persName ref="per024108">Niclaus Sche<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice>
                </persName> nach si<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gicht<reg>,</reg> so er da in gericht têt<reg>,</reg> <reg type="star">✳</reg> von dem vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <reg type="star">✳</reg> <persName ref="per014602">Uͦlman Gernass</persName> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1593464222475_179l9" n="N009"/>gentzklich ge<choice>
                    <abbr>wˀt</abbr>
                    <expan>wert</expan>
                </choice> ist <reg type="star">✳</reg> und in des selben <persName ref="per024108">Niclaus</persName> <reg type="star">✳</reg> guͦten nutz komen sint<reg>,</reg> <reg type="star">✳</reg> dis vorge<choice>
                    <abbr>schr₎</abbr>
                    <expan>schriben</expan>
                </choice> ligenden guͦtes <reg type="star">✳</reg> mit aller ehafti und
        <lb facs="#facs_1_region_1593464222475_179l10" n="N010"/>nùtzen<reg>,</reg> so darzuͦ gehoͤret<reg>,</reg> <reg type="star">✳</reg> und mit namen alles des rechten<reg>,</reg> <reg type="star">✳</reg> vorder <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ansprach<reg>,</reg> <reg type="star">✳</reg> so der selb <persName ref="per024108">Niclaus Sche<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice>
                </persName> dheins wegs
        <lb facs="#facs_1_region_1593464222475_179l11" n="N011"/>zuͦ dem selben vorge<choice>
                    <abbr>schrˀ</abbr>
                    <expan>schriben</expan>
                </choice> guͦt hatt<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>zeich sich <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenante</expan>
                </choice> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per024108">Niclaus Sche<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice>
                </persName> da in gericht in koͦffs wise <reg type="star">✳</reg> fùr sich und fùr alle
        <lb facs="#facs_1_region_1593464222475_179l12" n="N012"/>sin erben <reg type="star">✳</reg> gegen dem <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> <reg type="star">✳</reg> <persName ref="per014602">Uͦlman Gernass</persName> und gegen allen sinen erben <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tigot der selbe <persName ref="per024108">Niclaus</persName> <reg type="star">✳</reg> dem selben <persName ref="per014602">Uͦlman</persName> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1593464222475_179l13" n="N013"/>daz vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genante</expan>
                </choice> <reg type="star">✳</reg> ligent guͦt <reg type="star">✳</reg> da in gericht mit hand und mit munt<reg>,</reg> <reg type="star">✳</reg> als recht ist<reg>,</reg> <reg type="star">✳</reg> und als gericht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> urteil gab<reg>,</reg> <reg type="star">✳</reg> so <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>re
        <lb facs="#facs_1_region_1593464222475_179l14" n="N014"/>das uff den eid mit gesamnot<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urteil und urfrag <reg type="star">✳</reg> erteilt ward<reg>,</reg> <reg type="star">✳</reg> daz <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genante</expan>
                </choice> <reg type="star">✳</reg> <persName ref="per014602">Uͦlman Gernass</persName> <reg type="star">✳</reg> und sin erben<reg>,</reg> ob er <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1593464222475_179l15" n="N015"/>en<choice>
                    <abbr>wˀ</abbr>
                    <expan>were</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> an dem vorge<choice>
                    <abbr>schr₎</abbr>
