  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0458_r"/>
      <p facs="#facs_1_region_1507838632715_160">
        <lb facs="#facs_1_line_1507838651875_161" n="N001"/>Wir<reg>,</reg> die <orgName ref="org002627">eptissin und <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> <choice>
                        <abbr>cō</abbr>
                        <expan>con</expan>
                    </choice>vent gemeinlich ze Kùngsveld</orgName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jechen offenlich
        <lb facs="#facs_1_line_1507838654115_162" n="N002"/>
                <comment comment="Platzierung des Einschubs unsicher.">
                    <reg type="star">✳</reg> und recht</comment>
        <lb facs="#facs_1_line_1507838657896_163" n="N003"/>menlich mit disem brief <reg type="star">✳</reg> fùr uns und un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice>
        <lb facs="#facs_1_line_1507838663863_164" n="N004"/>nachkomen<reg>,</reg> <reg type="star">✳</reg> dz wir dur des ege<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>
                <choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <reg type="star">✳</reg> unsers <choice>
                    <abbr>gen₎</abbr>
                    <expan>genedigen</expan>
                </choice> <choice>
                    <abbr>hˀns</abbr>
                    <expan>herrns</expan>
                </choice> nùtzes und notdurft <reg type="star">✳</reg> wolbedacht und mit zittli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice>
                <reg>,</reg> guͦ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> vorbetrach<choice>
                    <abbr>tūg</abbr>
                    <expan>tung</expan>
                </choice>
        <lb facs="#facs_1_line_1507838671456_165" n="N005"/>
                <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧft und ze koͧ<choice>
                    <abbr>fen̄</abbr>
                    <expan>fenn</expan>
                </choice> ge<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> haben <reg type="star">✳</reg> in eines rech<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> und redlichen<reg>,</reg> <reg type="star">✳</reg> ewigen koͧfes un<choice>
                    <abbr>seˀ</abbr>
                    <expan>ser</expan>
                </choice> <persName ref="per007182">froͧw Margre<choice>
                        <abbr>thē</abbr>
                        <expan>then</expan>
                    </choice> von Hachen<choice>
                        <abbr>bˀg</abbr>
                        <expan>berg</expan>
                    </choice>
                </persName> und
        <lb facs="#facs_1_line_1507838678612_166" n="N006"/>
                <persName ref="per007220">froͧw Annen von Zo<choice>
                        <abbr>lˀn</abbr>
                        <expan>lern</expan>
                    </choice>
                    <reg>,</reg> klos<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice> da selbes ze Kùngsveld</persName>
                <reg>,</reg> <reg type="star">✳</reg> ein guͦt<reg>,</reg> ist gele<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> ze <placeName ref="loc009757">Talhein</placeName>
                <reg>,</reg> das da buwt und zinset <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <persName ref="per025790">Ber<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice>
        <lb facs="#facs_1_line_1507838686977_167" n="N007"/>von Tàlhein</persName>
                <reg>,</reg> <reg type="star">✳</reg> giltz <measure>jaͤrlich fùnf viertel ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure>
                <reg>,</reg> <measure>zwen mùt haber</measure>
                <reg>,</reg> <reg type="star">✳</reg> <measure>vier stuffel huͤnr</measure> und <measure>vierzig eiger</measure>
                <reg>.</reg> <reg type="star">✳</reg> und ha<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice>
        <lb facs="#facs_1_line_1507838694297_168" n="N008"/>das selb guͦt mit den vorbegriffe<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> zinsen beden un<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>scheidenlich in ewiges koͧfes wis <reg type="star">✳</reg> und recht ge<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> umb <measure>
                    <choice>
                        <abbr>ein̄</abbr>
                        <expan>einn</expan>
                    </choice> und <choice>
                        <abbr>zwē</abbr>
                        <expan>zwen</expan>
                    </choice>
                    <reg type="lb">¬</reg><lb facs="#facs_1_line_1507838702114_169" n="N009" break="no"/>zig guͦ<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> guldin</measure>
                <reg>,</reg> <del rend="strikethrough">ge<choice>
                        <abbr>bē</abbr>
                        <expan>ben</expan>
                    </choice>
                </del> der wir gar und gentzlich bezalt sint und oͧch in des vorge<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>
                <choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> unsers gotzhuss schin<choice>
                    <abbr>bˀn</abbr>
                    <expan>bern</expan>
                </choice> nutz
        <lb facs="#facs_1_line_1507838710499_170" n="N010"/>und fro<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> ko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> und be<choice>
                    <abbr>wēt</abbr>
                    <expan>went</expan>
                </choice> sint in semlicher masse<reg>,</reg> das die vor<choice>
                    <abbr>g₎</abbr>
                    <expan>genanten</expan>
                </choice> <choice>
                    <abbr>ꝑ</abbr>
                    <expan>per</expan>
                </choice>so<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice>
                <reg>,</reg> die <persName ref="per007182">
                    <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Ho<choice>
                        <abbr>chē</abbr>
                        <expan>chen</expan>
                    </choice>
                    <choice>
                        <abbr>bˀg</abbr>
                        <expan>berg</expan>
                    </choice>
                </persName> und die <persName ref="per007220">von Zolrr</persName>
                <reg>,</reg> bede und
        <lb facs="#facs_1_line_1507838717275_171" n="N011"/>ir jekliche in sun<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ir lebdag und die wil si bede <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> ir eine in leben ist dz obge<choice>
                    <abbr>nāt</abbr>
                    <expan>nant</expan>
                </choice> guͦt mit den vorge<choice>
                    <abbr>scʼb</abbr>
                    <expan>scrib</expan>
                </choice>
                <choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> zinsen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1507838726698_172" n="N012"/>nùtzen <reg type="star">✳</reg> jaͤrlich in zie<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice>
                <reg>,</reg> in ne<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>
                <reg>,</reg> nùtz<choice>
                    <abbr>z₎̄</abbr>
                    <expan>zen</expan>
                </choice> und nies<choice>
                    <abbr>sē</abbr>
                    <expan>sen</expan>
                </choice> sùl<choice>
                    <abbr>lēt</abbr>
                    <expan>lent</expan>
                </choice>
                <reg>,</reg> fridlich und ruͤwklich<reg>,</reg> an hind<choice>
                    <abbr>rūg</abbr>
                    <expan>rung</expan>
                </choice> und <choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>lichs sper<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice>
                <reg>.</reg> <reg type="star">✳</reg> doch also <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1507838734036_173" n="N013"/>
                <choice>
                    <abbr>wen̄</abbr>
                    <expan>wenn</expan>
                </choice> si bede <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
