  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0367_r"/>
      <p facs="#facs_1_region_1593463916667_139">
        <lb facs="#facs_1_region_1593463916667_139l1" n="N001"/>Allen dien<reg>,</reg> die disen brief an sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> kunde ich<reg>,</reg> <persName ref="per028419">Johans Mel<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice>
                    <reg>,</reg> der elter<reg>,</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
        <lb facs="#facs_1_region_1593463916667_139l2" n="N002"/>Zofingen</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gib offenlich mit disem brief fur mich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alle min erben<reg>,</reg> daz ich gelten unden
        <lb facs="#facs_1_region_1593463916667_139l3" n="N003"/>schuldig bin rech<choice>
                    <abbr>t₎</abbr>
                    <expan>ter</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redlicher geltschulde <measure>zweinzig phfunt angster</measure>
                <reg>,</reg> guͤter <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gene<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice>
                <reg>,</reg> dem <persName ref="per028420">erbern
        <lb facs="#facs_1_region_1593463916667_139l4" n="N004"/>man Hein<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> Frischzal<reg>,</reg> bur<choice>
                        <abbr>g₎</abbr>
                        <expan>ger</expan>
                    </choice> Zofingen</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sinen erben<reg>,</reg> ob er enwer<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> han im <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sinen erben har
        <lb facs="#facs_1_region_1593463916667_139l5" n="N005"/>umbe fùr <measure>zweinzig phfunt angster</measure> versetzt mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> aker<reg>,</reg> der gelegen ist bi dem poͧmlin <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sint
        <lb facs="#facs_1_region_1593463916667_139l6" n="N006"/>
                <measure>zwͤ jucharten</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> buwͤt den selben aker <persName ref="per028425">Ruͤdi Hone<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                    <reg>,</reg> och bur<choice>
                        <abbr>g₎</abbr>
                        <expan>ger</expan>
                    </choice> Zofin<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> der och jerlichs git
        <lb facs="#facs_1_region_1593463916667_139l7" n="N007"/>
                <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> dem selben aker <measure>zwͤ mal<choice>
                        <abbr>t₎</abbr>
                        <expan>ter</expan>
                    </choice> dinklen</measure> uf <date when="0001-11-30">sant Andres tag</date>
                <reg>,</reg> als man gewonlich kern zins
        <lb facs="#facs_1_region_1593463916667_139l8" n="N008"/>weret<reg>,</reg> der selben zins <reg type="star">✳</reg> ich och fùr mich<reg>,</reg> min erben<reg>,</reg> <reg type="star">✳</reg> <persName ref="per028420">Hein<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> Frischzal</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sine erben uf dem
        <lb facs="#facs_1_region_1593463916667_139l9" n="N009"/>aker <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>setzet han <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> git der selbe aker <measure>jerlichs zins ein mùt dink</measure> dem <orgName ref="org000149">gotzhus Zofin<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice>
                </orgName>
                <reg>.</reg>
        <lb facs="#facs_1_region_1593463916667_139l10" n="N010"/>doch hat mir der vorgenant <persName ref="per028420">Hein<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> Frischzal</persName> die liebe <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> genade getan<reg>,</reg> wa dz ist<reg>,</reg> dz
        <lb facs="#facs_1_region_1593463916667_139l13" n="N011"/>ich<reg>,</reg> der obgenant <persName ref="per028419">Johans Mel<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice>
                </persName>
                <reg>,</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min erben <reg type="star">✳</reg> keinest komen mit <measure>zweinzig phfunt <choice>
                        <abbr>angˀ</abbr>
                        <expan>angster</expan>
                    </choice>
                </measure>
        <lb facs="#facs_1_region_1593463916667_139l14" n="N012"/>
                <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> nu <date when="0001-06-24">sant Johans tag ze sùngichten</date> in den nechsten dryen jaren<reg>,</reg> so ist mir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> minen
        <lb facs="#facs_1_region_1593463916667_139l15" n="N013"/>erben der aker lidig mit dien <measure>zwͤn mal<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> <choice>
                        <abbr>dink̄</abbr>
                        <expan>dinkle</expan>
                    </choice>
                </measure>
                <reg>,</reg> der <persName ref="per028425">Ruͤdi Hone<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName> jerlichs <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> dem aker
        <lb facs="#facs_1_region_1593463916667_139l16" n="N014"/>git<reg>,</reg> wer aber<reg>,</reg> dz ich<reg>,</reg> der obgenant <persName ref="per028419">Johans Mel<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice>
                </persName>
                <reg>,</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min erben <reg type="star">✳</reg> dz uber sehen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> den aker
        <lb facs="#facs_1_region_1593463916667_139l17" n="N015"/>in den dryen jaren nùt wider loste <reg type="star">✳</reg> umbe <measure>zweinzig phfunt angster</measure>
                <reg>,</reg> so ist ez dannant hin
        <lb facs="#facs_1_region_1593463916667_139l18" n="N016"/>ein ewiger <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> stete kof unwiderruͤfklich <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> menklichem<reg>,</reg> wan wir mit rechten gedingen
        <lb facs="#facs_1_line_1608652733330_188" n="N017"/>also uber ein komen sint<reg>,</reg> <reg type="star">✳</reg> <unclear>dù</unclear> loben och ich<reg>,</reg> der obegant <persName ref="per028419">Johans Mel<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice>
                </persName>
                <reg>,</reg> fur mich<reg>,</reg> min erben <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1593463916667_139l21" n="N018"/>dise vorgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> gedingen rech<choice>
                    <abbr>t₎</abbr>
                    <expan>ter</expan>
                </choice> wer ze sin <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> da wider nit ze tuͦn<reg>,</reg> noch schaffen getan<reg>,</reg>
        <lb facs="#facs_1_region_1593463916667_139l22" n="N019"/>we<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> mit geistlichem noch mit weltlichem gericht<reg>,</reg> noch an gericht<reg>,</reg> mit kei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> sachen<reg>,</reg> si sigen
        <lb facs="#facs_1_region_1593463916667_139l24" n="N020"/>funden oder noch funden moͤchten werden<reg>,</reg> heimlich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> offenlich<reg>,</reg> da mit die vorgenanten
        <lb facs="#facs_1_region_1593463916667_139l26" n="N021"/>gedingen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sache ab gan moͤchte<reg>.</reg> <reg type="star">✳</reg> hie bi<reg>,</reg> da dis beschach<reg>,</reg> waren bi dis erbern lùt <persName ref="per028421">Chuͤnrat
        <lb facs="#facs_1_region_1593463916667_139l27" n="N022"/>Frizschal</persName>
                <reg>,</reg> <persName ref="per028422">Johans Frischzal</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per028423">Uͦlrich Judenknecht</persName>
                <reg>,</reg> <persName ref="per028424">Ruͤdi<choice>
                        <abbr>g₎</abbr>
                        <expan>ger</expan>
                    </choice> der stat schriber</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ander erber
        <lb facs="#facs_1_region_1593463916667_139l28" n="N023"/>lùten vil<reg>,</reg> die dis geding sahen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> horten<reg>.</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> des zuͦ ei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> waren urkùnde so han
        <lb facs="#facs_1_region_1593463916667_139l29" n="N024"/>ich<reg>,</reg> der obgenant <persName ref="per028419">Johans Mel<choice>
                        <abbr>wˀ</abbr>
                        <expan>wer</expan>
                    </choice>
                </persName> min eigen ingesiel gehenket an disen brief des <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1593463916667_139l30" n="N025"/>jares<reg>,</reg> do man zalt <date when="1373-02-26">von gottes geburt drùtzehen hundert jar<reg>,</reg> im <reg type="star">✳</reg> drù <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
