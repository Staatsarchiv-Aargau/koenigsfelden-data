  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0272_r"/>
      <p facs="#facs_1_region_1501227573702_266">
        <lb facs="#facs_1_line_1501227599537_267" n="N001"/>Wir<reg>,</reg> <persName ref="per000347">
                    <choice>
                        <abbr>Albr̄</abbr>
                        <expan>Albrecht</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> von gotes gnaden <reg type="star">✳</reg> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzog ze Oͤs<choice>
                        <abbr>terˀ</abbr>
                        <expan>terich</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> ze Sty<choice>
                        <abbr>rˀ</abbr>
                        <expan>re</expan>
                    </choice> <reg type="star">✳</reg> und ze Kernden</persName>
                <reg>,</reg> <reg type="star">✳</reg> tuͦn kunt<reg>,</reg> <reg type="star">✳</reg> umb die visschentz<reg>,</reg> <reg type="star">✳</reg> die wir haben <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501227602601_268" n="N002"/>in un<choice>
                    <abbr>sˀm</abbr>
                    <expan>serm</expan>
                </choice> lande <reg type="star">✳</reg> ze <placeName ref="loc000122">Ergowͤ</placeName>
                <reg>,</reg> <reg type="star">✳</reg> uff der <placeName ref="loc001093">Rùse</placeName>
                <reg>,</reg> <reg type="star">✳</reg> oberhalp un<choice>
                    <abbr>sˀr</abbr>
                    <expan>ser</expan>
                </choice> stat <placeName ref="loc000979">Premgarten</placeName>
                <reg>,</reg> <reg type="star">✳</reg> in der stilli <reg type="star">✳</reg> bi dem banne <reg type="star">✳</reg> ze <placeName ref="loc010735">Lunghoft</placeName>
                <reg>,</reg> <reg type="star">✳</reg> die von
        <lb facs="#facs_1_line_1501227605545_269" n="N003"/>un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> vor<choice>
                    <abbr>deˀn</abbr>
                    <expan>dern</expan>
                </choice> <reg type="star">✳</reg> und von uns <reg type="star">✳</reg> wilent der <persName ref="per012641">Barrer<reg>,</reg> <reg type="star">✳</reg> selig<reg>,</reg> von Arnstowͤ</persName> <reg type="star">✳</reg> in phandes wise <reg type="star">✳</reg> inne gehebt <reg type="star">✳</reg> und an sin er<choice>
                    <abbr>b̄n</abbr>
                    <expan>ben</expan>
                </choice> bracht hat<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501227609145_270" n="N004"/>das wir <reg type="star">✳</reg> von vlizzi<choice>
                    <abbr>gˀ</abbr>
                    <expan>ger</expan>
                </choice> <reg type="star">✳</reg> und ernsthaf<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> bette wegen<reg>,</reg> <reg type="star">✳</reg> der hochgebornen <reg type="star">✳</reg> un<choice>
                    <abbr>sˀr</abbr>
                    <expan>ser</expan>
                </choice> <persName ref="per006964">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzenlie<choice>
                        <abbr>bn̄</abbr>
                        <expan>ben</expan>
                    </choice> <reg type="star">✳</reg> swester<reg>,</reg> frowen Agnesen<reg>,</reg> <reg type="star">✳</reg> wilent kuniginn ze
        <lb facs="#facs_1_line_1501227613258_271" n="N005"/>Un<choice>
                        <abbr>gˀen</abbr>
                        <expan>geren</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> luterlichen durch got <reg type="star">✳</reg> gegunnen <reg type="star">✳</reg> und erloubet haben<reg>,</reg> <reg type="star">✳</reg> gunnen <reg type="star">✳</reg> und erlouben ouch <reg type="star">✳</reg> von besun<choice>
                    <abbr>dˀen</abbr>
                    <expan>deren</expan>
                </choice> gnaden <reg type="star">✳</reg> mit disem briefe<reg>,</reg>
        <lb facs="#facs_1_line_1501227620025_272" n="N006"/>fur uns <reg type="star">✳</reg> und unser erben<reg>,</reg> <reg type="star">✳</reg> den <orgName ref="org002627">er<choice>
                        <abbr>bˀen</abbr>
                        <expan>beren</expan>
                    </choice>
                    <reg>,</reg> geistlichen<reg>,</reg> <reg type="star">✳</reg> un<choice>
                        <abbr>sˀn</abbr>
                        <expan>sern</expan>
                    </choice> lieben andeͣchtigen<reg>,</reg> <reg type="star">✳</reg> der abtissin <reg type="star">✳</reg> und dem convent gemeinlich<reg>,</reg> <reg type="star">✳</reg> der klos<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>frowͤn
        <lb facs="#facs_1_line_1501227624296_273" n="N007"/>ze Kùngsvelt<reg>,</reg> <reg type="star">✳</reg> Sant Claren ordens<reg>,</reg> <reg type="star">✳</reg> in Chostent<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bystuͦm</orgName>
                <reg>,</reg> <reg type="star">✳</reg> daz si <reg type="star">✳</reg> zuͦ iren <reg type="star">✳</reg> und ir nachkomen <reg type="star">✳</reg> handen <reg type="star">✳</reg> diesel<choice>
                    <abbr>b̄n</abbr>
                    <expan>ben</expan>
                </choice> visschentz <reg type="star">✳</reg> mit allen <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501227628825_274" n="N008"/>nùtzen <reg type="star">✳</reg> und rechten<reg>,</reg> <reg type="star">✳</reg> die darzuͦ gehoͤrent<reg>,</reg> <reg type="star">✳</reg> loͤsen <reg type="star">✳</reg> sullen <reg type="star">✳</reg> und mugen<reg>,</reg> <reg type="star">✳</reg> von des obe<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per012641">Barrers<reg>,</reg> <reg type="star">✳</reg> seligen</persName>
                <reg>,</reg> erben<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> von un<choice>
                    <abbr>sˀm</abbr>
                    <expan>serm</expan>
                </choice> getruͤwen <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501227633793_275" n="N009"/>
                <persName ref="per012639">Hartman von Heidegg</persName>
                <reg>,</reg> <reg type="star">✳</reg> ob si <reg type="star">✳</reg> von kouffes wegen <reg type="star">✳</reg> an in komen ist<reg>,</reg> <reg type="star">✳</reg> oder wer <reg type="star">✳</reg> diselben <reg type="star">✳</reg> visschentz von uns <reg type="star">✳</reg> in phandes wise <reg type="star">✳</reg> inne hat <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501227637785_276" n="N010"/>umb als vil <reg type="star">✳</reg> guͦtes<reg>,</reg> <reg type="star">✳</reg> als die phantbriefe bewisent<reg>,</reg> <reg type="star">✳</reg> die der vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per012641">Barrer<reg>,</reg> selig</persName>
                <reg>,</reg> <reg type="star">✳</reg> dar uber gehebt hat <reg type="star">✳</reg> von un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> vor<choice>
                    <abbr>dˀen</abbr>
                    <expan>deren</expan>
                </choice> <reg type="star">✳</reg> oder von uns<reg>.</reg>
        <lb facs="#facs_1_line_1501227642161_277" n="N011"/>also <reg type="star">✳</reg> daz die vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice>
                <reg>,</reg> die <orgName ref="org002627">abtissin <reg type="star">✳</reg> und der convent <reg type="star">✳</reg> ze Kungsvelt</orgName>
                <reg>,</reg> <reg type="star">✳</reg> und alle ir nachkomen<reg>,</reg> <reg type="star">✳</reg> die ege<choice>
                    <abbr>nant̄̄</abbr>
                    <expan>nanten</expan>
                </choice> visschentze <reg type="star">✳</reg> in eins rechten
        <lb facs="#facs_1_line_1501227647449_278" n="N012"/>werenden phandes wise <reg type="star">✳</reg> von uns <reg type="star">✳</reg> und un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
