      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0327_r"/>
      <p facs="#facs_1_region_1501248723734_222">
        <lb facs="#facs_1_line_1501248735465_223" n="N001"/>Allen den<reg>,</reg> die disen brief an sehent oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> kùnde <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> tuͤn kund ich<reg>,</reg> <persName ref="per020302">Heinrich <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248739002_224" n="N002"/>Mùller<reg>,</reg> <reg type="star">✳</reg> ein bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> Zovingen</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich <reg type="star">✳</reg> offenlich mit disem brief <reg type="star">✳</reg> fùr mich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alle min <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248742572_225" n="N003"/>erben<reg>,</reg> <reg type="star">✳</reg> dz ich en phfangen han <reg type="star">✳</reg> von der <persName ref="per017554">erwirdigen <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> geistlicher frowͤn <reg type="star">✳</reg> Angnesen<reg>,</reg> <reg type="star">✳</reg> eptischin</persName>
                <reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248745368_226" n="N004"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dem <reg type="star">✳</reg> <orgName ref="org002627">covent gemeinlich <reg type="star">✳</reg> des <reg type="star">✳</reg> closters <reg type="star">✳</reg> ze <reg type="star">✳</reg> Cùngsvelt<reg>,</reg> gelegen <reg type="star">✳</reg> in Kostent<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bischtume</orgName>
                <reg>,</reg> nach
        <lb facs="#facs_1_line_1501248749145_227" n="N005"/>der stat rechtung <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gewonheit <reg type="star">✳</reg> <placeName ref="loc000857">Zovingen</placeName>
                <reg>,</reg> <reg type="star">✳</reg> mir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> erben<reg>,</reg> <reg type="star">✳</reg> ein mùli in der <reg type="star">✳</reg> stat<reg>,</reg> <reg type="star">✳</reg> ist genemmet
        <lb facs="#facs_1_line_1501248754273_228" n="N006"/>die <placeName ref="loc010005">Hell Mùli</placeName>
                <reg>,</reg> <reg type="star">✳</reg> also dz ich <reg type="star">✳</reg> <measure>alle jar <reg type="star">✳</reg> da von zins geben sol <reg type="star">✳</reg> zweinczig mùt korns</measure>
                <reg>,</reg> <reg type="star">✳</reg> des sol sin <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248758266_229" n="N007"/>
                <measure>zwen teil kerne</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> der <measure>dritteil <reg type="star">✳</reg> mùlikorn</measure>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sol och geben fùr <measure>ein swin <reg type="star">✳</reg> ein phfunt <choice>
                        <abbr>phenn̄</abbr>
                        <expan>phennig</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248762713_230" n="N008"/>Zovinger<reg>,</reg> genger <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> geber</measure>
                <reg>.</reg> <reg type="star">✳</reg> ich han <reg type="star">✳</reg> och <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> min erben <reg type="star">✳</reg> die vorgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> mùli enphfangen <reg type="star">✳</reg> alle <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248766354_231" n="N009"/>die wile<reg>,</reg> <reg type="star">✳</reg> und si in der vor<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>schriben<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> eptischin <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> des <reg type="star">✳</reg> <orgName ref="org002627">coventz <reg type="star">✳</reg> des <reg type="star">✳</reg> klosters <reg type="star">✳</reg> ze <reg type="star">✳</reg> Chùngsvelt</orgName>
        <lb facs="#facs_1_line_1501248770681_232" n="N010"/>han<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gewalt stat<reg>.</reg> <reg type="star">✳</reg> ich sol och die mùli in eren han<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> were<reg>,</reg> dz die mùli <reg type="star">✳</reg> ferbrunne <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248774886_233" n="N011"/>
                <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> selb fùr<reg>,</reg> <reg type="star">✳</reg> so sol ich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min erben die mùli wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <reg type="star">✳</reg> machen <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> buwͤn <reg type="star">✳</reg> an des <reg type="star">✳</reg> closters scha<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice>
                <reg>.</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248779506_234" n="N012"/>fùrbrùnne si aber <reg type="star">✳</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> gemei<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> fùre<reg>,</reg> <reg type="star">✳</reg> da vor got si<reg>,</reg> <reg type="star">✳</reg> so bin ich noch min erben nùtz schuldig <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248784618_235" n="N013"/>ze buwͤn <reg type="star">✳</reg> die mùli<reg>.</reg> <reg type="star">✳</reg> wer aber<reg>,</reg> dz <reg type="star">✳</reg> ich oder min erben von <reg type="star">✳</reg> der mùli muͤsten<reg>,</reg> <reg type="star">✳</reg> so sol man <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248789610_236" n="N014"/>mir <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> erben <reg type="star">✳</reg> ab legen <reg type="star">✳</reg> oder <reg type="star">✳</reg> widertuͤn <reg type="star">✳</reg> den <reg type="star">✳</reg> bù <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> besserung<reg>,</reg> so ich an die mùli <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248795277_237" n="N015"/>getan <reg type="star">✳</reg> hat<reg>,</reg> <reg type="star">✳</reg> ich oder min erben<reg>.</reg> <reg type="star">✳</reg> wer <choice>
                    <abbr>abˀ</abbr>
                    <expan>aber</expan>
                </choice>
                <reg>,</reg> dz ich die mùli <reg type="star">✳</reg> geboͤseret older geswecheret<reg>,</reg> so sol <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248800777_238" n="N016"/>
                <persName ref="per020302">Heinrich Mùller</persName> <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min erben den schaden <reg type="star">✳</reg> ab legen <reg type="star">✳</reg> nach dem<reg>,</reg> <reg type="star">✳</reg> als sich denne biderb lùte <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501248804589_239" n="N017"/>erkennent<reg>,</reg> <reg type="star">✳</reg> ùns bedenthalben <reg type="star">✳</reg> an geverde<reg>.</reg> <reg type="star">✳</reg> man sol och mir <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> erben die wuͤr<subst>
                    <del>e</del>
                    <add place="overwritten">i</add>
                </subst>nen <reg type="star">✳</reg> unde
        <lb facs="#facs_1_line_1501248809156_240" n="N018"/>wasser rùnsen lassen <reg type="star">✳</reg> gan <reg type="star">✳</reg> an die mùli<reg>,</reg> <reg type="star">✳</reg> als da har gewonlich <reg type="star">✳</reg> ist gesin<reg>,</reg> <reg type="star">✳</reg> an ge<choice>
                    <abbr>vˀ</abbr>
