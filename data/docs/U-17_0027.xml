      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0027_r"/>
      <p facs="#facs_1_region_1492770558030_167">
        <lb facs="#facs_1_line_1492770577016_169" n="N001"/>Ich<reg>,</reg> <persName ref="per025853">Suphie von Veringen</persName><reg>,</reg> <persName ref="per028221">hern Cuͦnrates<reg>,</reg> seligen<reg>,</reg> von Liehten<choice>
                        <abbr>bgˀ</abbr>
                        <expan>berg</expan>
                    </choice>
                </persName> eliche vrowe<reg>,</reg> <reg type="star">✳</reg> kùnde allen den<reg>,</reg>
        <lb facs="#facs_1_line_1492770580787_170" n="N002"/>die disen brief lesent<reg>,</reg> oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> daz ich mich verzigen han mit guͦtem willen <reg type="star">✳</reg> <placeName ref="loc008543">Altenburg</placeName>
        <lb facs="#facs_1_line_1492770584849_171" n="N003"/>und waz dar zuͦ hoͤret<reg>,</reg> <reg type="star">✳</reg> daz miner vrowen<reg>,</reg> <reg type="star">✳</reg> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> muͦter ist<reg>,</reg> <reg type="star">✳</reg> daz ich mich dez verzigen han an
        <lb facs="#facs_1_line_1492770588613_172" n="N004"/>mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> swes<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <persName ref="per007364">Annen</persName> hant<reg>,</reg> <reg type="star">✳</reg> <persName ref="per020725">grave Huges<reg>,</reg> seligen<reg>,</reg> vrowen von Muntvort</persName>
                <reg>.</reg> also swenne min vrowe<reg>,</reg> min
        <lb facs="#facs_1_line_1492770592436_173" n="N005"/>muͦter stùrbe<reg>,</reg> da vor got lange si<reg>,</reg> <reg type="star">✳</reg> daz ich<reg>,</reg> noch min erben daz vorgenant guͦt nut erben sullent<reg>.</reg>
        <lb facs="#facs_1_line_1492770596942_174" n="N006"/>und were oͧch<reg>,</reg> daz min swes<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> e stùrbe e min vrowe<reg>,</reg> min muͦ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice><reg>,</reg> so sullent mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> swes<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> erben daz vor<reg type="lb">¬</reg><lb facs="#facs_1_line_1492770601826_175" n="N007" break="no"/>genant guͦt erben und ich nùt<reg>,</reg> noch min erben<reg>.</reg> <reg type="star">✳</reg> daz diz war si und stet belibe<reg>,</reg> <reg type="star">✳</reg> so han ich min
        <lb facs="#facs_1_line_1492770606266_176" n="N008"/>ingesigel gehenket an disen brief zuͦ einem waren urkùnde<reg>.</reg> <reg type="star">✳</reg> und ich<reg>,</reg> <persName ref="per007297">Otte von Ohsenstein</persName><reg>,</reg> umbe die
        <lb facs="#facs_1_line_1492770611688_177" n="N009"/>bet mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> muͦ<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice><reg>,</reg> han oͧch min ingesigel gehenket an disen brief zuͦ ei<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> beste<choice>
                    <abbr>tū</abbr>
                    <expan>tun</expan>
                </choice>ge dez vor gescribenne
        <lb facs="#facs_1_line_1492770616132_178" n="N010"/>dinges<reg>.</reg> <reg type="star">✳</reg> diz geschach an dem <date when="1313-01-05">zwoͤlf<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> abende<reg>,</reg> <reg type="star">✳</reg> do me zalt von gottes gebùrte drùzehenhundert
        <lb facs="#facs_1_line_1492770619647_179" n="N011"/>jar<reg>,</reg> zweͦlf jar und dar nach in dem ersten jar</date>
                <reg>.</reg> <reg type="star">✳</reg>
            </p>
