  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0924g_r"/>
      <p facs="#facs_1_r1">
        <lb facs="#facs_1_r1l1" n="N001"/>Wir<reg>,</reg> die <orgName ref="org002627">äpptissinn <reg type="star">✳</reg> unnd convent gmeinlich <reg type="star">✳</reg> des gotzhusz Kùngsfel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName><reg>,</reg>
        <lb facs="#facs_1_r1l2" n="N002"/>thùnd kund <reg type="star">✳</reg> unnd bekennend offennlich mit disem brieff<reg>,</reg> <reg type="star">✳</reg> als wire <choice>
                    <abbr>dann̄</abbr>
                    <expan>danne</expan>
                </choice>
        <lb facs="#facs_1_r1l3" n="N003"/>ussz ordnung <reg type="star">✳</reg> unnd ansächenn <reg type="star">✳</reg> unnser gnädigenn <orgName ref="org004002">herrenn <reg type="star">✳</reg> unnd castenvoͤgtt</orgName><reg>,</reg>
        <lb facs="#facs_1_r1l4" n="N004"/>
                <orgName ref="org004002">schult<choice>
                        <abbr>hs̄n</abbr>
                        <expan>heissen</expan>
                    </choice> <reg type="star">✳</reg> unnd ràts <reg type="star">✳</reg> der statt Bernn</orgName><reg>,</reg> <reg type="star">✳</reg> schuldig <reg type="star">✳</reg> unnd pflichtig sind wor<choice>
                    <abbr>d₎</abbr>
                    <expan>den</expan>
                </choice>
        <lb facs="#facs_1_r1l5" n="N005"/>unnser <persName ref="per007313">mitschwester <reg type="star">✳</reg> fròwenn Felicita Geltingerin</persName> <reg type="star">✳</reg> ir guͦtt<reg>,</reg> <reg type="star">✳</reg> libdinng <reg type="star">✳</reg> unnd
        <lb facs="#facs_1_r1l6" n="N006"/>anders<reg>,</reg>
                <reg type="star">✳</reg> so sy in unnser gotzhusz gebraucht hàtt<reg>,</reg> <reg type="star">✳</reg> wider zuͤ bekerenn<reg>,</reg> <reg type="star">✳</reg> das
