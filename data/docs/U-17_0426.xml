      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0426_r"/>
      <p facs="#facs_1_region_1603812195779_406">
        <lb facs="#facs_1_region_1603812195779_406l2" n="N001"/>Ich<reg>,</reg> <persName ref="per000388">Engelhart <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>re zuͤ Winsperg</persName><reg>,</reg> <reg type="star">✳</reg> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> <orgName ref="org000088">gnedi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schaft von Oͤs<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich <reg type="star">✳</reg> lantvoͤgt</orgName><reg>,</reg> <reg type="star">✳</reg> tuͤn kunt<reg>,</reg> <reg type="star">✳</reg> dz fùr mich kam <reg type="star">✳</reg> der
        <lb facs="#facs_1_region_1603812195779_406l3" n="N002"/>
                <persName ref="per027694">vest ritter <choice>
                        <abbr>hˀn</abbr>
                        <expan>herrn</expan>
                    </choice> Henman von Rinach</persName> von lehen we<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> <reg type="star">✳</reg> der ob<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genanten</expan>
                </choice> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> <orgName ref="org000088">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schaft von Oͤs<choice>
                        <abbr>t₎</abbr>
                        <expan>terrich</expan>
                    </choice>
                </orgName> man <reg type="star">✳</reg> und offnet do
        <lb facs="#facs_1_region_1603812195779_406l4" n="N003"/>vor mir<reg>,</reg> <reg type="star">✳</reg> wie die <persName ref="per024107">erber froͧw <reg type="star">✳</reg> Adelheit Schererin <choice>
                        <abbr>vō</abbr>
                        <expan>von </expan>
                    </choice> Mellingen</persName> <reg type="star">✳</reg> <reg type="star">✳</reg> und <persName ref="per024108">Niclaus Scherer<reg>,</reg> selig</persName><reg>,</reg> ir sun<reg>,</reg> <reg type="star">✳</reg> in rech<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>
        <lb facs="#facs_1_region_1603812195779_406l5" n="N004"/>gemeind wise <reg type="star">✳</reg> dise nachgeschri<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> lehen <reg type="star">✳</reg> von der sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> herschaft <choice>
                    <abbr>hrˀ</abbr>
                    <expan>her</expan>
                </choice> bracht <reg type="star">✳</reg> und gehebt het<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> <reg type="star">✳</reg> und sy
        <lb facs="#facs_1_region_1603812195779_406l6" n="N005"/>noch hett und niesse<reg>.</reg> <reg type="star">✳</reg> item <measure>fùnf stuk gelts</measure> ze <placeName ref="loc009231">Birmistorf</placeName> in dem dorff<reg>,</reg> gele<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> zwùschent dem <placeName ref="loc009245">vare ze
        <lb facs="#facs_1_region_1603812195779_406l7" n="N006"/>Windisch</placeName> und ze <placeName ref="loc000020">Baden</placeName><reg>.</reg> <reg type="star">✳</reg> item <measure>drû stuk gelts</measure> <reg type="star">✳</reg> uf <placeName ref="loc013826">Rùdlon</placeName><reg>,</reg> ob dem <placeName ref="loc013827">Nûwenhof</placeName><reg>,</reg> <reg type="star">✳</reg> uf ennent dem <placeName ref="loc013830">var ze Wettin<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </placeName><reg>.</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1603812195779_406l8" n="N007"/>
                <choice>
                    <abbr>it̄</abbr>
                    <expan>item</expan>
                </choice> und <measure>zwoͤi stuk gelts</measure> <reg type="star">✳</reg> in der <placeName ref="loc013828">Obern Salach</placeName> ze <placeName ref="loc000504">Mellingen</placeName><reg>,</reg> <reg type="star">✳</reg> nempt man die <placeName ref="loc013829">Obrest Salach</placeName><reg>,</reg> <reg type="star">✳</reg> als disù guͤt
        <lb facs="#facs_1_region_1603812195779_406l9" n="N008"/>und stuk <reg type="star">✳</reg> der gemeind brief wol lùtert<reg>.</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> won nu der selb ir sun vor ir <reg type="star">✳</reg> in tods wise abgangen <choice>
                    <abbr>weˀ</abbr>
                    <expan>wer</expan>
                </choice> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1603812195779_406l10" n="N009"/>und das lehen von der gemeind we<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> an si <choice>
                    <abbr>kom̄</abbr>
                    <expan>komen</expan>
                </choice> <choice>
                    <abbr>weˀ</abbr>
                    <expan>wer</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> so hett si dasselb lehen und guͦt <reg type="star">✳</reg> mir<reg>,</reg> an miner
        <lb facs="#facs_1_region_1603812195779_406l11" n="N010"/>ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> herschaft statt<reg>,</reg> <reg type="star">✳</reg> als einem lantvogt <reg type="star">✳</reg> gewaltig und mechtig <reg type="star">✳</reg> semlicher und merer sach frilich
        <lb facs="#facs_1_region_1603812195779_406l12" n="N011"/>und bedachtlich <reg type="star">✳</reg> bi gantzer <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>mugent irs libes<reg>,</reg> gesunt ir sinn uff gesendt <reg type="star">✳</reg> bi dem <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per027694">von Rinach</persName> in
        <lb facs="#facs_1_region_1603812195779_406l13" n="N012"/>min hand als lehens und landes recht ist<reg>.</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> und batt mich<reg>,</reg> es wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>umb lihen <reg type="star">✳</reg> ze stund an statt
        <lb facs="#facs_1_region_1603812195779_406l14" n="N013"/>dem <persName ref="per014602">er<choice>
                        <abbr>bnˀ</abbr>
                        <expan>bern</expan>
                    </choice> Uͦlman Gernass <reg type="star">✳</reg> von Mellingen</persName> und <reg type="star">✳</reg> <persName ref="per014603">Frênen</persName><reg>,</reg> <reg type="star">✳</reg> siner eli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> husfroͤwen<reg>,</reg> <reg type="star">✳</reg> der <choice>
                    <abbr>egen̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per024107">Adelheit
        <lb facs="#facs_1_region_1603812195779_406l15" n="N014"/>Schêrerin</persName> tochter<reg>,</reg> <reg type="star">✳</reg> die bett ich von dem <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per027694">von Rinach</persName> erhort<reg>,</reg> <reg type="star">✳</reg> als von einem miner herschaft man<reg>,</reg>
        <lb facs="#facs_1_region_1603812195779_406l16" n="N015"/>bi dem lehen naͤch lehens recht ufgesendt werdent<reg>.</reg> <reg type="star">✳</reg> und nam dz lehen <reg type="star">✳</reg> und guͦ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> in namen der <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice>
        <lb facs="#facs_1_region_1603812195779_406l17" n="N016"/>
                <persName ref="per024107">Adelheit Schererin</persName> uff <reg type="star">✳</reg> und leh si an statt wider umb gegenwùrtenklich <reg type="star">✳</reg> dem <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per014602">Uͦlman Gernass</persName> und
        <lb facs="#facs_1_region_1603812195779_406l18" n="N017"/>siner husfroͤwen in rechter gemeind wise<reg>,</reg> <reg type="star">✳</reg> wz ich im von recht <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> von gewonheit mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> herschaft daran
        <lb facs="#facs_1_region_1603812195779_406l19" n="N018"/>ze lihen han<reg>,</reg> <reg type="star">✳</reg> lihen sol <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> mag <reg type="star">✳</reg> nach lehens und landes recht <reg type="star">✳</reg> dien zwein personen inn ze ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice><reg>,</reg> ze
        <lb facs="#facs_1_region_1603812195779_406l20" n="N019"/>niessen<reg>,</reg> besetzzen und entsetzzen<reg>,</reg> <reg type="star">✳</reg> als lehens recht ist<reg>,</reg> <reg type="star">✳</reg> doch mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> herschaft an der manschaft unschedlich<reg>,</reg>
        <lb facs="#facs_1_region_1603812195779_406l21" n="N020"/>mit einem trager<reg>,</reg> <reg type="star">✳</reg> ob es an die froͤwen ze vall kaͤm<reg>,</reg> <reg type="star">✳</reg> an geverd<reg>,</reg> <reg type="star">✳</reg> mit urkùnd ditz briefs<reg>,</reg> besigelt <choice>
                    <abbr>offenl̄</abbr>
                    <expan>offenlich</expan>
                </choice>
        <lb facs="#facs_1_region_1603812195779_406l22" n="N021"/>mit minem eigen anhangen<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice> insigel<reg>,</reg> <reg type="star">✳</reg> ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> ze <placeName ref="loc000020">Baden</placeName><reg>,</reg> <reg type="star">✳</reg> am <date when="1393-10-13">nechs<choice>
                        <abbr>tn̄</abbr>
                        <expan>ten</expan>
                    </choice> mentag vor sant Gallen
        <lb facs="#facs_1_region_1603812195779_406l23" n="N022"/>tag<reg>,</reg> <reg type="star">✳</reg> nach Crists gebùrt <reg type="star">✳</reg> tusent drùhun<choice>
                        <abbr>dtˀ</abbr>
                        <expan>dert</expan>
                    </choice> und nûntzig jar<reg>,</reg> <reg type="star">✳</reg> dar naͤch im dritten jar</date><reg>.</reg>
            </p>
