  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0286_r"/>
      <p facs="#facs_1_region_1501246999883_44">
        <lb facs="#facs_1_line_1501247005301_45" n="N001"/>Allen den<reg>,</reg> die disen brief sehent<reg>,</reg> lesent oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> kùnde und vergich ich<reg>,</reg> <persName ref="per013183">Gerung von Altwis</persName>
                <reg>,</reg> <reg type="star">✳</reg> der <persName ref="per006964">durlùchtigen<reg>,</reg> hocherboren fùrstin<reg>,</reg> frown Agnesen<reg>,</reg> wilent
        <lb facs="#facs_1_line_1501247009532_46" n="N002"/>kuniginn ze Un<choice>
                        <abbr>gˀn</abbr>
                        <expan>gern</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> miner gneͣdigen frowen<reg>,</reg> <reg type="star">✳</reg> vogt uff dem <placeName ref="loc009769">Boͤzberg</placeName> und in dem <placeName ref="loc009765">Eigen</placeName>
                <reg>,</reg> <reg type="star">✳</reg> daz ich ze gerichte sazz ze <placeName ref="loc009977">Brugg vor dem <choice>
                        <abbr>Obˀn</abbr>
                        <expan>Obern</expan>
                    </choice> Tor</placeName> <reg type="star">✳</reg> an offen<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> straz
        <lb facs="#facs_1_line_1501247012812_47" n="N003"/>an mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> gnedigen frowen stat<reg>,</reg> <reg type="star">✳</reg> und kamen da fùr mich fùr offen gericht die <persName ref="per012513">erberen<reg>,</reg> wolbescheiden <reg type="star">✳</reg> frow Elzbeth</persName>
                <reg>,</reg> <persName ref="per018944">Hartmans<reg>,</reg> seligen<reg>,</reg> des Truchsetzen von Wildeg</persName>
                <reg>,</reg>
        <lb facs="#facs_1_line_1501247016660_48" n="N004"/>seligen<reg>,</reg> elichû hùsfrow<reg>,</reg> mit dem <persName ref="per022288">vesten rit<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> <choice>
                        <abbr>hˀn</abbr>
                        <expan>herrn</expan>
                    </choice> Lùtolden von Frike</persName>
                <reg>,</reg> irem rechten und erkornem vogte<reg>,</reg> <reg type="star">✳</reg> und <persName ref="per011915">Uͦlrich</persName> und <persName ref="per011916">Al<choice>
                        <abbr>br̄</abbr>
                        <expan>brecht</expan>
                    </choice> die Truchsetzen</persName>
                <reg>,</reg> <reg type="star">✳</reg> ire elichen sùne<reg>,</reg> mit
        <lb facs="#facs_1_line_1501247020484_49" n="N005"/>
                <persName ref="per003512">Cuͦnraten von Wolon</persName>
                <reg>,</reg> <reg type="star">✳</reg> irem erkornen vogte<reg>,</reg> zuͦ einem teile<reg>,</reg> <reg type="star">✳</reg> und der erber<reg>,</reg> wolbescheiden <persName ref="per014847">Walther Hofmeister<reg>,</reg> <reg type="star">✳</reg> schafner und amptman des klosters ze Kunigsvelt</persName>
                <reg>,</reg>
        <lb facs="#facs_1_line_1501247025069_50" n="N006"/>zuͦ dem andern teile<reg>,</reg> <reg type="star">✳</reg> und verjahen da vor mir und vor offennem gerichte die vorgeschri<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <persName ref="per012513">frow Elzbeth die Truchsetzin</persName> und <persName ref="per011915">Ùlrich</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per011916">Al<choice>
                        <abbr>br̄</abbr>
                        <expan>brecht</expan>
                    </choice>
                </persName>
                <reg>,</reg> ire sùne<reg>,</reg>
        <lb facs="#facs_1_line_1501247029844_51" n="N007"/>mit dien egeschriben iren voͤgten <reg type="star">✳</reg> und fùr alle ir erben<reg>,</reg> <reg type="star">✳</reg> das si recht und redlich ze kouffennͤ geben hattin <reg type="star">✳</reg> den <orgName ref="org002627">erwirdigen<reg>,</reg> geistlichen frown<reg>,</reg> <reg type="star">✳</reg> der eptissin
        <lb facs="#facs_1_line_1501247034812_52" n="N008"/>und dem covent gemeinlich des klosters ze Kunigsvelt<reg>,</reg> <reg type="star">✳</reg> Sant Claren ordens<reg>,</reg> gelegen in Costen<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bystuͦm</orgName>
                <reg>,</reg> <reg type="star">✳</reg> und iren nachkomen <reg type="star">✳</reg> iren teil <reg type="star">✳</reg> der <placeName ref="loc010010">Eichhalden</placeName>
                <reg>,</reg> <reg type="star">✳</reg> den
        <lb facs="#facs_1_line_1501247039548_53" n="N009"/>man nenmet der <placeName ref="loc013042">Nider Teil</placeName>
                <reg>,</reg> wider <placeName ref="loc000390">Habspurg</placeName>
                <reg>,</reg> mit dem <placeName ref="loc012883">Nùwgerùt</placeName>
                <reg>,</reg> mit wunne<reg>,</reg> mit weide<reg>,</reg> <reg type="star">✳</reg> mit grunde und mit grate<reg>,</reg> mit holze<reg>,</reg> mit velde<reg>,</reg> mit wasen<reg>,</reg>
        <lb facs="#facs_1_line_1501247044252_54" n="N010"/>mit zwi<reg>,</reg> mit fundem und mit unfundem und mit namen mit aller zuͦgehorde<reg>,</reg> <reg type="star">✳</reg> umb <measure>achzig guldin<reg>,</reg> guͦter und vollen <choice>
                        <abbr>swˀer</abbr>
                        <expan>swerer</expan>
                    </choice> Flo<choice>
                        <abbr>renz̄</abbr>
                        <expan>renzer</expan>
                    </choice>
                </measure>
                <reg>,</reg> dersel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> guldin sù ouch
        <lb facs="#facs_1_line_1501247049860_55" n="N011"/>gantzlich und voͤlleclich von in ge<choice>
                    <abbr>wˀt</abbr>
                    <expan>wert</expan>
                </choice> sint <reg type="star">✳</reg> und in iren off<choice>
                    <abbr>enn̄</abbr>
                    <expan>fenen</expan>
                </choice> nutz <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> notùrft bekert hand<reg>.</reg> <reg type="star">✳</reg> die vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genante</expan>
                </choice> <persName ref="per012513">frow Elzbetha die Truchsetzen</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per011915">Uͦlrich</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per011916">Al<choice>
                        <abbr>br̄</abbr>
                        <expan>brecht</expan>
                    </choice>
                </persName>
                <reg>,</reg>
        <lb facs="#facs_1_line_1501247054164_56" n="N012"/>ire sune<reg>,</reg> mit iren beiden foͤgten <reg type="star">✳</reg> batten mich<reg>,</reg> den vorgeschri<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> rich<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>
                <reg>,</reg> erfarn an ei<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> urteilde<reg>,</reg> <reg type="star">✳</reg> wie si die vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genante</expan>
                </choice> <placeName ref="loc010010">Eichalden</placeName>
                <reg>,</reg> die ir lehen wz von der <orgName ref="org000088">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schaft von
