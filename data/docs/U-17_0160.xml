  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0160_r"/>
      <p facs="#facs_1_region_1496126891881_17">
        <lb facs="#facs_1_line_1496126920577_18" n="N001"/>Allen den<reg>,</reg> die disen brief an sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> kùnden <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jehen wir<reg>,</reg> <persName ref="per008554">Heinzi Wessen<choice>
                        <abbr>bgˀ</abbr>
                        <expan>berg</expan>
                    </choice>
                </persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per018714">Margareta</persName>
                <reg>,</reg> sin elich wirtin<reg>,</reg> seshâft ze <placeName ref="loc000480">Loͧffen<choice>
                        <abbr>bgˀ</abbr>
                        <expan>burg</expan>
                    </choice>
                </placeName>
                <reg>,</reg>
        <lb facs="#facs_1_line_1496126925169_19" n="N002"/>das wir einhelleklich<reg>,</reg> mit gesuntheit lips <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> sinnen dur ùn<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> not hant <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧft <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jehen<reg>,</reg> das wir recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redlich ze koͧffen gegeben hant den
        <lb facs="#facs_1_line_1496126931162_20" n="N003"/>
                <orgName ref="org002627">erwirdigen froͧwen <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> <reg type="star">✳</reg> <reg type="star">✳</reg> eptischin <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem <reg type="star">✳</reg> <reg type="star">✳</reg> convent des klosters ze Kùngsvelt</orgName> <reg type="star">✳</reg> ùn<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> trotten<reg>,</reg> lit an dem <choice>
                    <abbr>bˀ</abbr>
                    <expan>ber</expan>
                </choice>ge ze <placeName ref="loc007770">Waltzhuͦt</placeName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alle ùn<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> reben<reg>,</reg> die
        <lb facs="#facs_1_line_1496126945114_21" n="N004"/>lantgarbe <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alles das recht<reg>,</reg> das wir hant in dien twingen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in dien bannen ze <placeName ref="loc007770">Waltzhuͦt</placeName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze <placeName ref="loc012193">To<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>ren</placeName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in swelen bannen si gelegen sint<reg>,</reg> ane
        <lb facs="#facs_1_line_1496126950465_22" n="N005"/>ge<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>de<reg>,</reg> <reg type="star">✳</reg> fùr lidig <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> fùr ler <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> fùr recht erbe von dem <orgName ref="org002667">gotzhus sant Johans ze Klingnowe</orgName> umbe <measure>sibenzig march silbers<reg>,</reg> luters <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> loͤtiges Base<choice>
                        <abbr>lˀ</abbr>
                        <expan>ler</expan>
                    </choice> gewicht</measure>
                <reg>,</reg>
        <lb facs="#facs_1_line_1496126960353_23" n="N006"/>
                <choice>
                    <abbr>dˀ</abbr>
                    <expan>das </expan>
                </choice> wir von inan sint gewert gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gentzlich <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> die wir in ùn<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> eigen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redlich nùtze bekeret hant<reg>,</reg> <reg type="star">✳</reg> inan <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> iren nachkomen ze habende <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze niessende<reg>,</reg>
        <lb facs="#facs_1_line_1496126964889_24" n="N007"/>ze besetzende <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> entzetzenne<reg>,</reg> mit allen rechten<reg>,</reg> als wir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ùn<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> vor<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice> die vorgeschriben guͤ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> da har gehept <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> genossen hant <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> von erbeschaft an ùns
        <lb facs="#facs_1_line_1496126994105_25" n="N008"/>sint gevallen ane ge<choice>
                    <abbr>ˀv</abbr>
                    <expan>ver</expan>
                </choice>de<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> hant dù vorgeschriben guͤ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> dem <persName ref="per018886">er<choice>
                        <abbr>bnˀ</abbr>
                        <expan>bern</expan>
                    </choice> manne Heinrich Vilnacher<reg>,</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> vor<choice>
                        <abbr>gen̄</abbr>
                        <expan>genant</expan>
                    </choice> froͧwen schafner</persName>
                <reg>,</reg> <reg type="star">✳</reg> an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selben froͧwen stat uf
        <lb facs="#facs_1_line_1496127011504_26" n="N009"/>gegeben<reg>,</reg> lidig <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ler<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ge<choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tiget mit rech<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urteilde in offem gericht ze <placeName ref="loc000480">Loͧffen<choice>
                        <abbr>bgˀ</abbr>
                        <expan>burg</expan>
                    </choice>
                </placeName> in <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> stat<reg>,</reg> mit handen<reg>,</reg> worten <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
