  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0418_r"/>
      <p facs="#facs_1_region_1507832829194_74">
        <lb facs="#facs_1_line_1507832836236_75" n="N001"/>Allen<reg>,</reg> die disen brief sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> kuͦnd und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich ich<reg>,</reg> <persName ref="per025610">Heinrich Pfister<reg>,</reg> hofmeis<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> ze Kûngsveld</persName><reg>,</reg>
        <lb facs="#facs_1_line_1507832842179_76" n="N002"/>daz ich vor dem selben kloster <placeName ref="loc007695">Kûngsfeld</placeName> offelich ze gericht sassz in na<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> und anstat der <orgName ref="org002627">erwirdi<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice><reg>,</reg>
        <lb facs="#facs_1_line_1507832850130_77" n="N003"/>geistli<choice>
                        <abbr>chē</abbr>
                        <expan>chen</expan>
                    </choice> froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice><reg>,</reg> der eptissin ze Kuͤngsfel<choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName>
                <reg>.</reg> da fùr mich kamen an offem und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>ban<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> gericht die
        <lb facs="#facs_1_line_1507832856495_78" n="N004"/>beschei<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice> <persName ref="per026283">Gred Meigerin</persName> und <persName ref="per026282">Snelman</persName><reg>,</reg> ir eli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> man <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> vogt<reg>,</reg> an ei<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> teil <reg type="star">✳</reg> und <persName ref="per026286">Claus Mei<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName> und <persName ref="per026285">Hans
        <lb facs="#facs_1_line_1507832865624_79" n="N005"/>Meiger</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026287">Anna Meigerin</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026288">Gery Meigerin</persName> an dem andern teil<reg>.</reg> <reg type="star">✳</reg> und offna<choice>
                    <abbr>t₎</abbr>
                    <expan>ten</expan>
                </choice> die vorge<choice>
                    <abbr>nāt</abbr>
                    <expan>nanten</expan>
                </choice> beide
        <lb facs="#facs_1_line_1507832873271_80" n="N006"/>teil mit ira vogt und fùrspre<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice><reg>,</reg> daz sy hattent stoͤss <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> anspraͤch gehebt von irs vetterli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> erbes
        <lb facs="#facs_1_line_1507832882269_81" n="N007"/>wegen<reg>.</reg> <reg type="star">✳</reg> derselben stoͤss <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ansprach sy gentzlich und einhelklich ko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> ùf die <persName ref="per026290">beschei<choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice> Peter Elfin<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
        <lb facs="#facs_1_line_1507832891650_82" n="N008"/>von Lùppfen</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026302">Welthy Bùrgis ze Scherntz<reg>,</reg> gesessen ze Windisch</persName><reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ùf <persName ref="per026291">Wern<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Spaͧten ze Scherntz</persName>
        <lb facs="#facs_1_line_1507832901149_83" n="N009"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per025940">Heiny Reig von <choice>
                        <abbr>Scher̄tz</abbr>
                        <expan>Scherntz</expan>
                    </choice>
                </persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026292">Heiny Habrer von Habsburg</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026293">Werny Reig von Byra</persName>
                <reg>.</reg> diesel<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice>
        <lb facs="#facs_1_line_1507832910905_84" n="N010"/>och dar umb ùssproͤchen<reg>,</reg> also daz die vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per026286">Claus Meiger</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026285">Hans Meiger</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ira swes<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> soͤltent
        <lb facs="#facs_1_line_1507832919650_85" n="N011"/>geben der ege<choice>
                    <abbr>nant̄</abbr>
                    <expan>nanten</expan>
                </choice> <persName ref="per026283">Gred Meigerin</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026282">Hans Snel<choice>
                        <abbr>mā</abbr>
                        <expan>man</expan>
                    </choice>
                </persName><reg>,</reg> ir eli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> man<reg>,</reg> fur all ir ansprach<reg>,</reg> so sy von
        <lb facs="#facs_1_line_1507832930287_86" n="N012"/>des <choice>
                    <abbr>egen̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> erbteils wegen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> umb all ansprach<reg>,</reg> so sy untz ùf disen hutigen tag als diser brief
        <lb facs="#facs_1_line_1507832938985_87" n="N013"/>geben ist<reg>,</reg> gehept hettent<reg>,</reg> <reg type="star">✳</reg> <measure>fuͤnfzig pfuͦnt pfening</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> die matten<reg>,</reg> die man nempt <placeName ref="loc013433">Winkelmat<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice>
                </placeName><reg>,</reg>
        <lb facs="#facs_1_line_1507832948160_88" n="N014"/>gelegen ze <placeName ref="loc009557">Scherntz</placeName><reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> des selben geltz <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> der mat<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> wurdent die vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per026283">Gred Mei<choice>
                        <abbr>gˀin</abbr>
                        <expan>gerin</expan>
                    </choice>
                </persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per026282">Hans
        <lb facs="#facs_1_line_1507832956084_89" n="N015"/>Snel<choice>
                        <abbr>mā</abbr>
                        <expan>man</expan>
                    </choice>
                </persName> gar und gentzlich gericht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> bezalt<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sullent och die obege<choice>
                    <abbr>nant̄</abbr>
                    <expan>nanten</expan>
                </choice> <persName ref="per026286">Claus Mei<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sin
        <lb facs="#facs_1_line_1507832972943_90" n="N016"/>geswis<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>git ein weissery haben von der vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> <placeName ref="loc013433">Winkelmat<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice>
                </placeName> ùf ir matten<reg>,</reg> die man nempt <placeName ref="loc013434">
                    <choice>
                        <abbr>Akˀ</abbr>
                        <expan>Aker</expan>
                    </choice>mat<choice>
