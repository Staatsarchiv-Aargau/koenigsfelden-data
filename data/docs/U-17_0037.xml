  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0037_r"/>
      <p facs="#facs_1_region_1492769943969_64">
        <lb facs="#facs_1_line_1492769981467_65" n="N001"/>Allen<reg>,</reg> die disen brief sehent oder hoͤrent<reg>,</reg> <reg type="star">✳</reg> kùnd ich<reg>,</reg> <reg type="star">✳</reg> <persName ref="per020757">Elsbethe von Uͦtzingen</persName>
                <reg>,</reg> fùrweserin
        <lb facs="#facs_1_line_1492769985952_66" n="N002"/>
                <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <persName ref="per017313">erwirdigen vrowen<reg>,</reg> vron Elsbethen<reg>,</reg> von gottes gnaden <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> eptischin Zùrich</persName>
                <reg>,</reg> das fùr
        <lb facs="#facs_1_line_1492769989637_67" n="N003"/>mich kam <persName ref="per016431">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Ruͦdolf <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Jun<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> Mùl<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice>
                    <reg>,</reg> rit<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>
                    <reg>,</reg> von Zùrich</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mit mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> hant<reg>,</reg> <reg type="star">✳</reg> willen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gunst
        <lb facs="#facs_1_line_1492769993676_68" n="N004"/>ufgab frilich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ledeklich sinù eigen guͦter<reg>,</reg> so er hattte ze <placeName ref="loc009457">Hêntzchinkon</placeName>
                <reg>,</reg> ze <placeName ref="loc000954">Vilmêringen</placeName>
                <reg>,</reg>
        <lb facs="#facs_1_line_1492769997336_69" n="N005"/>ze <placeName ref="loc009989">Halwi<choice>
                        <abbr>lˀ</abbr>
                        <expan>ler</expan>
                    </choice>
                </placeName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und </expan>
                </choice> ze <placeName ref="loc000401">Hilfinkon</placeName>
                <reg>,</reg> dù emals waren <reg type="star">✳</reg> <persName ref="per020771">Wernhers von Vilmêringen</persName>
                <reg>,</reg> <reg type="star">✳</reg> <orgName ref="org002627">den erberen
        <lb facs="#facs_1_line_1492770001619_70" n="N006"/>
                    <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> geischlichen vrowen <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> <reg type="star">✳</reg> <reg type="star">✳</reg> eptischennen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent des klosters ze Kùngesvelt<reg>,</reg>
        <lb facs="#facs_1_line_1492770006024_71" n="N007"/>Sant Claren ordins<reg>,</reg> in Kostent<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bistum</orgName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> bindet sich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sin erben <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> zuͦ<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vorsei<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_line_1492770010578_72" n="N008"/>guͦtern wern ze sinne fùr recht eigen <orgName ref="org002627">
                    <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> vorgenanden erberen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> geischlichen vrowen<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770015434_73" n="N009"/>der <reg type="star">✳</reg> <reg type="star">✳</reg> êptischen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> des conventes des klos<choice>
                        <abbr>tˀs</abbr>
                        <expan>ters</expan>
                    </choice> ze Kùngsvelt</orgName>
                <reg>,</reg> <reg type="star">✳</reg> nach rechte <reg type="star">✳</reg> an allen stetten <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770020022_74" n="N010"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gerichten<reg>,</reg> <reg type="star">✳</reg> geischlichen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> weltlichen<reg>,</reg> <reg type="star">✳</reg> swa sis bedurfen<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> da er ez dur recht sol tuͦn<reg>.</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770024210_75" n="N011"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> vergicht oͧch offenlich an disem briefe<reg>,</reg> das er emphangen hat ze koͧfes wêgen <reg type="star">✳</reg> von den
        <lb facs="#facs_1_line_1492770028569_76" n="N012"/>vorschriben<orgName ref="org002627"> erberen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> geischlichen vrowen<reg>,</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> eptischen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent von Kùngsvelt</orgName>
                <reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770032302_77" n="N013"/>
                <measure>hundert <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> vier und zweintzich march guͦtes silbers gen<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> Zùrich gewicht</measure>
                <reg>,</reg> <reg type="star">✳</reg> die
        <lb facs="#facs_1_line_1492770036925_78" n="N014"/>ime <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sinen kinden ze guͦten statten worden sint<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ist dirre koͧf beschehen unschêdelich <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770041334_79" n="N015"/>
                <persName ref="per020777">fron Annen</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per020771">Wern<choice>
                        <abbr>hˀs</abbr>
                        <expan>hers</expan>
                    </choice> sêligen wirtin von Vilmeringen</persName>
                <reg>,</reg> <reg type="star">✳</reg> an dem lipgedinge<reg>,</reg> so sie hatte an
        <lb facs="#facs_1_line_1492770046034_80" n="N016"/>den vorgenanden guͦtern<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> das dis war si <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> veste belibe<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dur <persName ref="per016431">
                    <choice>
                        <abbr>hˀn</abbr>
                        <expan>hern</expan>
                    </choice> Ruͦdolfs bêtte<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770050472_81" n="N017"/>des vorseiten Mùl<choice>
                        <abbr>nˀs</abbr>
                        <expan>ners</expan>
                    </choice>
                </persName>
                <reg>,</reg> so gib ich disen brief besigelt den vorschriben <orgName ref="org002627">erberen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> geischli<reg type="hyphen">-</reg>
        <lb facs="#facs_1_line_1492770055311_82" n="N018" break="no"/>chen vrowen<reg>,</reg> der eptischen <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent ze Kùngsvelt</orgName>
                <reg>,</reg> <reg type="star">✳</reg> mit ûn<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> vrowen <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vorseiten
        <lb facs="#facs_1_line_1492770059432_83" n="N019"/>êptischen ingesigel ze einem waren urkùnde offenlich<reg>.</reg> <reg type="star">✳</reg> dis geschach <supplied>ze</supplied> <placeName ref="loc000065">Zùrich</placeName> <date when="1314-09-13">an dem
