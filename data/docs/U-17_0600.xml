    <p facs="#facs_2_region_1531829600315_218" rend="dorsual_20_regesta">
        <lb facs="#facs_2_line_1531829777548_225" n="N001"/>ist ein lechen
        <lb facs="#facs_2_line_1531829780116_226" n="N002"/>hat jetz Fridli Trùll <reg type="star">✳</reg>
            </p>
      <p facs="#facs_2_region_1531829580936_214" rend="dorsual_22_sigle">
