      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0909a_r"/>
      <p facs="#facs_1_region_1521838454701_32">
        <lb facs="#facs_1_region_1521838454701_32l1" n="N001"/>Wir<reg>,</reg> <persName ref="per012283">Kathrina Truchsaͤssinn von Waldtpurg<reg>,</reg> äpptissinn</persName><reg>,</reg> <reg type="star">✳</reg> unnd gmein <orgName ref="org002627">con<choice>
                        <abbr>vēt</abbr>
                        <expan>vent</expan>
                    </choice> froͧ<choice>
                        <abbr>wnn̄</abbr>
                        <expan>wenn</expan>
                    </choice> des gotzhusz <reg type="star">✳</reg> unnd
        <lb facs="#facs_1_region_1521838454701_32l2" n="N002"/>kûngklichenn stiffs <reg type="star">✳</reg> Küngsfeldenn<reg>,</reg> <reg type="star">✳</reg> Sannt Clarenn ordenns<reg>,</reg> <reg type="star">✳</reg> Costannt<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bistunns</orgName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>,</reg> beken<choice>
                    <abbr>n̄ed</abbr>
                    <expan>nend</expan>
                </choice> offenlich mit
        <lb facs="#facs_1_region_1521838454701_32l3" n="N003"/>disem brieff<reg>,</reg> <reg type="star">✳</reg> das wir mit einhelligem raͧtt<reg>,</reg> <reg type="star">✳</reg> guͤtter vorbetrach<choice>
                    <abbr>tun̄g</abbr>
                    <expan>tunng</expan>
                </choice><reg>,</reg> mere<choice>
                    <abbr>r̄n</abbr>
                    <expan>ren</expan>
                </choice> unnsers gotzhusz nutz zefür<reg type="lb">¬</reg><lb facs="#facs_1_region_1521838454701_32l4" n="N004" break="no"/>drenn <reg type="star">✳</reg> unnd schadenn ze wänn<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> ouch besunders zanngk<reg>,</reg> <reg type="star">✳</reg> spänn <reg type="star">✳</reg> unnd vil irrungs <reg type="star">✳</reg> unnd abganngs<reg>,</reg>
        <lb facs="#facs_1_region_1521838454701_32l5" n="N005"/>so unnsz daͧhar erwachsenn ist<reg>,</reg> <reg type="star">✳</reg> angsächenn <reg type="star">✳</reg> unnd zü unnsernn hann<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice> gebràcht unnd in eins ewy<choice>
                    <abbr>g₎</abbr>
                    <expan>gen</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l6" n="N006"/>kouffs wisz <reg type="star">✳</reg> zù anndernn unnse<choice>
                    <abbr>rnn̄</abbr>
                    <expan>renn</expan>
                </choice> zächenn<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice> erkoͧfft habennd ein zaͤchenndly<reg>,</reg> genannpt der ostann zaͤch<reg type="lb">¬</reg><lb facs="#facs_1_region_1521838454701_32l7" n="N007" break="no"/>ennden<reg>,</reg> <reg type="star">✳</reg> der dem <orgName ref="org005850">gotzhusz Bòtzenn</orgName> zuͤ gehoͤrig <reg type="star">✳</reg> unnd vormàls vom <persName ref="per024719">Füchslinn zuͦ Loͧffennberg</persName> daran
        <lb facs="#facs_1_region_1521838454701_32l8" n="N008"/>erkoufft gwäsenn ist<reg>,</reg> <reg type="star">✳</reg> jaͤrlich vallennd <reg type="star">✳</reg> unnd gànnd <reg type="star">✳</reg> von etli<choice>
                    <abbr>chnn̄</abbr>
                    <expan>chenn</expan>
                </choice> guͤttren zuͦ <placeName ref="loc011207">Effinn<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice>
                </placeName> in den drig<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> zel<choice>
                    <abbr>g₎</abbr>
                    <expan>gen</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l9" n="N009"/>gelaͤgenn<reg>.</reg> <reg type="star">✳</reg> dà der zaͤchennd susz hievor ùber houpt unnser ist<reg>.</reg> unnd habennd also sòli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l11" n="N010"/>kouff <reg type="star">✳</reg> unnd zàchenndenn <reg type="star">✳</reg> uff <reg type="star">✳</reg> unnd an unnsz genom<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> <reg type="star">✳</reg> der màs <reg type="star">✳</reg> unnd mit soͤlicher beladnus<reg>,</reg> das
        <lb facs="#facs_1_region_1521838454701_32l12" n="N011"/>wir <reg type="star">✳</reg> unnd unnsers gotzhuses ewygg nàchkom<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> <reg type="star">✳</reg> vermeldt<choice>
                    <abbr>tm̄</abbr>
                    <expan>tem</expan>
                </choice> unnser <orgName ref="org005850">liebenn fròwenn gotzhusz zuͦ Boͤ<reg type="lb">¬</reg><lb facs="#facs_1_region_1521838454701_32l13" n="N012" break="no"/>tzenn</orgName> <reg type="star">✳</reg> von unnserm kor<choice>
                    <abbr>nn̄</abbr>
                    <expan>nen</expan>
                </choice> zächenn<choice>
                    <abbr>d₎</abbr>
                    <expan>den</expan>
                </choice> daͧselbs <reg type="star">✳</reg> jaͤrlich sonnd uszrichtenn unnd gaͤbenn <reg type="star">✳</reg> oder verschaffen ze
        <lb facs="#facs_1_region_1521838454701_32l14" n="N013"/>werdenn<reg>,</reg> <reg type="star">✳</reg> namlich <measure>drü maltter güts</measure><reg>,</reg> <reg type="star">✳</reg> dero sonnd <measure>zweyg an ker<choice>
                        <abbr>nn̄</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure> <reg type="star">✳</reg> unnd das dritt <measure>an haberr</measure> sin <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>.</reg>
        <lb facs="#facs_1_region_1521838454701_32l15" n="N014"/>unnd die selbenn stuck gebenn unnd verschaffenn zü gemelts <orgName ref="org005850">gotzhuses Böt<choice>
                        <abbr>znn̄</abbr>
                        <expan>zenn</expan>
                    </choice>
                </orgName> unnd ye sinen pflägrenn sich<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l16" n="N015"/>hann<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice> <reg type="star">✳</reg> gehaltt <reg type="star">✳</reg> unnd gwaltt ganntz fùr alles verhefftenn<reg>,</reg> <reg type="star">✳</reg> verbiettenn <reg type="star">✳</reg> unnd entwern krieg<reg>,</reg> <reg type="star">✳</reg> àcht<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>bàn̄</abbr>
                    <expan>bànn</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l17" n="N016"/>unnd susz fùr all mangell <reg type="star">✳</reg> infaͤll <reg type="star">✳</reg> unnd gebraͤstenn<reg>,</reg> dem <reg type="star">✳</reg> wir <reg type="star">✳</reg> unnd unnser nàchkom<choice>
                    <abbr>mēn</abbr>
                    <expan>menn</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l18" n="N017"/>trüwlich gelabenn <reg type="star">✳</reg> unnd nachkom<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> soͤllen <reg type="star">✳</reg> unnd wellennd by unnsernn güttenn trüwen<reg>,</reg> wir<choice>
                    <abbr>d₎</abbr>
                    <expan>den</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l19" n="N018"/>unnd erenn<reg>.</reg> unnd ist also soͤlicher kouff <reg type="star">✳</reg> unnd verkòuff ergann<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> in ùnnserm namen
        <lb facs="#facs_1_region_1521838454701_32l21" n="N019"/>vor dem <persName ref="per013569">ersammen Hannsen Bürrer<reg>,</reg> unnserm hoffmeyster</persName><reg>,</reg> <reg type="star">✳</reg> so dann usz <reg type="star">✳</reg> unnd von der
        <lb facs="#facs_1_region_1521838454701_32l22" n="N020"/>kilchenn <orgName ref="org005850">unnser liebenn froͧwenn zuͦ Boͤtzenn</orgName> <reg type="star">✳</reg> unnd gmeiner underta<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> <reg type="star">✳</reg> der <orgName ref="org005849">pffar Elfin<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice>
                </orgName>
        <lb facs="#facs_1_region_1521838454701_32l23" n="N021"/>zuͤgehoͤrig<reg>,</reg> <reg type="star">✳</reg> sundrenn enpfälchs wägenn <reg type="star">✳</reg> mit ir aller gmeinli<choice>
                    <abbr>chm̄</abbr>
                    <expan>chem</expan>
                </choice> geheissz<reg>,</reg> <reg type="star">✳</reg> wüssenn <reg type="star">✳</reg> unnd wil<choice>
                    <abbr>ln̄</abbr>
                    <expan>len</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l24" n="N022"/>unnd vollem gwaltt <reg type="star">✳</reg> der <persName ref="per024716">wirdig unnd geistlich herr Michell Sager<reg>,</reg> <reg type="star">✳</reg> unnser lütpriester
        <lb facs="#facs_1_region_1521838454701_32l25" n="N023"/>zü Ellfinn<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice></persName><reg>,</reg> <persName ref="per023811">Cünrat Lenngloͧwer<reg>,</reg> wirt zü Effinn<choice>
                        <abbr>gēn</abbr>
                        <expan>genn</expan>
                    </choice></persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per024718">Hanns Has</persName> <reg type="star">✳</reg> unnd <persName ref="per024717">Weltty Pauly
        <lb facs="#facs_1_region_1521838454701_32l26" n="N024"/>zü Bötzenn<reg>,</reg> <reg type="star">✳</reg> der zitt gotzhusz pfläger</persName><reg>,</reg> unnd desz alles zu wärem unnd ves<choice>
                    <abbr>tm̄</abbr>
                    <expan>tem</expan>
                </choice>
        <lb facs="#facs_1_region_1521838454701_32l27" n="N025"/>urkùnd so habenn wir<reg>,</reg> obbemaͤldttenn aͤpptissinn<reg>,</reg> <reg type="star">✳</reg> unnd convent <reg type="star">✳</reg> gedauchtem gotz<reg type="lb">¬</reg><lb facs="#facs_1_region_1521838454701_32l28" n="N026" break="no"/>husz disenn brieff mit unnsers gotzhusses unnd con<choice>
                    <abbr>vēts</abbr>
                    <expan>vents</expan>
                </choice> angehännck<choice>
                    <abbr>tm̄</abbr>
                    <expan>tem</expan>
                </choice> secrett
        <lb facs="#facs_1_region_1521838454701_32l29" n="N027"/>insigell verwart <reg type="star">✳</reg> unnd besiglett gaͤbenn uff den nächstenn <date when="1518-07-18">suntag vor sannt Maria
        <lb facs="#facs_1_region_1521838454701_32l30" n="N028"/>Magdalennen tag<reg>,</reg> <reg type="star">✳</reg> von Cristy gottes geburt zaltt <reg type="star">✳</reg> fünfftzächennhundert unnd achtzächenn jàr</date><reg>.</reg>
            </p>
