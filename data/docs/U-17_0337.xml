        <lb facs="#facs_1_line_1501249595191_349" n="N006"/>fùr reht ei<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>sechs mùt rog<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> jerlichs geltz</measure> ab dem hoff ze <placeName ref="loc012307">Buschikon</placeName>
                <reg>,</reg> mit solicher beschei<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice>heit<reg>,</reg> <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>
                <reg>,</reg> dz er <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> sin erben<reg>,</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> der den hoff
        <lb facs="#facs_1_line_1501249599910_350" n="N007"/>inne hat<reg>,</reg> die ege<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>ten froͧwen <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> jetz benemp<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <measure>sechs mùt rog<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </measure> nùt <date when="0001-11-11">jerlich wertin uff sant Martis tag</date>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dz zwen zins den
        <lb facs="#facs_1_line_1501249604767_351" n="N008"/>dritten erlùffin<reg>,</reg> so soͤlt <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ob<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genante</expan>
