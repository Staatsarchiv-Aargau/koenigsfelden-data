                <reg>,</reg> <persName ref="per003512">Cuͦnrat <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wola</persName>
        <lb facs="#facs_1_line_1501249581158_346" n="N003"/>mit <persName ref="per003511">An<choice>
