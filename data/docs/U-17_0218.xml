        <lb facs="#facs_1_line_1501237423462_1029" n="N001"/>Allen den<reg>,</reg> die disen brief ansehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> kùnde <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich <reg type="star">✳</reg> <reg type="star">✳</reg> ich<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per019693">Johans <choice>
                        <abbr>Schulth̄</abbr>
                        <expan>Schultheiss</expan>
                    </choice><reg>,</reg> <reg type="star">✳</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> ze Arowe</persName><reg>,</reg> <reg type="star">✳</reg> daz ich <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧft han
        <lb facs="#facs_1_line_1501237427494_1030" n="N002"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze koͧfen hingege<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redelich mit disem gegenwùrtigen brief von mir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> den <orgName ref="org002627">er<choice>
                        <abbr>bˀen</abbr>
                        <expan>beren</expan>
                    </choice><reg>,</reg> geistlichen frowen<reg>,</reg>
        <lb facs="#facs_1_line_1501237431359_1031" n="N003"/>der eptissen <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convente dez gotzhuses ze Kùngesvelt<reg>,</reg> Sant Claren ordens<reg>,</reg> gele<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> in Costentzer bistten</orgName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir nachkomen
        <lb facs="#facs_1_line_1501237436766_1032" n="N004"/>zehen stùcke geltes an korne<reg>,</reg> eins mùte ha<choice>
                    <abbr>bˀn</abbr>
                    <expan>bern</expan>
                </choice> minder<reg>,</reg> <reg type="star">✳</reg> ist gele<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> ze <placeName ref="loc009358">Grenikon</placeName> in dem banne <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gent die er<choice>
                    <abbr>bˀen</abbr>
                    <expan>beren</expan>
                </choice> lùte<reg>,</reg> die hie nach
        <lb facs="#facs_1_line_1501237443630_1033" n="N005"/>geschriben stant<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per020012">Ruͦdolf <choice>
                        <abbr>Bˀ</abbr>
                        <expan>Ber</expan>
                    </choice>ger</persName> git <measure>fùnf mùt ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure><reg>,</reg> <reg type="star">✳</reg> <measure>zwein mùt ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per011905">Pe<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> Schertleip</persName> git <reg type="star">✳</reg> <measure>drie mùt ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure><reg>,</reg> <reg type="star">✳</reg> <measure>ein mal<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure><reg>,</reg>
        <lb facs="#facs_1_line_1501237449398_1034" n="N006"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per020013"><choice>
                        <abbr>Uͦlr̄</abbr>
                        <expan>Uͦlrich</expan>
                    </choice> Walo</persName> git <measure>drie mùt rog<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </measure> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>zwein mùt ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ei<choice>
                    <abbr>gˀ</abbr>
                    <expan>ger</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> huͦnr gent die sel<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> lùte alz gewonlich ist<reg>,</reg> <reg type="star">✳</reg> umb <measure>hun<choice>
                        <abbr>dˀt</abbr>
                        <expan>dert</expan>
                    </choice> phunt</measure> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501237456711_1035" n="N007"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>si<choice>
                        <abbr>bē</abbr>
                        <expan>ben</expan>
                    </choice> phunt</measure> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>fùnf schillinge <choice>
                        <abbr>pfē</abbr>
                        <expan>pfen</expan>
                    </choice>
                    <choice>
                        <abbr>niḡ</abbr>
                        <expan>ning</expan>
                    </choice> Zovin<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> mùntze</measure><reg>,</reg> <reg type="star">✳</reg> dero ich ze mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> noͤ<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> durf<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <reg type="star">✳</reg> von den <choice>
                    <abbr>egn̄</abbr>
                    <expan>egenanten</expan>
                </choice> frowen bin ge<choice>
                    <abbr>wˀt</abbr>
                    <expan>wert</expan>
                </choice> gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1501237463390_1036" n="N008"/>gentzlich<reg>.</reg> <reg type="star">✳</reg> daz selbe gelt gib ich den <choice>
                    <abbr>vorḡ</abbr>
                    <expan>vorgenanten</expan>
                </choice> frowen <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir nachkomen <reg type="star">✳</reg> von mir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> frilich ze niessenne <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ruͦweclich zebesetzenne
        <lb facs="#facs_1_line_1501237468927_1037" n="N009"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze entset<choice>
