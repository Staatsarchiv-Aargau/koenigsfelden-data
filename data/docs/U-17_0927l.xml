      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0927l_r"/>
      <p facs="#facs_1_region_1513086612602_18">
        <lb facs="#facs_1_region_1513086612602_18l1" n="N001"/>Ich<reg>,</reg> <persName ref="per007283">Elisabeth <reg type="star">✳</reg> von Lanndenberg</persName>
                <reg>,</reg> <reg type="star">✳</reg> des <persName ref="per012292">edlenn<reg>,</reg> <reg type="star">✳</reg> straͤnngenn <reg type="star">✳</reg> herrenn Abrechtenns von der Britten Landenberg<reg>,</reg> ritters</persName>
                <reg>,</reg>
        <lb facs="#facs_1_region_1513086612602_18l2" n="N002"/>eliche tochter<reg>,</reg> <reg type="star">✳</reg> thuͤn kundt <reg type="star">✳</reg> unnd zewùssenn allermencklichem<reg>,</reg> <reg type="star">✳</reg> als ich dann ussz sundrenn ursachenn<reg>,</reg> <reg type="star">✳</reg> mich darzuͦ bewe<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1513086612602_18l3" n="N003" break="no"/>gennd<reg>,</reg> <reg type="star">✳</reg> in den willenn kom<choice>
                    <abbr>mēn</abbr>
                    <expan>menn</expan>
                </choice> bin<reg>,</reg> <reg type="star">✳</reg> mich von dem <orgName ref="org002627">gotzhussz Kùngsfeldenn</orgName> zefuͤgenn <reg type="star">✳</reg> unnd den waͤltlichen stàtt
        <lb facs="#facs_1_region_1513086612602_18l4" n="N004"/>anzuͤnem<choice>
                    <abbr>mēn</abbr>
                    <expan>menn</expan>
                </choice>
                <reg>.</reg> <reg type="star">✳</reg> dahar sich nun begaͤbenn<reg>,</reg> <reg type="star">✳</reg> das mit mir <reg type="star">✳</reg> des guͦts halb<reg>,</reg> <reg type="star">✳</reg> so ich an barem geltt<reg>,</reg> <reg type="star">✳</reg> kleidernn <reg type="star">✳</reg> unnd
        <lb facs="#facs_1_region_1513086612602_18l5" n="N005"/>kleinette<choice>
                    <abbr>rn̄</abbr>
                    <expan>ren</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> ouch in libdinngs wisz <reg type="star">✳</reg> in soͤlich gotzhusz gebraucht<reg>,</reg> <reg type="star">✳</reg> sölicher gestaltt verkommen ist <reg type="star">✳</reg> dàmit mich
        <lb facs="#facs_1_region_1513086612602_18l6" n="N006"/>wol benuͤgt<reg>.</reg> <reg type="star">✳</reg> ha<choice>
                    <abbr>rum̄b</abbr>
                    <expan>rummb</expan>
                </choice> mich dàrgaͤgenn <reg type="star">✳</reg> schuldigenn pflichtenn nàch <reg type="star">✳</reg> zuͤ erzoͤugenn<reg>,</reg> <reg type="star">✳</reg> so sag unnd laussenn
        <lb facs="#facs_1_region_1513086612602_18l7" n="N007"/>ich <reg type="star">✳</reg> das obbemeldt gotzhussz<reg>,</reg> <reg type="star">✳</reg> ouch des castenvoͤgtt<reg>,</reg> <reg type="star">✳</reg> verwaltter <reg type="star">✳</reg> unnd inhaber unnd dero nachkommen <reg type="star">✳</reg> fùr
        <lb facs="#facs_1_region_1513086612602_18l8" n="N008"/>mich <reg type="star">✳</reg> unnd mine erbenn <reg type="star">✳</reg> unnd alle die<reg>,</reg> so disz sach mag beruͤrenn<reg>,</reg> <reg type="star">✳</reg> obbemelts mins zuͤ gebrauchten guͦts<reg>,</reg> <reg type="star">✳</reg> libdings<reg>,</reg>
        <lb facs="#facs_1_region_1513086612602_18l9" n="N009"/>ouch kleidernn unnd kleinetternn halb <reg type="star">✳</reg> quitt<reg>,</reg> <reg type="star">✳</reg> ledig <reg type="star">✳</reg> unnd lòssz<reg>.</reg> <reg type="star">✳</reg> also <reg type="star">✳</reg> unnd der manssenn<reg>,</reg> <reg type="star">✳</reg> das sy da<choice>
                    <abbr>rum̄b</abbr>
                    <expan>rummb</expan>
                </choice> <reg type="star">✳</reg> jetz
        <lb facs="#facs_1_region_1513086612602_18l10" n="N010"/>unnd hienach <reg type="star">✳</reg> gantz geruͤwigett <reg type="star">✳</reg> unnd unersuͤcht sin <reg type="star">✳</reg> unnd belibenn<reg>,</reg> <reg type="star">✳</reg> ouch ich dahar <reg type="star">✳</reg> an dasselb gotzhus <reg type="star">✳</reg> dhein witter
        <lb facs="#facs_1_region_1513086612602_18l11" n="N011"/>vord<choice>
                    <abbr>rnn̄g</abbr>
                    <expan>runng</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> anspràch <reg type="star">✳</reg> nach geraͤchtikeitt <reg type="star">✳</reg> habenn <reg type="star">✳</reg> nàch fùrnaͤm<choice>
                    <abbr>mēn</abbr>
                    <expan>menn</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> sunder dasseb gotzhus <reg type="star">✳</reg> min <reg type="star">✳</reg> unnd der minen halb
        <lb facs="#facs_1_region_1513086612602_18l12" n="N012"/>inallweg <reg type="star">✳</reg> unbeladenn <reg type="star">✳</reg> unnd unangevochtenn soͤlle belibenn <reg type="star">✳</reg> ungehindrett des alles <reg type="star">✳</reg> so ich darwider <reg type="star">✳</reg> zuͤ hilff <reg type="star">✳</reg> schirm und
        <lb facs="#facs_1_region_1513086612602_18l13" n="N013"/>hanndthabung <reg type="star">✳</reg> bruchenn<reg>,</reg> <reg type="star">✳</reg> dàmit ich das obbemeldt gotzhusz <reg type="star">✳</reg> oder jemand von minet waͤgenn <reg type="star">✳</reg> unruͤwigenn <reg type="star">✳</reg> unnd an<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1513086612602_18l14" n="N014" break="no"/>spraͤchenn moͤchtenn<reg>,</reg> <reg type="star">✳</reg> dann ich mich <reg type="star">✳</reg> fùr mich <reg type="star">✳</reg> unnd die minen <reg type="star">✳</reg> desselbenn<reg>,</reg> <reg type="star">✳</reg> ouch aller fùnd uszzùg <reg type="star">✳</reg> unnd gevaͤrdenn<reg>,</reg>
        <lb facs="#facs_1_region_1513086612602_18l15" n="N015"/>dispensationen<reg>,</reg> <reg type="star">✳</reg> ouch geistlicher <reg type="star">✳</reg> unnd waͤltlicher fri<choice>
                    <abbr>gnn̄g</abbr>
                    <expan>gunng</expan>
                </choice> <reg type="star">✳</reg> unnd fùrsaͤ<choice>
                    <abbr>chnn̄g</abbr>
                    <expan>chunng</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> wie <reg type="star">✳</reg> oder ussz was grunds <reg type="star">✳</reg> die uszgebràcht
        <lb facs="#facs_1_region_1513086612602_18l16" n="N016"/>unnd erlanngtt moͤchtenn werdenn<reg>,</reg> <reg type="star">✳</reg> unnd besunder ouch des rechtenn<reg>,</reg> <reg type="star">✳</reg> so ein gmeine verzi<choice>
                    <abbr>chnn̄g</abbr>
                    <expan>chunng</expan>
                </choice> <reg type="star">✳</reg> àn vorganng
        <lb facs="#facs_1_region_1513086612602_18l17" n="N017"/>einer sunderbarnn <reg type="star">✳</reg> als unnùtz widerspricht <reg type="star">✳</reg> entzigenn <reg type="star">✳</reg> unnd begaͤbenn hab<reg>,</reg> <reg type="star">✳</reg> alles in uffrechtenn<reg>,</reg> guͤttenn trùwen
        <lb facs="#facs_1_region_1513086612602_18l18" n="N018"/>unnd namlich mit hannd <reg type="star">✳</reg> unnd gewalt miner frùndtschafft <reg type="star">✳</reg> gehanndlet <reg type="star">✳</reg> unnd volzogenn<reg>.</reg> <reg type="star">✳</reg> das ouch wir<reg>,</reg> <persName ref="per012295">Goͤtt<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1513086612602_18l19" n="N019" break="no"/>hartt</persName> <reg type="star">✳</reg> unnd <persName ref="per012296 \u0009">Michell <reg type="star">✳</reg> von der Breittenn Lanndenberg</persName>
                <reg>,</reg> <reg type="star">✳</reg> innamen unnser bruͦders <reg type="star">✳</reg> unnd her<choice>
                    <abbr>rēn</abbr>
                    <expan>renn</expan>
                </choice> vatters <reg type="star">✳</reg> beken<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice>
                <reg>,</reg>
        <lb facs="#facs_1_region_1513086612602_18l20" n="N020"/>sölicher gstalt beschaͤchenn <reg type="star">✳</reg> unnd disz alles <reg type="star">✳</reg> mit unnserm gunst<reg>,</reg> <reg type="star">✳</reg> wùssenn <reg type="star">✳</reg> unnd willenn <reg type="star">✳</reg> beschlossenn sin<reg>,</reg> <reg type="star">✳</reg> unnd haben
        <lb facs="#facs_1_region_1513086612602_18l21" n="N021"/>ouch dem allen zuͤ bestannd <reg type="star">✳</reg> unnd guͤtter sicherheitt <reg type="star">✳</reg> uff bittlich ersuͦchenn <reg type="star">✳</reg> der obbemeldttenn <persName ref="per007283">fròwenn
        <lb facs="#facs_1_region_1513086612602_18l22" n="N022"/>Elisabettenn</persName> <reg type="star">✳</reg> unnd <persName ref="per012296">Michels von Landenberg</persName>
                <reg>,</reg> <reg type="star">✳</reg> miner liebenn basenn <reg type="star">✳</reg> unnd vettrens<reg>,</reg> <reg type="star">✳</reg> unnd sunders inna<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1513086612602_18l23" n="N023" break="no"/>men mines bruͤders<reg>,</reg> obbemeldt<reg>,</reg> <reg type="star">✳</reg> ich<reg>,</reg> gemeldtter <persName ref="per012295">Goͤtthart von Landenberg</persName>
                <reg>,</reg> <reg type="star">✳</reg> disernn brieff <reg type="star">✳</reg> mit minem anha<reg type="lb">¬</reg>
        <lb facs="#facs_1_line_1564053386865_575" n="N024" break="no"/>ngendenn sigell verwartt<reg>,</reg> <reg type="star">✳</reg> uffrichtenn <reg type="star">✳</reg> mir <reg type="star">✳</reg> unnd minen erbenn <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1564053187514_564" n="N025"/>
                <add place="above">
                    <reg type="star">✳</reg> nit <reg type="star">✳</reg>
                </add>
        <lb facs="#facs_1_line_1564053386864_574" n="N026"/>àn schadenn <reg type="star">✳</reg> unnd zuͤ handen des beruͤrt<choice>
                    <abbr>t₎</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_region_1513086612602_18l25" n="N027"/>
