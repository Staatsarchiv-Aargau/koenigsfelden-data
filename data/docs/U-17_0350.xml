      <pb facs="#facs_2" n="verso" xml:id="img_0002" source="U-17_0350_v"/>
      <p facs="#facs_2_region_1578658610830_5035" rend="dorsual_06_doppel">
        <lb facs="#facs_2_line_1578658613961_5038" n="N001"/>der brief als uns des <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> Wollen
        <lb facs="#facs_2_line_1578658616721_5043" n="N002"/>guͦt ze aigen geben ist</p>
      <p facs="#facs_2_region_1578658628418_5049" rend="dorsual_08_auftakt">
        <lb facs="#facs_2_line_1578658632356_5052" n="N001"/>
