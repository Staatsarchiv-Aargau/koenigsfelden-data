  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0266_01_r"/>
      <p facs="#facs_1_region_1501227135218_212">
        <lb facs="#facs_1_line_1501227141932_213" n="N001"/>
                <persName ref="per000347">Wir<reg>,</reg> Al<choice>
                        <abbr>br̄</abbr>
                        <expan>brecht</expan>
                    </choice>
                    <reg>,</reg> von gots gna<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzog ze Oͤste<choice>
                        <abbr>rˀ</abbr>
                        <expan>rich</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> ze Sty<choice>
                        <abbr>rˀ</abbr>
                        <expan>re</expan>
                    </choice> <reg type="star">✳</reg> und ze Kern<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> herr ze Chrein<reg>,</reg> uf der Marich und ze Portenowͤ<reg>,</reg> <reg type="star">✳</reg> graf ze Habspurg <reg type="star">✳</reg> und
        <lb facs="#facs_1_line_1501227147405_214" n="N002"/>ze Ky<choice>
                        <abbr>bgͦ</abbr>
                        <expan>beͦrg</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> lantgraf ze <choice>
                        <abbr>Obˀn</abbr>
                        <expan>Obern</expan>
                    </choice> Elsazze <reg type="star">✳</reg> und <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>re ze Phirt</persName>
                <reg>,</reg> <reg type="star">✳</reg> embieten allen christan menschen<reg>,</reg> die disen brief <reg type="star">✳</reg> sehen oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> nu und hie nach<reg>,</reg> un<choice>
                    <abbr>seˀn</abbr>
                    <expan>sern</expan>
                </choice> gruͤzz
        <lb facs="#facs_1_line_1501227152893_215" n="N003"/>und erkantnuͤzze<reg>,</reg> <reg type="star">✳</reg> di<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> nachgeschribe<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> dinge<reg>.</reg> <reg type="star">✳</reg> es zimt wol durchluͤchtigem adel <reg type="star">✳</reg> und fùrstli<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> chlarheit<reg>,</reg> geistlich zuͤcht <reg type="star">✳</reg> und alle gotzhùser under sinem gewalt und
        <lb facs="#facs_1_line_1501227159460_216" n="N004"/>sun<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>lich si<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> vor<choice>
                    <abbr>deˀn</abbr>
                    <expan>dern</expan>
                </choice> und sin sel<choice>
                    <abbr>bˀs</abbr>
                    <expan>bers</expan>
                </choice> stiftung<reg>,</reg> <reg type="star">✳</reg> ze wir<choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> und mit sunderlichen friheiten und eren gnediklich ze beden<choice>
                    <abbr>chen̄</abbr>
                    <expan>chenn</expan>
                </choice>
                <reg>,</reg> dar umb<reg>,</reg> <reg type="star">✳</reg> wan rili<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice> und seliger gedaͤchtnuzze<reg>,</reg>
        <lb facs="#facs_1_line_1501227166164_217" n="N005"/>die aller <persName ref="per007254">durchluchtigest frowͤ Elizabet von Kerndn<reg>,</reg> <reg type="star">✳</reg> etzwennͤ Roͤmische kuniginn<reg>,</reg> <reg type="star">✳</reg> unser liebe frowͤ</persName> und muͤ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>
                <reg>,</reg> und nach ir ytzund<reg>,</reg> <reg type="star">✳</reg> die <persName ref="per006964">hochgeborn<reg>,</reg> unser liebe swes<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501227171532_218" n="N006"/>frowͤ Agnes<reg>,</reg> wilunt kuniginn ze Un<choice>
                        <abbr>gˀen</abbr>
                        <expan>gern</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> mit gunst<reg>,</reg> hilf und willen unser <reg type="star">✳</reg> und guͤ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> gedaͤchtnuzze <reg type="star">✳</reg> uns<choice>
                    <abbr>rˀ</abbr>
                    <expan>rer</expan>
                </choice> bruͤder<reg>,</reg> seligen<reg>,</reg> <reg type="star">✳</reg> <orgName ref="org000088">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzog ze Oͤste<choice>
                        <abbr>rˀ</abbr>
                        <expan>rich</expan>
                    </choice>
                </orgName>
                <reg>,</reg> den got genade<reg>,</reg> <reg type="star">✳</reg> gestiftet <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1501227179163_219" n="N007"/>gebawͤn <reg type="star">✳</reg> und bewidemt habent <reg type="star">✳</reg> in un<choice>
                    <abbr>sˀm</abbr>
                    <expan>serm</expan>
                </choice> lande ze <placeName ref="loc000122">Ergowͤ</placeName>
                <reg>,</reg> <reg type="star">✳</reg> daz <orgName ref="org002627">chlos<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> ze Kungsvelt<reg>,</reg> Sant Claren ordens<reg>,</reg> <reg type="star">✳</reg> in Chostentzer bystuͦm</orgName>
                <reg>,</reg> <reg type="star">✳</reg> gote zelob <reg type="star">✳</reg> und allen unseren
        <lb facs="#facs_1_line_1501227187252_220" n="N008"/>vor<choice>
                    <abbr>deˀn</abbr>
                    <expan>dern</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> unse<choice>
                    <abbr>rˀ</abbr>
                    <expan>rer</expan>
                </choice> und un<choice>
                    <abbr>sˀr</abbr>
                    <expan>ser</expan>
                </choice> erben selen ze troste<reg>,</reg> <reg type="star">✳</reg> so wizzen alle und besun<choice>
