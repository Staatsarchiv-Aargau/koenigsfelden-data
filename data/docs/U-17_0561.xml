  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0561_r"/>
      <p facs="#facs_1_region_1517169411112_10">
        <lb facs="#facs_1_region_1517169411112_10l1" n="N001"/>Ich<reg>,</reg> <persName ref="per026096">Dietrich Guͦtjar<reg>,</reg> zuͦ dien zy<choice>
                        <abbr>tn̄</abbr>
                        <expan>ten</expan>
                    </choice> lùt<choice>
                        <abbr>pˀes</abbr>
                        <expan>pries</expan>
                    </choice>ter zuͦ Togerren</persName><reg>,</reg> tuͦn kunt allermenglichem mit disem briefe<reg>,</reg> als die <orgName ref="org002627">eerwirdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice><reg>,</reg> geischli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice>
        <lb facs="#facs_1_region_1517169411112_10l2" n="N002"/>froͧwen<reg>,</reg> min gnedige froͧwe<reg>,</reg> die aͤptissin <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> der convent des closters zuͦ Kùngsveld</orgName><reg>,</reg> mir die sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> kilchen zuͦ <placeName ref="loc012193">Tog<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>ren</placeName> geli<choice>
                    <abbr>hn̄</abbr>
                    <expan>hen</expan>
                </choice> hant nach
        <lb facs="#facs_1_region_1517169411112_10l3" n="N003"/>lùt <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sag der <choice>
                    <abbr>pn̄</abbr>
                    <expan>presen</expan>
                </choice>ta<choice>
                    <abbr>tōn</abbr>
                    <expan>tion</expan>
                </choice><reg>,</reg> so ich dar umb <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> hab<reg>,</reg> dz ich da mit wolbedahtem muͦte <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mit guͦtem räte fùr mich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alle min er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice>
        <lb facs="#facs_1_region_1517169411112_10l4" n="N004"/>geordnet <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> vermacht hab den <choice>
                    <abbr>egñ</abbr>
                    <expan>egenan</expan>
                </choice>
                <choice>
                    <abbr>t₎</abbr>
                    <expan>ten</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> gnedi<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> froͧwen und al<choice>
                    <abbr>lē</abbr>
                    <expan>len</expan>
                </choice> iren nachko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice><reg>,</reg> ob dz beschehe<reg>,</reg> dz ich <del rend="strikethrough">dz ich</del> uff und by der
        <lb facs="#facs_1_region_1517169411112_10l5" n="N005"/>sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> kilchen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> lùt<choice>
                    <abbr>pˀs</abbr>
                    <expan>pries</expan>
                </choice>
                <choice>
                    <abbr>tˀye</abbr>
                    <expan>terye</expan>
                </choice> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> todes we<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> abgienge<reg>,</reg> wer sich <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> mines guͦtes <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> erbes un<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>ziehen wurde<reg>,</reg> der sol vor al<choice>
                    <abbr>lē</abbr>
                    <expan>len</expan>
                </choice> dingen
        <lb facs="#facs_1_region_1517169411112_10l6" n="N006"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> vor aller schulde<reg>,</reg> so ich gelten sölte<reg>,</reg> den sel<choice>
                    <abbr>bn̄</abbr>
