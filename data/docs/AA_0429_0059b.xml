        <lb facs="#facs_103_r1l17" n="N001"/>
                <hi rend="rubricated">Wie die bruͤder durch min fro<choice>
                        <abbr>wn̄</abbr>
                        <expan>wen</expan>
                    </choice> sol<choice>
                        <abbr>ln̄</abbr>
                        <expan>len</expan>
