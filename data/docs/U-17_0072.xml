  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0072_r"/>
      <p facs="#facs_1_region_1514992835744_4">
        <lb facs="#facs_1_region_1514992835744_4l1" n="N001"/>Wir<reg>,</reg> <persName ref="per020051">Adelheit<reg>,</reg> von gottes gnaden ebtischin <reg type="star">✳</reg> ze Sekingen</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> daz <orgName ref="org001209">capital</orgName> gemeinlich<reg>,</reg> tuͦn kùnt <reg type="star">✳</reg> allen den<reg>,</reg> <reg type="star">✳</reg> die di<choice>
                    <abbr>sē</abbr>
                    <expan>sen</expan>
                </choice>
        <lb facs="#facs_1_region_1514992835744_4l2" n="N002"/>brief sehent <reg type="star">✳</reg> oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> daz wir verluhen haben dien <orgName ref="org002627">erwirdegen vroͧwen<reg>,</reg> <reg type="star">✳</reg> der ebtischin <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent
        <lb facs="#facs_1_region_1514992835744_4l3" n="N003"/>von Kungesvelt</orgName>
                <reg>,</reg> <reg type="star">✳</reg> den zehenden <reg type="star">✳</reg> ze <placeName ref="loc010034">Òtwissingen</placeName>
                <reg>,</reg> <reg type="star">✳</reg> ze <placeName ref="loc009457">Henschinkon</placeName> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze <placeName ref="loc009903">Totinchon</placeName>
                <reg>,</reg> <reg type="star">✳</reg> den <persName ref="per013767">Marchwart Truͧtman<reg>,</reg> <reg type="star">✳</reg> burger
        <lb facs="#facs_1_region_1514992835744_4l4" n="N004"/>ze Lu<choice>
                        <abbr>cˀ</abbr>
                        <expan>cer</expan>
                    </choice>ron</persName>
                <reg>,</reg> von uns ze erbe hatte<reg>,</reg> <reg type="star">✳</reg> den si <reg type="star">✳</reg> von im gekofet hant<reg>,</reg> <reg type="star">✳</reg> ze rechtem erbe <reg type="star">✳</reg> der vorge<choice>
                    <abbr>nā̄</abbr>
                    <expan>nan</expan>
                </choice>ten ebtischin <reg type="star">✳</reg> und
        <lb facs="#facs_1_region_1514992835744_4l5" n="N005"/>dem convent<reg>,</reg> <reg type="star">✳</reg> mit dien gedin<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> daz si <measure>jerlich <reg type="star">✳</reg> zehen muͦt rog<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
