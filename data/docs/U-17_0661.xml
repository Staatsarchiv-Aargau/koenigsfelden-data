  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0661_r"/>
      <p facs="#facs_1_region_1523261172383_4">
        <lb facs="#facs_1_region_1523261172383_4l1" n="N001"/>Ich<reg>,</reg> <persName ref="per011305">Marquart von Baldegg<reg>,</reg> ritter<reg>,</reg> <choice>
                        <abbr>herˀ</abbr>
                        <expan>herr</expan>
                    </choice> ze Schenkemberg</persName><reg>,</reg> be<choice>
                    <abbr>ken̄</abbr>
                    <expan>kenn</expan>
                </choice> offenlich mitt disem brieff<reg>,</reg> <reg type="star">✳</reg> das uff si<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice>
        <lb facs="#facs_1_region_1523261172383_4l2" n="N002"/>dat der <persName ref="per013242">hochge<choice>
                        <abbr>lerˀt</abbr>
                        <expan>lerrt</expan>
                    </choice> und erwirdig her Hartman von Baldegg<reg>,</reg> tuͦm<choice>
                        <abbr>hˀr</abbr>
                        <expan>herr</expan>
                    </choice>
                </persName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice><reg>,</reg> min lieber bruͤ<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice><reg>,</reg> nach mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> bevelh<reg type="lb">¬</reg><lb facs="#facs_1_region_1523261172383_4l3" n="N003" break="no"/>nisse von mins absins wegen <reg type="star">✳</reg> an mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> statt <persName ref="per011487">Uͦlrich Lener von Stilli</persName> und si<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> lechenserben ein vierden
        <lb facs="#facs_1_region_1523261172383_4l4" n="N004"/>teil <reg type="star">✳</reg> ei<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> matten<reg>,</reg> gelegen in <placeName ref="loc012192">Ital</placeName><reg>,</reg> der mittlosten<reg>,</reg> die <choice>
