  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0110_r"/>
      <p facs="#facs_1_region_1500389456123_182">
        <lb facs="#facs_1_line_1500389463687_183" n="N001"/>Allen den<reg>,</reg> die disen brief ansehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> tuͤn kunt <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> verjehen wir<reg>,</reg> <persName ref="per013080">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>ther</expan>
                    </choice> Vasolt<reg>,</reg> der alt vogt von Sekingen</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per028254">Anna<reg>,</reg> sin eli<reg type="hyphen">-</reg><lb facs="#facs_1_line_1500389466758_184" n="N002" break="no"/>che wirtinne</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per028255">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>ther</expan>
                    </choice>
                </persName><reg>,</reg> ir bei<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> sun<reg>,</reg> <reg type="star">✳</reg> <persName ref="per028256">swes<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> Elsi</persName><reg>,</reg> des selben vogtes swes<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028257">Heini</persName><reg>,</reg> des vogtes <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> der selben <persName ref="per028256">swes<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> Elsinun</persName> bruͦ<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>sun<reg>,</reg> <reg type="star">✳</reg> das
        <lb facs="#facs_1_line_1500389469847_185" n="N003"/>wir hein geben ze koͧfende den <orgName ref="org002627">geistlichen<reg>,</reg> er<choice>
                        <abbr>bˀen</abbr>
                        <expan>beren</expan>
                    </choice> froͧwen<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> eptissin <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent<reg>,</reg> <reg type="star">✳</reg> Sant Claren ordens ze Kùngesvelt<reg>,</reg> in Costen<reg type="hyphen">-</reg><lb facs="#facs_1_line_1500389473422_186" n="N004" break="no"/>zer bistem</orgName><reg>,</reg> ane alle geverde <reg type="star">✳</reg> fùr lidig eigen ùnser guͦt<reg>,</reg> das wir hatten in dem dorfe <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in dem banne <reg type="star">✳</reg> ze <placeName ref="loc009758">Schinzenach</placeName><reg>,</reg> <reg type="star">✳</reg> das da
        <lb facs="#facs_1_line_1500389476854_187" n="N005"/>buwent <reg type="star">✳</reg> <reg type="star">✳</reg> <persName ref="per028258">die Lendin</persName> <reg type="star">✳</reg> mit allen den nùtzen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> rehten<reg>,</reg> so dar zuͦ hoͤret<reg>,</reg> als oͧch wir das selb guͤt mit allen dingen unz har besessen
        <lb facs="#facs_1_line_1500389479782_188" n="N006"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> genossen hein<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> giltet jergelich <measure>fùnf mùt kernen</measure><reg>,</reg> <reg type="star">✳</reg> <measure>sehs mùt habern</measure><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ein swin<reg>,</reg> sol gelten <measure>zehen schilling phenning</measure><reg>,</reg> <unclear alternative="vij">
                    <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice>
                </unclear> huͤnr
        <lb facs="#facs_1_line_1500389482814_189" n="N007"/>umb <measure>zwenzig mark silbers<reg>,</reg> genges <choice>
                        <sic>
                            <choice>
                                <abbr>un̄</abbr>
                                <expan>und</expan>
                            </choice> <choice>
                                <abbr>un̄</abbr>
                                <expan>und</expan>
                            </choice>
                        </sic>
                        <corr>und</corr>
                    </choice> gebes Basler gewiht</measure><reg>,</reg> des wir von inen genzelich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gar gewert sin<reg>.</reg> <reg type="star">✳</reg> dis selb vorgenant
        <lb facs="#facs_1_line_1500389492126_190" n="N008"/>guͦt hein wir den vorgeseiten<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <orgName ref="org002627">der eptissin <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> <reg type="star">✳</reg> <reg type="star">✳</reg> dem convent ze Kungesvelt</orgName><reg>,</reg> uf gegeben <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> eigenlich gevertigot mit aller
        <lb facs="#facs_1_line_1500389499447_191" n="N009"/>der gewarsami <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> si<choice>
                    <abbr>chˀ</abbr>
                    <expan>cher</expan>
                </choice>heit<reg>,</reg> <reg type="star">✳</reg> worten <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> werken<reg>,</reg> so dar zuͦ hoert<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> notdurftig was <reg type="star">✳</reg> vor gerihte offenlich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in gerihtes wise<reg>,</reg> <reg type="star">✳</reg> als vor
        <lb facs="#facs_1_line_1500389504167_192" n="N010"/>dem <orgName ref="org006779">
                    <choice>
                        <abbr>schulth̄</abbr>
                        <expan>schultheissen</expan>
                    </choice><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> dem rat <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> den bur<choice>
                        <abbr>gˀn</abbr>
                        <expan>gern</expan>
                    </choice> von Sekingen</orgName> mit gesamnoten urteilen uf dem eit erteilet wart<reg>.</reg> <reg type="star">✳</reg> wir verzihen ùns
        <lb facs="#facs_1_line_1500389507974_193" n="N011"/>oͧch <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> hein ùns verzigen al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice> der ansprach<reg>,</reg> <reg type="star">✳</reg> vorderung<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> rehten<reg>,</reg> <reg type="star">✳</reg> so wir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ùnser erben moͤhten getuͦn umb das vorgenant guͦt
        <lb facs="#facs_1_line_1500389511575_194" n="N012"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gelt ze <placeName ref="loc009758">Schinzenach</placeName> gen den vorgeseiten<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> <orgName ref="org002627">der eptissin <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent ze Kungesvelt</orgName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gen iren nachkomenden<reg>.</reg> <reg type="star">✳</reg> also <reg type="star">✳</reg> das
        <lb facs="#facs_1_line_1500389515367_195" n="N013"/>wir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> erben<reg>,</reg> <reg type="star">✳</reg> si <del>v</del> an dem selben guͦt <reg type="star">✳</reg> nu<reg>,</reg> <reg type="star">✳</reg> oder hie <reg type="star">✳</reg> nach <reg type="star">✳</reg> nie<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> bekùmbern noch irren sulen mit deheinen geistlichen oder welt<reg type="hyphen">-</reg><lb facs="#facs_1_line_1500389519399_196" n="N014" break="no"/>lich gerihten<reg>,</reg> <reg type="star">✳</reg> <choice>
