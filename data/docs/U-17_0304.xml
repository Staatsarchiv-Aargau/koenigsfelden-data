        <lb facs="#facs_2_line_1594802879260_511" n="N001"/>umb dz fuͦder win so die fruͦ<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice>
        <lb facs="#facs_2_line_1594802882301_516" n="N002"/>den <choice>
