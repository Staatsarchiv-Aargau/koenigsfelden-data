      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0472_r"/>
      <p facs="#facs_1_region_1549529968983_25">
        <lb facs="#facs_1_region_1549529968983_25l1" n="N001"/>Ich<reg>,</reg> <persName ref="per008304">gräff <choice>
                        <abbr>Hˀ</abbr>
                        <expan>Her</expan>
                    </choice>man von Sultz<reg>,</reg> miner genedigen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schafft von Ös<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich lantvogt</persName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice>
                <reg>,</reg> tuͦn kund<reg>,</reg> dz fûr mich kam
        <lb facs="#facs_1_region_1549529968983_25l2" n="N002"/>der <persName ref="per010793">veste Hartman von Rûmlang</persName> von diser nächgeschri<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> guͤter wëgen<reg>,</reg> die er in pfandes wise <reg type="star">✳</reg> untz
        <lb facs="#facs_1_region_1549529968983_25l3" n="N003"/>hër von <orgName ref="org000088">mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> genedigen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schafft von Ös<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName>
                <reg>,</reg> vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genant</expan>
                </choice>
                <reg>,</reg> inne gehept und genossen hett<reg>.</reg> dez ersten
        <lb facs="#facs_1_region_1549529968983_25l4" n="N004"/>ze <placeName ref="loc009219">Gebenstorff</placeName> <persName ref="per028622">Heinrich Boswile</persName> git <measure>zwën mûtt kernen</measure>
                <reg>,</reg> U<persName ref="per028623">ͦlrich Boswil</persName> <measure>zwen mût <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>nen</measure>
                <reg>,</reg> <persName ref="per028624">Uͦlrich von Linden<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1549529968983_25l5" n="N005" break="no"/>mag</persName> <measure>zwey viertel roggen</measure> und <measure>zwey viertel haber</measure>
                <reg>.</reg> so denne <persName ref="per028625">Johansz von Rûsz</persName> <measure>zwey viertel roggen</measure>
        <lb facs="#facs_1_region_1549529968983_25l6" n="N006"/>und <persName ref="per028626">
                    <choice>
                        <abbr>Wˀn</abbr>
                        <expan>Wern</expan>
                    </choice>her in der Huͦbe</persName> <measure>sëchz viertel <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>nen</measure> und <measure>sûbenthalb mûtt habren</measure>
                <reg>.</reg> item <persName ref="per028627">Anna Abdorff</persName> <measure>einen
        <lb facs="#facs_1_region_1549529968983_25l7" n="N007"/>mût roggen</measure> und <measure>zwen mût ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure>
                <reg>.</reg> so denne <persName ref="per028628">Heinrich Körnly</persName> <measure>zwey viertel ker<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </measure> und <measure>zwey viertel
        <lb facs="#facs_1_region_1549529968983_25l8" n="N008"/>habren</measure>
                <reg>.</reg> item <persName ref="per028629">Johans Kelner</persName> <measure>zwey viertel <reg type="star">✳</reg> ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure>
                <reg>.</reg> aber <persName ref="per028630">Johans Kelner von Walthusern</persName> <measure>ein schwin<reg>,</reg>
        <lb facs="#facs_1_region_1549529968983_25l9" n="N009"/>dz sol <reg type="star">✳</reg> nùn schil<choice>
                        <abbr>liḡ </abbr>
                        <expan>ling</expan>
                    </choice>gelten</measure>
                <reg>,</reg> da fûr git er <measure>einliff schilling</measure>
                <reg>.</reg> <choice>
                    <abbr>itē</abbr>
                    <expan>item</expan>
                </choice> so git <persName ref="per028631">Uͦlrich Stûdly</persName> <measure>ein schwyn<reg>,</reg> dz sol <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1549529968983_25l10" n="N010"/>öch nùn schilling gelten</measure>
                <reg>,</reg> <reg type="star">✳</reg> da fùr git er <measure>zwoͤlff schilling</measure>
                <reg>.</reg> und offnet<reg>,</reg> wie dz er alle sine rechte<reg>,</reg> <reg type="star">✳</reg> so er
        <lb facs="#facs_1_region_1549529968983_25l11" n="N011"/>zuͦ den ob<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> guͤtern hett <reg type="star">✳</reg> oder haben möcht<reg>,</reg> ze köffende ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> hette dem <persName ref="per027694">
                    <choice>
                        <abbr>frō</abbr>
                        <expan>from</expan>
                    </choice>men<reg>,</reg> vesten <reg type="star">✳</reg> <choice>
                        <abbr>hˀn</abbr>
                        <expan>hern</expan>
                    </choice> Hanman
        <lb facs="#facs_1_region_1549529968983_25l12" n="N012"/>von Rinach</persName> umb <measure>hun<choice>
                        <abbr>dˀt</abbr>
                        <expan>dert</expan>
                    </choice> und sëchzig guldin</measure> und batte mich<reg>,</reg> wend dz alles pfand <choice>
                    <abbr>weˀ</abbr>
                    <expan>were</expan>
                </choice>
                <reg>,</reg> dz ich von
        <lb facs="#facs_1_region_1549529968983_25l13" n="N013"/>der <choice>
                    <abbr>egn̄</abbr>
                    <expan>egenanten</expan>
                </choice> miner <orgName ref="org000088">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schafft von Oͤsterrich</orgName> wëgen mi<choice>
                    <abbr>nē </abbr>
                    <expan>nen</expan>
                </choice>gunst und willen darzuͦ gaͤbe<reg>.</reg> <reg type="star">✳</reg> der bette
        <lb facs="#facs_1_region_1549529968983_25l14" n="N014"/>hab ich in ge<choice>
                    <abbr>wˀtt</abbr>
                    <expan>wertt</expan>
                </choice> und dem ob<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per010793">von Rûmlang</persName> dez verköffens <reg type="star">✳</reg> und dem vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per027694">von Rinach<reg>,</reg> ritter</persName>
                <reg>,</reg>
        <lb facs="#facs_1_region_1549529968983_25l15" n="N015"/>und sinen er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> dez köffes an statt und in namen der vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <orgName ref="org000088">mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> genëdigen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schafft von Ös<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName>
        <lb facs="#facs_1_region_1549529968983_25l16" n="N016"/>und min selbs gegùnt und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gûnst öch dz alles also mit krafft disz brieffz<reg>,</reg> wie ich dz billich tuͦn 
        <lb facs="#facs_1_region_1549529968983_25l17" n="N017"/>sol und mag<reg>,</reg> doch unschëdlich mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <orgName ref="org000088">
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>schafft von Ös<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName> an allen iren rëchten<reg>,</reg> pfand<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1549529968983_25l18" n="N018" break="no"/>schafft und losunge<reg>,</reg> <reg type="star">✳</reg> ze urkûnde besigelt mit minem eigen anhangenden ingesigel<reg>.</reg> <reg type="star">✳</reg> geben
        <lb facs="#facs_1_region_1549529968983_25l19" n="N019"/>an <date when="1406-04-23">sant Goͤrien tag dez järes<reg>,</reg> do man zalte von gottes geburt vierzechen hundert und sëchs jär</date>
                <reg>.</reg>
            </p>
