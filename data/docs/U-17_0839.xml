        <lb facs="#facs_2_line_1581615227570_2841" n="N001"/>j jùchar<choice>
                    <abbr>t₎</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_2_line_1581615232258_2846" n="N002"/>lit vor dem nider thor <reg type="star">✳</reg> am Etspa<choice>
                    <abbr>chˀ</abbr>
