                    </choice>
                </persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028491">Claus von Gundoldingen</persName>
        <lb facs="#facs_1_line_1496217261354_254" n="N012"/>
                <choice>
                    <abbr>un̄</abbr>
