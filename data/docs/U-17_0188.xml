        <lb facs="#facs_1_line_1496137785302_30" n="N003" break="no"/>ster <persName ref="per006964">froͧ Agnes<reg>,</reg> <reg type="star">✳</reg> wilent chùniginn ze Un<choice>
                        <abbr>gˀn</abbr>
                        <expan>gern</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> <measure>fùnf swin geltes</measure>
                <reg>,</reg> <reg type="star">✳</reg> die ab un<choice>
                    <abbr>snˀ</abbr>
                    <expan>sern</expan>
                </choice> mùlinen ze <placeName ref="loc000857">Zovingen</placeName>
        <lb facs="#facs_1_line_1496137788806_31" n="N004"/>gant<reg>,</reg> <reg type="star">✳</reg> dieselben swin <reg type="star">✳</reg> von ùns <reg type="star">✳</reg> <choice>
