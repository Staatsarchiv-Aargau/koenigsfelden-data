      <p facs="#facs_2_region_1533019806384_5" rend="dorsual_05_uber_3">
        <lb facs="#facs_2_line_1533019860131_14" n="N001"/>Ve<choice>
                    <abbr>rī</abbr>
                    <expan>rin</expan>
                </choice>gen</p>
      <p facs="#facs_2_region_1533019812562_7" rend="dorsual_15_textualis">
        <lb facs="#facs_2_line_1533019878607_16" n="N001"/>f ij</p>
    <p facs="#facs_2_region_1533019828246_9" rend="dorsual_20_regesta">
        <lb facs="#facs_2_line_1533019918214_20" n="N001"/>wist <choice>
                    <abbr>um̄</abbr>
                    <expan>umb</expan>
                </choice> das
        <lb facs="#facs_2_line_1533019920720_21" n="N002"/>guͦtt Al<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>bùrg <reg type="star">✳</reg> so
        <lb facs="#facs_2_line_1533019923361_22" n="N003"/>vor der Feringeri was</p>
      <p facs="#facs_2_region_1533019803421_4" rend="dorsual_22_sigle">
        <lb facs="#facs_2_line_1533019835132_10" n="N001"/>K
        <lb facs="#facs_2_line_1533019837766_11" n="N002"/>B <reg type="star">✳</reg> 15</p>
      <p facs="#facs_2_region_1533019810229_6" rend="dorsual_23_sammelschicht_vor">
        <lb facs="#facs_2_line_1533019869900_15" n="N001"/>dz guͦt lit ze Alten<choice>
                    <abbr>bgˀ</abbr>
                    <expan>burg</expan>
                </choice>
            </p>
      <p facs="#facs_2_region_1533019794507_2" rend="dorsual_31_staag">
        <lb facs="#facs_2_line_1533019887338_17" n="N001"/>
                <choice>
                    <abbr>Königsf<reg>.</reg>
                    </abbr>
