                    <expan>und</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vorgenande <persName ref="per010791">Jo<choice>
                        <abbr>hās</abbr>
                        <expan>hans</expan>
