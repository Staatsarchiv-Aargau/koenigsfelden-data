        <lb facs="#facs_1_region_1523477247793_39l1" n="N001"/>Wir nachbenemp<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>
                <reg>,</reg> <persName ref="per011756">Pe<choice>
                        <abbr>terˀ</abbr>
                        <expan>terr</expan>
                    </choice> von Win<choice>
                        <abbr>gaˀt</abbr>
                        <expan>gart</expan>
                    </choice>
                    <choice>
                        <abbr>tn̄</abbr>
                        <expan>ten</expan>
                    </choice>
                    <reg>,</reg> derzit vogtt zuͦ Schenc<choice>
                        <abbr>kē</abbr>
                        <expan>ken</expan>
                    </choice>
                    <choice>
                        <abbr>bgˀ</abbr>
                        <expan>berg</expan>
                    </choice>
                </persName>
                <reg>,</reg> und <persName ref="per016680">Uͦlrich <choice>
                        <abbr>Kilcherˀ</abbr>
                        <expan>Kilcherr</expan>
                    </choice>
                    <reg>,</reg> gesch<reg type="hyphen">-</reg><lb facs="#facs_1_region_1523477247793_39l2" n="N002" break="no"/>wor<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> un<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice>vogtt zuͦ Vilin<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </persName>
                <reg>,</reg> tuͦnd kundt mengkli<choice>
                    <abbr>chm̄</abbr>
                    <expan>chem</expan>
                </choice> mitt disem brieff<reg>,</reg> das wir mit gunst<reg>,</reg> wùs<choice>
                    <abbr>sn̄</abbr>
                    <expan>sen</expan>
                </choice>
        <lb facs="#facs_1_region_1523477247793_39l3" n="N003"/>und wil<choice>
                    <abbr>ln̄</abbr>
                    <expan>len</expan>
                </choice> und durch <choice>
                    <abbr>mittll̄</abbr>
                    <expan>mittllen</expan>
                </choice> des <persName ref="per016683">hochwirdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> geystli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> her<choice>
                        <abbr>rn̄</abbr>
                        <expan>ren</expan>
                    </choice>
                    <reg>,</reg> <choice>
                        <abbr>herˀ</abbr>
                        <expan>herr</expan>
                    </choice> Heinrichs Ster<choice>
                        <abbr>rn̄</abbr>
                        <expan>ren</expan>
                    </choice>
                    <reg>,</reg> derzit gardy<reg type="lb">¬</reg><lb facs="#facs_1_region_1523477247793_39l4" n="N004" break="no"/>ans zuͦ Kùngsvel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </persName>
                <reg>,</reg> die <orgName ref="org002628">erwirdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> geystli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> he<choice>
                        <abbr>rn̄</abbr>
                        <expan>ren</expan>
                    </choice> gemeinen covent zuͦ Kùngsvel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName>
                <reg>,</reg> <persName ref="per016681">Martin
        <lb facs="#facs_1_region_1523477247793_39l5" n="N005"/>Hoͤus<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> von Ursprung<reg>,</reg> irn zinsz man</persName>
                <reg>,</reg> an einem<reg>,</reg> und <persName ref="per016682">Clausz Stoͤu<choice>
                        <abbr>bn̄</abbr>
                        <expan>ben</expan>
                    </choice> von Villnachern</persName> andersteyls
        <lb facs="#facs_1_region_1523477247793_39l6" n="N006"/>der irrung und anvord<choice>
                    <abbr>rūg</abbr>
                    <expan>rung</expan>
                </choice> der <measure>achtze<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> guldin</measure> halb<reg>,</reg> harruͤoernde von we<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> dessz hofs zuͦ
        <lb facs="#facs_1_region_1523477247793_39l7" n="N007"/>
                <placeName ref="loc011255">Ursprung</placeName>
                <reg>,</reg> innachvolgen<choice>
                    <abbr>deˀ</abbr>
