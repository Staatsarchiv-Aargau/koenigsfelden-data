  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0740_r_01"/>
      <p facs="#facs_1_region_1523222443183_18">
        <lb facs="#facs_1_region_1523222443183_18l1" n="N001"/>Ich<reg>,</reg> <persName ref="per011255">Jaͤggli Huͤpschˀ von Buͤblikon</persName>
                <reg>,</reg> bekenn offenlich mit
        <lb facs="#facs_1_region_1523222443183_18l2" n="N002"/>di<choice>
                    <abbr>sm̄</abbr>
                    <expan>sem</expan>
                </choice> brieff<reg>,</reg> dz ich <measure>ein juchart ackers</measure> ze <placeName ref="loc009167">Wo<choice>
                        <abbr>ln̄</abbr>
                        <expan>len</expan>
                    </choice>schwile</placeName>
                <reg>,</reg>
        <lb facs="#facs_1_region_1523222443183_18l3" n="N003"/>gele<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> in dem byfang<reg>,</reg> stost ein sit an die strassz<reg>,</reg> die
        <lb facs="#facs_1_region_1523222443183_18l4" n="N004"/>hinder <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> kil<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> hin abgat<reg>,</reg> <reg type="star">✳</reg> ander sit an des <persName ref="per011751">Moͤrin<choice>
                        <abbr>gsˀ</abbr>
                        <expan>gers</expan>
                    </choice>
                </persName>
        <lb facs="#facs_1_region_1523222443183_18l5" n="N005"/>guͦt<reg>,</reg> ist le<choice>
                    <abbr>hn̄</abbr>
                    <expan>hen</expan>
                </choice> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> <placeName ref="loc000390">Hapspurg</placeName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> dem <persName ref="per011224">fro<choice>
                        <abbr>mē</abbr>
                        <expan>men</expan>
                    </choice>
                    <reg>,</reg> ves<choice>
                        <abbr>tn̄</abbr>
                        <expan>ten</expan>
                    </choice> <choice>
                        <sic>
                            <unclear>judck<choice>
                                    <abbr>heˀ</abbr>
                                    <expan>herrn</expan>
                                </choice>
                            </unclear>
                        </sic>
                        <corr>junckherr</corr>
                    </choice>
                    <reg>,</reg>
        <lb facs="#facs_1_region_1523222443183_18l6" n="N006"/>Hans Arnol<choice>
                        <abbr>d₎</abbr>
                        <expan>den</expan>
                    </choice> Segiser</persName>
                <reg>,</reg> minez liebn̄ hernˀ<reg>,</reg> ze rech<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> manle<choice>
                    <abbr>ch̄</abbr>
                    <expan>chen</expan>
                </choice>
                <reg>,</reg>
        <lb facs="#facs_1_region_1523222443183_18l7" n="N007"/>nach innhalt des le<choice>
                    <abbr>hn̄</abbr>
                    <expan>hen</expan>
                </choice> brieffs<reg>,</reg> <reg type="star">✳</reg> den ich <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> im enpfan<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg>
        <lb facs="#facs_1_region_1523222443183_18l8" n="N008"/>im ouch gelopt <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gesworn hab <reg type="star">✳</reg> da<choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> <choice>
