      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0935a_r"/>
      <p facs="#facs_1_region_1512220239788_32">
        <lb facs="#facs_1_region_1512220239788_32l1" n="N001"/>Ich<reg>,</reg> <persName ref="per013620">Heinricus Schad<reg>,</reg> <reg type="star">✳</reg> von Vilinngen ussz Schwàbenn<reg>,</reg> <reg type="star">✳</reg> yetz zuͤ Stràszburg seshafft</persName>
                <reg>,</reg> <reg type="star">✳</reg> bekenn <reg type="star">✳</reg> unnd
        <lb facs="#facs_1_region_1512220239788_32l2" n="N002"/>thuͤn kundt allmencklichem offennlich mit disem brieff<reg>,</reg> <reg type="star">✳</reg> das ich in voͤgttlicher wisz <reg type="star">✳</reg> mit vollem
        <lb facs="#facs_1_region_1512220239788_32l5" n="N003"/>gwaltt <reg type="star">✳</reg> unnd als ein elicher gmachel <reg type="star">✳</reg> <persName ref="per007136">Elenenn Rinndsmùllin<reg>,</reg> <reg type="star">✳</reg> vormàls zuͦ Kùngsfelden gwäsenn</persName>
                <reg>,</reg>
        <lb facs="#facs_1_region_1512220239788_32l3" n="N004"/>uff hùtt dato <reg type="star">✳</reg> von der <persName ref="per012283">wolgebo<choice>
                        <abbr>rn̄</abbr>
                        <expan>ren</expan>
                    </choice>nen<reg>,</reg> <reg type="star">✳</reg> edlenn <reg type="star">✳</reg> unnd geistlichenn <reg type="star">✳</reg> froͧwenn Katherinen Truchsaͤssin
        <lb facs="#facs_1_region_1512220239788_32l4" n="N005"/>von Waldtpurg<reg>,</reg> aͤpptissinn zuͤ Kuͧngesfeldenn</persName>
                <reg>,</reg> <reg type="star">✳</reg> ussz gheisz <reg type="star">✳</reg> unnd bevaͤlch <reg type="star">✳</reg> der <orgName ref="org004002">gròszmechtigenn<reg>,</reg> <reg type="star">✳</reg> edlen<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1512220239788_32l6" n="N006"/>straͤnngen<reg>,</reg> <reg type="star">✳</reg> fùrsichtigenn<reg>,</reg> <reg type="star">✳</reg> from<choice>
                        <abbr>mēn</abbr>
                        <expan>menn</expan>
                    </choice> <reg type="star">✳</reg> unnd wysenn <reg type="star">✳</reg> miner gnaͤdigenn herrenn von Bernn</orgName> <reg type="star">✳</reg> und des
        <lb facs="#facs_1_region_1512220239788_32l7" n="N007"/>
                <persName ref="per012152">fùrnaͤmen<reg>,</reg> wysenn <reg type="star">✳</reg> Benedictenn Matstetters<reg>,</reg> ir gnàdenn amptmans vermelts gotzhussz</persName>
                <reg>,</reg> <reg type="star">✳</reg> bar ingno<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1512220239788_32l8" n="N008" break="no"/>menn <reg type="star">✳</reg> unnd empfangenn hab <reg type="star">✳</reg> <measure>hundertt Rinischer guldinn<reg>,</reg> <reg type="star">✳</reg> namlich fùr yedenn sibentzä<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice>hal<choice>
                        <abbr>bn̄</abbr>
                        <expan>ben</expan>
                    </choice>
        <lb facs="#facs_1_region_1512220239788_32l9" n="N009"/>schwitzer batzenn gezaltt</measure>
                <reg>,</reg> <reg type="star">✳</reg> houptgults<reg>,</reg> <reg type="star">✳</reg> die iro bishar von vermàldtenn gotzhussz <reg type="star">✳</reg> in libdinngs wisz mit
        <lb facs="#facs_1_region_1512220239788_32l10" n="N010"/>
                <measure>fùnff Rinischenn guldinn</measure> <reg type="star">✳</reg> <date when="0001-11-11">Martiny vellig <reg type="star">✳</reg> järlich</date> verzinnst sinnd <choice>
                    <abbr>ꝫꝭ</abbr>
                    <expan>et cetera</expan>
                </choice>
                <reg>.</reg> unnd so ich nu ob<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1512220239788_32l11" n="N011" break="no"/>anzoͤugtter <choice>
                    <abbr>sum̄</abbr>
                    <expan>summe</expan>
                </choice> g<unclear>o</unclear>lds innam<choice>
                    <abbr>ēn</abbr>
                    <expan>men</expan>
                </choice> <reg type="star">✳</reg> unnd zuͦ handenn <reg type="star">✳</reg> min <reg type="star">✳</reg> unnd miner elichenn huszfròwenn <reg type="star">✳</reg> bar <reg type="star">✳</reg> wol
        <lb facs="#facs_1_region_1512220239788_32l12" n="N012"/>bezaltt unnd contentiert bin <reg type="star">✳</reg> nàch minem willenn <reg type="star">✳</reg> unnd guͤttenn benuͤgenn <choice>
                    <abbr>ꝫꝭ</abbr>
                    <expan>et cetera</expan>
                </choice>
                <reg>.</reg> ha<choice>
                    <abbr>rum̄b</abbr>
                    <expan>rummb</expan>
                </choice> so zell <reg type="star">✳</reg> unnd sag
        <lb facs="#facs_1_region_1512220239788_32l13" n="N013"/>ich <reg type="star">✳</reg> als billich <reg type="star">✳</reg> min gnaͤdig herrenn<reg>,</reg> obbemeldt schult<choice>
                    <abbr>h̄es</abbr>
                    <expan>heiss</expan>
                </choice> <reg type="star">✳</reg> unnd raͤtt gaͤgenwirtig <reg type="star">✳</reg> unnd kùnfftig<reg>,</reg> <reg type="star">✳</reg> iro
        <lb facs="#facs_1_region_1512220239788_32l14" n="N014"/>gnàdenn <orgName ref="org002627">gotzhussz Kùngsfeldenn</orgName>
                <reg>,</reg> <reg type="star">✳</reg> òch des amptlùtt <reg type="star">✳</reg> gaͤgenwirtig <reg type="star">✳</reg> unnd kùnfftig <reg type="star">✳</reg> fùr mich<reg>,</reg> <reg type="star">✳</reg> gmelte min
        <lb facs="#facs_1_region_1512220239788_32l15" n="N015"/>eliche husfroͧwenn<reg>,</reg> <reg type="star">✳</reg> unnser beder erbenn <reg type="star">✳</reg> unnd nàchkom<choice>
                    <abbr>mēn</abbr>
                    <expan>menn</expan>
                </choice> <reg type="star">✳</reg> unnd mencklichs von unnse<unclear>rt</unclear> waͤgenn <reg type="star">✳</reg> soͤ<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1512220239788_32l16" n="N016" break="no"/>licher <measure>hundertt guldinenn</measure>
                <reg>,</reg> <reg type="star">✳</reg> aller libdinng zinnsenn<reg>,</reg> davon gevallenn<reg>,</reg> <reg type="star">✳</reg> ouch andrer vordrungen <reg type="star">✳</reg> unnd
        <lb facs="#facs_1_region_1512220239788_32l17" n="N017"/>anspràchenn<reg>,</reg> <reg type="star">✳</reg> wie ich<reg>,</reg> <reg type="star">✳</reg> min huszfroͧw <reg type="star">✳</reg> oder unnsre erbenn<reg>,</reg> <reg type="star">✳</reg> die jemmer fùrnaͤmmen moͤchtennd<reg>,</reg> <reg type="star">✳</reg> gantz
        <lb facs="#facs_1_region_1512220239788_32l18" n="N018"/>quitt<reg>,</reg> <reg type="star">✳</reg> unanspraͤchig<reg>,</reg> <reg type="star">✳</reg> ledig <reg type="star">✳</reg> unnd lössz <choice>
                    <abbr>ꝫꝭ</abbr>
                    <expan>et cetera</expan>
                </choice>
                <reg>.</reg> unnd ob <choice>
                    <abbr>um̄b</abbr>
                    <expan>ummb</expan>
                </choice> die angezoͤugtenn <reg type="star">✳</reg> unnd bezalltenn hundert
        <lb facs="#facs_1_region_1512220239788_32l19" n="N019"/>guldinn <reg type="star">✳</reg> oder annders<reg>,</reg> <reg type="star">✳</reg> jetz <reg type="star">✳</reg> oder hienàch<reg>,</reg> <reg type="star">✳</reg> brieff<reg>,</reg> <reg type="star">✳</reg> sigell <reg type="star">✳</reg> oder annder gwarsam<choice>
                    <abbr>ēn</abbr>
                    <expan>men</expan>
                </choice> erfunden wurdennd<reg>,</reg>
        <lb facs="#facs_1_region_1512220239788_32l20" n="N020"/>die sollend unnserthalb <reg type="star">✳</reg> unnd unser erbenn <reg type="star">✳</reg> gantz krafftlos<reg>,</reg> <reg type="star">✳</reg> unnùtz<reg>,</reg> <reg type="star">✳</reg> tòd<reg>,</reg> <reg type="star">✳</reg> hin <reg type="star">✳</reg> unnd ab sin <reg type="star">✳</reg> unnd unnsz zuͤ
        <lb facs="#facs_1_region_1512220239788_32l21" n="N021"/>dheiner gwerd erschiessen<reg>,</reg> <reg type="star">✳</reg> alles inuffraͤchteren<reg>,</reg> guͦttenn trùwen <reg type="star">✳</reg> unnd crafft disz brieffs<reg>,</reg> <reg type="star">✳</reg> der des
        <lb facs="#facs_1_region_1512220239788_32l22" n="N022"/>zuͤ wàrem <reg type="star">✳</reg> unnd vestenn urkùnd<reg>,</reg> <reg type="star">✳</reg> durch miner bitt willenn <reg type="star">✳</reg> ussz gebrust eigenns sigels<reg>,</reg> <reg type="star">✳</reg> mit desz <persName ref="per013569">ersam<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1512220239788_32l23" n="N023" break="no"/>men<reg>,</reg> wysenn <reg type="star">✳</reg> Hannsen Bùrrers<reg>,</reg> <reg type="star">✳</reg> altt hoffmeister zuͤ Kùngsfeldenn<reg>,</reg> <reg type="star">✳</reg> burger zuͤ Brugg</persName>
                <reg>,</reg> eignem angehaͤ<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1512220239788_32l24" n="N024" break="no"/>ncktenn sigell<reg>,</reg> <reg type="star">✳</reg> im <reg type="star">✳</reg> unnd sinen erbenn àn schadenn<reg>,</reg> <reg type="star">✳</reg> mich<reg>,</reg> <reg type="star">✳</reg> min eliche h<unclear alternative="uu">u</unclear>szfroͧwenn <reg type="star">✳</reg> unnd unnsre
        <lb facs="#facs_1_region_1512220239788_32l25" n="N025"/>erbenn<reg>,</reg> <reg type="star">✳</reg> alles desz zeùbersagenn<reg>,</reg> <reg type="star">✳</reg> so obstàtt <choice>
                    <abbr>ꝫꝭ</abbr>
                    <expan>et cetera</expan>
                </choice> besiglett <reg type="star">✳</reg> unnd verwart<reg>,</reg> gebenn ist <reg type="star">✳</reg> uff den <date when="1527-01-05">heligenn dryg
        <lb facs="#facs_1_region_1512220239788_32l26" n="N026"/>kùngenn abennd<reg>,</reg> <reg type="star">✳</reg> was der fùnfft tag jenners<reg>,</reg> <reg type="star">✳</reg> von der geburt <choice>
                        <abbr>xpī</abbr>
                        <expan>Christi</expan>
                    </choice>
                    <reg>,</reg> unnsers herrenn<reg>,</reg> gezaltt fùnff<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1512220239788_32l27" n="N027" break="no"/>tzaͤchennhundertt <reg type="star">✳</reg> zwaͤntzig unnd sibenn jàr</date>
                <reg>.</reg>
            </p>
