  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0052_r"/>
      <p facs="#facs_1_region_1495619716122_34">
        <lb facs="#facs_1_line_1495619721803_35" n="N001"/>Wir<reg>,</reg> <persName ref="per000342">Lûpolt<reg>,</reg> von gotz gnaden <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzog ze Osterrich und ze Stir</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jehen und tuͦn kunt allen den<reg>,</reg>
        <lb facs="#facs_1_line_1495619725304_36" n="N002"/>die disen brief ansehent und horent lesen<reg>,</reg> <reg type="star">✳</reg> daz wir von un<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> und uns<choice>
                    <abbr>reˀ</abbr>
                    <expan>rer</expan>
                </choice> bruͦ<choice>
                    <abbr>dˀe</abbr>
                    <expan>dere</expan>
                </choice> wegen <reg type="star">✳</reg> dem
        <lb facs="#facs_1_line_1495619729272_37" n="N003"/>
                <persName ref="per007128">er<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice> rit<choice>
                        <abbr>tˀe</abbr>
                        <expan>tere</expan>
                    </choice> <choice>
                        <abbr>Wˀn</abbr>
                        <expan>Wern</expan>
                    </choice>
                    <choice>
                        <abbr>h̄</abbr>
                        <expan>her</expan>
                    </choice> von Wolon</persName> <reg type="star">✳</reg> vùr die <measure>fùnftzig march sil<choice>
                        <abbr>bˀs</abbr>
                        <expan>bers</expan>
                    </choice>
                </measure>
                <reg>,</reg> die wir im schuldig sint<reg>,</reg> <reg type="star">✳</reg> ein teil vûr
        <lb facs="#facs_1_line_1495619734400_38" n="N004"/>
                <measure>ein ros</measure> <reg type="star">✳</reg> und ein teil umb sinen dienst<reg>,</reg> gesetzet haben <measure>fûnftzig stûcke geltes</measure> <reg type="star">✳</reg> uf den guͦ<choice>
                    <abbr>tˀen</abbr>
                    <expan>teren</expan>
                </choice>
                <reg>,</reg> die hie
        <lb facs="#facs_1_line_1495619739271_39" n="N005"/>nach gescriben sint<reg>,</reg> <reg type="star">✳</reg> ze habend<reg>,</reg> ze nûtzend <reg type="star">✳</reg> und ze nieszend im und sinen erben <reg type="star">✳</reg> als lange<reg>,</reg> untz wir <reg type="star">✳</reg> und
        <lb facs="#facs_1_line_1495619745337_40" n="N006"/>un<choice>
                    <abbr>sˀe</abbr>
                    <expan>sere</expan>
                </choice> bruͦ<choice>
                    <abbr>dˀe</abbr>
                    <expan>dere</expan>
                </choice> oder erben <reg type="star">✳</reg> daz gelt von inen erlosèn mit <measure>fûnftzig marchen sil<choice>
                        <abbr>bˀs</abbr>
                        <expan>bers</expan>
                    </choice>
                </measure>
                <reg>.</reg> <reg type="star">✳</reg> und sint ditz die
        <lb facs="#facs_1_line_1495619750451_41" n="N007"/>guͦter<reg>.</reg> <reg type="star">✳</reg> in dem <placeName ref="loc009765">Eigen</placeName> ze <placeName ref="loc000963">Habs<choice>
                        <abbr>bˀg</abbr>
                        <expan>burg</expan>
                    </choice>
                </placeName>: <reg type="star">✳</reg> uf <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> knaben guͦt <orgName ref="org007552">von Stetten</orgName> <reg type="star">✳</reg> <measure>nûn mût rocken</measure> <reg type="star">✳</reg> und <measure>ein vierteil</measure>
                <reg>,</reg>
        <lb facs="#facs_1_line_1495619755053_42" n="N008"/>
                <measure>sehs mût ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure> <reg type="star">✳</reg> und <measure>drûvierteil</measure>
                <reg>.</reg> uf <persName ref="per013020">
                    <choice>
                        <abbr>Berch̄</abbr>
                        <expan>Berchtold</expan>
                    </choice> <reg type="star">✳</reg> Reber</persName> guͦt <reg type="star">✳</reg> <measure>drù vierteil rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013021">Hedewig Gosiko<reg type="lb">¬</reg><lb facs="#facs_1_line_1495619759508_43" n="N009" break="no"/>min</persName> guͦt <measure>vierdhalb mût rocken</measure> <reg type="star">✳</reg> und <measure>zwen mùt ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013022">
                    <choice>
                        <abbr>Heinr̄</abbr>
                        <expan>Heinrich</expan>
                    </choice> Siggin<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName> guͦt <measure>dri mût rocken</measure> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1495619765000_44" n="N010"/>und <measure>fûnftzehen vierteil <reg type="star">✳</reg> ha<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013023">
                    <choice>
                        <abbr>Berch̄</abbr>
                        <expan>Berchtold</expan>
                    </choice> guͦt in <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Schûr</persName> <measure>zwei vierteil rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013024">
                    <choice>
                        <abbr>Conr̄</abbr>
                        <expan>Conrad</expan>
                    </choice> guͦt am
        <lb facs="#facs_1_line_1495619769571_45" n="N011"/>Borren</persName> <measure>ein mût rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013315">Contzins guͦt von Wolun</persName> <reg type="star">✳</reg> <measure>ein vierteil rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013025">Meier <choice>
                        <abbr>Conr̄</abbr>
                        <expan>Conrad</expan>
                    </choice>
                </persName> guͦt <measure>ein
        <lb facs="#facs_1_line_1495619775512_46" n="N012"/>vierteil rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> ze <placeName ref="loc009558">Birrloͧf</placeName>: uf <persName ref="per028435">
                    <choice>
                        <abbr>Conr̄</abbr>
                        <expan>Conrad</expan>
                    </choice> guͦt am Stade</persName> <reg type="star">✳</reg> <measure>sehsthalb vierteil rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013026">
                    <choice>
                        <abbr>Uͦlr̄</abbr>
                        <expan>Uͦlrich</expan>
                    </choice> guͦt am Stade</persName> <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1495619781919_47" n="N013"/>
                <measure>fûnfthalb viertel rocken</measure>
                <reg>.</reg> uf <persName ref="per013027">
                    <choice>
                        <abbr>Conr̄</abbr>
                        <expan>Conrad</expan>
                    </choice> guͦt uf <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Flu</persName>ͤ <reg type="star">✳</reg> <measure>zwen mûtte rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per029134">
                    <choice>
                        <abbr>Uͦlr̄</abbr>
                        <expan>Uͦlrich</expan>
                    </choice> Vischer</persName> guͦt <measure>fùnf vier<reg type="lb">¬</reg><lb facs="#facs_1_line_1495619787088_48" n="N014" break="no"/>tel rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> ze <placeName ref="loc009557">Scherntz</placeName>: uf <persName ref="per013051">
                    <choice>
                        <abbr>Heinr̄</abbr>
                        <expan>Heinrich</expan>
                    </choice> Meier</persName> <reg type="star">✳</reg> und <persName ref="per013052">
                    <choice>
                        <abbr>Uͦlr̄</abbr>
                        <expan>Uͦlrich</expan>
                    </choice> Zùricher</persName> <reg type="star">✳</reg> <measure>ein mût rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> ze <placeName ref="loc008679">Lupfang</placeName>: uf
        <lb facs="#facs_1_line_1495619792933_49" n="N015"/>
                <persName ref="per013097">
                    <choice>
                        <abbr>Conr̄</abbr>
                        <expan>Conrad</expan>
                    </choice> Reio</persName> guͦt <measure>zwen mût rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013054">Ruͦdolfes Weber</persName> guͦt <reg type="star">✳</reg> <measure>ein mût rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013053">Burchart Mo<choice>
                        <abbr>sˀ</abbr>
                        <expan>ser</expan>
                    </choice>
                </persName>
        <lb facs="#facs_1_line_1495619799271_50" n="N016"/>guͦt <reg type="star">✳</reg> <measure>zehen viertel rocken</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013055">Peter Bachman</persName> guͦt <reg type="star">✳</reg> <measure>dri mût rocken</measure> <reg type="star">✳</reg> und <measure>ein vierteil</measure>
                <reg>.</reg> <reg type="star">✳</reg> uf <persName ref="per013058">
                    <choice>
