  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0184_r"/>
      <p facs="#facs_1_region_1496138957974_187">
        <lb facs="#facs_1_line_1496138964217_188" n="N001"/>Allen den<reg>,</reg> die disen brief ansechent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> nû <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hie nach<reg>,</reg> <reg type="star">✳</reg> kûnde ich<reg>,</reg> <persName ref="per019313">Johans Meyer <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Reitnoͧwe<reg>,</reg> schultheis ze Sùrse</persName>
                <reg>,</reg> daz fûr mich kam
        <lb facs="#facs_1_line_1496138968409_189" n="N002"/>fûr offen gerichte<reg>,</reg> do ich ze gerichte sas ze <placeName ref="loc001063">Sùrse</placeName> in <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> stat<reg>,</reg> <reg type="star">✳</reg> dû <persName ref="per019318">erber froͧwe<reg>,</reg> froͧ Heilga von Schenken</persName>
                <reg>,</reg> <persName ref="per019323">Jacobs seli<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> von Senchen<reg>,</reg> eines edeln knechtes</persName>
                <reg>,</reg>
        <lb facs="#facs_1_line_1496138972201_190" n="N003"/>elichû wirtin<reg>,</reg> <reg type="star">✳</reg> mit ir kinden<reg>,</reg> <persName ref="per019324">Jeglin</persName>
                <reg>,</reg> <persName ref="per019326">Heinin</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per019328">Jennin</persName>
                <reg>,</reg> ir sûnen<reg>,</reg> und <persName ref="per019329">Greton</persName> ir toch<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <reg type="star">✳</reg> und bevogtot sich da vor offen<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> gerichte mit rech<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urteil und dû
        <lb facs="#facs_1_line_1496138976689_191" n="N004"/>ege<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>ten ire kint<reg>,</reg> <reg type="star">✳</reg> want si und irû kint ir rechtes vogetes nit han mochton und ward ir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir kinden<reg>,</reg> den e<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice>
                <reg>,</reg> vor gericht mit urteil ze vogt gegeben
        <lb facs="#facs_1_line_1496138980578_192" n="N005"/>
                <persName ref="per019334">jung<choice>
                        <abbr>hˀ</abbr>
                        <expan>herr</expan>
                    </choice> Uͦlrich von Bûttenkon von Schenchen</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>zech sich <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selb <persName ref="per019334">jung<choice>
                        <abbr>hˀ</abbr>
                        <expan>herr</expan>
                    </choice> Uͦlrich <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Bùttenkon</persName>
                <reg>,</reg> e<choice>
                    <abbr>gen̄</abbr>
                    <expan>genant</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> mit si<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> hant <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vorbe<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice>ten froͧwen<reg>,</reg> <persName ref="per019318">froͧ Helgen</persName>
                <reg>,</reg> und
        <lb facs="#facs_1_line_1496138984418_193" n="N006"/>ir kinden<reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> obge<choice>
                    <abbr>nā</abbr>
                    <expan>nan</expan>
                </choice>ten<reg>,</reg> <reg type="star">✳</reg> eins vierden teils eines legenzechenden<reg>,</reg> gelegen ze <placeName ref="loc009802">Seon</placeName> in dem dorfe<reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> die selben froͧwen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ire kint an horte<reg>,</reg> den si rech<add place="above">t</add> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> rede<reg type="lb">¬</reg>
        <lb facs="#facs_1_line_1496138988522_194" n="N007" break="no"/>lich hant ze koͧffen gegeben<reg>,</reg> <reg type="star">✳</reg> den <orgName ref="org002627">erwirdigen<reg>,</reg> geistli<choice>
                        <abbr>chē</abbr>
                        <expan>chen</expan>
                    </choice> froͧwen<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> der eptischin <reg type="star">✳</reg> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem <reg type="star">✳</reg> <reg type="star">✳</reg> con<choice>
                        <abbr>vē</abbr>
                        <expan>ven</expan>
                    </choice>te <reg type="star">✳</reg> dez klosters ze Kûngesvelt<reg>,</reg> gele<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> in Kostenzer bystoͧm</orgName>
                <reg>,</reg>
        <lb facs="#facs_1_line_1496138992738_195" n="N008"/>umb <measure>hûndert <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> viertzig phûnt <choice>
                        <abbr>phen̄in</abbr>
                        <expan>phennin</expan>
                    </choice>gen<reg>,</reg> nûwer Zofinger genger <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> genemer</measure>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> si oͧch <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> inen gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gentzlich ge<choice>
                    <abbr>wˀt</abbr>
                    <expan>wert</expan>
                </choice> sint<reg>,</reg> <reg type="star">✳</reg> dez si <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jechent an disem gegen<reg type="lb">¬</reg>
        <lb facs="#facs_1_line_1496138996555_196" n="N009" break="no"/>werti<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> briefe<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in iren nûtz <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> notdùrft bekeret sint und oͧch den selben zechen<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> ze lechen hatton <reg type="star">✳</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> ûn<choice>
                    <abbr>sˀn</abbr>
                    <expan>sern</expan>
                </choice> <orgName ref="org000088">hochgebor<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> genedi<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren <choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice> herztogon
        <lb facs="#facs_1_line_1496139000778_197" n="N010"/>
                    <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Oͤsterrich</orgName>
                <reg>.</reg> und hant sich oͧch <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>zigen dez vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genant</expan>
                </choice> vierden teils dez zechen<choice>
                    <abbr>dē</abbr>
