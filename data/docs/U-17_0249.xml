        <lb facs="#facs_1_line_1504546464674_56" n="N010"/>
                <choice>
                    <abbr>wū</abbr>
                    <expan>wun</expan>
                </choice>ne <reg type="star">✳</reg> und weide <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> wie si ge<choice>
                    <abbr>nāt</abbr>
                    <expan>nant</expan>
                </choice> sint <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> werdent<reg>,</reg> <reg type="star">✳</reg> die gelegen sint in dem banne <reg type="star">✳</reg> ze
        <lb facs="#facs_1_line_1504546467386_57" n="N011"/>
                <placeName ref="loc009231">Birmersdorf</placeName><reg>,</reg> <reg type="star">✳</reg> untz an <measure>driie mark geltes</measure><reg>,</reg> als vor bescheiden ist<reg>,</reg> <reg type="star">✳</reg> gegeben haben <reg type="star">✳</reg> wiz<choice>
                    <abbr>zēt</abbr>
                    <expan>zent</expan>
                </choice>lich <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1504546470442_58" n="N012"/>fùr uns<reg>,</reg> <reg type="star">✳</reg> un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> bruͦ<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> und erben <reg type="star">✳</reg> und geben mit disem brief recht und redelich <reg type="star">✳</reg> in wise <reg type="star">✳</reg> ei<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice>
        <lb facs="#facs_1_line_1504546473891_59" n="N013"/>ewigen<reg>,</reg> unwi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>ruͤfflichen gabe<reg>,</reg> <reg type="star">✳</reg> die <choice>
                    <abbr>mā</abbr>
                    <expan>man</expan>
                </choice> nennet un<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> den lebenden<reg>,</reg> <reg type="star">✳</reg> den vor<choice>
                    <abbr>gen̄̄</abbr>
                    <expan>genanten</expan>
                </choice> uns<choice>
                    <abbr>rˀ</abbr>
                    <expan>rer</expan>
                </choice> hert<choice>
                    <abbr>zē</abbr>
                    <expan>zen</expan>
                </choice>
                <reg type="hyphen">-</reg><lb facs="#facs_1_line_1504546476843_60" n="N014" break="no"/>lieben peseln <reg type="star">✳</reg> <persName ref="per006964">frowen Agnesen<reg>,</reg> wilent kùngin ze Un<choice>
                        <abbr>gˀn</abbr>
                        <expan>gern</expan>
                    </choice></persName><reg>,</reg> <reg type="star">✳</reg> und dem <choice>
                    <abbr>egen̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <orgName ref="org002627">con<choice>
                        <abbr>vēt</abbr>
                        <expan>vent</expan>
                    </choice> uns<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice> stift
        <lb facs="#facs_1_line_1504546480339_61" n="N015"/>ze Kùngsvelt</orgName> <reg type="star">✳</reg> gemeinlich und sun<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>lich <reg type="star">✳</reg> nach gelegenheit <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> kuͤnftigen wande<choice>
                    <abbr>lūg</abbr>
                    <expan>lung</expan>
                </choice> in <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>selben
        <lb facs="#facs_1_line_1504546483907_62" n="N016"/>sache<reg>.</reg> <reg type="star">✳</reg> mit urkùnd ditz briefs<reg>,</reg> <reg type="star">✳</reg> der mit un<choice>
                    <abbr>sˀm</abbr>
                    <expan>serm</expan>
                </choice> anhangendem ingesigel <choice>
