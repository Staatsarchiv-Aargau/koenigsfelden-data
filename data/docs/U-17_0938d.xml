  <text>
    <body>
      <pb facs="#facs_1" n="S.  1" xml:id="img_0001" source="U-17_0938d_s01"/>
      <p facs="#facs_1_region_1523449485165_25">
        <lb facs="#facs_1_region_1523449485165_25l1" n="N001"/>Ich<reg>,</reg> <persName ref="per013155">Elsbeth Weͣchterin von Windisch<reg>,</reg> <reg type="star">✳</reg> an der Rysz gele<choice>
                        <abbr>gen̄</abbr>
                        <expan>genn</expan>
                    </choice><reg>,</reg>
        <lb facs="#facs_1_region_1523449485165_25l4" n="N002"/>wylent ein leyg schwester zuͦ Küngesfelden gsin</persName><reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1523449485165_25l5" n="N003"/>jetz <persName ref="per013156">Ruͦdolffen Ustris <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Aͤrlibach am Zùrich See</persName> <reg type="star">✳</reg> eeliche
        <lb facs="#facs_1_region_1523449485165_25l6" n="N004"/>husfrow<reg>,</reg> <reg type="star">✳</reg> bekenn allermengklichem mit disem brieff<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1523449485165_25l7" n="N005"/>als mit gemeltem minem eeli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> <choice>
                    <abbr>man̄</abbr>
                    <expan>mann</expan>
                </choice> unnd vogt<reg>,</reg>
        <lb facs="#facs_1_region_1523449485165_25l8" n="N006"/>als dann die <orgName ref="org004002">edlen <reg type="star">✳</reg> stren<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice><reg>,</reg> <reg type="star">✳</reg> vesten<reg>,</reg> fùrsichti<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> und wy<choice>
                        <abbr>sē</abbr>
                        <expan>sen</expan>
                    </choice>
        <lb facs="#facs_1_region_1523449485165_25l9" n="N007"/>hern<reg>,</reg> schulthes unnd rät der statt Bern<reg>,</reg> <reg type="star">✳</reg> unser sonn<choice>
                        <abbr>derˀ</abbr>
                        <expan>derr</expan>
                    </choice>
        <lb facs="#facs_1_region_1523449485165_25l10" n="N008"/>gneͣdig herren <reg type="star">✳</reg> von dem gotzhus zuͦ Küngsfel<choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName> mir ver<reg type="lb">¬</reg><lb facs="#facs_1_region_1523449485165_25l11" n="N009" break="no"/>ordnet habent zegeben <measure>zweyhundert pfund haller guͦtter
        <lb facs="#facs_1_region_1523449485165_25l12" n="N010"/>Berner müntz unnd weͣrschafft</measure> <reg type="star">✳</reg> so bald ich ei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> bider<choice>
                    <abbr>mā</abbr>
                    <expan>mann</expan>
                </choice>
        <lb facs="#facs_1_region_1523449485165_25l13" n="N011"/>zuͦ der <del rend="strikethrough">der</del> ee werden neͣme<reg>.</reg> <reg type="star">✳</reg> allso hat nuͦn der <persName ref="per013157"><choice>
                        <abbr>from̄</abbr>
                        <expan>fromm</expan>
                    </choice> und
        <lb facs="#facs_1_region_1523449485165_25l14" n="N012"/>vest junckher Jörg Schöni vonn Bern<reg>,</reg> diser zit hofmeis<choice>
                        <abbr>terˀ</abbr>
                        <expan>terr</expan>
                    </choice>
        <lb facs="#facs_1_region_1523449485165_25l15" n="N013"/>des gotzhuses zuͦ Küngsfel<choice>
                        <abbr>dē</abbr>
                        <expan>den</expan>
                    </choice></persName><reg>,</reg> us empfelch gedachter unser
        <lb facs="#facs_1_region_1523449485165_25l16" n="N014"/>
                <orgName ref="org004002">gnedi<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> her<choice>
                        <abbr>rē</abbr>
                        <expan>ren</expan>
                    </choice> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Bern</orgName> uns uff dato dis brieffs ùber<reg type="lb">¬</reg><lb facs="#facs_1_region_1523449485165_25l17" n="N015" break="no"/>anttwurt <measure>hundert pfund Bern müntz</measure> <reg type="star">✳</reg> und wellent
        <lb facs="#facs_1_region_1523449485165_25l18" n="N016"/>mir<reg>,</reg> gemel<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <persName ref="per013155">Elsbe<choice>
                        <abbr>thē</abbr>
                        <expan>then</expan>
                    </choice> Wächterin</persName><reg>,</reg> von jetz ùber ein jar die
        <lb facs="#facs_1_region_1523449485165_25l19" n="N017"/>ùbri<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <measure>hundert pfund</measure> <reg type="star">✳</reg> oͧch werden lassen<reg>.</reg> <reg type="star">✳</reg> hierumb so
        <lb facs="#facs_1_region_1523449485165_25l20" n="N018"/>sagent unnd lassent wir<reg>,</reg> obgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> beyde eemenschen<reg>,</reg>
        <lb facs="#facs_1_region_1523449485165_25l21" n="N019"/>und sonder ich<reg>,</reg> die frow<reg>,</reg> <reg type="star">✳</reg> als mit gemeltem minem eli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice>
        <lb facs="#facs_1_region_1523449485165_25l22" n="N020"/>man und vogt <reg type="star">✳</reg> fùr ùnns<reg>,</reg> <reg type="star">✳</reg> all unser erben und nachko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>
        <lb facs="#facs_1_region_1523449485165_25l23" n="N021"/>die obgenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> unser <orgName ref="org004002">gnedig her<choice>
                        <abbr>rē</abbr>
                        <expan>ren</expan>
                    </choice> vonn Bern</orgName><reg>,</reg> <reg type="star">✳</reg> den gedach<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_region_1523449485165_25l24" n="N022"/>
                <del rend="strikethrough">juckhern</del> <persName ref="per013157">Jör<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> Schöni<reg>,</reg> den hofmeis<choice>
                        <abbr>terˀ</abbr>
                        <expan>terr</expan>
                    </choice></persName><reg>,</reg> <reg type="star">✳</reg> all ir er<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> unnd
        <lb facs="#facs_1_region_1523449485165_25l25" n="N023"/>nachko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> des gemel<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <orgName ref="org009673">gotzhuses Küngsfelden</orgName> unnd weͣr
        <lb facs="#facs_1_region_1523449485165_25l26" n="N024"/>hierumb quittierens notturftig ist oder wirdet <reg type="star">✳</reg> der
        <lb facs="#facs_1_region_1523449485165_25l27" n="N025"/>gemel<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> hundert pfun<choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice><reg>,</reg> uff dise liechtmes ùberantwurt<reg>,</reg>
        <lb facs="#facs_1_region_1523449485165_25l28" n="N026"/>gantz quitt<reg>,</reg> unansprechig<reg>,</reg> ledig und losz<reg>,</reg> <reg type="star">✳</reg> in chrafft diss
