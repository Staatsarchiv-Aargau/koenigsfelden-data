    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0011a_r"/>
      <p facs="#facs_1_region_1511446106898_1">
        <lb facs="#facs_1_region_1511446106898_1l1" n="N001"/>
                <persName ref="per012255">Clemens <choice>
                        <abbr>ep̄s</abbr>
                        <expan>episcopus</expan>
                    </choice> servus servo<choice>
                        <abbr>ꝝ</abbr>
                        <expan>rum</expan>
                    </choice> dei</persName> <orgName ref="org002768">dilectis filiis <reg type="star">✳</reg> <reg type="star">✳</reg> ministro generali et fratribus ordinis Mino<choice>
                        <abbr>ꝝ</abbr>
                        <expan>rum</expan>
                    </choice>
                </orgName> <choice>
                    <abbr>sal̄t</abbr>
                    <expan>salutem</expan>
                </choice> et <choice>
                    <abbr>apl̄i</abbr>
                    <expan>apostoli</expan>
                </choice>cam <choice>
                    <abbr>bēn</abbr>
                    <expan>benedictionem</expan>
                </choice>
                <reg>.</reg> <reg type="star">✳</reg> religionis
        <lb facs="#facs_1_region_1511446106898_1l2" n="N002"/>
                <choice>
                    <abbr>vr̄e</abbr>
                    <expan>vestre</expan>
                </choice> merita necnon <persName ref="per007254">dilecte in <choice>
                        <abbr>xp̄o</abbr>
                        <expan>Christo</expan>
                    </choice> filie nobilis mulieris Elizabeth</persName>
                <reg>,</reg> relicte clare memorie <persName ref="per004951">Alberti regis Ro<choice>
                        <abbr>man̄</abbr>
                        <expan>manorum</expan>
                    </choice>
                </persName> vidue<reg>,</reg> <reg type="star">✳</reg> devotionis sinceritas nos inducunt<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1511446106898_1l3" n="N003"/>ut ad <choice>
                    <abbr>vr̄i</abbr>
                    <expan>vestri</expan>
                </choice> propagationem ordinis promptis intendamus affectibus ac eiusdem nobilis piis desideriis favorabiliter annuamus<reg>.</reg> <reg type="star">✳</reg> cum ita<choice>
                    <abbr>qƺ</abbr>
                    <expan>que</expan>
                </choice> predicta nobilis<reg>,</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1511446106898_1l4" n="N004"/>sicut ex parte sua nuper accepimus<reg>,</reg> cupiens terrena in celestia et transitoria in eterna felici <choice>
                    <abbr>cō</abbr>
                    <expan>com</expan>
                </choice>mercio conmutare<reg>,</reg> <reg type="star">✳</reg> quoddam monasterium et alia edificia opor<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1511446106898_1l5" n="N005" break="no"/>tuna pro habitatione <orgName ref="org002628">Fratrum ordinis <choice>
                        <abbr>vr̄i</abbr>
                        <expan>vestri</expan>
                    </choice> ydonea in <choice>
                        <abbr>dioc̄</abbr>
                        <expan>diocesi</expan>
                    </choice> Constanti<choice>
                        <abbr>en̄</abbr>
                        <expan>ensi</expan>
                    </choice>
                </orgName> in loco videlicet de <placeName ref="loc007696">Chunigesvelde</placeName>
                <reg>,</reg> <reg type="star">✳</reg> ubi dictus rex manibus impio<choice>
                    <abbr>ꝝ</abbr>
                    <expan>rum</expan>
                </choice>
        <lb facs="#facs_1_region_1511446106898_1l6" n="N006"/>occubuit<reg>,</reg> ad laudem divini nominis ac pro eiusdem regis anime remedio construi faciat propriis sumptibus et expensis<reg>,</reg> que vobis
        <lb facs="#facs_1_region_1511446106898_1l7" n="N007"/>pio ducta proposito concedere desiderat<reg>,</reg> <reg type="star">✳</reg> ut ibidem perpetuis futuris temporibus domino serviatis<reg>.</reg> <reg type="star">✳</reg> nos volentes vos et ordinem <choice>
                    <abbr>ip̄m</abbr>
                    <expan>ipsum</expan>
                </choice>
        <lb facs="#facs_1_region_1511446106898_1l8" n="N008"/>favore prosequi gratioso<reg>,</reg> dictam<choice>
                    <abbr>qƺ</abbr>
                    <expan>que</expan>
                </choice> nobilem in huiusmodi suo laudabili proposito confovere<reg>,</reg> <reg type="star">✳</reg> eius supplicationibus inclinati<reg>,</reg> vobis reci<reg type="lb">¬</reg>
        <lb facs="#facs_1_region_1511446106898_1l9" n="N009" break="no"/>piendi pro habitatione <reg type="star">✳</reg> huiusmodi monasterium et edificia prelibata sine iuris preiudicio alieni<reg>,</reg> <reg type="star">✳</reg> constitutione felicis recordationis
        <lb facs="#facs_1_region_1511446106898_1l10" n="N010"/>
                <persName ref="per012253">Bonifatii <choice>
                        <abbr>p̄p</abbr>
                        <expan>pape</expan>
                    </choice> <reg type="star">✳</reg> VIII predecessoris <choice>
                        <abbr>nr̄i</abbr>
                        <expan>nostri</expan>
                    </choice>
                </persName>
                <reg>,</reg> que prohibet<reg>,</reg> ne fratres <choice>
                    <abbr>ip̄i</abbr>
                    <expan>ipsi</expan>
                </choice> ad alia loca se transferant <reg type="star">✳</reg> seu illa de novo recipiant sine
        <lb facs="#facs_1_region_1511446106898_1l11" n="N011"/>
                <orgName ref="org000057">
                    <choice>
                        <abbr>apl̄i</abbr>
                        <expan>apostoli</expan>
                    </choice>ce sedis</orgName> licentia speciali<reg>,</reg> et qualibet alia super hoc in contrarium edita non obstante<reg>,</reg> auctoritate presentium indulgemus<reg>.</reg> <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1511446106898_1l12" n="N012"/>nulli ergo omnino hominum liceat <reg type="star">✳</reg> hanc paginam <choice>
