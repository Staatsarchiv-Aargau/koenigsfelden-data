      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0031_r"/>
      <p facs="#facs_1_region_1492770246460_108">
        <lb facs="#facs_1_line_1492770252391_109" n="N001"/>Allen den<reg>,</reg> <reg type="star">✳</reg> die disen brief sehend<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoerend lesen<reg>,</reg> <reg type="star">✳</reg> nu <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> har nach<reg>,</reg> <reg type="star">✳</reg> kùnd
        <lb facs="#facs_1_line_1492770255867_110" n="N002"/>ich<reg>,</reg> <reg type="star">✳</reg> <persName ref="per015724"><choice>
                        <abbr>hˀ</abbr>
                        <expan>herr</expan>
                    </choice> Walther<reg>,</reg> techan von Windische</persName><reg>,</reg> <reg type="star">✳</reg> das ich dem erberrem man <reg type="star">✳</reg> <persName ref="per028222">Pe<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>
        <lb facs="#facs_1_line_1492770259988_111" n="N003"/>dem Meyer von Husen</persName> <reg type="star">✳</reg> und sinen erben <reg type="star">✳</reg> han fuͤrlùhen den hof ze <placeName ref="loc008677">Husen</placeName><reg>,</reg>
        <lb facs="#facs_1_line_1492770263136_112" n="N004"/>da er uf sizet<reg>,</reg> <reg type="star">✳</reg> den ich koifte von <reg type="star">✳</reg> <persName ref="per011070">her Dietmar von Hagberch</persName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> e mals
        <lb facs="#facs_1_line_1492770266824_113" n="N005"/>gewesen was <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org004821">von Kien<choice>
                        <abbr>bgˀ</abbr>
                        <expan>berg</expan>
                    </choice>
                </orgName><reg>.</reg> <reg type="star">✳</reg> von dem selben hof sol man jerlich geben <reg type="star">✳</reg> dem
        <lb facs="#facs_1_line_1492770271144_114" n="N006"/>
                <orgName ref="org002627">klos<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> ze Kùngesvelt</orgName> <reg type="star">✳</reg> <measure>nùn mùt roggen</measure><reg>,</reg> <reg type="star">✳</reg> <measure>zvein mùt <reg type="star">✳</reg> kernen</measure><reg>,</reg> <reg type="star">✳</reg> <measure>zvei malter</measure>
        <lb facs="#facs_1_line_1492770275436_115" n="N007"/>
                <measure>habern</measure><reg>,</reg> <reg type="star">✳</reg> <measure>zvei svin</measure>
                <measure><reg>,</reg> dù ein pfunt gelten sun</measure><reg>,</reg> <reg type="star">✳</reg> <measure>hundert eyer</measure><reg>,</reg> <reg type="star">✳</reg> <measure>zvei herbest</measure>
        <lb facs="#facs_1_line_1492770279684_116" n="N008"/>
                <measure>huͤnrn</measure><reg>,</reg> <reg type="star">✳</reg> und <measure>ein <reg type="star">✳</reg> vasnacht hùern</measure><reg>.</reg> <reg type="star">✳</reg> und ze einem offennen ùrkuͤnde der
        <lb facs="#facs_1_line_1492770282991_117" n="N009"/>sache und einer beste<choice>
                    <abbr>tū</abbr>
                    <expan>tun</expan>
                </choice>ge ewigir<reg>,</reg> <reg type="star">✳</reg> so gib ich inen disen brief<reg>,</reg> bùsigele <reg type="star">✳</reg>
        <lb facs="#facs_1_line_1492770286531_118" n="N010"/>mit minem ingesigel<reg>.</reg> <reg type="star">✳</reg> dir brief ward gegeben <reg type="star">✳</reg> ze <placeName ref="loc005974">
                    <choice>
                        <abbr>Wī</abbr>
                        <expan>Win</expan>
                    </choice>dische</placeName><reg>,</reg> do man zalte
        <lb facs="#facs_1_line_1492770290451_119" n="N011"/>
                <date when="1313-10-16">von gottes geburt tusenth <reg type="star">✳</reg> drùhun<choice>
                        <abbr>dtˀ</abbr>
                        <expan>dert</expan>
                    </choice> jar<reg>,</reg> dar nach in dem drizehenden jare
        <lb facs="#facs_1_line_1492770294737_120" n="N012"/>an sant Gallen tag</date><reg>.</reg> <reg type="star">✳</reg> da ze gegen was <reg type="star">✳</reg> <persName ref="per013068">Ruͦdolf <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Sa<choice>
                        <abbr>f̄n</abbr>
                        <expan>fen</expan>
                    </choice>wiler</persName><reg>,</reg> <persName ref="per012944">
                    <reg type="star">✳</reg> Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> der Schoͤ<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice></persName><reg>,</reg>
        <lb facs="#facs_1_line_1492770298308_121" n="N013"/>
                <persName ref="per020814">Ruͦdolf <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Scherer<reg>,</reg> <choice>
                        <abbr>bͦger</abbr>
                        <expan>buͦrger</expan>
                    </choice> von Brugga</persName><reg>,</reg> <persName ref="per028223"><choice>
                        <abbr>Ruͦ<reg type="star">✳</reg>
                        </abbr>
                        <expan>Ruͦdolf</expan>
                    </choice> Meyer von Husen</persName><reg>,</reg> <persName ref="per028224">Chuͤ Steilli</persName><reg>,</reg>
        <lb facs="#facs_1_line_1492770301562_122" n="N014"/>
                <persName ref="per019500">Heini Tierlin</persName> <reg type="star">✳</reg> und an<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> lùten vil<reg>.</reg> <reg type="star">✳</reg>
            </p>
