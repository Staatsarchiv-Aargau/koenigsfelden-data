  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0913_r"/>
      <p facs="#facs_1_r1">
        <lb facs="#facs_1_r1l16" n="N001"/>Ich<reg>,</reg> <persName ref="per013569">Hanns Bürrer<reg>,</reg> miner gnädi<choice>
                        <abbr>gnn̄</abbr>
                        <expan>genn</expan>
                    </choice> frò<choice>
                        <abbr>wnn̄</abbr>
                        <expan>wenn</expan>
                    </choice> àpptissinn unnd con<choice>
                        <abbr>vēts</abbr>
                        <expan>vents</expan>
                    </choice> zuͤ Küngsfel<choice>
                        <abbr>dnn̄</abbr>
                        <expan>denn</expan>
                    </choice> hoffmeyster</persName>
                <reg>,</reg> <reg type="star">✳</reg> thün kundt menckli<choice>
                    <abbr>chm̄</abbr>
                    <expan>chem</expan>
                </choice> mitt
        <lb facs="#facs_1_r1l17" n="N002"/>disem brieff<reg>,</reg> <reg type="star">✳</reg> das ich uff hütt<reg>,</reg> da<choice>
                    <abbr>tum̄</abbr>
                    <expan>tumm</expan>
                </choice> des <reg type="star">✳</reg> von enpfälchenns unnd gwalts waͤ<choice>
                    <abbr>gnn̄</abbr>
                    <expan>genn</expan>
                </choice> <reg type="star">✳</reg> der yetz<choice>
                    <abbr>gnā</abbr>
                    <expan>gnan</expan>
                </choice>
                <choice>
                    <abbr>t₎</abbr>
                    <expan>ten</expan>
                </choice> mi<choice>
                    <abbr>neˀ</abbr>
                    <expan>ner</expan>
                </choice> gnädi<choice>
                    <abbr>gnn̄</abbr>
                    <expan>genn</expan>
                </choice> frò<choice>
                    <abbr>wnn̄</abbr>
                    <expan>wenn</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> dem <persName ref="per012205">ersa<choice>
                        <abbr>mē</abbr>
                        <expan>men</expan>
                    </choice>
        <lb facs="#facs_1_r1l19" n="N003"/>Hannsen Kindhuser</persName>
                <reg>,</reg> <reg type="star">✳</reg> wilenn <persName ref="per012204">Hann<choice>
                        <abbr>snn̄</abbr>
                        <expan>senn</expan>
                    </choice> Kindhusers seli<choice>
                        <abbr>gnn̄</abbr>
                        <expan>genn</expan>
                    </choice> eli<choice>
                        <abbr>chnn̄</abbr>
                        <expan>chenn</expan>
                    </choice> <choice>
                        <abbr>sun̄</abbr>
                        <expan>sunn</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> von Bremgart<choice>
                        <abbr>tn̄</abbr>
                        <expan>ten</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> ein <measure commodity="field" quantity="1" unit="Juchart" type="area">juchartt ackers</measure>
                <reg>,</reg> <reg type="star">✳</reg> die gmeldt<choice>
                    <abbr>teˀ</abbr>
                    <expan>ter</expan>
                </choice> sin vatter selig
        <lb facs="#facs_1_r1l18" n="N004"/>in lehenns wisz inghept hàtt<reg>,</reg> <reg type="star">✳</reg> dà selbs zuͤ <placeName ref="loc000979">Bremgart<choice>
                        <abbr>tn̄</abbr>
                        <expan>ten</expan>
                    </choice>
                </placeName> <reg type="star">✳</reg> uff der ebny glaͤgenn<reg>,</reg> <reg type="star">✳</reg> stost zuͤ einer sittenn an <placeName ref="loc013661">Herweg</placeName> <reg type="star">✳</reg> unnd zur ann<choice>
                    <abbr>drē</abbr>
                    <expan>dren</expan>
                </choice>
        <lb facs="#facs_1_r1l20" n="N005"/>sit<choice>
                    <abbr>tnn̄</abbr>
                    <expan>tenn</expan>
                </choice> an <persName ref="per012370">Jacob Jegers</persName> acker<reg>,</reg> <reg type="star">✳</reg> der manle<choice>
                    <abbr>chnn̄</abbr>
                    <expan>chenn</expan>
                </choice> ist vom <placeName ref="loc000390">schlosz Hapspurg</placeName> <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> mit aller zuͤghoͤrd <reg type="star">✳</reg> zuͤ rechtem manle<choice>
                    <abbr>chnn̄</abbr>
                    <expan>chenn</expan>
                </choice> geli<choice>
                    <abbr>chnn̄</abbr>
                    <expan>chenn</expan>
                </choice> hab <choice>
                    <abbr>ꝫ</abbr>
                    <expan>et</expan>
                </choice>
                <choice>
                    <abbr>cꝭ</abbr>
                    <expan> cetera</expan>
                </choice>
                <reg>.</reg>
        <lb facs="#facs_1_r1l15" n="N006"/>lich im ouch hiemit in krafft disz brieffs<reg>,</reg> <reg type="star">✳</reg> wasz ich im von rechts we<choice>
                    <abbr>gnn̄</abbr>
                    <expan>genn</expan>
                </choice> <reg type="star">✳</reg> daran ze li<choice>
                    <abbr>chnn̄</abbr>
                    <expan>chenn</expan>
                </choice> hab<reg>,</reg> <reg type="star">✳</reg> alles nàch lechenns recht<reg>,</reg> sitt und
        <lb facs="#facs_1_r1l21" n="N007"/>gewonheitt<reg>,</reg> <reg type="star">✳</reg> sölichs nun hinfùr inzeha<choice>
                    <abbr>benn̄</abbr>
                    <expan>benne</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> zenut<choice>
                    <abbr>znn̄</abbr>
                    <expan>zenn</expan>
                </choice> <reg type="star">✳</reg> unnd zeniessen<reg>,</reg> zebeset<choice>
                    <abbr>znn̄</abbr>
                    <expan>zenn</expan>
                </choice> <reg type="star">✳</reg> unnd entset<choice>
                    <abbr>znn̄</abbr>
                    <expan>zenn</expan>
                </choice> <reg type="star">✳</reg> unnd dàmit thün unnd laus<choice>
                    <abbr>snn̄</abbr>
                    <expan>senn</expan>
                </choice> <reg type="star">✳</reg> nàch
        <lb facs="#facs_1_r1l22" n="N008"/>sinem wil<choice>
                    <abbr>lnn̄</abbr>
                    <expan>lenn</expan>
                </choice> unnd geval<choice>
                    <abbr>lnn̄</abbr>
                    <expan>lenn</expan>
                </choice> <reg type="star">✳</reg> unnd wie manlechenns recht ist<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dann̄</abbr>
                    <expan>danne</expan>
                </choice> er mir hier<choice>
                    <abbr>umb̄</abbr>
                    <expan>umbe</expan>
                </choice> <reg type="star">✳</reg> geloptt <reg type="star">✳</reg> unnd geschwo<choice>
                    <abbr>rnn̄</abbr>
                    <expan>renn</expan>
                </choice> hàtt<reg>,</reg> <reg type="star">✳</reg> obbemeldt<choice>
                    <abbr>t₎</abbr>
                    <expan>ten</expan>
                </choice>
        <lb facs="#facs_1_r1l23" n="N009"/>mi<choice>
                    <abbr>nen̄</abbr>
                    <expan>nenn</expan>
                </choice> gnaͤdi<choice>
                    <abbr>gnn̄</abbr>
                    <expan>genn</expan>
                </choice> frò<choice>
                    <abbr>wnn̄</abbr>
                    <expan>wenn</expan>
                </choice> <reg type="star">✳</reg> von soͤlichs lechenns wegenn gewertig unnd ghor<choice>
                    <abbr>sam̄</abbr>
                    <expan>samm</expan>
                </choice> zesind <reg type="star">✳</reg> unnd alles dasz zetünd<reg>,</reg> <reg type="star">✳</reg> so lechenn lütt <reg type="star">✳</reg> irnn le<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice>her<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice>
        <lb facs="#facs_1_r1l24" n="N010"/>von rechts wegenn zetuͤnd <reg type="star">✳</reg> schuldig unnd pflichtig sinnd<reg>.</reg> <reg type="star">✳</reg> des zuͤ wàrem urkünd<reg>,</reg> <reg type="star">✳</reg> so hab ich min eygenn sigell<reg>,</reg> <reg type="star">✳</reg> miner <choice>
