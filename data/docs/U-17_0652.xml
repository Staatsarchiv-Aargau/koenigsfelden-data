      <p facs="#facs_3_region_1562677960034_2630" rend="dorsual_20_regesta">
        <lb facs="#facs_3_line_1601822143561_89" n="N001"/>wist umb die ziegel <del rend="strikethrough">mat<choice>
                        <abbr>t₎</abbr>
                        <expan>ten</expan>
                    </choice>
                </del>
