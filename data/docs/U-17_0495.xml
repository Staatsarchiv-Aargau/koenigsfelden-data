  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0495_r"/>
      <p facs="#facs_1_region_1593465985223_429">
        <lb facs="#facs_1_region_1593465985223_429l1" n="N001"/>Ich<reg>,</reg> <persName ref="per028636">Hans von Kottwil<reg>,</reg> do <choice>
                        <abbr>schulh̄</abbr>
                        <expan>schultheis</expan>
                    </choice> ze Surse</persName>
                <reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> tuͦn kunt offenlich mit disem brief<reg>,</reg>
        <lb facs="#facs_1_region_1593465985223_429l2" n="N002"/>dz ich an stat <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in namen <orgName ref="org000088">mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> gnaͤdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> <choice>
                        <abbr>heˀr</abbr>
                        <expan>herr</expan>
                    </choice>schaft <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Oͤs<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName> offenlich ze gericht sas ze
        <lb facs="#facs_1_region_1593465985223_429l3" n="N003"/>
                <placeName ref="loc001063">Surse</placeName> in <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> stat<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ka<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> da fùr mich in gericht die <persName ref="per028710">erber lùte <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice> Lang Uͤli Halter</persName>
        <lb facs="#facs_1_region_1593465985223_429l4" n="N004"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028711">Margareth<reg>,</reg> sin eliche wirtin<reg>,</reg> nu ze mal seshaft ze Surse</persName>
                <reg>,</reg> mit fùrspre<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> offnetend<reg>,</reg>
        <lb facs="#facs_1_region_1593465985223_429l5" n="N005"/>wie dz si bede einhelklich <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧft <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze koͧffen ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> hetten in eines ewi<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> rech<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> koͧffes
        <lb facs="#facs_1_region_1593465985223_429l6" n="N006"/>wise fùr si <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> dem <persName ref="per028712">bescheiden Uͦlin Halter von Bira</persName> <measure>fier juchar<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> achers</measure>
                <reg>,</reg> ligent
        <lb facs="#facs_1_region_1593465985223_429l7" n="N007"/>uf dem <placeName ref="loc010057">Birveld</placeName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ein len<reg>,</reg> nempt man <placeName ref="loc013860">Claws Hal<choice>
                        <abbr>tˀs</abbr>
                        <expan>ters</expan>
                    </choice> Len</placeName>
                <reg>,</reg> lit ze <placeName ref="loc008681">Bira</placeName> in dem dorffe<reg>,</reg>
        <lb facs="#facs_1_region_1593465985223_429l8" n="N008"/>umb <measure>sibenzechen phunt <choice>
                        <abbr>phē</abbr>
                        <expan>phen</expan>
                    </choice>
                    <choice>
                        <abbr>nī</abbr>
                        <expan>nin</expan>
                    </choice>ger Zo<choice>
                        <abbr>vī</abbr>
                        <expan>vin</expan>
                    </choice>
                    <choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> mùntz</measure>
                <reg>,</reg> des selben geltz si <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jachen gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_region_1593465985223_429l9" n="N009"/>gentzlich von im bezalt sin<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> liessen an recht<reg>,</reg> wie si im die selben <measure>fier jucharten
        <lb facs="#facs_1_region_1593465985223_429l10" n="N010"/>ackers</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dz len <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tigen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> uf ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> soͤltend<reg>,</reg> dz er dar an ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice> nu <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> hie
        <lb facs="#facs_1_region_1593465985223_429l11" n="N011"/>nach ewenklich<reg>.</reg> <reg type="star">✳</reg> har umb fragt ich<reg>,</reg> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanter</expan>
                </choice> rich<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>
                <reg>,</reg> rechtes uf den eid<reg>,</reg> wz recht <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice>
                <reg>.</reg> da
        <lb facs="#facs_1_region_1593465985223_429l12" n="N012"/>
                <choice>
                    <abbr>waˀt</abbr>
                    <expan>wart</expan>
                </choice> erteilt einhelklich uf den eid<reg>,</reg> dz si nu bede die vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <measure>fier juchar<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> achers</measure>
        <lb facs="#facs_1_region_1593465985223_429l13" n="N013"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dz len fùr si <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> uf ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> soͤlten <del rend="strikethrough">in min</del> uss ir be<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> hend in min<reg>,</reg> des ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice>
        <lb facs="#facs_1_region_1593465985223_429l14" n="N014"/>rich<choice>
                    <abbr>tˀs</abbr>
                    <expan>ters</expan>
                </choice>
                <reg>,</reg> hand<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ich <choice>
                    <abbr>den̄</abbr>
                    <expan>denn</expan>
                </choice> die vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <measure>fier juchar<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> achers</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> dz len antwùrte uss
        <lb facs="#facs_1_region_1593465985223_429l15" n="N015"/>minr hand als us eins rech<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> r<subst>
                    <del>e</del>
                    <add place="overwritten">i</add>
                </subst>chtˀs hand in hend <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gewalt des ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per028712">Uͤlis Hal<choice>
                        <abbr>tˀs</abbr>
                        <expan>ters</expan>
                    </choice>
        <lb facs="#facs_1_region_1593465985223_429l16" n="N016"/>
                    <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Bira</persName> ze handen sin selbs <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sinr er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice>
                <reg>.</reg> dz oͧch ich<reg>,</reg> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanter</expan>
                </choice> rich<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>
                <reg>,</reg> taͤt mit al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice> gewar<reg type="lb">¬</reg><lb facs="#facs_1_region_1593465985223_429l17" n="N017" break="no"/>samy<reg>,</reg> <choice>
                    <abbr>woˀ</abbr>
                    <expan>wor</expan>
                </choice>
                <choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>
                <choice>
                    <abbr>ckē</abbr>
                    <expan>cken</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ge<choice>
                    <abbr>bˀ</abbr>
                    <expan>ber</expan>
                </choice>den<reg>,</reg> so dar zuͦ gehoͤrt<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> also dz <choice>
                    <abbr>abˀ</abbr>
                    <expan>aber</expan>
                </choice> nach mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> urfrag ge<reg type="lb">¬</reg><lb facs="#facs_1_region_1593465985223_429l18" n="N018" break="no"/>sampt urteil gab einhelklich uf den eid<reg>,</reg> dz disù <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>
