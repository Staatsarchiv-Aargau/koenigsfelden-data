  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0565_r"/>
      <p facs="#facs_1_region_1508272558165_172">
        <lb facs="#facs_1_line_1508272563969_173" n="N001"/>Ich<reg>,</reg> <persName ref="per026965">Uͦlrich Kilcher<reg>,</reg> vogt uff dem Boͤtzberg</persName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>giche offenlich mitt disem brieff<reg>,</reg> <reg type="star">✳</reg> dz ich ze <placeName ref="loc000195">Brugg</placeName> vor dem <placeName ref="loc011832">Nidern Thor</placeName> uss
        <lb facs="#facs_1_line_1508272567525_174" n="N002"/>ze gericht gesesen bin uff <date when="1429-06-21">sant Albanus tag<reg>,</reg> <reg type="star">✳</reg> des ja<choice>
                        <abbr>r₎</abbr>
                        <expan>res</expan>
                    </choice><reg>,</reg> doman zalt von gottes gebùrt thusing vierhun<choice>
                        <abbr>dtˀ</abbr>
                        <expan>dert</expan>
                    </choice> <choice>
                        <abbr>zwēt</abbr>
                        <expan>zwent</expan>
                    </choice>zig und nùn<choice>
                        <abbr>d₎</abbr>
                        <expan>den</expan>
                    </choice> jar</date><reg>,</reg>
        <lb facs="#facs_1_line_1508272571349_175" n="N003"/>da fùr mich in <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>ban<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> gerichte kaͧment <reg type="star">✳</reg> <persName ref="per019515">Hans Mei<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> von Umikon</persName><reg>,</reg> <reg type="star">✳</reg> schaffner <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org002628">er<choice>
                        <abbr>bnˀ</abbr>
                        <expan>bern</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren<reg>,</reg> <reg type="star">✳</reg> des gardians und gemei<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> conventz ze
        <lb facs="#facs_1_line_1508272575099_176" n="N004"/>Kùngsveld<reg>,</reg> Sant Francissen ordens<reg>,</reg> Costant<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bystuͦms</orgName><reg>,</reg> an statt und in irem na<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> an einem <reg type="star">✳</reg> und <persName ref="per016067">Hans Vi<choice>
                        <abbr>schˀ</abbr>
                        <expan>scher</expan>
                    </choice> ab Boͤtz<choice>
                        <abbr>bgˀ</abbr>
                        <expan>berg</expan>
                    </choice>
                </persName> am an<choice>
                    <abbr>dnˀ</abbr>
                    <expan>dern</expan>
                </choice> teilen<reg>,</reg>
        <lb facs="#facs_1_line_1508272578742_177" n="N005"/>mitt fùr<choice>
                    <abbr>sp̄</abbr>
                    <expan>spre</expan>
                </choice>chen<reg>.</reg> <reg type="star">✳</reg> offnot <persName ref="per019515">Hans Mei<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName> von <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren wegen<reg>,</reg> <reg type="star">✳</reg> wie dz die vor<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genanten</expan>
                </choice> sin <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren hettend ein matten<reg>,</reg> ge<choice>
                    <abbr>nāt</abbr>
                    <expan>nant</expan>
                </choice> <placeName ref="loc013866">
                    <choice>
                        <abbr>Nū</abbr>
                        <expan>Nun</expan>
                    </choice>nenmatt</placeName><reg>,</reg> gelegen
        <lb facs="#facs_1_line_1508272582388_178" n="N006"/>an dem <placeName ref="loc013559">Linnberg</placeName><reg>.</reg> <reg type="star">✳</reg> die sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> matten <reg type="star">✳</reg> hettend sin <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren gelichen <persName ref="per027231">Hein<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> Vi<choice>
                        <abbr>schˀ</abbr>
                        <expan>scher</expan>
                    </choice> ab Boͤtzberg</persName> mitt an<choice>
                    <abbr>dnˀ</abbr>
                    <expan>dern</expan>
                </choice> guͤtern<reg>.</reg> <reg type="star">✳</reg> an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selben mat<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> sumpte
        <lb facs="#facs_1_line_1508272585782_179" n="N007"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> irte er sy<reg>,</reg> <reg type="star">✳</reg> dz <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per027231">Hein<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> Vi<choice>
                        <abbr>schˀ</abbr>
                        <expan>scher</expan>
                    </choice>
                </persName> sich klegte <reg type="star">✳</reg> und sinen <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren die an<choice>
                    <abbr>dnˀ</abbr>
                    <expan>dern</expan>
                </choice> guͤ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> uff geben woͤlt<reg>,</reg> getrùwte da wol<reg>,</reg> er soͤlt si<reg>,</reg> die <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice>
        <lb facs="#facs_1_line_1508272590806_180" n="N008"/>sin <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren<reg>,</reg> an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ob<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genanten</expan>
                </choice> matten ungesumpt làssen<reg>,</reg> <reg type="star">✳</reg> und batend gerichtes zuͦ <choice>
                    <abbr>im̄e</abbr>
                    <expan>imme</expan>
                </choice> gicht <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> loͧgen<reg>.</reg> <reg type="star">✳</reg> disz <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>antwurt <persName ref="per016067">Hans Vi<choice>
                        <abbr>schˀ</abbr>
                        <expan>scher</expan>
                    </choice>
                </persName> durch sinen fùr<choice>
                    <abbr>sp̄</abbr>
                    <expan>spre</expan>
                </choice>
                <choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice>
        <lb facs="#facs_1_line_1508272594111_181" n="N009"/>und <choice>
                    <abbr>sp̃ch</abbr>
                    <expan>sprach</expan>
                </choice>: <reg type="star">✳</reg> die <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenante</expan>
                </choice> matt <reg type="star">✳</reg> <choice>
                    <abbr>weˀ</abbr>
                    <expan>were</expan>
                </choice> inne an ko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> von einem sinem frùnd<reg>,</reg> genant <persName ref="per026973">Pinggeser</persName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> hette sy lang zit einem gardian gezinset
        <lb facs="#facs_1_line_1508272597375_182" n="N010"/>mitt <measure>zwein viertel <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>nen</measure><reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>selb sin frùnd <choice>
                    <abbr>weˀ</abbr>
                    <expan>were</expan>
                </choice> nu leider darzuͦ ko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice><reg>,</reg> dz er si nùtt bu<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> noch ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> moͤcht<reg>,</reg> <reg type="star">✳</reg> der soͤlt <choice>
                    <abbr>im̄</abbr>
                    <expan>imm</expan>
                </choice> gelten <reg type="star">✳</reg> und gaͤbe
        <lb facs="#facs_1_line_1508272600463_183" n="N011"/>
                <choice>
                    <abbr>im̄</abbr>
                    <expan>imm</expan>
                </choice> sin recht <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>selben matten fùr sin schuld<reg>,</reg> <reg type="star">✳</reg> und kaͤment fùr den gardian zem klos<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> mittenan<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> und naͤme ein gardian die matten
        <lb facs="#facs_1_line_1508272603024_184" n="N012"/>vom <persName ref="per026973">Pinggeser</persName> uff <reg type="star">✳</reg> und liche die <choice>
                    <abbr>im̄</abbr>
                    <expan>imm</expan>
                </choice> in al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
