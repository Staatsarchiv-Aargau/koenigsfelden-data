  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0333_r"/>
      <p facs="#facs_1_region_1501249286061_296">
        <lb facs="#facs_1_line_1501249291556_297" n="N001"/>Allen den<reg>,</reg> die disen brief sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> kùnd <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gih ich<reg>,</reg> <persName ref="per020314">Uͦl<choice>
                        <abbr>r₎</abbr>
                        <expan>rich</expan>
                    </choice> <choice>
                        <abbr>Schulth̄</abbr>
                        <expan>Schultheiss</expan>
                    </choice>
                    <reg>,</reg> un<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice>
        <lb facs="#facs_1_line_1501249294166_298" n="N002"/>vogt ze Baden</persName>
                <reg>,</reg> das ich enpfangen han von den <orgName ref="org002627">erwirdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice>
                    <reg>,</reg> gaistli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> frowen<reg>,</reg> <reg type="star">✳</reg> der
        <lb facs="#facs_1_line_1501249296975_299" n="N003"/>ebtisse<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent ze Kùngsvelt<reg>,</reg> Sant Claren ordens</orgName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> si mir <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice>
        <lb facs="#facs_1_line_1501249300127_300" n="N004"/>er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redlich ze ai<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> rech<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> ir hus ze <placeName ref="loc000482">Lentzburg</placeName>
                <reg>,</reg> das gele<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> ist
        <lb facs="#facs_1_line_1501249304119_301" n="N005"/>zwischent <persName ref="per020315">Welti von Scherentz</persName> ze ai<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> siten<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> zuͦ der an<choice>
                    <abbr>drē</abbr>
                    <expan>dren</expan>
                </choice> siten <persName ref="per020316">Ruͦtzman von
        <lb facs="#facs_1_line_1501249307613_302" n="N006"/>Bottenwile</persName>
                <reg>,</reg> umb <measure>drithalb pfunt Zovin<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> mùntz<reg>,</reg> die des jares geng <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> gebe sint</measure>
                <reg>,</reg>
        <lb facs="#facs_1_line_1501249310929_303" n="N007"/>die ich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> den vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> frôwen sol <choice>
                    <abbr>wˀen</abbr>
                    <expan>weren</expan>
                </choice> uff <date when="0001-11-11">sant Martis tag</date>
                <reg>.</reg> <reg type="star">✳</reg> wa ich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice>
        <lb facs="#facs_1_line_1501249315368_304" n="N008"/>min er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> das guͦt nit <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>ten uff den vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genan</expan>
                </choice>ten tag den <orgName ref="org002627">erwirdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice>
                    <reg>,</reg> gaistli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice>
        <lb facs="#facs_1_line_1501249319141_305" n="N009"/>froͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice> ze Kùngsvelt</orgName> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> ir nachko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>
                <reg>,</reg> so sol ir ampt<choice>
                    <abbr>mā</abbr>
                    <expan>mann</expan>
                </choice> li<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice> bi vailem guͦt uff min
        <lb facs="#facs_1_line_1501249322680_306" n="N010"/>
                <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> scha<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice>
                <reg>.</reg> mit soli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> gedin<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> <choice>
                    <abbr>wˀi</abbr>
                    <expan>weri</expan>
                </choice>
                <reg>,</reg> das das hus ab <choice>
                    <abbr>brū</abbr>
                    <expan>brun</expan>
                </choice>ni<reg>,</reg> da vor got si<reg>,</reg> so sol
        <lb facs="#facs_1_line_1501249325935_307" n="N011"/>ich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> den vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genan</expan>
                </choice>ten frôwen ir hus wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ma<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> in allen weg<reg>,</reg> als es jetz
        <lb facs="#facs_1_line_1501249330807_308" n="N012"/>gemachet ist<reg>,</reg> ân <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> fro<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> scha<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice>
                <reg>,</reg> ân all ge<choice>
                    <abbr>wˀd</abbr>
                    <expan>werd</expan>
                </choice>
                <reg>.</reg> wa ich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> des
        <lb facs="#facs_1_line_1501249334823_309" n="N013"/>nit tettint<reg>,</reg> so mugent die erwirdi<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> gaistli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> frowen <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> ir amptlùt wol mir <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice>
        <lb facs="#facs_1_line_1501249338490_310" n="N014"/>mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> guͦt vasen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> an griffen<reg>,</reg> es si in<choice>
                    <abbr>dˀt</abbr>
                    <expan>dert</expan>
                </choice> hus <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> ussrent hus<reg>,</reg> untz das ich <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> min
        <lb facs="#facs_1_line_1501249341991_311" n="N015"/>er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> den vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> frôwen ir hus wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> gema<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> ân all ge<choice>
                    <abbr>vˀd</abbr>
                    <expan>verd</expan>
                </choice>
                <reg>,</reg> als es och jetz gemachet
        <lb facs="#facs_1_line_1501249345727_312" n="N016"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> volbraht ist<reg>.</reg> <reg type="star">✳</reg> und des ze ai<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> waren<reg>,</reg> stetten urkùnd <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ewi<choice>
                    <abbr>gˀ</abbr>
