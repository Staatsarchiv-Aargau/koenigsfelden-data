  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0937a_r"/>
      <p facs="#facs_1_region_1512223949543_64">
        <lb facs="#facs_1_region_1512223949543_64l1" n="N001"/>Ich<reg>,</reg> <persName ref="per007289">Walpurg Trucksaͤssin<reg>,</reg> gebo<choice>
                        <abbr>rn̄e</abbr>
                        <expan>rene</expan>
                    </choice> von Waltpurg</persName><reg>,</reg> <reg type="star">✳</reg> unnd <persName ref="per007312">
                    <choice>
                        <abbr>Irm̄ell</abbr>
                        <expan>Irmmell</expan>
                    </choice> von Froͧnsperg</persName> thuͦn kûndt <reg type="star">✳</reg> unnd zuͦwù<choice>
                    <abbr>sen̄</abbr>
                    <expan>senn</expan>
                </choice>
        <lb facs="#facs_1_region_1512223949543_64l2" n="N002"/>allermengklichem<reg>,</reg> <reg type="star">✳</reg> als wir dann <reg type="star">✳</reg> usz sunde<choice>
                    <abbr>rn̄</abbr>
                    <expan>ren</expan>
                </choice> ursachen<reg>,</reg> <reg type="star">✳</reg> unns darzuͦ bewegent<reg>,</reg> <reg type="star">✳</reg> inn den willen kom<choice>
                    <abbr>ēn</abbr>
                    <expan>men</expan>
                </choice> sind <reg type="star">✳</reg> unns <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice>
        <lb facs="#facs_1_region_1512223949543_64l3" n="N003"/>dem <orgName ref="org002627">gottshusz Kùngsfell<choice>
                        <abbr>d₎</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName> zuͦfuͤgen und den waͤlltli<choice>
                    <abbr>ch₎</abbr>
                    <expan>chen</expan>
                </choice> statt anzuͦnaͤ<choice>
                    <abbr>men̄</abbr>
                    <expan>menn</expan>
                </choice>
                <reg>.</reg> <reg type="star">✳</reg> dahar sich <choice>
                    <abbr>nū</abbr>
                    <expan>nun</expan>
                </choice> begeben<reg>,</reg> <reg type="star">✳</reg> das mitt
        <lb facs="#facs_1_region_1512223949543_64l4" n="N004"/>unns der guͦttshalb<reg>,</reg> <reg type="star">✳</reg> so wir an ba<choice>
                    <abbr>rem̄</abbr>
                    <expan>remm</expan>
                </choice> gellt<reg>,</reg> <reg type="star">✳</reg> cleyder <reg type="star">✳</reg> und cleinotte<choice>
                    <abbr>rn̄</abbr>
                    <expan>ren</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> ouch in libdings wysz <reg type="star">✳</reg> in soͤlich gotts<reg type="lb">¬</reg><lb facs="#facs_1_region_1512223949543_64l5" n="N005" break="no"/>husz gebrachtt<reg>,</reg> <reg type="star">✳</reg> soͤlicher gestallt verko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> ist<reg>,</reg> das unns wol benuͤgtt<reg>.</reg> <reg type="star">✳</reg> harumb unns dargaͤgen schuldi<choice>
                    <abbr>g₎</abbr>
                    <expan>gen</expan>
                </choice> pflich<choice>
                    <abbr>te₎</abbr>
                    <expan>ten</expan>
                </choice>
