  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0352_r"/>
      <p facs="#facs_1_region_1501224276041_27">
        <lb facs="#facs_1_line_1501224298936_28" n="N001"/>Wir<reg>,</reg> <persName ref="per012691">Johans<reg>,</reg> von gottes gnaden byschof ze Brichsen<reg>,</reg> <reg type="star">✳</reg> der durlùchtigen<reg>,</reg> hochgebornen fùrsten<reg>,</reg> un<choice>
                        <abbr>sˀr</abbr>
                        <expan>ser</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren <reg type="star">✳</reg> <reg type="star">✳</reg> der hertzogen von Oͤsterich kantzler</persName>
                <reg>,</reg> <reg type="star">✳</reg> und <persName ref="per013239">graf Ruͦdolf von Nydow<reg>,</reg> <reg type="star">✳</reg> derselben un<choice>
                        <abbr>srˀ</abbr>
                        <expan>ser</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren von Oͤsterich houptman <reg type="star">✳</reg> und
        <lb facs="#facs_1_line_1501224302672_29" n="N002"/>lantvogt <reg type="star">✳</reg> in irn landen ze Swaben<reg>,</reg> <reg type="star">✳</reg> ze Ergoͤw <reg type="star">✳</reg> und ze Thurgoͤw</persName>
                <reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jechen <reg type="star">✳</reg> und tuͤn kunt offenlich mit disem briefe<reg>,</reg> <reg type="star">✳</reg> daz wir von sunderlichs heissens <reg type="star">✳</reg> und emphelhens wegen <reg type="star">✳</reg> des <persName ref="per000337">durluchtigen<reg>,</reg> hochgebornen fùrsten<reg>,</reg> <reg type="star">✳</reg> unsers
        <lb facs="#facs_1_line_1501224306751_30" n="N003"/>gnedigen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren <reg type="star">✳</reg> herztog Lùpoltz von Oͤsterich</persName>
                <reg>,</reg> <reg type="star">✳</reg> der fùr sichselben <reg type="star">✳</reg> und den <persName ref="per000336">durlùchtigen<reg>,</reg> hochgebornen fùrsten<reg>,</reg> <reg type="star">✳</reg> unsern gnedigen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren hertzog Albrech<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> von Oͤste<choice>
                        <abbr>rˀ</abbr>
                        <expan>rich</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> sinen bruͦder<reg>,</reg> <reg type="star">✳</reg> uns des <reg type="star">✳</reg> durch den <persName ref="per013240">fromen<reg>,</reg> vesten man <reg type="star">✳</reg> Reynhar<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice>
        <lb facs="#facs_1_line_1501224311456_31" n="N004"/>den Weͣchinger<reg>,</reg> <reg type="star">✳</reg> sinen hofmeister</persName>
                <reg>,</reg> <reg type="star">✳</reg> vollen gewalt gege<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> und ùns das <reg type="star">✳</reg> von irn wegen empholhen hat<reg>,</reg> <reg type="star">✳</reg> den <orgName ref="org002627">erwirdigen<reg>,</reg> geistlichen frowen<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> der ebtischin <reg type="star">✳</reg> und <reg type="star">✳</reg> <reg type="star">✳</reg> den klosterfrowen gemeinlich ze Kùngsvelt <reg type="star">✳</reg> und demsel<choice>
                        <abbr>bn̄</abbr>
                        <expan>ben</expan>
                    </choice>
        <lb facs="#facs_1_line_1501224316064_32" n="N005"/>gotzhuse <reg type="star">✳</reg> und dem convent <reg type="star">✳</reg> Sant Claren ordens<reg>,</reg> <reg type="star">✳</reg> in Kostentzer bystom</orgName> <reg type="star">✳</reg> und allen irn nachkomen <reg type="star">✳</reg> eweklich <reg type="star">✳</reg> an stat <reg type="star">✳</reg> und in namen <reg type="star">✳</reg> der vor<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <orgName ref="org000088">un<choice>
                        <abbr>sˀr</abbr>
                        <expan>ser</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren von Oͤsterich</orgName> <reg type="star">✳</reg> und <choice>
                    <abbr>irˀ</abbr>
                    <expan>irer</expan>
                </choice> er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> lu<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice>lich durch got <reg type="star">✳</reg> und durch der sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice>
        <lb facs="#facs_1_line_1501224319264_33" n="N006"/>
                <orgName ref="org000088">un<choice>
                        <abbr>sˀr</abbr>
                        <expan>ser</expan>
                    </choice> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren von Oͤste<choice>
                        <abbr>rˀ</abbr>
                        <expan>rich</expan>
                    </choice>
                </orgName> <reg type="star">✳</reg> vordren <reg type="star">✳</reg> und irselbs selen <reg type="star">✳</reg> und liben <reg type="star">✳</reg> heiles <reg type="star">✳</reg> und gelùkes <reg type="star">✳</reg> willen<reg>,</reg> <reg type="star">✳</reg> mit al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> orde<choice>
                    <abbr>nūg</abbr>
                    <expan>nung</expan>
                </choice>
                <reg>,</reg> bescheidenheit <reg type="star">✳</reg> und kraft<reg>,</reg> <reg type="star">✳</reg> worten<reg>,</reg> werken <reg type="star">✳</reg> und geberden<reg>,</reg> <reg type="star">✳</reg> die nach geistlichen <reg type="star">✳</reg> und weltlichen rechten<reg>,</reg> <reg type="star">✳</reg> fryheiten <reg type="star">✳</reg> und
        <lb facs="#facs_1_line_1501224323775_34" n="N007"/>gesatzten <reg type="star">✳</reg> in dheinen weg <reg type="star">✳</reg> datzuͦ gehoͤrent<reg>,</reg> <reg type="star">✳</reg> gege<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> recht <reg type="star">✳</reg> und redlich <reg type="star">✳</reg> in wise <reg type="star">✳</reg> und forme einer ewigen<reg>,</reg> <reg type="star">✳</reg> unwi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>ruͤfflichen gabe<reg>,</reg> <reg type="star">✳</reg> die man nennet <reg type="star">✳</reg> under den lebenden<reg>,</reg> <reg type="star">✳</reg> und geben ouch in dem namen<reg>,</reg> als davor<reg>,</reg> wissentlich
        <lb facs="#facs_1_line_1501224327998_35" n="N008"/>mit disem briefe <reg type="star">✳</reg> fùr frye<reg>,</reg> ledig eigen <reg type="star">✳</reg> den frânwalt <reg type="star">✳</reg> und das holtz <reg type="star">✳</reg> gelegen un<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> der <placeName ref="loc011662">burg ze Brunegg <reg type="star">✳</reg> in Ergoͤw</placeName>
                <reg>,</reg> <reg type="star">✳</reg> das man von alter nennet den <placeName ref="loc011663">Hag</placeName>
                <reg>.</reg> <reg type="star">✳</reg> der selbe walt untz har <reg type="star">✳</reg> vor diser gabe <reg type="star">✳</reg> der ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <orgName ref="org000088">un<choice>
                        <abbr>sˀr</abbr>
                        <expan>ser</expan>
                    </choice>
        <lb facs="#facs_1_line_1501224336286_36" n="N009"/>
                    <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren von Oͤsterich</orgName> <reg type="star">✳</reg> fry<reg>,</reg> ledig eigen <reg type="star">✳</reg> und ir besunder holtz <reg type="star">✳</reg> und franwalt <reg type="star">✳</reg> gewesen ist<reg>,</reg> <reg type="star">✳</reg> also daz nieman anders <reg type="star">✳</reg> kein recht darinne gehebt hat<reg>,</reg> <reg type="star">✳</reg> noch zuͦ dheinem twinge <reg type="star">✳</reg> oder banne gehoͤret hat<reg>,</reg> <reg type="star">✳</reg> denne daz die <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>schaft
        <lb facs="#facs_1_line_1501224343574_37" n="N010"/>sun<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>lich <reg type="star">✳</reg> dasselbe holtz <reg type="star">✳</reg> fùr irn rechten gesunderten franwalt <reg type="star">✳</reg> her bracht <reg type="star">✳</reg> und genossen hat<reg>,</reg> <reg type="star">✳</reg> als ouch dù <persName ref="per006964">durlùchtigi<reg>,</reg> hochgeborne fùrstinn <reg type="star">✳</reg> seliger gedechtnùsse<reg>,</reg> <reg type="star">✳</reg> frow Angnes<reg>,</reg> wilent kuniginn ze Ungern<reg>,</reg> stiffterin
        <lb facs="#facs_1_line_1501224349830_38" n="N011"/>und vollebringerin des <choice>
                        <abbr>egen̄</abbr>
                        <expan>egenanten</expan>
                    </choice> gotzhuses <reg type="star">✳</reg> und klosters <reg type="star">✳</reg> ze Kùngsvelt</persName>
                <reg>,</reg> <reg type="star">✳</reg> dero got genade<reg>,</reg> <reg type="star">✳</reg> densel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
