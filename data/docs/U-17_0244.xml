  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0244_r"/>
      <p facs="#facs_1_region_1515952472770_2">
        <lb facs="#facs_1_region_1515952472770_2l1" n="N001"/>Wir<reg>,</reg> <persName ref="per000347">Albrecht <reg type="star">✳</reg> von gotes gnaden <reg type="star">✳</reg> hertzog <reg type="star">✳</reg> ze Oͤste<choice>
                        <abbr>rˀ</abbr>
                        <expan>rich</expan>
                    </choice><reg>,</reg> <reg type="star">✳</reg> ze Steyr <reg type="star">✳</reg> und ze Kernden</persName><reg>,</reg> <reg type="star">✳</reg> tuͤn kund <reg type="star">✳</reg> mit di<reg type="lb">¬</reg><lb facs="#facs_1_region_1515952472770_2l2" n="N002" break="no"/>sem brief<reg>,</reg> <reg type="star">✳</reg> daz wir un<choice>
                    <abbr>srˀ</abbr>
                    <expan>ser</expan>
                </choice> lieben swes<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice><reg>,</reg> <persName ref="per006964">vrowͤn Agnesen<reg>,</reg> <reg type="star">✳</reg> weilent kuniginn ze Un<choice>
                        <abbr>gˀn</abbr>
                        <expan>gern</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> und den<orgName ref="org002627"> er<choice>
                        <abbr>bˀn</abbr>
                        <expan>bern</expan>
                    </choice>
                </orgName><reg>,</reg>
        <lb facs="#facs_1_region_1515952472770_2l3" n="N003"/>
                <orgName ref="org002627">geistlei<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice><reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> der abtessinn und dem convent ze Kunigsveld<reg>,</reg> <reg type="star">✳</reg> Sand Claren ordens<reg>,</reg> <reg type="star">✳</reg> gelegen in
        <lb facs="#facs_1_region_1515952472770_2l4" n="N004"/>Chostentzer bystum</orgName><reg>,</reg> <reg type="star">✳</reg> und dem lutbries<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> ze <placeName ref="loc010715">Ernlispach</placeName> <reg type="star">✳</reg> und iͤrm meyer<reg>,</reg> <reg type="star">✳</reg> der gesezzen ist <reg type="star">✳</reg> uf
        <lb facs="#facs_1_region_1515952472770_2l5" n="N005"/>dem hof ze <placeName ref="loc008451">Ernlispach<reg>,</reg> gelegen in Basler bystûm</placeName><reg>,</reg> <reg type="star">✳</reg> burkrecht geben ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <reg type="star">✳</reg> in un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
