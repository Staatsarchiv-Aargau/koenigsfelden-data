  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0065_r"/>
      <p facs="#facs_1_region_1496743152711_1">
        <lb facs="#facs_1_line_1496743214204_2" n="N001"/>Wir<reg>,</reg> <persName ref="per000342">Lupold<reg>,</reg> von gots gnaden hertzog ze Osterrich <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> ze Steyr<reg>,</reg> herre ze Chreyen<reg>,</reg> uf <choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice>
        <lb facs="#facs_1_line_1496743220902_3" n="N002"/>Marich <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> ze Portenowe<reg>,</reg> <reg type="star">✳</reg> graf ze Habspurg <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> ze Kyburg <reg type="star">✳</reg> und lantgraf in dem Obern
        <lb facs="#facs_1_line_1496743227719_4" n="N003"/>Elsasse</persName>
                <reg>,</reg> <reg type="star">✳</reg> verjehen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> tuͦn chùnt allen den<reg>,</reg> <reg type="star">✳</reg> die diesen brief an sehent <reg type="star">✳</reg> oder hoͤrent lesen<reg>,</reg> daz
        <lb facs="#facs_1_line_1496743234526_5" n="N004"/>unser liebe swes<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <persName ref="per006964">Agnes<reg>,</reg> <reg type="star">✳</reg> von selben gots gnaden weilent chunigin ze Ungern</persName>
                <reg>,</reg> <reg type="star">✳</reg> hat mit
        <lb facs="#facs_1_line_1496743242674_6" n="N005"/>unsers lieben her<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> pruͦ<choice>
                    <abbr>dˀ</abbr>
                    <expan>ders</expan>
                </choice>
                <reg>,</reg> des <persName ref="per007252">hochgeborn chunig <choice>
                        <abbr>Frid̄</abbr>
                        <expan>Fridrich</expan>
                    </choice> von Rom</persName>
                <reg>,</reg> guͤten willen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1496743251328_7" n="N006"/>uͤrlob <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mit unserm <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> aller unser pruͦ<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> guͤtem willen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> uͤrlob geloͤset von dem
        <lb facs="#facs_1_line_1496743259143_8" n="N007"/>
                <persName ref="per007128">erbern ritter Wernhern von Wolan</persName> <reg type="star">✳</reg> <choice>
                    <abbr>um̄</abbr>
                    <expan>umb</expan>
                </choice> <measure>zwenzich mark</measure> <measure>zwenzich stucke geltes</measure> uf
        <lb facs="#facs_1_line_1496743267928_9" n="N008"/>den guͤtern<reg>,</reg> <reg type="star">✳</reg> die hie nach geschriben sint: <reg type="star">✳</reg> in dem <placeName ref="loc009765">Aigen</placeName> zu <placeName ref="loc008679">Lupfanch</placeName> uf <persName ref="per013097">Chunrad Reien</persName>
        <lb facs="#facs_1_line_1496743276560_10" n="N009"/>guͤte <reg type="star">✳</reg> <measure>zwen mutte roken</measure>
                <reg>,</reg> <reg type="star">✳</reg> uf <persName ref="per013054">Ruͦdolfes Weber</persName> guͤte <measure>ein mutte roken</measure>
                <reg>,</reg> uf <persName ref="per013053">Burchart
        <lb facs="#facs_1_line_1496743284872_11" n="N010"/>Moser</persName> guͤte <reg type="star">✳</reg> <measure>zehen viertail roken</measure>
                <reg>,</reg> <reg type="star">✳</reg> uf <persName ref="per013055">Peter Pachman</persName> guͤte <measure>dri mutte rocken</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1496743292208_12" n="N011"/>
                <measure>ein viertail</measure>
                <reg>,</reg> uf <persName ref="per013058">Ulri<choice>
                        <abbr>ch̄</abbr>
                        <expan>chen</expan>
                    </choice> <reg type="star">✳</reg> Rapreswile</persName> guͤte <measure>fùnf viertail rocken</measure>
                <reg>,</reg> <reg type="star">✳</reg> uf <persName ref="per012952">Chunrad Mey<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName>
        <lb facs="#facs_1_line_1496743300248_13" n="N012"/>guͤte <reg type="star">✳</reg> <measure>druͤ viertail rocken</measure>
                <reg>,</reg> <reg type="star">✳</reg> uf <persName ref="per013056">Arnold Pechen</persName> guͤte <measure>neun viertail rocken</measure>
                <reg>,</reg> uf
        <lb facs="#facs_1_line_1496743308281_14" n="N013"/>
                <persName ref="per013096">Chun<choice>
                        <abbr>rad̄</abbr>
                        <expan>raden</expan>
                    </choice> <reg type="star">✳</reg> Elvin<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
                </persName> guͤte <measure>zwei viertail rocken</measure>
                <reg>,</reg> uf <persName ref="per013053">Purchard Moser</persName> guͤte <reg type="star">✳</reg> <measure>drey mùtte
        <lb facs="#facs_1_line_1496743315761_15" n="N014"/>habern</measure> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>ein viertail</measure>
                <reg>,</reg> <reg type="star">✳</reg> uf <persName ref="per013058">Ulrich Rapreswile</persName> guͤte <measure>neun viertail habern</measure>
