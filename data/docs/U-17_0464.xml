                    <expan>ser</expan>
                </choice> er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> der vor<choice>
                    <abbr>gẽn</abbr>
                    <expan>genan</expan>
                </choice>ten <persName ref="per007221">Margaretan</persName><reg>,</reg> un<choice>
