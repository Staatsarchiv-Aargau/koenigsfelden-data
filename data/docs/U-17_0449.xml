  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0449_r"/>
      <p facs="#facs_1_region_1507833717192_192">
        <lb facs="#facs_1_line_1507837218737_1" n="N001"/>Ich<reg>,</reg> <persName ref="per026605">Cuͦn<choice>
                        <abbr>r₎</abbr>
                        <expan>rat</expan>
                    </choice> Ernst<reg>,</reg> lùpries<choice>
                        <abbr>t₎</abbr>
                        <expan>ter</expan>
                    </choice> ze Brugg</persName>
                <reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>giche mitt urkùnd diss brieffes<reg>,</reg> <reg type="star">✳</reg> als mir die erwirdigen<reg>,</reg> geistli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> froͧ<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice>
                <reg>,</reg> die
        <lb facs="#facs_1_line_1507837225041_2" n="N002"/>eptissin und convent<reg>,</reg> die gnad hand getaͧn an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> jerli<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> zin<choice>
                    <abbr>s₎</abbr>
                    <expan>sen</expan>
                </choice>
                <reg>,</reg> so ich <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> frovastlich untz har ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> habe<reg>,</reg> <reg type="star">✳</reg> dz ich <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice>
        <lb facs="#facs_1_line_1507837230892_3" n="N003"/>sol von disen nechsten zwein <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gang<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> fròvasten <measure>sechtze<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> pfunt</measure>
                <reg>,</reg> die ich <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> oͧch gen<choice>
                    <abbr>wt₎</abbr>
                    <expan>wert</expan>
                </choice> hab<reg>,</reg> <reg type="star">✳</reg> und dz ich <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> hin<choice>
                    <abbr>nāt</abbr>
                    <expan>nant</expan>
                </choice>hin
        <lb facs="#facs_1_line_1507837236213_4" n="N004"/>oͧch bi guͦten trùwen ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> sol <measure>alle frovasten acht pfunt</measure>
                <reg>.</reg> und darùber so ich denn meist mag und nùtt darun<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> dz
        <lb facs="#facs_1_line_1507837241741_5" n="N005"/>ich darumb von <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> gna<choice>
                    <abbr>dn̄</abbr>
                    <expan>den</expan>
                </choice> we<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> so si mir haran erzoͤgt hand<reg>,</reg> <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>spro<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> habe<reg>,</reg> wz <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> abgaͧt an den <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> zwein
        <lb facs="#facs_1_line_1507837249166_6" n="N006"/>froͧvasten <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> inen hin<choice>
                    <abbr>nāt</abbr>
                    <expan>nant</expan>
                </choice>hin von mir uss staͧt ùber die <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> acht pfunt<reg>,</reg> so ich <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> minr geweren<reg>,</reg> denn als ich von
        <lb facs="#facs_1_line_1507837254582_7" n="N007"/>al<choice>
                    <abbr>t₎</abbr>
                    <expan>ter</expan>
                </choice> har frovastlich getaͧn hab<reg>,</reg> <reg type="star">✳</reg> dz ich es geschla<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> hab uff alles min guͦt<reg>,</reg> ligendes und varendes<reg>,</reg> gùlt und gelt<reg>,</reg> nùtzit
        <lb facs="#facs_1_line_1507837259783_8" n="N008"/>uss geno<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>
                <reg>.</reg> <reg type="star">✳</reg> also dz ich dz selbig guͦt<reg>,</reg> so ich jetz hab <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> gewinn<reg>,</reg> nùtt <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>fen<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice>
                <reg>,</reg> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>set<choice>
                    <abbr>zē</abbr>
                    <expan>zen</expan>
                </choice> noch bekùm<choice>
                    <abbr>bˀn</abbr>
                    <expan>bern</expan>
                </choice> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>tuͦn sol<reg>,</reg> <reg type="star">✳</reg> denn dz ich es
        <lb facs="#facs_1_line_1507837264750_9" n="N009"/>nut<choice>
                    <abbr>zē</abbr>
                    <expan>zen</expan>
                </choice>
                <reg>,</reg> niessen<reg>,</reg> beset<choice>
                    <abbr>zē</abbr>
                    <expan>zen</expan>
                </choice> und entset<choice>
                    <abbr>zē</abbr>
                    <expan>zen</expan>
                </choice> sol und mag zuͦ mines lips rech<choice>
                    <abbr>t₎</abbr>
                    <expan>ter</expan>
                </choice> notùrft<reg>,</reg> aͧn gvar<reg>.</reg> <reg type="star">✳</reg> und wz ich nach tode des <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> guͦtes
        <lb facs="#facs_1_line_1507837270231_10" n="N010"/>laͧsse<reg>,</reg> <reg type="star">✳</reg> dz <reg type="star">✳</reg> das die <choice>
                    <abbr>egn̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> min froͧ<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> und wem si dz entpfelend vorab ne<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> soͤnd <reg type="star">✳</reg> untz <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> bezalt <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>de die summ geltz<reg>,</reg> als vil
        <lb facs="#facs_1_line_1507837275848_11" n="N011"/>dz gebùrt<reg>,</reg> so ich <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> dennecht <choice>
                    <abbr>ùbˀ</abbr>
                    <expan>ùber</expan>
                </choice> die vor<choice>
                    <abbr>gn̄̄</abbr>
                    <expan>genanten</expan>
                </choice> acht pfunt schuldig belibe vor <choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>lichem aͧn menlichs su<choice>
                    <abbr>mūg</abbr>
                    <expan>mung</expan>
                </choice> und wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice>red<reg>.</reg>
        <lb facs="#facs_1_line_1507837280776_12" n="N012"/>und <choice>
                    <abbr>wen̄</abbr>
                    <expan>wenn</expan>
                </choice> si <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>sum̄</abbr>
                    <expan>summ</expan>
                </choice> bezalt <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>dend<reg>,</reg> <reg type="star">✳</reg> dz denn dz ùbrig guͦt<reg>,</reg> es sy ligend <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> varend<reg>,</reg> wz ich gelaͧssen hab<reg>,</reg> <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>den <reg type="star">✳</reg> und fallen
        <lb facs="#facs_1_line_1507837284720_13" n="N013"/>sol mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> frùnden und dem es von rechtz we<choice>
                    <abbr>gē</abbr>
                    <expan>gen</expan>
                </choice> <choice>
                    <abbr>wˀ</abbr>
                    <expan>wer</expan>
                </choice>den sol<reg>,</reg> <reg type="star">✳</reg> uss geno<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> fùnftze<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> <choice>
                    <abbr>gl̄</abbr>
                    <expan>guldin</expan>
                </choice>
                <reg>,</reg> so den sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> froͧ<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> oͧch vorab
        <lb facs="#facs_1_line_1507837290216_14" n="N014"/>
                <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> minem guͦte vallen sol<reg>,</reg> als wir des vor ziten ùber kamend<reg>.</reg> <reg type="star">✳</reg> mag <choice>
                    <abbr>abˀ</abbr>
                    <expan>aber</expan>
                </choice> ich bi guͦ<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> trù<choice>
                    <abbr>wē</abbr>
                    <expan>wen</expan>
                </choice> <choice>
                    <abbr>ùbˀ</abbr>
                    <expan>ùber</expan>
                </choice> mines lips noturft
        <lb facs="#facs_1_line_1507837296185_15" n="N015"/>me geweren frovastlich denne acht pfund<reg>,</reg> dz sol ich oͧch tuͦn <reg type="star">✳</reg> bi mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> trùw<reg>,</reg> so ich ha<choice>
                    <abbr>rūb</abbr>
                    <expan>rumb</expan>
                </choice> ge<choice>
                    <abbr>bn̄</abbr>
