  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0365_r"/>
      <p facs="#facs_1_region_1516706090448_4">
        <lb facs="#facs_1_region_1516706090448_4l1" n="N001"/>Ich<reg>,</reg> <persName ref="per020273">Hiltpolt Bruͦder<reg>,</reg> schultheisse ze Waltzhuͦt</persName><reg>,</reg> <reg type="star">✳</reg> tuͦn kunt allen den<reg>,</reg> die disen brief an sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich<reg>,</reg> <reg type="star">✳</reg> das ich offenlich ze gerichte sasse ze <placeName ref="loc007770">Waltzhuͦt</placeName> in der stat an offen<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice><reg>,</reg>
        <lb facs="#facs_1_region_1516706090448_4l2" n="N002"/>fryen strasse <reg type="star">✳</reg> an der aller <orgName ref="org000088">dùrlùchten<reg>,</reg> hocherbor<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> fùrsten <reg type="star">✳</reg> stat<reg>,</reg> mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> gnedigen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren<reg>,</reg> der <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>zogen von Oͤs<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName><reg>,</reg> <reg type="star">✳</reg> und kament da fùr mich in gerichte <reg type="star">✳</reg> <persName ref="per024264">Uͦlrich der Keller
        <lb facs="#facs_1_region_1516706090448_4l3" n="N003"/>von Waltzhuͦt<reg>,</reg> <reg type="star">✳</reg> phruͦnder <reg type="star">✳</reg> des gotzhuses ze Kùngsvelden<reg>,</reg> Sant Claren ordens<reg>,</reg> in Costent<choice>
                        <abbr>zˀ</abbr>
                        <expan>zer</expan>
                    </choice> bystuͦm</persName> gelegen<reg>,</reg> <reg type="star">✳</reg> ze einem teile<reg>,</reg> <reg type="star">✳</reg> und ze dem andren teile der bescheiden
        <lb facs="#facs_1_region_1516706090448_4l4" n="N004"/>
                <persName ref="per024217">Walther Moshein<reg>,</reg> bur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> ze Brugg</persName><reg>,</reg> <reg type="star">✳</reg> in namen und an stat <reg type="star">✳</reg> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> <orgName ref="org002627">gnedi<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice> froͧwen<reg>,</reg> <reg type="star">✳</reg> der eptischin<reg>,</reg> und des gotzhuses gemeinlichen ze Kùngsvelte</orgName> <reg type="star">✳</reg> als iro gewis<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice>
        <lb facs="#facs_1_region_1516706090448_4l5" n="N005"/>botte in di<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> nachgeschribe<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> sache<reg>,</reg> <reg type="star">✳</reg> und staltent sich ze beiden teilen vor mir in gericht mit fùrsprechen<reg>.</reg> <reg type="star">✳</reg> und offnote der <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per024264">Uͦlrich Keller</persName> mit fùrsprechen<reg>,</reg>
        <lb facs="#facs_1_region_1516706090448_4l6" n="N006"/>das er von rech<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> bekantnust und von sun<choice>
                    <abbr>dˀn</abbr>
                    <expan>dern</expan>
                </choice> trùwen und gnaden<reg>,</reg> so im die <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> <reg type="star">✳</reg> dù <orgName ref="org002627">eptischin und der convent ze Kùngsvelde</orgName> <reg type="star">✳</reg> da her vil jaren getan hettin
        <lb facs="#facs_1_region_1516706090448_4l7" n="N007"/>und von hin noch wol tuͦn moͤchtin<reg>,</reg> <reg type="star">✳</reg> den selben minen froͧwen<reg>,</reg> <reg type="star">✳</reg> der <orgName ref="org002627">eptischin und dem convent gemeinlichen ze Kùngsveld</orgName><reg>,</reg> <reg type="star">✳</reg> machen<reg>,</reg> <reg type="star">✳</reg> fuͤgen <reg type="star">✳</reg> und ordenon woͤlti
        <lb facs="#facs_1_region_1516706090448_4l8" n="N008"/>alles sin guͦt<reg>,</reg> ligendes und varendes<reg>,</reg> <reg type="star">✳</reg> das er des tags hetti und hie nach je<choice>
                    <abbr>mˀ</abbr>
                    <expan>mer</expan>
                </choice> gewunne<reg>,</reg> <reg type="star">✳</reg> wie guͦt geheissen und genant <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice><reg>,</reg>
                <reg type="star">✳</reg> wa <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> an welhen stetten es
        <lb facs="#facs_1_region_1516706090448_4l9" n="N009"/>gelegen <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice><reg>,</reg> und batt im ze ervarnde an einer urteild<reg>,</reg> ob er das tuͦn moͤcht <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> wie er es tuͦn soͤlti<reg>,</reg> <reg type="star">✳</reg> das es kraft<choice>
                    <abbr>bˀ</abbr>
                    <expan>ber</expan>
                </choice> <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice> nu und hie nach und oͧch recht
        <lb facs="#facs_1_region_1516706090448_4l10" n="N010"/>
                <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> und batt im oͧch da bi an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selben urteild ze ervarnde<reg>,</reg> <reg type="star">✳</reg> ob er das gemechte und das fuͤgen <reg type="star">✳</reg> wol tuͦn moͤchti <reg type="star">✳</reg> an des ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per024217">Walt<choice>
                        <abbr>hˀs</abbr>
                        <expan>hers</expan>
                    </choice> Mosheins</persName> hant<reg>,</reg>
        <lb facs="#facs_1_region_1516706090448_4l11" n="N011"/>
                <reg type="star">✳</reg> den oͧch die ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> <reg type="star">✳</reg> die eptischin und <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> convent <reg type="star">✳</reg> dar umb <reg type="star">✳</reg> fùr gericht gesant hettin<reg>,</reg> <reg type="star">✳</reg> als oͧch <choice>
                    <abbr>dˀo</abbr>
                    <expan>dero</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selbe <persName ref="per024217">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Moͤsin</persName> offnote und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jach<reg>,</reg> <reg type="star">✳</reg> das er dar umb
        <lb facs="#facs_1_region_1516706090448_4l12" n="N012"/>an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> froͧwen stat gemeinlichen <reg type="star">✳</reg> da <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> ze iren handen <reg type="star">✳</reg> das gemechte uff ze nemende<reg>.</reg> <reg type="star">✳</reg> und da bi<reg>,</reg> do batt im <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genante</expan>
                </choice> <persName ref="per024217">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Moͤsin</persName>
        <lb facs="#facs_1_region_1516706090448_4l13" n="N013"/>an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selben urteild ze ervarnde<reg>,</reg> <reg type="star">✳</reg> ob er das gemechte wol <reg type="star">✳</reg> an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> si<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> froͧwen stat<reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> eptischin und des conventes<reg>,</reg> <choice>
                    <sic>wff</sic>
                    <corr>uff</corr>
                </choice> nemen moͤchti<reg>,</reg> <reg type="star">✳</reg> das es
        <lb facs="#facs_1_region_1516706090448_4l14" n="N014"/>kraft hetti und oͧch recht nu und hie nach<reg>.</reg> <reg type="star">✳</reg> dar umb fragte ich <reg type="star">✳</reg> und wart oͧch erteilet nach mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> frage mit gesamno<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urteild uff den eid nach
        <lb facs="#facs_1_region_1516706090448_4l15" n="N015"/>rede und wi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> rede<reg>,</reg> sid der ob<choice>
                    <abbr>gen̄</abbr>
                    <expan>genante</expan>
                </choice> <persName ref="per024264">Uͦlrich Keller</persName> <reg type="star">✳</reg> da vorgericht stuͤnde gesunt libes und siner sinnen<reg>,</reg> <reg type="star">✳</reg> als er offnote<reg>,</reg> <reg type="star">✳</reg> und oͧch das gemechte also
        <lb facs="#facs_1_region_1516706090448_4l16" n="N016"/>machen und fuͤgen woͤltin <reg type="star">✳</reg> den <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> minen froͤwen<reg>,</reg> <reg type="star">✳</reg> und oͧch <reg type="star">✳</reg> der <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per024217">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Moͤsin</persName> da stuͤndi an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> selben mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> froͧwen stat und oͧch das gemechte
        <lb facs="#facs_1_region_1516706090448_4l17" n="N017"/>ze iren handen uff nemen woͤlti<reg>,</reg> <reg type="star">✳</reg> das der <persName ref="per024264">Kel<choice>
                        <abbr>lˀ</abbr>
                        <expan>ler</expan>
                    </choice>
                </persName> das mit sinr hant wol uff geben<reg>,</reg> <reg type="star">✳</reg> machen <reg type="star">✳</reg> und fuͤgen moͤchti <reg type="star">✳</reg> und das es oͧch der <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per024217">Walther
        <lb facs="#facs_1_region_1516706090448_4l18" n="N018"/>Moͤsin</persName> <reg type="star">✳</reg> an mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> froͧwen stat und ze iren handen wol uff nemen moͤchti<reg>.</reg> do das alles urteild gab<reg>,</reg> <reg type="star">✳</reg> do stuͦnd der <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per024264">Uͦlrich der Keller</persName> dar
        <lb facs="#facs_1_region_1516706090448_4l19" n="N019"/>und machte und fuͤgte <reg type="star">✳</reg> dem <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per024217">Walther Moͤsin</persName> <reg type="star">✳</reg> an sin hant <reg type="star">✳</reg> ze der <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> froͧwen hant und an ir stat alles sin guͦt<reg>,</reg> ligentz und varentz<reg>,</reg>
        <lb facs="#facs_1_region_1516706090448_4l20" n="N020"/>wie guͤt geheissen und genant ist<reg>,</reg> <reg type="star">✳</reg> wa oder an welhen stetten es gelegen ist<reg>,</reg> fùr sich und alle sin erben<reg>,</reg> <reg type="star">✳</reg> und nam oͧch <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenante</expan>
                </choice> <persName ref="per024217">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Moͤsin</persName> das
        <lb facs="#facs_1_region_1516706090448_4l21" n="N021"/>gemechte <reg type="star">✳</reg> also uff <reg type="star">✳</reg> an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> mi<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> froͧwen stat und ze iren handen<reg>.</reg> und geschach dis alles mit aller der sicherheit<reg>,</reg> worten und werken<reg>,</reg> so
        <lb facs="#facs_1_region_1516706090448_4l22" n="N022"/>deheins wegs hier zuͦ gehorte nach recht und nach guͦ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> gewonheit <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> stat ze <placeName ref="loc007770">Waltzhuͦt</placeName><reg>,</reg> und wart oͧch erteilt uff den eid<reg>,</reg> das es billich und
        <lb facs="#facs_1_region_1516706090448_4l23" n="N023"/>von recht nu und hie nach krafttber sin soͤlle<reg>.</reg> dis dinges sint gezùge: <persName ref="per020300">Pe<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> Fridrich</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per016771">Ruͦdolf Notstein</persName><reg>,</reg> <persName ref="per025600">Johans Notstein genant Wolf</persName><reg>,</reg>
        <lb facs="#facs_1_region_1516706090448_4l24" n="N024"/>
                <persName ref="per020433">Burkart Kybur<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice></persName><reg>,</reg> <persName ref="per024271">Claus Roto</persName><reg>,</reg> <persName ref="per024273">Johans von Grasoͤwe</persName><reg>,</reg> <persName ref="per024274">Chuͦntzman <choice>
                        <abbr>Hˀ</abbr>
                        <expan>Her</expan>
                    </choice>man</persName><reg>,</reg> <persName ref="per024275">Johans Urribold</persName><reg>,</reg> <persName ref="per025601">Heinrich un<choice>
                        <abbr>dˀm</abbr>
                        <expan>derm</expan>
                    </choice> Buͤl</persName> <reg type="star">✳</reg> und <persName ref="per024276">Arnolt Bi<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice>man<reg>,</reg> bur<choice>
                        <abbr>gˀe</abbr>
                        <expan>gere</expan>
                    </choice>
        <lb facs="#facs_1_region_1516706090448_4l25" n="N025"/>ze Waltzhuͦt</persName><reg>,</reg> und an<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> er<choice>
                    <abbr>bˀ</abbr>
                    <expan>ber</expan>
                </choice> lùten vil<reg>.</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> hier <choice>
                    <abbr>ûbˀ</abbr>
                    <expan>ûber</expan>
                </choice> ze warem und offennem ûrkûnde al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice> vorgeschribe<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
