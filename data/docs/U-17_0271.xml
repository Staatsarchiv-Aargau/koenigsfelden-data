  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0271_r"/>
      <p facs="#facs_1_region_1501242747951_135">
        <lb facs="#facs_1_line_1501242761524_136" n="N001"/>Allen den<reg>,</reg> die disen brief ansehent <reg type="star">✳</reg> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> <reg type="star">✳</reg> kund ich<reg>,</reg> <persName ref="per012691">Johans von Platzheim<reg>,</reg> <reg type="star">✳</reg> kil<choice>
                        <abbr>chˀ</abbr>
                        <expan>cher</expan>
                    </choice>re der kilchen ze sant Sterien <reg type="star">✳</reg> in Bysen<choice>
                        <abbr>tzˀ</abbr>
                        <expan>tzer</expan>
                    </choice> bystuͦm <reg type="star">✳</reg> und
        <lb facs="#facs_1_line_1501242767477_137" n="N002"/>obris<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> schri<choice>
                        <abbr>bˀ</abbr>
                        <expan>ber</expan>
                    </choice>
                </persName> des <persName ref="per000335">hocherbornen fuͤrsten<reg>,</reg> mins gnedigen <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>ren<reg>,</reg> <reg type="star">✳</reg> <choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice>tzog <choice>
                        <abbr>Ruͦd̄</abbr>
                        <expan>Ruͦdolf</expan>
                    </choice> <reg type="star">✳</reg> von Oͤste<choice>
                        <abbr>rˀ</abbr>
                        <expan>rich</expan>
                    </choice>
                </persName><reg>,</reg> <reg type="star">✳</reg> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich offenlich<reg>,</reg> <reg type="star">✳</reg> daz ich <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͤuffet han recht <reg type="star">✳</reg> und red<reg type="lb">¬</reg><lb facs="#facs_1_line_1501242772716_138" n="N003" break="no"/>lich <reg type="star">✳</reg> und <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͤuffe mit disem brief <reg type="star">✳</reg> fuͤr mich und alle min erben <reg type="star">✳</reg> eins rechten<reg>,</reg> steten <reg type="star">✳</reg> und ewigen kouffes <reg type="star">✳</reg> den <orgName ref="org002627">erwirdigen<reg>,</reg> geistli<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice> froͤwn<reg>,</reg>
        <lb facs="#facs_1_line_1501242779660_139" n="N004"/>der eptischin <reg type="star">✳</reg> und dem convent dez klos<choice>
                        <abbr>tˀs</abbr>
                        <expan>ters</expan>
                    </choice> ze Kùngsvelt<reg>,</reg> <reg type="star">✳</reg> Sant Claren ordens<reg>,</reg> Kosten<choice>
                        <abbr>tzˀ</abbr>
                        <expan>tzer</expan>
                    </choice> bystums</orgName><reg>,</reg> <reg type="star">✳</reg> an des sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> irs clos<choice>
                    <abbr>tˀs</abbr>
                    <expan>ters</expan>
                </choice> stat <reg type="star">✳</reg> dis nachgeschri<choice>
                    <abbr>b̄n</abbr>
                    <expan>ben</expan>
                </choice>
        <lb facs="#facs_1_line_1501242785255_140" n="N005"/>guͤ<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <reg type="star">✳</reg> umb <measure>sechshun<choice>
                        <abbr>dˀt</abbr>
                        <expan>dert</expan>
                    </choice> und zweinzig guldin<reg>,</reg> <reg type="star">✳</reg> guͤ<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> <reg type="star">✳</reg> und vollen swe<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice> von Florentz</measure><reg>,</reg> <reg type="star">✳</reg> die ich von inen gar gezalt <reg type="star">✳</reg> und gewegen emphangen <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_line_1501242790149_141" n="N006"/>in mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> schin<choice>
                    <abbr>bˀn</abbr>
                    <expan>bern</expan>
                </choice> nuͤtze gentzlich bekert han<reg>,</reg> <reg type="star">✳</reg> dez ich an disem brief <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>gich<reg>,</reg> <reg type="star">✳</reg> und han diesel<choice>
                    <abbr>b̄n</abbr>
                    <expan>ben</expan>
                </choice> froͤwen <reg type="star">✳</reg> und ir wissenthaf<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> phle<choice>
                    <abbr>gˀ</abbr>
                    <expan>ger</expan>
                </choice> <reg type="star">✳</reg> und
        <lb facs="#facs_1_line_1501242795734_142" n="N007"/>schaf<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> an ir stat <reg type="star">✳</reg> in namen irs <choice>
                    <abbr>egen̄̄</abbr>
                    <expan>egenanten</expan>
                </choice> clos<choice>
                    <abbr>tˀs</abbr>
                    <expan>ters</expan>
                </choice> <reg type="star">✳</reg> dirr selben nachgeschriben<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> guͤ<choice>
                    <abbr>tˀn</abbr>
