      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0409_r"/>
      <p facs="#facs_1_region_1593464720538_240">
        <lb facs="#facs_1_region_1593464720538_240l1" n="N001"/>Allen den<reg>,</reg> die disen brief an sehent <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> hoͤrent besen<reg>,</reg> kùnd ich<reg>,</reg> <persName ref="per028476">Ruͦdolf Le<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice><reg>,</reg> vogt ze Gebistorf</persName><reg>,</reg> dz ich offenlich ze gerichte sas ze <placeName ref="loc009219">Gebistorf</placeName> in dem dorfe
        <lb facs="#facs_1_region_1593464720538_240l2" n="N002"/>an off<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice><reg>,</reg> fry<choice>
                    <abbr>gˀ</abbr>
                    <expan>ger</expan>
                </choice> strasse<reg>,</reg> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> heisens wegen <persName ref="per028542">juncher Lùtoldes zuͦ dem Tor<reg>,</reg> un<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice>vogtes ze Baden</persName> zuͦ den ziten<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> an stat <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <orgName ref="org000088">durchlùchten fùrsten<reg>,</reg> der
        <lb facs="#facs_1_region_1593464720538_240l3" n="N003"/>herzogen <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Oͤsterich</orgName><reg>,</reg> un<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice> gnaͤdigen <choice>
                    <abbr>hˀ</abbr>
                    <expan>her</expan>
                </choice>ren<reg>,</reg> da fùr mich kament in offen<reg>,</reg> verbanne gericht <persName ref="per028503">Ruͤdi <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Rùs</persName> mit <persName ref="per028504">Adelheiten</persName><reg>,</reg> si<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> elichen wirtin<reg>,</reg>
        <lb facs="#facs_1_region_1593464720538_240l4" n="N004"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mit <persName ref="per028543">Hans</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028544">Chuͤnin</persName><reg>,</reg> iro bei<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> elichen kint<reg>,</reg> zuͦ einem teil <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per024834">Hans Fors<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice></persName><reg>,</reg> gesessen oben in dem dorf ze <placeName ref="loc005974">Windisch</placeName><reg>.</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> offenbart <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> vorgenat
        <lb facs="#facs_1_region_1593464720538_240l5" n="N005"/>
                <persName ref="per028503">Ruͤdi <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Rùs</persName><reg>,</reg> <persName ref="per028504">Adelheit</persName><reg>,</reg> sin eliche wirtin<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> iro kint<reg>,</reg> dz si recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redlich ze koͧffen geben hàt eines rechten<reg>,</reg> redlichen koͧffes unwi<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> ruͤflich
        <lb facs="#facs_1_region_1593464720538_240l6" n="N006"/>iro <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> menlichs einen sechzehenden teil des <placeName ref="loc009245">vares ze Windisch</placeName><reg>,</reg> an <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> <placeName ref="loc001093">Rùs</placeName><reg>,</reg> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> iro recht erb <choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice> <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> egenan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> ùnser <orgName ref="org000088">herschaft <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Oͤs<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>rich</orgName><reg>,</reg> dem
        <lb facs="#facs_1_region_1593464720538_240l7" n="N007"/>
                <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per024834">Hansen Fors<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice>
                </persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sinen erben<reg>,</reg> ob er en<choice>
                    <abbr>wˀe</abbr>
                    <expan>were</expan>
                </choice><reg>,</reg> umb <measure>fùnf <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> fierzig phunt phen<choice>
                        <abbr>nīg</abbr>
                        <expan>ning</expan>
                    </choice><reg>,</reg> die do ze maͤl geng <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> geb warent</measure><reg>,</reg> dero wir gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gentzklich
        <lb facs="#facs_1_region_1593464720538_240l8" n="N008"/>ge<choice>
                    <abbr>wˀt</abbr>
                    <expan>wert</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> bezalt sint<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> batent inen die vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per028503">Ruͤdi <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Rùs</persName><reg>,</reg> <persName ref="per028504">Adelheit</persName><reg>,</reg> sin eliche wirtin<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ir bei<choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> kint ze erfarent an ei<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> urteil<reg>,</reg> wie si den vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice>
        <lb facs="#facs_1_region_1593464720538_240l9" n="N009"/>
                <persName ref="per024834">Hansen Vorster</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sin erben besorgen soͤltent<reg>,</reg> dz si daran habent <choice>
                    <abbr>wˀent</abbr>
                    <expan>werent</expan>
                </choice><reg>.</reg> do fragt ich<reg>,</reg> obgenan<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> <persName ref="per028476">Le<choice>
                        <abbr>dˀ</abbr>
                        <expan>der</expan>
                    </choice><reg>,</reg> vogt</persName><reg>,</reg> umb uff den eid<reg>,</reg> do wart mit einhelli<choice>
                    <abbr>gˀ</abbr>
                    <expan>ger</expan>
                </choice>
        <lb facs="#facs_1_region_1593464720538_240l10" n="N010"/>urteil erteilt<reg>,</reg> dz die <choice>
                    <abbr>egen̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per028503">Ruͤdi</persName><reg>,</reg> <persName ref="per028504">Adelheit</persName><reg>,</reg> sin wip<reg>,</reg> <persName ref="per028543">Hans</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028544">Chuͦni</persName><reg>,</reg> iro kind<reg>,</reg> dar gie<choice>
                    <abbr>g₎</abbr>
                    <expan>gen</expan>
                </choice> <choice>
                    <abbr>ūn</abbr>
                    <expan>und</expan>
                </choice> sich erzigen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> uf geben den vorbe<choice>
                    <abbr>nē</abbr>
                    <expan>nemp</expan>
                </choice>ten sechzehenden teil
        <lb facs="#facs_1_region_1593464720538_240l11" n="N011"/>an min hant<reg>,</reg> dz si oͧch getan hant<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gaben uf <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> entzigen sich an min hant des jetz<choice>
                    <abbr>gen̄</abbr>
                    <expan>genanten</expan>
                </choice> sechzehenden teil des vares fùr sich selber <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alle
        <lb facs="#facs_1_region_1593464720538_240l12" n="N012"/>ir erben <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> nachkomen al<choice>
                    <abbr>lˀ</abbr>
                    <expan>ler</expan>
                </choice> <choice>
                    <abbr>dˀ</abbr>
                    <expan>der</expan>
                </choice> rechten<reg>,</reg> so er <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> sin erben dar an habent <choice>
                    <abbr>wˀen</abbr>
                    <expan>weren</expan>
                </choice><reg>,</reg> <choice>
                    <abbr>un̄</abbr>
