    <body>
            <pb facs="#facs_97" n="fol. 47r" xml:id="img_0097" source="saa-0428_047r"/>
      <p facs="#facs_97_region_1571404160344_3041" rend="Stücknummer_Kopialbuch" type="marginal">
        <lb facs="#facs_97_line_1571404167332_3047" n="N001"/>
