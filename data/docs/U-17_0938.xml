        <lb facs="#facs_1_region_1512224514137_70l3" n="N001"/>Allenn de<choice>
                    <abbr>nen̄</abbr>
                    <expan>nenn</expan>
                </choice><reg>,</reg> <reg type="star">✳</reg> so di<choice>
                    <abbr>sen̄</abbr>
                    <expan>senn</expan>
                </choice> brieff ansechen <reg type="star">✳</reg> oder hoͤ<choice>
