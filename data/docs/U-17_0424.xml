  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0424_r"/>
      <p facs="#facs_1_region_1593465102137_290">
        <lb facs="#facs_1_region_1593465102137_290l1" n="N001"/>Allen den<reg>,</reg> die disen brief sechend oder hoͤrend lesen<reg>,</reg> <reg type="star">✳</reg> kùnd ich<reg>,</reg> <persName ref="per014819">Hans <choice>
                        <abbr>Schulth̄</abbr>
                        <expan>Schultheiss</expan>
                    </choice> ze Lentzburg</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> vergich of<choice>
                    <abbr>fen̄</abbr>
                    <expan>fenn</expan>
                </choice>lich mit disem brief<reg>,</reg> dz ich
        <lb facs="#facs_1_region_1593465102137_290l2" n="N002"/>an der <orgName ref="org000088">hochgebornen fùrsten<reg>,</reg> der hertzogen ze Oͤsterrich<reg>,</reg> mi<choice>
                        <abbr>nˀ</abbr>
                        <expan>ner</expan>
                    </choice> gnedigen herren</orgName>
                <reg>,</reg> stat vor der statt ze <placeName ref="loc000482">Lentzburg</placeName> under dem <placeName ref="loc009849">Sarbach</placeName> of<choice>
                    <abbr>fen̄</abbr>
                    <expan>fenn</expan>
                </choice>lich ze
        <lb facs="#facs_1_region_1593465102137_290l3" n="N003"/>gerichte sas<reg>,</reg> da in gericht fùr mich kamen <persName ref="per028561">Verena Huͦber</persName>
                <reg>,</reg> wilent <persName ref="per028560">Cuͦnratz Renolde<reg>,</reg> seligen</persName>
                <reg>,</reg> elichù wirtin<reg>,</reg> mit <persName ref="per028559">Walthˀ Renold</persName>
                <reg>,</reg> irm wissēt
        <lb facs="#facs_1_region_1593465102137_290l4" n="N004"/>haftem vogt<reg>,</reg> der ir vor mir mit gericht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> urteil ze ei<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice> vogt ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> wart <choice>
                    <abbr>ùbˀ</abbr>
                    <expan>ùber</expan>
                </choice> dis nachgeschri<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> sach<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028563">Hensli</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028564">Cleͧwi</persName>
                <reg>,</reg> der jetz<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per028561">Vere<choice>
                        <abbr>nē</abbr>
                        <expan>nen</expan>
                    </choice>
                </persName>
        <lb facs="#facs_1_region_1593465102137_290l5" n="N005"/>elichù kint<reg>,</reg> mit <persName ref="per028562">Hans Renold</persName>
                <reg>,</reg> des <choice>
                    <abbr>egn̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per028560">Cuͦn<choice>
                        <abbr>r₎</abbr>
                        <expan>rad</expan>
                    </choice> Renolds<reg>,</reg> seli<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice>
                </persName>
                <reg>,</reg> bruͦder<reg>,</reg> irm wishenthaftem vogt<reg>,</reg> mit fùrsprechen ze ei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> teil <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028565">Cuͦn<choice>
                        <abbr>r₎</abbr>
                        <expan>rad</expan>
                    </choice> Leinbach
        <lb facs="#facs_1_region_1593465102137_290l6" n="N006"/>von Brunegg</persName> mit fùrsprechen ze dem andren teil<reg>.</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> offnete <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> verjachen do vor mir in gericht die vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per028561">Vrena Huͦber</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028563">Hensli</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028564">Clewi</persName>
                <reg>,</reg>
        <lb facs="#facs_1_region_1593465102137_290l7" n="N007"/>ir kind<reg>,</reg> mit irn wissenthaften voͤgten <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mit fùrsprechen<reg>,</reg> dz si frilich<reg>,</reg> willeklich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> unbetwungenlich recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redlich ze ei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> ewigen<reg>,</reg> stetten
        <lb facs="#facs_1_region_1593465102137_290l8" n="N008"/>koͧf ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> hetten dem <choice>
                    <abbr>egn̄</abbr>
                    <expan>egenanten</expan>
                </choice> <persName ref="per028565">wisen<reg>,</reg> bescheidnen Cuͦnrat Leinbach</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> allen si<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> er<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> <measure>zwein <reg type="star">✳</reg> mùtt kernengelts ewiges zinses</measure> umb <measure>zwentzi<choice>
                        <abbr>gˀ</abbr>
                        <expan>ger</expan>
                    </choice>
        <lb facs="#facs_1_region_1593465102137_290l9" n="N009"/>guldin<reg>,</reg> guͦ<choice>
                        <abbr>tˀ</abbr>
                        <expan>ter</expan>
                    </choice> an gold <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> vollen swe<choice>
                        <abbr>rˀ</abbr>
                        <expan>rer</expan>
                    </choice> an der gewiht</measure>
                <reg>,</reg> der sy oͧch gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gentzlich von im bezalt <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gewert we<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in ir nutz bewendet weren<reg>,</reg>
        <lb facs="#facs_1_region_1593465102137_290l10" n="N010"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gand dis <measure>zwein mùt <choice>
                        <abbr>kˀ</abbr>
                        <expan>ker</expan>
                    </choice>nengeltz</measure> ab dryn matten<reg>,</reg> gelegen ze <placeName ref="loc011686">Niderlentz</placeName>
                <reg>,</reg> hinden in langen matten <add place="above">eychen</add>
                <reg>,</reg> waren ze etlichen zitten <persName ref="per028566">
                    <choice>
                        <abbr>Jē</abbr>
                        <expan>Jen</expan>
                    </choice>nis Huͤbers
        <lb facs="#facs_1_region_1593465102137_290l11" n="N011"/>von Niderlentz</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ab ei<choice>
                    <abbr>nˀ</abbr>
                    <expan>ner</expan>
                </choice> matten<reg>,</reg> wz des vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per028560">Cuͤnis Renolds<reg>,</reg> seli<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
                </persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> hat si gekoͧft <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> <persName ref="per028559">Walt<choice>
                        <abbr>hˀ</abbr>
                        <expan>her</expan>
                    </choice> Renold</persName>
                <reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> wer der koͧff also beschechen<reg>,</reg>
        <lb facs="#facs_1_region_1593465102137_290l12" n="N012"/>
                <choice>
                    <abbr>wen̄</abbr>
                    <expan>wenn</expan>
                </choice> die ob<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per028561">Verena</persName>
                <reg>,</reg> <persName ref="per028563">Hensli</persName> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per028564">Cleͧwi</persName>
                <reg>,</reg> ir kint<reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> all ir erben dem vor<choice>
                    <abbr>gn̄</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per028565">Cuͦn<choice>
                        <abbr>r₎</abbr>
                        <expan>rad</expan>
                    </choice> Leinbach</persName> <choice>
                    <abbr>odˀ</abbr>
                    <expan>oder</expan>
                </choice> si<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> erben jerlich gewer<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> die <choice>
                    <abbr>egn̄</abbr>
