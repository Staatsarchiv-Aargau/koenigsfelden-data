  <text>
    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0358_r"/>
      <p facs="#facs_1_region_1593463788248_127">
        <lb facs="#facs_1_region_1593463788248_127l1" n="N001"/>Allen den<reg>,</reg> die disen brief an sehent <reg type="star">✳</reg> oder hoͤrrent lesen<reg>,</reg> <reg type="star">✳</reg> nu oder her nach<reg>,</reg> kùnden wir<reg>,</reg> <reg type="star">✳</reg> <persName ref="per011915">Uͦlman
        <lb facs="#facs_1_region_1593463788248_127l2" n="N002"/>Truchsetz</persName> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per011916">Albrecht Truchsetz<reg>,</reg> gebruͦder<reg>,</reg> <reg type="star">✳</reg> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wildegg<reg>,</reg> edel knecht</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>jehen offenlich <reg type="star">✳</reg> fùr ùns
        <lb facs="#facs_1_region_1593463788248_127l3" n="N003"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ùnser erben<reg>,</reg> <reg type="star">✳</reg> daz wir <choice>
                    <abbr>vˀ</abbr>
                    <expan>ver</expan>
                </choice>koͧffet hant <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ze koͧffenne hant gegeben recht <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> redlich in eines
        <lb facs="#facs_1_region_1593463788248_127l4" n="N004"/>rechten koͧffes wis<reg>,</reg> <reg type="star">✳</reg> dem wolbescheiden<reg>,</reg> <reg type="star">✳</reg> <persName ref="per017886">Henman <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Wolon<reg>,</reg> <reg type="star">✳</reg> ùnserm oͤhen<reg>,</reg> ein edel knecht</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice>
        <lb facs="#facs_1_region_1593463788248_127l5" n="N005"/>sinen erben <reg type="star">✳</reg> den hindern turn <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> hofstat<reg>,</reg> <reg type="star">✳</reg> so dar zuͦ gehoͤrt<reg>,</reg> <reg type="star">✳</reg> gelegen ze <placeName ref="loc000390">Hapspurg</placeName> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ùnser
        <lb facs="#facs_1_region_1593463788248_127l6" n="N006"/>burglehen waz<reg>,</reg> <reg type="star">✳</reg> mit aller zuͦ gehoͤrt<reg>,</reg> <reg type="star">✳</reg> es si mit holtz<reg>,</reg> mit veld<reg>,</reg> mit wunn<reg>,</reg> mit weid<reg>,</reg> <reg type="star">✳</reg> mit was<choice>
                    <abbr>sˀ</abbr>
                    <expan>ser</expan>
                </choice>
                <reg>,</reg>
        <lb facs="#facs_1_region_1593463788248_127l7" n="N007"/>wasser rùns<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> alle die rech<choice>
                    <abbr>tūg</abbr>
                    <expan>tung</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> so wir ze <placeName ref="loc000390">Hapspurg</placeName> hattent<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> sunderlich <measure>vier mùt rog<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice>
        <lb facs="#facs_1_region_1593463788248_127l8" n="N008"/>geltz</measure> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <measure>zwen mùt haber geltz</measure>
                <reg>,</reg> <reg type="star">✳</reg> die oͧch zuͦ dem <choice>
                    <abbr>vor̄g₎</abbr>
                    <expan>vorgenanten</expan>
                </choice> turn <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> geses gehoͤrrent<reg>,</reg> <reg type="star">✳</reg> umb <measure>fùnfzig
        <lb facs="#facs_1_region_1593463788248_127l9" n="N009"/>
                    <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> sechs phunt <reg type="star">✳</reg> stebler phen<choice>
                        <abbr>nīg</abbr>
                        <expan>ning</expan>
                    </choice>
                </measure>
                <reg>,</reg> <reg type="star">✳</reg> die do ze mal geng <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> geb warent<reg>,</reg> <reg type="star">✳</reg> dero wir gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gentz<reg type="hyphen">-</reg>
        <lb facs="#facs_1_region_1593463788248_127l10" n="N010" break="no"/>klich bericht <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gewert sint <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in ùnser schinberen nutz komen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> bekert sint<reg>,</reg> <reg type="star">✳</reg> von dem
        <lb facs="#facs_1_region_1593463788248_127l11" n="N011"/>
                <choice>
                    <abbr>vor̄g₎</abbr>
                    <expan>vorgenanten</expan>
                </choice> <persName ref="per017886">Henman von Wolon</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> hant oͧch gelopt des <choice>
                    <abbr>vor̄g₎</abbr>
                    <expan>vorgenanten</expan>
                </choice> koͧffes recht wer ze sinde <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> nimer
        <lb facs="#facs_1_region_1593463788248_127l12" n="N012"/>me anze sprechenne<reg>,</reg> <reg type="star">✳</reg> mit worten<reg>,</reg> <reg type="star">✳</reg> noch mit werken<reg>,</reg> <reg type="star">✳</reg> mit reten noch mit geteten<reg>,</reg> <reg type="star">✳</reg> noch mit keinem
        <lb facs="#facs_1_region_1593463788248_127l13" n="N013"/>artikel<reg>,</reg> er si geschriben oder ungeschriben<reg>,</reg> <reg type="star">✳</reg> fùnden oder noch funden moͤcht werden<reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> da mitte
        <lb facs="#facs_1_region_1593463788248_127l14" n="N014"/>der <choice>
                    <abbr>vor̄g₎</abbr>
                    <expan>vorgenante</expan>
                </choice> koͧff gehindert <reg type="star">✳</reg> oder vernichtet <reg type="star">✳</reg> moͤcht werden<reg>,</reg> <reg type="star">✳</reg> daz dem <choice>
                    <abbr>vor̄g₎</abbr>
                    <expan>vorgenanten</expan>
                </choice> <persName ref="per017886">Henman von Wolon</persName>
        <lb facs="#facs_1_region_1593463788248_127l15" n="N015"/>oder sinen erben gebresten moͤcht bringen <reg type="star">✳</reg> oder enphan <reg type="star">✳</reg> an geistlichen <reg type="star">✳</reg> oder an weltlichen gerichte
        <lb facs="#facs_1_region_1593463788248_127l16" n="N016"/>oder anͧ gericht<reg>,</reg> <reg type="star">✳</reg> anͧ alle geverd<reg>,</reg> <reg type="star">✳</reg> da bi waz<reg>,</reg> <reg type="star">✳</reg> <persName ref="per028402">Hans Schoͤnbrot</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per022675">Johans Evinger</persName>
                <reg>,</reg> <reg type="star">✳</reg> <persName ref="per028403">Wernher Schroͤter<reg>,</reg>
        <lb facs="#facs_1_region_1593463788248_127l17" n="N017"/>burger ze Brugg</persName>
                <reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ander erberlùten vil<reg>.</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> des ze einem waren <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> stetten urkùnd <reg type="star">✳</reg> alr der
        <lb facs="#facs_1_region_1593463788248_127l18" n="N018"/>vorgeschribner dingen<reg>,</reg> <reg type="star">✳</reg> so hant wir<reg>,</reg> die obgenanten <reg type="star">✳</reg> <persName ref="per011915">Uͦlman Truchsetz</persName> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> <persName ref="per011916">Albrecht Truchsetz</persName>
                <reg>,</reg>
        <lb facs="#facs_1_region_1593463788248_127l19" n="N019"/>ùnser jetweder sin eigen insigel <reg type="star">✳</reg> gehenket <reg type="star">✳</reg> fùr ùns <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> unser erben <reg type="star">✳</reg> an disen brief<reg>,</reg> <reg type="star">✳</reg> der geben
        <lb facs="#facs_1_region_1593463788248_127l20" n="N020"/>ist ze <placeName ref="loc000195">Brugg</placeName> in der stat<reg>,</reg> <reg type="star">✳</reg> an dem <date when="1371-08-22">nechsten fritag vor sant Bartholomei tag<reg>,</reg> <reg type="star">✳</reg> des heiligen
        <lb facs="#facs_1_region_1593463788248_127l21" n="N021"/>zwoͤlf botten<reg>,</reg> <reg type="star">✳</reg> do man zalt von gottes gebùrt drùzehen hundert jar<reg>,</reg> <reg type="star">✳</reg> dar nach <reg type="star">✳</reg> in dem syben<reg type="hyphen">-</reg>
        <lb facs="#facs_1_region_1593463788248_127l23" n="N022" break="no"/>zigosten <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> eim jarr</date>
                <reg>.</reg> <reg type="star">✳</reg>
