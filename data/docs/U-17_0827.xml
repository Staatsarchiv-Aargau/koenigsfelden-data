    <body>
      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0827_r"/>
      <p facs="#facs_1_TextRegion_1582096977422_141">
        <lb facs="#facs_1_region_1522604772898_13l2" n="N001"/>Staatsarchiv
        <lb facs="#facs_1_region_1522604772898_13l5" n="N002"/>AARGAU</p>
      <p facs="#facs_1_TextRegion_1582096977422_140">
        <lb facs="#facs_1_region_1522604772898_13l3" n="N001"/>Wir<reg>,</reg> <orgName ref="org004687">schult<choice>
                        <abbr>hs̄</abbr>
                        <expan>heiss</expan>
                    </choice> unnd ratt zuͤ Ba<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                    <reg>,</reg> im Ergoͤw</orgName>
                <reg>,</reg> thu<choice>
                    <abbr>ēd</abbr>
                    <expan>end</expan>
                </choice>
        <lb facs="#facs_1_region_1522604772898_13l4" n="N002"/>kund offenlich mit di<choice>
                    <abbr>sm̄</abbr>
                    <expan>sem</expan>
                </choice> brieff<reg>,</reg> das uff hùtt sin datt
        <lb facs="#facs_1_region_1522604772898_13l7" n="N003"/>vor unns in unser raatstu<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> als wir zuͦ ratt geses<choice>
                    <abbr>sn̄</abbr>
                    <expan>sen</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> erschi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice>
        <lb facs="#facs_1_region_1522604772898_13l8" n="N004"/>sind <reg type="star">✳</reg> der fürnaͤm und wissz <reg type="star">✳</reg> <persName ref="per016072">Niclaus Meyer</persName> inna<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> und
        <lb facs="#facs_1_region_1522604772898_13l9" n="N005"/>anstatt<reg>,</reg> ouch als hoffmeis<choice>
                    <abbr>teˀ</abbr>
                    <expan>ter</expan>
                </choice> unser <orgName ref="org002627">gnaͤdi<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice> fro<choice>
                        <abbr>wn̄</abbr>
                        <expan>wen</expan>
                    </choice>
                    <reg>,</reg> aͤptissin
        <lb facs="#facs_1_region_1522604772898_13l10" n="N006"/>und eines conventz zuͤ Küngsfel<choice>
                        <abbr>d₎</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName>
                <reg>,</reg> an ei<choice>
                    <abbr>nē</abbr>
                    <expan>nem</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> ouch der
        <lb facs="#facs_1_region_1522604772898_13l11" n="N007"/>
                <persName ref="per012089">edell<reg>,</reg> vest juncker Heinrich Trucksessz <reg type="star">✳</reg> von Wolhu<choice>
                        <abbr>sn̄</abbr>
                        <expan>sen</expan>
                    </choice>
                    <reg>,</reg> by
        <lb facs="#facs_1_region_1522604772898_13l12" n="N008"/>uns zuͤ Ba<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice> saͤshafft</persName>
                <reg>,</reg> an<choice>
                    <abbr>dnˀ</abbr>
                    <expan>dern</expan>
                </choice> teill<reg>.</reg> <reg type="star">✳</reg> und der nachgen<choice>
                    <abbr>d₎</abbr>
                    <expan>den</expan>
                </choice>
        <lb facs="#facs_1_region_1522604772898_13l13" n="N009"/>sach <reg type="star">✳</reg> hat sich der bestimpt hoffmeis<choice>
                    <abbr>teˀ</abbr>
                    <expan>ter</expan>
                </choice> in na<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice>
                <reg>,</reg> wie
        <lb facs="#facs_1_region_1522604772898_13l14" n="N010"/>ob statt<reg>,</reg> uff den <choice>
                    <abbr>gnt₎</abbr>
                    <expan>genanten</expan>
                </choice> <persName ref="per012089">juncker Heinri<choice>
                        <abbr>chn̄</abbr>
                        <expan>chen</expan>
                    </choice>
                </persName> zuͦ kunntschafft
        <lb facs="#facs_1_region_1522604772898_13l15" n="N011"/>gezo<choice>
                    <abbr>gn̄</abbr>
                    <expan>gen</expan>
                </choice>
                <reg>,</reg> also uff sin beger sagt geruͤerter <persName ref="per012089">juncker
        <lb facs="#facs_1_region_1522604772898_13l16" n="N012"/>Heinrich</persName>
                <reg>,</reg> <reg type="star">✳</reg> wie im woll wüssend sye<reg>,</reg> <reg type="star">✳</reg> das <persName ref="per016074">Walther</persName>
        <lb facs="#facs_1_region_1522604772898_13l17" n="N013"/>und <persName ref="per011836">Thüring <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Halwill<reg>,</reg> die al<choice>
                        <abbr>tn̄</abbr>
                        <expan>ten</expan>
                    </choice>
                    <reg>,</reg> <reg type="star">✳</reg> beid saͤli<choice>
                        <abbr>gē</abbr>
                        <expan>gen</expan>
                    </choice> gedaͤchtnus<reg>,</reg>
        <lb facs="#facs_1_region_1522604772898_13l18" n="N014"/>uff Wildegg sesshafft syend gewe<choice>
                        <abbr>sn̄</abbr>
                        <expan>sen</expan>
                    </choice>
                </persName>
                <reg>,</reg> <reg type="star">✳</reg> habend er mit
        <lb facs="#facs_1_region_1522604772898_13l19" n="N015"/>
                <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> <reg type="star">✳</reg> und sy mit im<reg>,</reg> <reg type="star">✳</reg> sy für sich selbs <reg type="star">✳</reg> und er für sich
        <lb facs="#facs_1_region_1522604772898_13l20" n="N016"/>selbs zuͦ men<choice>
                    <abbr>gē</abbr>
                    <expan>gem</expan>
                </choice> vall gejagt <reg type="star">✳</reg> in den ho<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> und
        <lb facs="#facs_1_region_1522604772898_13l21" n="N017"/>nid<choice>
                    <abbr>rē</abbr>
                    <expan>ren</expan>
                </choice> gerich<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> <reg type="star">✳</reg> und wildban<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> <reg type="star">✳</reg> im <placeName ref="loc009765">Ei<choice>
                        <abbr>gn̄</abbr>
                        <expan>gen</expan>
                    </choice>
                </placeName> und im
        <lb facs="#facs_1_region_1522604772898_13l22" n="N018"/>ampt<reg>,</reg> und der sel<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> gegin al<choice>
                    <abbr>ln̄t</abbr>
                    <expan>lent</expan>
                </choice>hal<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> doch alweg
        <lb facs="#facs_1_region_1522604772898_13l23" n="N019"/>mit urlob<reg>,</reg> wüs<choice>
                    <abbr>sn̄</abbr>
                    <expan>sen</expan>
                </choice> unnd wil<choice>
                    <abbr>ln̄</abbr>
                    <expan>len</expan>
                </choice> einer <orgName ref="org002627">aͤptissin und eines
        <lb facs="#facs_1_region_1522604772898_13l24" n="N020"/>conventz zuͦ Küngsfel<choice>
                        <abbr>dn̄</abbr>
                        <expan>den</expan>
                    </choice>
                </orgName>
                <reg>.</reg> er hab ouch sust nie ge<reg type="lb">¬</reg><lb facs="#facs_1_region_1522604772898_13l25" n="N021" break="no"/>hoͤrt<reg>,</reg> das die benamp<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> <orgName ref="org001293">von Halwil</orgName> <reg type="star">✳</reg> und ye dhei<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice>
                <reg>,</reg>
        <lb facs="#facs_1_region_1522604772898_13l26" n="N022"/>so uff <placeName ref="loc009250">Wildegg</placeName> seshafft gewe<choice>
                    <abbr>sn̄</abbr>
                    <expan>sen</expan>
                </choice> syend<reg>,</reg> <reg type="star">✳</reg> für sich selbs
        <lb facs="#facs_1_region_1522604772898_13l27" n="N023"/>unnd an urlob der <choice>
                    <abbr>gn̄</abbr>
                    <expan>genan</expan>
                </choice>
                <choice>
                    <abbr>t₎</abbr>
                    <expan>ten</expan>
                </choice> fro<choice>
                    <abbr>wn̄</abbr>
                    <expan>wen</expan>
                </choice> solichs ze thuͤnd
        <lb facs="#facs_1_region_1522604772898_13l28" n="N024"/>gerechtikeit gehept habend<reg>.</reg> doch ha<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> die be<reg type="lb">¬</reg><lb facs="#facs_1_region_1522604772898_13l29" n="N025" break="no"/>stimp<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> <orgName ref="org001293">von Halwill</orgName> <reg type="star">✳</reg> sich solicher gstalt und mas<choice>
                    <abbr>sz₎</abbr>
                    <expan>szen</expan>
                </choice>
        <lb facs="#facs_1_region_1522604772898_13l30" n="N026"/>gehal<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice>
                <reg>,</reg> <reg type="star">✳</reg> ob sy im kloster oder ru<choice>
                    <abbr>chn̄</abbr>
                    <expan>chen</expan>
                </choice> zuͤ <placeName ref="loc007695">Kùngsfel<choice>
                        <abbr>d₎</abbr>
                        <expan>den</expan>
                    </choice>
                </placeName>
        <lb facs="#facs_1_region_1522604772898_13l31" n="N027"/>gejagt hettend<reg>,</reg> <reg type="star">✳</reg> meinne er nitt das <choice>
                    <abbr>inē</abbr>
                    <expan>inen</expan>
                </choice> solichs
        <lb facs="#facs_1_region_1522604772898_13l32" n="N028"/>von den berueͤr<choice>
                    <abbr>tn̄</abbr>
                    <expan>ten</expan>
                </choice> fro<choice>
                    <abbr>wn̄</abbr>
                    <expan>wen</expan>
                </choice> <reg type="star">✳</reg> gespert und verseit were
        <lb facs="#facs_1_region_1522604772898_13l33" n="N029"/>wer<choice>
                    <abbr>d₎</abbr>
                    <expan>den</expan>
                </choice>
                <reg>.</reg> diser sag vor unns gethan<reg>,</reg> <reg type="star">✳</reg> begert der
        <lb facs="#facs_1_region_1522604772898_13l34" n="N030"/>
                <choice>
                    <abbr>gn̄t</abbr>
                    <expan>genant</expan>
                </choice> hoffmeis<choice>
                    <abbr>teˀ</abbr>
                    <expan>ter</expan>
                </choice> in obgestimp<choice>
                    <abbr>teˀ</abbr>
                    <expan>ter</expan>
                </choice> gstalt <reg type="star">✳</reg> oùch brieffs<reg>,</reg>
        <lb facs="#facs_1_region_1522604772898_13l35" n="N031"/>der im mit unser statt in gedruck<choice>
                    <abbr>tm̄</abbr>
                    <expan>tem</expan>
                </choice> secret
        <lb facs="#facs_1_region_1522604772898_13l36" n="N032"/>insigell versigelt zuͤ ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
                </choice> erkent ist<reg>,</reg> ge<choice>
                    <abbr>bn̄</abbr>
                    <expan>ben</expan>
