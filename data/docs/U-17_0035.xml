      <pb facs="#facs_1" n="recto" xml:id="img_0001" source="U-17_0035_r"/>
      <p facs="#facs_1_region_1514993766247_5">
        <lb facs="#facs_1_region_1514993766247_5l1" n="N001"/>Ich<reg>,</reg> <reg type="star">✳</reg> <persName ref="per007364">Anne</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per020725">graven Huges saͤligen êwirtinne von Montfort</persName><reg>,</reg> tuͦn kunt und vergihe offenlich an disem briefe allen den<reg>,</reg> die
        <lb facs="#facs_1_region_1514993766247_5l2" n="N002"/>in ansehent<reg>,</reg> <reg type="star">✳</reg> lesent <reg type="star">✳</reg> oder hoͤrent lesen<reg>,</reg> <reg type="star">✳</reg> daz ich mit guͦter betrahtunge<reg>,</reg> mit verdahtem sinne <reg type="star">✳</reg> und mit rate willen <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> verhengenuͤst
        <lb facs="#facs_1_region_1514993766247_5l3" n="N003"/>miner kinde<reg>,</reg> <persName ref="per020727">gra<choice>
                        <abbr>vē</abbr>
                        <expan>ven</expan>
                    </choice> <reg type="star">✳</reg> Fridriches</persName><reg>,</reg> <reg type="star">✳</reg> <persName ref="per020728">gra<choice>
                        <abbr>vē</abbr>
                        <expan>ven</expan>
                    </choice> <reg type="star">✳</reg> Huges</persName> und ander miner erben <reg type="star">✳</reg> han gegeben ze kovende reht und redelich den <orgName ref="org002627">erberen
        <lb facs="#facs_1_region_1514993766247_5l4" n="N004"/>vroͧwen<reg>,</reg> der <reg type="star">✳</reg> <reg type="star">✳</reg> abtissen und dem convent des klosters ze Kuͤngesvelt<reg>,</reg> Sant Claren ordens</orgName><reg>,</reg> alles daz guͦt<reg>,</reg> daz ich in dem <placeName ref="loc009765">Aigen</placeName>
        <lb facs="#facs_1_region_1514993766247_5l5" n="N005"/>hatte<reg>,</reg> dem man sprichet daz <placeName ref="loc012980">Guͦt <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Veringen</placeName> ald daz guͦt ze <placeName ref="loc008543">Altenburch</placeName><reg>,</reg> <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> swaz dar zuͦ gehoͤret<reg>,</reg> gesuͤchtes <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ungesuͦchtes<reg>,</reg>
        <lb facs="#facs_1_region_1514993766247_5l6" n="N006"/>fuͤr lediges <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> rehtes aigen<reg>,</reg> <reg type="star">✳</reg> dez ich reh<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> erbe bin <reg type="star">✳</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> mich angerbt ist <choice>
                    <abbr>vō</abbr>
                    <expan>von</expan>
                </choice> miner vroͧwen seligen<reg>,</reg> miner muͦter<reg>,</reg> <persName ref="per007140">vro <reg type="star">✳</reg> Verenun <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1514993766247_5l7" n="N007"/>von Veringen</persName><reg>,</reg> umb <reg type="star">✳</reg> <measure>drithalbhundert <reg type="star">✳</reg> march guͦtes und loͤtiges silbers <comment comment="Verschreiber ohne Streichung.">k</comment> Zuͤrichs gweges</measure>
                <reg>.</reg> des sel<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> guͣtes ich gar <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> gaͤnzlich
        <lb facs="#facs_1_region_1514993766247_5l8" n="N008"/>gwert bin <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> in mi<choice>
                    <abbr>nē</abbr>
                    <expan>nen</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> miner kinde nutz ko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> ist<reg>.</reg> <reg type="star">✳</reg> und han den selben <orgName ref="org002627">vroͧ<choice>
                        <abbr>wē</abbr>
                        <expan>wen</expan>
                    </choice> <choice>
                        <abbr>un̄</abbr>
                        <expan>und</expan>
                    </choice> dem convent des klosters ze Kuͤnegesvelt</orgName>
        <lb facs="#facs_1_region_1514993766247_5l9" n="N009"/>daz selbe guͦt gevertiget und bestaetet mit mines vogtes hant<reg>,</reg> <persName ref="per020730">gra<choice>
                        <abbr>vē</abbr>
                        <expan>ven</expan>
                    </choice> <reg type="star">✳</reg> Uͦlrichs <reg type="star">✳</reg> <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Montfort</persName><reg>,</reg> der mir vor gerihte mit reh<choice>
                    <abbr>tˀ</abbr>
                    <expan>ter</expan>
                </choice> urtail ze
        <lb facs="#facs_1_region_1514993766247_5l10" n="N010"/>vogt gege<choice>
                    <abbr>bē</abbr>
                    <expan>ben</expan>
                </choice> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ertailet wart<reg>.</reg> <reg type="star">✳</reg> und sol ich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> miniu kint<reg>,</reg> diu vorgnan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice><reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> min erben des guͦtes gwer sin nach reht fuͤr lediges
        <lb facs="#facs_1_region_1514993766247_5l11" n="N011"/>aigen an allen <choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> stetten<reg>,</reg> da es der <orgName ref="org002627">convent des vorgnan<choice>
                        <abbr>tē</abbr>
                        <expan>ten</expan>
                    </choice> klos<choice>
                        <abbr>tˀs</abbr>
                        <expan>ters</expan>
                    </choice> ze Kuͤnegesvelt</orgName> bedarf oder notdurftig ist<reg>.</reg> <reg type="star">✳</reg> ich enzihe mich och
        <lb facs="#facs_1_region_1514993766247_5l12" n="N012"/>
                <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> miniu kint<reg>,</reg> diu vorgnan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice><reg>,</reg> <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> unser erben aller der ansprache<reg>,</reg> mit der wir immer widerko<choice>
                    <abbr>mē</abbr>
                    <expan>men</expan>
                </choice> moͤhtent ze dem vorgnan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> guͤte
        <lb facs="#facs_1_region_1514993766247_5l13" n="N013"/>vor gaistli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> oder weltli<choice>
                    <abbr>chē</abbr>
                    <expan>chen</expan>
                </choice> gerihte<reg>.</reg> <reg type="star">✳</reg> ditz verjehen wir alle gmainlich <choice>
                    <abbr>un̄</abbr>
                    <expan>und</expan>
                </choice> ainmuͦtliche offenlich an disem briefe<reg>.</reg> ich<reg>,</reg> <persName ref="per020730">graf <reg type="star">✳</reg> Uͦlrich <reg type="star">✳</reg>
        <lb facs="#facs_1_region_1514993766247_5l14" n="N014"/>
                    <choice>
                        <abbr>vō</abbr>
                        <expan>von</expan>
                    </choice> Montfort</persName><reg>,</reg> kunde och an disem briefe<reg>,</reg> daz ich <choice>
                    <abbr>dē</abbr>
                    <expan>den</expan>
                </choice> vorgnan<choice>
                    <abbr>tē</abbr>
                    <expan>ten</expan>
                </choice> kof bestaͤtet han mit miner hant<reg>,</reg> wan ich der vorgnan<choice>
